{"data":[{"book":"UJobe","chapter":"33","verse":"1","text":"“Khathesi-ke, Jobe, lalela amazwi ami;"},{"book":"UJobe","chapter":"33","verse":"2","text":"Sengizawuvula umlomo wami;"},{"book":"UJobe","chapter":"33","verse":"3","text":"Amazwi ami aphuma enhliziyweni eqotho;"},{"book":"UJobe","chapter":"33","verse":"4","text":"UMoya kaNkulunkulu wangenza;"},{"book":"UJobe","chapter":"33","verse":"5","text":"Ngiphendula nxa usenelisa;"},{"book":"UJobe","chapter":"33","verse":"6","text":"Lami nginjengawe phambi kukaNkulunkulu;"},{"book":"UJobe","chapter":"33","verse":"7","text":"Ukungesaba kwakho makungakwethusi,"},{"book":"UJobe","chapter":"33","verse":"8","text":"Kodwa ukhulumile ngisizwa,"},{"book":"UJobe","chapter":"33","verse":"9","text":"‘Ngimsulwa njalo kangilasono;"},{"book":"UJobe","chapter":"33","verse":"10","text":"Ikanti uNkulunkulu ubone okusolekayo kimi;"},{"book":"UJobe","chapter":"33","verse":"11","text":"Uyazikhina inyawo zami ngezibopho;"},{"book":"UJobe","chapter":"33","verse":"12","text":"Kodwa ngiyakutshela ukuthi lapha uyalahleka,"},{"book":"UJobe","chapter":"33","verse":"13","text":"Kungani umsola"},{"book":"UJobe","chapter":"33","verse":"14","text":"Ngempela uNkulunkulu uyakhuluma,"},{"book":"UJobe","chapter":"33","verse":"15","text":"Kungaba ngephupho, kube ngombono ebusuku"},{"book":"UJobe","chapter":"33","verse":"16","text":"angakhuluma ezindlebeni zabo,"},{"book":"UJobe","chapter":"33","verse":"17","text":"ukuze aguqule umuntu ekwenzeni okubi"},{"book":"UJobe","chapter":"33","verse":"18","text":"ukulondoloza umphefumulo wakhe ungayi egodini,"},{"book":"UJobe","chapter":"33","verse":"19","text":"Loba umuntu angajeziswa ngobuhlungu esembhedeni wakhe,"},{"book":"UJobe","chapter":"33","verse":"20","text":"aze ezwe umzimba wakhe usunengwa yikudla"},{"book":"UJobe","chapter":"33","verse":"21","text":"Inyama yakhe iyacikizeka iphele,"},{"book":"UJobe","chapter":"33","verse":"22","text":"Umoya wakhe uyasondela egodini,"},{"book":"UJobe","chapter":"33","verse":"23","text":"Kodwa nxa kulengilosi eceleni kwakhe,"},{"book":"UJobe","chapter":"33","verse":"24","text":"ukuba lomusa kuye isithi,"},{"book":"UJobe","chapter":"33","verse":"25","text":"bese inyama yakhe ivuselelwa njengeyomntwana;"},{"book":"UJobe","chapter":"33","verse":"26","text":"Uyakhuleka kuNkulunkulu amthokozele,"},{"book":"UJobe","chapter":"33","verse":"27","text":"Kuthi-ke abesebuya ebantwini athi,"},{"book":"UJobe","chapter":"33","verse":"28","text":"Wawuhlenga umphefumulo wami ukuze ungayi egodini,"},{"book":"UJobe","chapter":"33","verse":"29","text":"UNkulunkulu uyazenza zonke lezizinto emuntwini"},{"book":"UJobe","chapter":"33","verse":"30","text":"ukubuyisa umphefumulo wakhe ungayi egodini,"},{"book":"UJobe","chapter":"33","verse":"31","text":"Nanzelela, Jobe, ulalele kimi;"},{"book":"UJobe","chapter":"33","verse":"32","text":"Nxa ulolutho lokutsho, ngiphendula;"},{"book":"UJobe","chapter":"33","verse":"33","text":"Kodwa nxa lungekho lalela kimi;"}]}