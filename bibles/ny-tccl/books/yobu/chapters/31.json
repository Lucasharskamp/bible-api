{"data":[{"book":"Yobu","chapter":"31","verse":"1","text":"“Ndinachita pangano ndi maso anga"},{"book":"Yobu","chapter":"31","verse":"2","text":"Kodi Mulungu kumwambako wandisungira zotani?"},{"book":"Yobu","chapter":"31","verse":"3","text":"Kodi si chiwonongeko kwa anthu oyipa,"},{"book":"Yobu","chapter":"31","verse":"4","text":"Kodi Mulungu saona zochita zanga,"},{"book":"Yobu","chapter":"31","verse":"5","text":"“Ngati ndachita zinthu mwachiphamaso,"},{"book":"Yobu","chapter":"31","verse":"6","text":"Mulungu andiyeze ndi muyeso wake wolungama"},{"book":"Yobu","chapter":"31","verse":"7","text":"ngati mayendedwe anga asempha njira,"},{"book":"Yobu","chapter":"31","verse":"8","text":"Pamenepo ena adye zimene ndinadzala,"},{"book":"Yobu","chapter":"31","verse":"9","text":"“Ngati mtima wanga unakopekapo ndi mkazi,"},{"book":"Yobu","chapter":"31","verse":"10","text":"pamenepo mkazi wanga aphikire mwamuna wina chakudya,"},{"book":"Yobu","chapter":"31","verse":"11","text":"Pakuti zimenezo zikanakhala zochititsa manyazi,"},{"book":"Yobu","chapter":"31","verse":"12","text":"Ndi moto umene umayaka mpaka chiwonongeko;"},{"book":"Yobu","chapter":"31","verse":"13","text":"“Ngati ndinkapondereza mlandu wa akapolo anga aamuna kapena aakazi,"},{"book":"Yobu","chapter":"31","verse":"14","text":"ndidzatani pamene Mulungu adzanditsutsa?"},{"book":"Yobu","chapter":"31","verse":"15","text":"Kodi amene anapanga ine mʼmimba mwa amayi anga si yemwe anapanganso iwo?"},{"book":"Yobu","chapter":"31","verse":"16","text":"“Ngati ndinawamana aumphawi zinthu zimene ankazikhumba,"},{"book":"Yobu","chapter":"31","verse":"17","text":"ngati chakudya changa ndinadya ndekha,"},{"book":"Yobu","chapter":"31","verse":"18","text":"chonsechotu kuyambira unyamata wanga ndinamulera monga abambo ake,"},{"book":"Yobu","chapter":"31","verse":"19","text":"ngati ndinaona wina aliyense akuzunzika ndi usiwa,"},{"book":"Yobu","chapter":"31","verse":"20","text":"ndipo ngati iyeyo sananditamandepo"},{"book":"Yobu","chapter":"31","verse":"21","text":"ngati ndinaopsezapo mwana wamasiye,"},{"book":"Yobu","chapter":"31","verse":"22","text":"pamenepo phewa langa lipokonyeke,"},{"book":"Yobu","chapter":"31","verse":"23","text":"Popeza ine ndinaopa kwambiri chiwonongeko chochokera kwa Mulungu,"},{"book":"Yobu","chapter":"31","verse":"24","text":"“Ngati ndinayika mtima wanga pa chuma"},{"book":"Yobu","chapter":"31","verse":"25","text":"ngati ndinakondwera chifukwa choti chuma changa chinali chambiri,"},{"book":"Yobu","chapter":"31","verse":"26","text":"ngati pamene ndinaona dzuwa likuwala,"},{"book":"Yobu","chapter":"31","verse":"27","text":"ndipo kuti mtima wanga unakopeka nazo"},{"book":"Yobu","chapter":"31","verse":"28","text":"pamenepo zimenezinso zikanakhala machimo oti ndilangidwe nawo,"},{"book":"Yobu","chapter":"31","verse":"29","text":"“Ngati ndinasangalala ndi kuwonongeka kwa mdani wanga,"},{"book":"Yobu","chapter":"31","verse":"30","text":"ine sindinachimwe ndi pakamwa panga"},{"book":"Yobu","chapter":"31","verse":"31","text":"ngati anthu amene ndimakhala nawo mʼnyumba mwanga sananenepo kuti,"},{"book":"Yobu","chapter":"31","verse":"32","text":"Komatu mlendo sindinamusiye pa msewu usiku wonse,"},{"book":"Yobu","chapter":"31","verse":"33","text":"ngati ndinabisa tchimo langa monga amachitira anthu ena,"},{"book":"Yobu","chapter":"31","verse":"34","text":"chifukwa choopa gulu la anthu,"},{"book":"Yobu","chapter":"31","verse":"35","text":"“Aa, pakanakhala wina wondimva!"},{"book":"Yobu","chapter":"31","verse":"36","text":"Ndithu ine ndikanakoleka kalatayo pa phewa langa,"},{"book":"Yobu","chapter":"31","verse":"37","text":"Ndikanamufotokozera zonse zimene ndinachita;"},{"book":"Yobu","chapter":"31","verse":"38","text":"“Ngati minda yanga ikulira monditsutsa ine"},{"book":"Yobu","chapter":"31","verse":"39","text":"ngati ndinadya za mʼminda mwake osapereka ndalama"},{"book":"Yobu","chapter":"31","verse":"40","text":"pamenepo mʼmindamo mumere namsongole mʼmalo mwa tirigu"}]}