{"data":[{"book":"Masalimo","chapter":"77","verse":"1","text":"Ndinafuwulira Mulungu kupempha thandizo;"},{"book":"Masalimo","chapter":"77","verse":"2","text":"Pamene ndinali pa masautso ndinafunafuna Ambuye;"},{"book":"Masalimo","chapter":"77","verse":"3","text":"Ndinakumbukira Inu Mulungu, ndipo ndinabuwula;"},{"book":"Masalimo","chapter":"77","verse":"4","text":"Munagwira zikope zanga kuti ndisagone"},{"book":"Masalimo","chapter":"77","verse":"5","text":"Ndinaganizira za masiku akale,"},{"book":"Masalimo","chapter":"77","verse":"6","text":"Ndinakumbukira nyimbo zanga usiku."},{"book":"Masalimo","chapter":"77","verse":"7","text":"“Kodi Ambuye adzatikana mpaka muyaya?"},{"book":"Masalimo","chapter":"77","verse":"8","text":"Kodi Chikondi chake chosatha chija chatheratu?"},{"book":"Masalimo","chapter":"77","verse":"9","text":"Kodi Mulungu wayiwala kukhala wokoma mtima?"},{"book":"Masalimo","chapter":"77","verse":"10","text":"Ndipo ndinaganiza, “Pa izi ine ndidzapemphanso:"},{"book":"Masalimo","chapter":"77","verse":"11","text":"Ine ndidzakumbukira ntchito za Yehova;"},{"book":"Masalimo","chapter":"77","verse":"12","text":"Ndidzakumbukira ntchito zanu"},{"book":"Masalimo","chapter":"77","verse":"13","text":"Njira zanu Mulungu ndi zoyera."},{"book":"Masalimo","chapter":"77","verse":"14","text":"Inu ndinu Mulungu wochita zodabwitsa;"},{"book":"Masalimo","chapter":"77","verse":"15","text":"Ndi dzanja lanu lamphamvu munawombola anthu anu,"},{"book":"Masalimo","chapter":"77","verse":"16","text":"Madzi anakuonani Mulungu,"},{"book":"Masalimo","chapter":"77","verse":"17","text":"Mitambo inakhuthula madzi ake pansi,"},{"book":"Masalimo","chapter":"77","verse":"18","text":"Bingu lanu linamveka mʼmphepo ya kamvuluvulu,"},{"book":"Masalimo","chapter":"77","verse":"19","text":"Njira yanu inadutsa pa nyanja,"},{"book":"Masalimo","chapter":"77","verse":"20","text":"Inu munatsogolera anthu anu ngati gulu la nkhosa"}]}