{"data":[{"book":"Mapisarema","chapter":"106","verse":"1","text":"Rumbidzai Jehovha."},{"book":"Mapisarema","chapter":"106","verse":"2","text":"Ndianiko angaparidza mabasa aJehovha esimba,"},{"book":"Mapisarema","chapter":"106","verse":"3","text":"Vakaropafadzwa vanochengeta kururamisira,"},{"book":"Mapisarema","chapter":"106","verse":"4","text":"Ndirangarirei, imi Jehovha, pamunoratidza nyasha kuvanhu venyu,"},{"book":"Mapisarema","chapter":"106","verse":"5","text":"kuti ndifadzwe nokubudirira kwavasanangurwa venyu,"},{"book":"Mapisarema","chapter":"106","verse":"6","text":"Takatadza, sezvakaita madzibaba edu;"},{"book":"Mapisarema","chapter":"106","verse":"7","text":"Madzibaba edu paakanga ari muIjipiti,"},{"book":"Mapisarema","chapter":"106","verse":"8","text":"Kunyange zvakadaro akavaponesa nokuda kwezita rake,"},{"book":"Mapisarema","chapter":"106","verse":"9","text":"Akarayira Gungwa Dzvuku iro rikapwa;"},{"book":"Mapisarema","chapter":"106","verse":"10","text":"Akavaponesa kubva muruoko rwomuvengi;"},{"book":"Mapisarema","chapter":"106","verse":"11","text":"Mvura zhinji yakafukidza vadzivisi vavo;"},{"book":"Mapisarema","chapter":"106","verse":"12","text":"Ipapo vakatenda vimbiso dzake"},{"book":"Mapisarema","chapter":"106","verse":"13","text":"Asi vakakurumidza kukanganwa zvaakanga aita,"},{"book":"Mapisarema","chapter":"106","verse":"14","text":"Havana kuzvidzora pakukara kwavo mugwenga;"},{"book":"Mapisarema","chapter":"106","verse":"15","text":"Saka akavapa zvavakakumbira,"},{"book":"Mapisarema","chapter":"106","verse":"16","text":"Vakaitira Mozisi godo mumusasa,"},{"book":"Mapisarema","chapter":"106","verse":"17","text":"Nyika yakazaruka ikamedza Dhatani;"},{"book":"Mapisarema","chapter":"106","verse":"18","text":"Moto wakapfuta pakati pavateveri vavo;"},{"book":"Mapisarema","chapter":"106","verse":"19","text":"Vakaumba mhuru paHorebhi,"},{"book":"Mapisarema","chapter":"106","verse":"20","text":"Vakatsinhanisa kubwinya kwavo"},{"book":"Mapisarema","chapter":"106","verse":"21","text":"Vakakanganwa Mwari akavaponesa,"},{"book":"Mapisarema","chapter":"106","verse":"22","text":"mabasa anoshamisa munyika yaHamu"},{"book":"Mapisarema","chapter":"106","verse":"23","text":"Saka akati, achavaparadza,"},{"book":"Mapisarema","chapter":"106","verse":"24","text":"Ipapo vakazvidza nyika inofadza;"},{"book":"Mapisarema","chapter":"106","verse":"25","text":"Vakanyunyuta vari mumatende"},{"book":"Mapisarema","chapter":"106","verse":"26","text":"Saka akapika akasimudza ruoko rwake"},{"book":"Mapisarema","chapter":"106","verse":"27","text":"achaparadza zvizvarwa zvavo pakati pendudzi,"},{"book":"Mapisarema","chapter":"106","verse":"28","text":"Vakazvibatanidza pamwe chete naBhaari wePeori,"},{"book":"Mapisarema","chapter":"106","verse":"29","text":"Vakatsamwisa Jehovha namabasa avo akaipa,"},{"book":"Mapisarema","chapter":"106","verse":"30","text":"Asi Finehasi akasimuka,"},{"book":"Mapisarema","chapter":"106","verse":"31","text":"Izvozvo zvakanzi kwaari kururama"},{"book":"Mapisarema","chapter":"106","verse":"32","text":"Vakatsamwisa Jehovha pamvura zhinji yeMeribha,"},{"book":"Mapisarema","chapter":"106","verse":"33","text":"nokuti vakamukira Mweya waMwari,"},{"book":"Mapisarema","chapter":"106","verse":"34","text":"Havana kuparadza marudzi"},{"book":"Mapisarema","chapter":"106","verse":"35","text":"asi vakavhengana nendudzi,"},{"book":"Mapisarema","chapter":"106","verse":"36","text":"Vakanamata zvifananidzo zvavo,"},{"book":"Mapisarema","chapter":"106","verse":"37","text":"Vakabayira vanakomana vavo"},{"book":"Mapisarema","chapter":"106","verse":"38","text":"Vakateura ropa risina mhosva,"},{"book":"Mapisarema","chapter":"106","verse":"39","text":"Vakazvisvibisa nezvavakaita;"},{"book":"Mapisarema","chapter":"106","verse":"40","text":"Naizvozvo Jehovha akatsamwira vanhu vake,"},{"book":"Mapisarema","chapter":"106","verse":"41","text":"Akavaisa mumaoko endudzi,"},{"book":"Mapisarema","chapter":"106","verse":"42","text":"Vavengi vavo vakavadzvinyirira,"},{"book":"Mapisarema","chapter":"106","verse":"43","text":"Akavarwira kakawanda,"},{"book":"Mapisarema","chapter":"106","verse":"44","text":"Asi akatarisa kutambudzika kwavo"},{"book":"Mapisarema","chapter":"106","verse":"45","text":"akarangarira sungano yake nokuda kwavo,"},{"book":"Mapisarema","chapter":"106","verse":"46","text":"Akaita kuti vanzwirwe"},{"book":"Mapisarema","chapter":"106","verse":"47","text":"Tiponesei, imi Jehovha Mwari wedu,"},{"book":"Mapisarema","chapter":"106","verse":"48","text":"Jehovha ngaarumbidzwe, iye Mwari waIsraeri,"}]}