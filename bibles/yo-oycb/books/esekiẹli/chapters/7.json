{"data":[{"book":"Esekiẹli","chapter":"7","verse":"1","text":"Ọ̀rọ̀ Olúwa tọ̀ mí wá:"},{"book":"Esekiẹli","chapter":"7","verse":"2","text":"“Ọmọ ènìyàn, báyìí ni Olúwa Olódùmarè wí sí ilé Israẹli:"},{"book":"Esekiẹli","chapter":"7","verse":"3","text":"Òpin tí dé sí ọ báyìí,"},{"book":"Esekiẹli","chapter":"7","verse":"4","text":"Ojú mi kò ní i dá ọ sì,"},{"book":"Esekiẹli","chapter":"7","verse":"5","text":"“Èyí ni Olúwa Olódùmarè wí:"},{"book":"Esekiẹli","chapter":"7","verse":"6","text":"Òpin ti dé!"},{"book":"Esekiẹli","chapter":"7","verse":"7","text":"Ìparun ti dé sórí rẹ,"},{"book":"Esekiẹli","chapter":"7","verse":"8","text":"Mo ṣetán láti tú ìbínú gbígbóná mi lé ọ lórí"},{"book":"Esekiẹli","chapter":"7","verse":"9","text":"Ojú mi kò ní i dá ọ sí,"},{"book":"Esekiẹli","chapter":"7","verse":"10","text":"“ ‘Ọjọ́ náà dé!"},{"book":"Esekiẹli","chapter":"7","verse":"11","text":"Ìwà ipá ti di ọ̀pá ìwà búburú;"},{"book":"Esekiẹli","chapter":"7","verse":"12","text":"Àkókò náà dé!"},{"book":"Esekiẹli","chapter":"7","verse":"13","text":"Nítorí pé òǹtajà kì yóò rí gbà padà"},{"book":"Esekiẹli","chapter":"7","verse":"14","text":"“ ‘Wọ́n ti fọn ìpè ogun,"},{"book":"Esekiẹli","chapter":"7","verse":"15","text":"Idà wà ní ìta,"},{"book":"Esekiẹli","chapter":"7","verse":"16","text":"Gbogbo àwọn tí ó bọ́ nínú wọn yóò sálà,"},{"book":"Esekiẹli","chapter":"7","verse":"17","text":"Gbogbo ọwọ́ yóò rọ,"},{"book":"Esekiẹli","chapter":"7","verse":"18","text":"Wọn yóò wọ aṣọ ọ̀fọ̀,"},{"book":"Esekiẹli","chapter":"7","verse":"19","text":"“ ‘Wọn yóò dà fàdákà wọn sí ojú pópó,"},{"book":"Esekiẹli","chapter":"7","verse":"20","text":"Wọ́n ń ṣe ìgbéraga pẹ̀lú ohun ọ̀ṣọ́ wọn tí ó lẹ́wà,"},{"book":"Esekiẹli","chapter":"7","verse":"21","text":"Èmi yóò sì fi àwọn nǹkan wọ̀nyí ṣe ìjẹ fún àjèjì"},{"book":"Esekiẹli","chapter":"7","verse":"22","text":"Èmi yóò gbójú mi kúrò lára wọn,"},{"book":"Esekiẹli","chapter":"7","verse":"23","text":"“ ‘Rọ ẹ̀wọ̀n irin!"},{"book":"Esekiẹli","chapter":"7","verse":"24","text":"Èmi yóò mú kí orílẹ̀-èdè ti ó búburú jùlọ"},{"book":"Esekiẹli","chapter":"7","verse":"25","text":"Nígbà tí ìpayà bá dé,"},{"book":"Esekiẹli","chapter":"7","verse":"26","text":"Wàhálà lórí wàhálà yóò dé,"},{"book":"Esekiẹli","chapter":"7","verse":"27","text":"Ọba yóò ṣọ̀fọ̀,"}]}