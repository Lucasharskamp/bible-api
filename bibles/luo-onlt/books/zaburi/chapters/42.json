{"data":[{"book":"Zaburi","chapter":"42","verse":"1","text":"Mana kaka mwanda gamo yweyo kamanyo kuonde ma pi nitie,"},{"book":"Zaburi","chapter":"42","verse":"2","text":"Chunya riyo oloyo mar dwaro Nyasaye."},{"book":"Zaburi","chapter":"42","verse":"3","text":"Aseywak mi pi wang始a osechalo chiemba"},{"book":"Zaburi","chapter":"42","verse":"4","text":"Aparo gigi duto"},{"book":"Zaburi","chapter":"42","verse":"5","text":"Ang始o momiyo ikuyo, yaye chunya?"},{"book":"Zaburi","chapter":"42","verse":"6","text":"Chunya lit;"},{"book":"Zaburi","chapter":"42","verse":"7","text":"Kut luongore gi kut"},{"book":"Zaburi","chapter":"42","verse":"8","text":"Jehova Nyasaye chiko herane godiechieng始,"},{"book":"Zaburi","chapter":"42","verse":"9","text":"Awacho ni Nyasaye ma lwandana niya,"},{"book":"Zaburi","chapter":"42","verse":"10","text":"Chokena tho omako kendo rama malit"},{"book":"Zaburi","chapter":"42","verse":"11","text":"Ang始o momiyo ikuyo, yaye chunya?"}]}