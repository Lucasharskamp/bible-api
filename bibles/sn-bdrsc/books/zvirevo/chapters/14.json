{"data":[{"book":"Zvirevo","chapter":"14","verse":"1","text":"Mukadzi akachenjera anovaka imba yake,"},{"book":"Zvirevo","chapter":"14","verse":"2","text":"Uyo ano mufambiro wakarurama anotya Jehovha,"},{"book":"Zvirevo","chapter":"14","verse":"3","text":"Kutaura kwebenzi kunouyisa shamhu kumusana kwaro,"},{"book":"Zvirevo","chapter":"14","verse":"4","text":"Kana pasina nzombe, chidyiro hachina chinhu,"},{"book":"Zvirevo","chapter":"14","verse":"5","text":"Chapupu chechokwadi hachinyengeri,"},{"book":"Zvirevo","chapter":"14","verse":"6","text":"Mudadi anotsvaka uchenjeri asi anohushaya,"},{"book":"Zvirevo","chapter":"14","verse":"7","text":"Gara kure nomunhu benzi,"},{"book":"Zvirevo","chapter":"14","verse":"8","text":"Uchenjeri hwomunhu akangwara ndiko kunzwisisa nzira dzake,"},{"book":"Zvirevo","chapter":"14","verse":"9","text":"Mapenzi anoseka kutendeuka pazvivi,"},{"book":"Zvirevo","chapter":"14","verse":"10","text":"Mwoyo mumwe nomumwe unoziva kutambudzika kwawo,"},{"book":"Zvirevo","chapter":"14","verse":"11","text":"Imba yeakaipa ichaparadzwa,"},{"book":"Zvirevo","chapter":"14","verse":"12","text":"Kune nzira inoita seyakanaka kumunhu,"},{"book":"Zvirevo","chapter":"14","verse":"13","text":"Kunyange mukuseka mwoyo unogona kurwadziwa,"},{"book":"Zvirevo","chapter":"14","verse":"14","text":"Vasingatendi vachapiwa mubayiro wakazara wenzira dzavo,"},{"book":"Zvirevo","chapter":"14","verse":"15","text":"Munhu asina mano anongotenda zvose zvose,"},{"book":"Zvirevo","chapter":"14","verse":"16","text":"Munhu akachenjera anotya Jehovha uye anovenga zvakaipa,"},{"book":"Zvirevo","chapter":"14","verse":"17","text":"Munhu anokurumidza kutsamwa anoita zvinhu zvoupenzi,"},{"book":"Zvirevo","chapter":"14","verse":"18","text":"Vasina mano vachagara nhaka youpenzi,"},{"book":"Zvirevo","chapter":"14","verse":"19","text":"Vanhu vakaipa vachagwadama pamberi pavakanaka,"},{"book":"Zvirevo","chapter":"14","verse":"20","text":"Varombo vanoraswa kunyange navavakidzani vavo,"},{"book":"Zvirevo","chapter":"14","verse":"21","text":"Uyo anozvidza muvakidzani wake anotadza,"},{"book":"Zvirevo","chapter":"14","verse":"22","text":"Ko, vanoronga zvakaipa havarasiki here?"},{"book":"Zvirevo","chapter":"14","verse":"23","text":"Kushanda nesimba kwose kunopa mubayiro,"},{"book":"Zvirevo","chapter":"14","verse":"24","text":"Upfumi hwavakachenjera ikorona yavo,"},{"book":"Zvirevo","chapter":"14","verse":"25","text":"Chapupu chechokwadi chinoponesa upenyu,"},{"book":"Zvirevo","chapter":"14","verse":"26","text":"Uyo anotya Jehovha ane nhare yakasimba,"},{"book":"Zvirevo","chapter":"14","verse":"27","text":"Kutya Jehovha ndicho chitubu choupenyu,"},{"book":"Zvirevo","chapter":"14","verse":"28","text":"Mukuwanda kwavanhu ndimo mune ukuru hwamambo,"},{"book":"Zvirevo","chapter":"14","verse":"29","text":"Munhu ane mwoyo murefu anonzwisisa,"},{"book":"Zvirevo","chapter":"14","verse":"30","text":"Mwoyo wakagadzikana unopa upenyu kumuviri,"},{"book":"Zvirevo","chapter":"14","verse":"31","text":"Uyo anomanikidza varombo anoratidza kuti anozvidza Musiki wavo,"},{"book":"Zvirevo","chapter":"14","verse":"32","text":"Kana njodzi yauya, akaipa anowisirwa pasi,"},{"book":"Zvirevo","chapter":"14","verse":"33","text":"Uchenjeri hunogara mumwoyo yavanohunzvera"},{"book":"Zvirevo","chapter":"14","verse":"34","text":"Kururama kunosimudzira rudzi,"},{"book":"Zvirevo","chapter":"14","verse":"35","text":"Mambo anofarira muranda akachenjera,"}]}