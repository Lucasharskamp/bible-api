{"data":[{"book":"Zaburi","chapter":"78","verse":"1","text":"Enyi watu wangu, sikieni mafundisho yangu,"},{"book":"Zaburi","chapter":"78","verse":"2","text":"Nitafungua kinywa changu kwa mafumbo,"},{"book":"Zaburi","chapter":"78","verse":"3","text":"yale ambayo tuliyasikia na kuyajua,"},{"book":"Zaburi","chapter":"78","verse":"4","text":"Hatutayaficha kwa watoto wao;"},{"book":"Zaburi","chapter":"78","verse":"5","text":"Aliagiza amri kwa Yakobo"},{"book":"Zaburi","chapter":"78","verse":"6","text":"ili kizazi kijacho kizijue,"},{"book":"Zaburi","chapter":"78","verse":"7","text":"Ndipo wangeweka tumaini lao kwa Mungu,"},{"book":"Zaburi","chapter":"78","verse":"8","text":"Ili wasifanane na baba zao,"},{"book":"Zaburi","chapter":"78","verse":"9","text":"Watu wa Efraimu, ingawa walijifunga pinde,"},{"book":"Zaburi","chapter":"78","verse":"10","text":"Hawakulishika agano la Mungu"},{"book":"Zaburi","chapter":"78","verse":"11","text":"Walisahau aliyokuwa ameyatenda,"},{"book":"Zaburi","chapter":"78","verse":"12","text":"Alitenda miujiza machoni mwa baba zao,"},{"book":"Zaburi","chapter":"78","verse":"13","text":"Aliigawanya bahari akawapitisha,"},{"book":"Zaburi","chapter":"78","verse":"14","text":"Aliwaongoza kwa wingu mchana"},{"book":"Zaburi","chapter":"78","verse":"15","text":"Alipasua miamba jangwani"},{"book":"Zaburi","chapter":"78","verse":"16","text":"alitoa vijito kutoka kwenye jabali lililochongoka,"},{"book":"Zaburi","chapter":"78","verse":"17","text":"Lakini waliendelea kutenda dhambi dhidi yake,"},{"book":"Zaburi","chapter":"78","verse":"18","text":"Kwa makusudi walimjaribu Mungu,"},{"book":"Zaburi","chapter":"78","verse":"19","text":"Walinena dhidi ya Mungu, wakisema,"},{"book":"Zaburi","chapter":"78","verse":"20","text":"Alipopiga mwamba, maji yalitoka kwa nguvu,"},{"book":"Zaburi","chapter":"78","verse":"21","text":"Bwana alipowasikia, alikasirika sana,"},{"book":"Zaburi","chapter":"78","verse":"22","text":"kwa kuwa hawakumwamini Mungu,"},{"book":"Zaburi","chapter":"78","verse":"23","text":"Hata hivyo alitoa amri kwa anga zilizo juu"},{"book":"Zaburi","chapter":"78","verse":"24","text":"akawanyeshea mana ili watu wale;"},{"book":"Zaburi","chapter":"78","verse":"25","text":"Watu walikula mkate wa malaika,"},{"book":"Zaburi","chapter":"78","verse":"26","text":"Aliachia upepo wa mashariki kutoka kwenye mbingu"},{"book":"Zaburi","chapter":"78","verse":"27","text":"Aliwanyeshea nyama kama mavumbi,"},{"book":"Zaburi","chapter":"78","verse":"28","text":"Aliwafanya washuke ndani ya kambi yao,"},{"book":"Zaburi","chapter":"78","verse":"29","text":"Walikula na kusaza,"},{"book":"Zaburi","chapter":"78","verse":"30","text":"Kabla hawajamaliza kula walichokitamani,"},{"book":"Zaburi","chapter":"78","verse":"31","text":"hasira ya Mungu ikawaka juu yao,"},{"book":"Zaburi","chapter":"78","verse":"32","text":"Licha ya haya yote, waliendelea kutenda dhambi,"},{"book":"Zaburi","chapter":"78","verse":"33","text":"Kwa hiyo akamaliza siku zao katika ubatili"},{"book":"Zaburi","chapter":"78","verse":"34","text":"Kila mara Mungu alipowaua baadhi yao,"},{"book":"Zaburi","chapter":"78","verse":"35","text":"Walikumbuka kwamba Mungu alikuwa Mwamba wao,"},{"book":"Zaburi","chapter":"78","verse":"36","text":"Lakini walimdanganya kwa vinywa vyao,"},{"book":"Zaburi","chapter":"78","verse":"37","text":"mioyo yao haikuwa thabiti kwake,"},{"book":"Zaburi","chapter":"78","verse":"38","text":"Hata hivyo alikuwa na huruma,"},{"book":"Zaburi","chapter":"78","verse":"39","text":"Alikumbuka kwamba wao walikuwa nyama tu,"},{"book":"Zaburi","chapter":"78","verse":"40","text":"Mara ngapi walimwasi jangwani"},{"book":"Zaburi","chapter":"78","verse":"41","text":"Walimjaribu Mungu mara kwa mara,"},{"book":"Zaburi","chapter":"78","verse":"42","text":"Hawakukumbuka uwezo wake,"},{"book":"Zaburi","chapter":"78","verse":"43","text":"siku aliyoonyesha ishara zake za ajabu huko Misri,"},{"book":"Zaburi","chapter":"78","verse":"44","text":"Aligeuza mito yao kuwa damu,"},{"book":"Zaburi","chapter":"78","verse":"45","text":"Aliwapelekea makundi ya mainzi yakawala,"},{"book":"Zaburi","chapter":"78","verse":"46","text":"Aliruhusu tunutu kuharibu mimea yao,"},{"book":"Zaburi","chapter":"78","verse":"47","text":"Aliharibu mizabibu yao kwa mvua ya mawe"},{"book":"Zaburi","chapter":"78","verse":"48","text":"Aliwaachia mifugo yao mvua ya mawe,"},{"book":"Zaburi","chapter":"78","verse":"49","text":"Aliwafungulia hasira yake kali,"},{"book":"Zaburi","chapter":"78","verse":"50","text":"Aliitengenezea njia hasira yake,"},{"book":"Zaburi","chapter":"78","verse":"51","text":"Aliwapiga wazaliwa wote wa kwanza wa Misri,"},{"book":"Zaburi","chapter":"78","verse":"52","text":"Lakini aliwatoa watu wake kama kundi,"},{"book":"Zaburi","chapter":"78","verse":"53","text":"Aliwaongoza salama, wala hawakuogopa,"},{"book":"Zaburi","chapter":"78","verse":"54","text":"Hivyo akawaleta hadi kwenye mpaka wa nchi yake takatifu,"},{"book":"Zaburi","chapter":"78","verse":"55","text":"Aliyafukuza mataifa mbele yao,"},{"book":"Zaburi","chapter":"78","verse":"56","text":"Lakini wao walimjaribu Mungu,"},{"book":"Zaburi","chapter":"78","verse":"57","text":"Kama baba zao, hawakuwa thabiti wala waaminifu,"},{"book":"Zaburi","chapter":"78","verse":"58","text":"Wakamkasirisha Mungu kwa mahali pao pa juu pa kuabudia miungu,"},{"book":"Zaburi","chapter":"78","verse":"59","text":"Wakati Mungu alipowasikia, alikasirika sana,"},{"book":"Zaburi","chapter":"78","verse":"60","text":"Akaiacha hema ya Shilo,"},{"book":"Zaburi","chapter":"78","verse":"61","text":"Akalipeleka Sanduku la nguvu zake utumwani,"},{"book":"Zaburi","chapter":"78","verse":"62","text":"Aliachia watu wake wauawe kwa upanga,"},{"book":"Zaburi","chapter":"78","verse":"63","text":"Moto uliwaangamiza vijana wao,"},{"book":"Zaburi","chapter":"78","verse":"64","text":"makuhani wao waliuawa kwa upanga,"},{"book":"Zaburi","chapter":"78","verse":"65","text":"Ndipo Bwana alipoamka kama vile kuamka usingizini,"},{"book":"Zaburi","chapter":"78","verse":"66","text":"Aliwapiga na kuwashinda adui zake,"},{"book":"Zaburi","chapter":"78","verse":"67","text":"Ndipo alipozikataa hema za Yosefu,"},{"book":"Zaburi","chapter":"78","verse":"68","text":"lakini alilichagua kabila la Yuda,"},{"book":"Zaburi","chapter":"78","verse":"69","text":"Alijenga patakatifu pake kama vilele,"},{"book":"Zaburi","chapter":"78","verse":"70","text":"Akamchagua Daudi mtumishi wake"},{"book":"Zaburi","chapter":"78","verse":"71","text":"Kutoka kuchunga kondoo alimleta"},{"book":"Zaburi","chapter":"78","verse":"72","text":"Naye Daudi aliwachunga kwa uadilifu wa moyo,"}]}