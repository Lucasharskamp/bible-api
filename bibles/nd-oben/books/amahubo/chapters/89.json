{"data":[{"book":"AmaHubo","chapter":"89","verse":"1","text":"Ngizahlabelela ngothando lukaThixo olukhulu lanininini;"},{"book":"AmaHubo","chapter":"89","verse":"2","text":"Ngizamemezela ukuthi uthando lwakho lumi luqinile laphakade,"},{"book":"AmaHubo","chapter":"89","verse":"3","text":"Wathi, “Sengenze isivumelwano lalowo okhethiweyo wami,"},{"book":"AmaHubo","chapter":"89","verse":"4","text":"‘Ngizawumisa umndeni wakho laphakade,"},{"book":"AmaHubo","chapter":"89","verse":"5","text":"Amazulu ayazidumisa izimanga zakho, Oh Thixo,"},{"book":"AmaHubo","chapter":"89","verse":"6","text":"Phela ngubani onjengoThixo phakathi kwabaphilayo ezulwini na?"},{"book":"AmaHubo","chapter":"89","verse":"7","text":"Enhlanganisweni yabangcwele uNkulunkulu uyesatshwa kakhulu;"},{"book":"AmaHubo","chapter":"89","verse":"8","text":"Yebo Thixo Nkulunkulu Somandla, ngubani onjengawe na?"},{"book":"AmaHubo","chapter":"89","verse":"9","text":"Uyabusa phezu kolwandle olugubhazelayo;"},{"book":"AmaHubo","chapter":"89","verse":"10","text":"Wamchoboza uRahabi njengomunye wababuleweyo;"},{"book":"AmaHubo","chapter":"89","verse":"11","text":"Ngawakho amazulu, ngowakho njalo lomhlaba;"},{"book":"AmaHubo","chapter":"89","verse":"12","text":"Wayidala inyakatho kanye leningizimu;"},{"book":"AmaHubo","chapter":"89","verse":"13","text":"Ingalo yakho igcwele amandla;"},{"book":"AmaHubo","chapter":"89","verse":"14","text":"Ukulunga lokwahlulela kuhle yikho okuyinsika yesihlalo sakho sobukhosi;"},{"book":"AmaHubo","chapter":"89","verse":"15","text":"Babusisiwe labo asebekufundile ukukubabaza,"},{"book":"AmaHubo","chapter":"89","verse":"16","text":"Bayathokoza ngebizo lakho ilanga lonke;"},{"book":"AmaHubo","chapter":"89","verse":"17","text":"Ngoba uyibukhosi babo lamandla abo,"},{"book":"AmaHubo","chapter":"89","verse":"18","text":"Ngempela isihlangu sethu ngesikaThixo, iNkosi yethu"},{"book":"AmaHubo","chapter":"89","verse":"19","text":"Endulo wakhuluma ngombono,"},{"book":"AmaHubo","chapter":"89","verse":"20","text":"Sengifumane uDavida inceku yami;"},{"book":"AmaHubo","chapter":"89","verse":"21","text":"Isandla sami sizamsekela;"},{"book":"AmaHubo","chapter":"89","verse":"22","text":"Akulasitha esizamncindezela ukuthi athele kuso;"},{"book":"AmaHubo","chapter":"89","verse":"23","text":"Ngizazichoboza izitha zakhe phambi kwakhe"},{"book":"AmaHubo","chapter":"89","verse":"24","text":"Uthando lwami oluthembekileyo luzaba laye,"},{"book":"AmaHubo","chapter":"89","verse":"25","text":"Ngizabeka isandla sakhe phezu kolwandle,"},{"book":"AmaHubo","chapter":"89","verse":"26","text":"Uzamemeza kimi athi, ‘UnguBaba,"},{"book":"AmaHubo","chapter":"89","verse":"27","text":"Njalo ngizamisa abe lizibulo lami,"},{"book":"AmaHubo","chapter":"89","verse":"28","text":"Ngizalugcina uthando lwami kuye laphakade,"},{"book":"AmaHubo","chapter":"89","verse":"29","text":"Inzalo yakhe ngizayimisa nini lanini,"},{"book":"AmaHubo","chapter":"89","verse":"30","text":"Aluba amadodana akhe edela umlayo wami"},{"book":"AmaHubo","chapter":"89","verse":"31","text":"nxa besephula izimemezelo zami"},{"book":"AmaHubo","chapter":"89","verse":"32","text":"ngizajezisa isono sabo ngoswazi,"},{"book":"AmaHubo","chapter":"89","verse":"33","text":"kodwa kangiyikulususa uthando lwami kuye,"},{"book":"AmaHubo","chapter":"89","verse":"34","text":"Kangiyikusephula isivumelwano sami"},{"book":"AmaHubo","chapter":"89","verse":"35","text":"Ngavele ngafunga kanye ngobungcwele bami"},{"book":"AmaHubo","chapter":"89","verse":"36","text":"ukuthi inzalo yakhe izaqhubeka laphakade"},{"book":"AmaHubo","chapter":"89","verse":"37","text":"sizakuma laphakade njengenyanga,"},{"book":"AmaHubo","chapter":"89","verse":"38","text":"Kodwa usumalile, usumlahlile,"},{"book":"AmaHubo","chapter":"89","verse":"39","text":"Ususikhalele isivumelwano lenceku yakho"},{"book":"AmaHubo","chapter":"89","verse":"40","text":"Usuyibhidlizile yonke imiduli yakhe,"},{"book":"AmaHubo","chapter":"89","verse":"41","text":"Bonke abedlula khona bamphangile;"},{"book":"AmaHubo","chapter":"89","verse":"42","text":"Usuphakamisile isandla sokunene sezitha zakhe;"},{"book":"AmaHubo","chapter":"89","verse":"43","text":"Uyibuyisele emuva inkemba yakhe ebukhali"},{"book":"AmaHubo","chapter":"89","verse":"44","text":"Usuyiqedile inkazimulo yakhe"},{"book":"AmaHubo","chapter":"89","verse":"45","text":"Uluqumile lwaba lufitshane usuku lobutsha bakhe;"},{"book":"AmaHubo","chapter":"89","verse":"46","text":"Koze kube nini, Oh Thixo? Uzazifihla kokuphela na?"},{"book":"AmaHubo","chapter":"89","verse":"47","text":"Khumbula ukwedlula masinyane kwempilo yami."},{"book":"AmaHubo","chapter":"89","verse":"48","text":"Ngumuntu bani ongaphila angaze abona ukufa,"},{"book":"AmaHubo","chapter":"89","verse":"49","text":"Oh Thixo, lungaphi uthando lwakho lwakudala olukhulu,"},{"book":"AmaHubo","chapter":"89","verse":"50","text":"Khumbula, Thixo ukuthi inceku yakho iklolodelwa njani,"},{"book":"AmaHubo","chapter":"89","verse":"51","text":"izigcono izitha zakho eziklolode ngazo, Oh Thixo,"},{"book":"AmaHubo","chapter":"89","verse":"52","text":"Udumo kalube kuThixo laphakade!"}]}