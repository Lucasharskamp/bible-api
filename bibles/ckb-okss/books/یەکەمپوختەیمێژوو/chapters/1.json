{"data":[{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"1","text":"ئادەم، شیت، ئەنۆش،"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"2","text":"قێنان، مەهلەلئێل، یارەد،"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"3","text":"حەنۆخ، مەتوشالح، لامەخ"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"4","text":"کوڕەکانی نوح: سام، حام و یافەت."},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"5","text":"کوڕەکانی یافەت:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"6","text":"کوڕەکانی گۆمەر:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"7","text":"کوڕەکانی یاڤان:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"8","text":"کوڕەکانی حام:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"9","text":"کوڕەکانی کوش:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"10","text":"نەمرود لە نەوەی کوش لەدایکبوو،"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"11","text":"میسر باوکی"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"12","text":"پاتروسی و کەفتۆری بوو، هەروەها باوکی کەسلوحییەکانیش بوو کە فەلەستییەکانیان لێ کەوتنەوە."},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"13","text":"کەنعان باوکی سەیدا بوو،"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"14","text":"هەروەها یەبوسی، ئەمۆری، گرگاشی،"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"15","text":"حیڤی، عەرقی، سینی1‏:15 سینییەکان: لەوانەیە خەڵکی شارێک لە نزیکی شاری عەرقە لە لوبنان بووبێت. بڕوانە پەیدابوون 10‏:17‏‏.‏،"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"16","text":"ئەرڤادی، چیماری و حەماتییەکانیشی بوو."},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"17","text":"کوڕەکانی سام:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"18","text":"ئەرپەکشاد باوکی شالەح بوو،"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"19","text":"عێبەریش دوو کوڕی بوو،"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"20","text":"یۆقتان ئەم کوڕانەی هەبوو:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"21","text":"هەدۆرام، ئوزال، دیقلە،"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"22","text":"عۆبال، ئەبیمائێل، شەبا،"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"23","text":"ئۆفیر، حەڤیلا و یۆڤاڤ. هەموو ئەمانە کوڕانی یۆقتان بوون."},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"24","text":"سام، ئەرپەکشاد، شالەح،"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"25","text":"عێبەر، پەلەگ، ڕەعو،"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"26","text":"سەروگ، ناحۆر، تارەح و"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"27","text":"ئەبرام، کە ئیبراهیمە."},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"28","text":"کوڕەکانی ئیبراهیم: ئیسحاق و ئیسماعیل."},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"29","text":"ئەمانەش نەوەکانیانن:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"30","text":"میشماع، دوما، مەسسا، حەدەد، تێما،"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"31","text":"یەتور، نافیش و قێدما،"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"32","text":"قەتورە کە کەنیزەی ئیبراهیم بوو ئەم کوڕانەی هەبوو:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"33","text":"کوڕەکانی میدیان:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"34","text":"ئیبراهیم ئیسحاقی بوو."},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"35","text":"کوڕەکانی عیسۆ:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"36","text":"کوڕەکانی ئەلیفاز:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"37","text":"کوڕەکانی ڕەعوئێل:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"38","text":"کوڕەکانی سێعیر:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"39","text":"کوڕەکانی لۆتان: حۆری و هۆمام."},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"40","text":"کوڕەکانی شۆڤاڵ:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"41","text":"کوڕەکەی عەنا:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"42","text":"کوڕەکانی ئێچەر:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"43","text":"ئەمانەش ئەو پاشایانەن کە لە خاکی ئەدۆم پاشایەتییان دەکرد پێش ئەوەی هیچ پاشایەکی ئیسرائیلی پاشایەتی بکات:"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"44","text":"کە بەلەع مرد، یۆڤاڤی کوڕی زەرەحی خەڵکی بۆزرا وەک پاشا جێگەی گرتەوە."},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"45","text":"کە یۆڤاڤ مرد، حوشامی خەڵکی خاکی تێمانییەکان وەک پاشا جێگەی گرتەوە."},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"46","text":"کە حوشام مرد، هەدەدی کوڕی بەدەد وەک پاشا جێگەی گرتەوە، لە وڵاتی مۆئاب شکستی بە میدیان هێنا. ناوی شارەکەشی عەڤیت بوو."},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"47","text":"کە هەدەد مرد، سەمڵەی خەڵکی مەسرێقە وەک پاشا جێگەی گرتەوە."},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"48","text":"کە سەمڵە مرد، شائوولی خەڵکی ڕەحۆبۆت، ئەو شارەی لە کەناری ڕووبار1‏:48 لەسەر ڕووبارە: دەشێت ڕووباری فورات بێت.‏ بوو، وەک پاشا جێگەی گرتەوە."},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"49","text":"کە شائوول مرد، بەعل‌حانانی کوڕی عەکبۆر وەک پاشا جێگەی گرتەوە."},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"50","text":"کە بەعل‌حانان مرد، هەدەد وەک پاشا جێگەی گرتەوە. ناوی شارەکەشی پاعوو بوو، ژنەکەشی ناوی میهێتەبێل بوو، کچی مەترێدی کچی مێزاهاب بوو."},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"51","text":"ئینجا هەدەدیش مرد."},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"52","text":"ئۆهۆلیبامە، ئێلە، پینۆن،"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"53","text":"قەنەز، تێمان، میبسار،"},{"book":"یەکەم پوختەی مێژوو","chapter":"1","verse":"54","text":"مەگدئێل و عیرام."}]}