{"data":[{"book":"Ngeche","chapter":"23","verse":"1","text":"Ka ibedo mondo ichiem gi ruoth,"},{"book":"Ngeche","chapter":"23","verse":"2","text":"ka in jawuoro to dimbri"},{"book":"Ngeche","chapter":"23","verse":"3","text":"Kik igomb chiembe mamit,"},{"book":"Ngeche","chapter":"23","verse":"4","text":"Kik iwe tich bari ndasi ni mondo ibed ja-mwandu,"},{"book":"Ngeche","chapter":"23","verse":"5","text":"Ngʼi mwandu mana matin nono, to bangʼe gidhi,"},{"book":"Ngeche","chapter":"23","verse":"6","text":"Kik icham chiemb jagwondo,"},{"book":"Ngeche","chapter":"23","verse":"7","text":"nimar en ngʼat"},{"book":"Ngeche","chapter":"23","verse":"8","text":"Ibiro ngʼogo mano matin mane ichamo"},{"book":"Ngeche","chapter":"23","verse":"9","text":"Kik iwuo ne ngʼat mofuwo,"},{"book":"Ngeche","chapter":"23","verse":"10","text":"Kik igol kidi mar kiewo machon,"},{"book":"Ngeche","chapter":"23","verse":"11","text":"nimar Jal moritogi en ratego;"},{"book":"Ngeche","chapter":"23","verse":"12","text":"Ket chunyi ne puonj"},{"book":"Ngeche","chapter":"23","verse":"13","text":"Kik itamri kumo nyathi;"},{"book":"Ngeche","chapter":"23","verse":"14","text":"Kikumo nyathi kod kede"},{"book":"Ngeche","chapter":"23","verse":"15","text":"Wuoda, ka chunyi riek,"},{"book":"Ngeche","chapter":"23","verse":"16","text":"chunya ma iye nobed mamor"},{"book":"Ngeche","chapter":"23","verse":"17","text":"Kik ibed gi nyiego e chunyi nikech joricho;"},{"book":"Ngeche","chapter":"23","verse":"18","text":"Adier, ka itimo kamano to inibed gi geno e ndalo mabiro,"},{"book":"Ngeche","chapter":"23","verse":"19","text":"Wuoda, winja, eka inibed mariek,"},{"book":"Ngeche","chapter":"23","verse":"20","text":"Kik iriwri gi jogo ma madho kongʼo mangʼeny"},{"book":"Ngeche","chapter":"23","verse":"21","text":"nimar jomer kod jowuoro bedo jochan,"},{"book":"Ngeche","chapter":"23","verse":"22","text":"Winj wuonu, nikech en ema nonywoli,"},{"book":"Ngeche","chapter":"23","verse":"23","text":"Adiera gi rieko gi puonjruok kod ngʼeyo tiend wach ema nyaka iyudi,"},{"book":"Ngeche","chapter":"23","verse":"24","text":"Wuon ngʼat makare nigi mor maduongʼ;"},{"book":"Ngeche","chapter":"23","verse":"25","text":"Mad wuonu gi minu bed mamor;"},{"book":"Ngeche","chapter":"23","verse":"26","text":"Wuoda, miya chunyi, kendo ket wengegi orit yorena,"},{"book":"Ngeche","chapter":"23","verse":"27","text":"nimar dhako ma jachode en bur matut,"},{"book":"Ngeche","chapter":"23","verse":"28","text":"Ka jamecho obuto korito,"},{"book":"Ngeche","chapter":"23","verse":"29","text":"En ngʼa man-gi masira?"},{"book":"Ngeche","chapter":"23","verse":"30","text":"Jogo madeko oko ka kongʼo,"},{"book":"Ngeche","chapter":"23","verse":"31","text":"Kik ingʼi kongʼo kochiek mowalo,"},{"book":"Ngeche","chapter":"23","verse":"32","text":"Bangʼe to okecho ka thuol"},{"book":"Ngeche","chapter":"23","verse":"33","text":"Wengeni biro neno gik mayoreyore"},{"book":"Ngeche","chapter":"23","verse":"34","text":"Ibiro chalo ngʼat ma nindo e nam mar apaka matek,"},{"book":"Ngeche","chapter":"23","verse":"35","text":"Ibiro wacho niya, “Gigoya, to ok ahinyra!"}]}