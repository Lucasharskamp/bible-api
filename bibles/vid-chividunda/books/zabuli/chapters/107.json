{"data":[{"book":"Zabuli","chapter":"107","verse":"1","text":"â€œMlonje malumbo kwa Mkulu Nguluwi kwa vila heye ka yanojile,"},{"book":"Zabuli","chapter":"107","verse":"2","text":"Mlonje ahyo hemwe womgomboligwe na Mkulu Nguluwi,"},{"book":"Zabuli","chapter":"107","verse":"3","text":"Nakawakungajizani kulawa isi za chijenzi,"},{"book":"Zabuli","chapter":"107","verse":"4","text":"Wamwenga nawalubaluba hela kuli chiwala,"},{"book":"Zabuli","chapter":"107","verse":"5","text":"Nawona nzala na ng'halu,"},{"book":"Zabuli","chapter":"107","verse":"6","text":"Kuli kugaya kwa hewo nawamlilila Mkulu Nguluwi,"},{"book":"Zabuli","chapter":"107","verse":"7","text":"Nakawalongoza kuli nzila igoloche,"},{"book":"Zabuli","chapter":"107","verse":"8","text":"Mlonje malumbo kwa Mkulu Nguluwi kwa igano lya kwimilila lya heye,"},{"book":"Zabuli","chapter":"107","verse":"9","text":"Kanguwanwezaga wawele na ng'halu,"},{"book":"Zabuli","chapter":"107","verse":"10","text":"Wamwe nawekala kuli jiza na uchiwa,"},{"book":"Zabuli","chapter":"107","verse":"11","text":"kwa vila nawabelenhuka malajizo ga Nguluwi,"},{"book":"Zabuli","chapter":"107","verse":"12","text":"Nawali watochile lukami kwa mijito jing'ang'ale,"},{"book":"Zabuli","chapter":"107","verse":"13","text":"Kuli kugaya kwa hewo nawamlilila Mkulu Nguluwi,"},{"book":"Zabuli","chapter":"107","verse":"14","text":"Nakawalavya kuli jiza na uchiwa,"},{"book":"Zabuli","chapter":"107","verse":"15","text":"Mlonje malumbo kwa Mkulu Nguluwi kwa igano lya kwimilila lya heye,"},{"book":"Zabuli","chapter":"107","verse":"16","text":"Heye kangubenaga na kuchumula lwizi lwa shaba,"},{"book":"Zabuli","chapter":"107","verse":"17","text":"Kwa upoka, wamwe nawadita gehile,"},{"book":"Zabuli","chapter":"107","verse":"18","text":"nawachizudila chilyo cheng'ha,"},{"book":"Zabuli","chapter":"107","verse":"19","text":"Kuli kugaya kwa hewo nawamlilila Mkulu Nguluwi,"},{"book":"Zabuli","chapter":"107","verse":"20","text":"Kwa ulonzi wa heye nakawalohola,"},{"book":"Zabuli","chapter":"107","verse":"21","text":"Mlonje malumbo kwa Mkulu Nguluwi kwa igano lya kwimilila lya heye,"},{"book":"Zabuli","chapter":"107","verse":"22","text":"Wamlavile nhambiko za kulonga malumbo,"},{"book":"Zabuli","chapter":"107","verse":"23","text":"Wamwenga nawali mmajendo kuli bahali kwa meli,"},{"book":"Zabuli","chapter":"107","verse":"24","text":"Nawagona madito ga Mkulu Nguluwi,"},{"book":"Zabuli","chapter":"107","verse":"25","text":"Nakalajiza, nakambuza chivuluvumbi chikulu,"},{"book":"Zabuli","chapter":"107","verse":"26","text":"Nawatozigwa kuchanya kuli ulanga, hamba hasi kuli mazi menji,"},{"book":"Zabuli","chapter":"107","verse":"27","text":"Nawatigasika na kuzebeduka kota wagalile,"},{"book":"Zabuli","chapter":"107","verse":"28","text":"Kuli kugaya kwa hewo nawamlilila Mkulu Nguluwi,"},{"book":"Zabuli","chapter":"107","verse":"29","text":"Nakachidita chila chivuluvumbi chikulu chisindamale,"},{"book":"Zabuli","chapter":"107","verse":"30","text":"Aho nawasangalala kwa kuwa na kusindamala,"},{"book":"Zabuli","chapter":"107","verse":"31","text":"Mlonje malumbo kwa Mkulu Nguluwi kwa igano lya kwimilila lya heye,"},{"book":"Zabuli","chapter":"107","verse":"32","text":"Mumtogole kuli nhing'hano ya wanhu,"},{"book":"Zabuli","chapter":"107","verse":"33","text":"Nguluwi nakagalula nzanda ziwa kugunuche,"},{"book":"Zabuli","chapter":"107","verse":"34","text":"Misanga jiwele na mboto nakajidita jiwe misanga jiwele na mwino,"},{"book":"Zabuli","chapter":"107","verse":"35","text":"Nakagalula kugunuche nakuwa mizimbo ja mazi,"},{"book":"Zabuli","chapter":"107","verse":"36","text":"Nakawahilika ako wawele na nzala,"},{"book":"Zabuli","chapter":"107","verse":"37","text":"Nawalima migunda na kunyala mizabibu,"},{"book":"Zabuli","chapter":"107","verse":"38","text":"Nakawatambichiza wanhu wa heye nawonjezeka,"},{"book":"Zabuli","chapter":"107","verse":"39","text":"Hamba lowapunguche na lowawele ng'hochele,"},{"book":"Zabuli","chapter":"107","verse":"40","text":"nakawabeza wakulu wowawagazile,"},{"book":"Zabuli","chapter":"107","verse":"41","text":"Ila nakawalohola walingugana utanzi kuli magazo ga hewo,"},{"book":"Zabuli","chapter":"107","verse":"42","text":"Watanganiche howoyona mbuli ayo wosangalala,"},{"book":"Zabuli","chapter":"107","verse":"43","text":"Wawele na luhala wejese kwa chiwalo cha mbuli azi,"}]}