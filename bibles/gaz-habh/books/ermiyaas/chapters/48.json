{"data":[{"book":"Ermiyaas","chapter":"48","verse":"1","text":"Waaʼee Moʼaab:"},{"book":"Ermiyaas","chapter":"48","verse":"2","text":"Moʼaab siʼachi hin jajamtu;"},{"book":"Ermiyaas","chapter":"48","verse":"3","text":"Iyya Hooronaayimii dhufu,"},{"book":"Ermiyaas","chapter":"48","verse":"4","text":"Moʼaab ni cabdi;"},{"book":"Ermiyaas","chapter":"48","verse":"5","text":"Isaan akka malee booʼaa,"},{"book":"Ermiyaas","chapter":"48","verse":"6","text":"Baqadhaa! Lubbuu keessan baafadhaa;"},{"book":"Ermiyaas","chapter":"48","verse":"7","text":"Isin waan hojii keessanii fi qabeenya keessan amanattaniif,"},{"book":"Ermiyaas","chapter":"48","verse":"8","text":"Balleessituun sun magaalaa hundatti ni dhufa;"},{"book":"Ermiyaas","chapter":"48","verse":"9","text":"Moʼaabitti soogidda firfirsi;"},{"book":"Ermiyaas","chapter":"48","verse":"10","text":"“Namni garaa hirʼuudhaan hojii Waaqayyoo hojjetu abaaramaa haa taʼu!"},{"book":"Ermiyaas","chapter":"48","verse":"11","text":"“Moʼaab akkuma daadhii wayinii kan siicoo isaa irratti hafe"},{"book":"Ermiyaas","chapter":"48","verse":"12","text":"Garuu barri ani itti namoota okkotee keessaa dhangalaasan  ergu tokko ni dhufa."},{"book":"Ermiyaas","chapter":"48","verse":"13","text":"“Yoos akkuma manni Israaʼel"},{"book":"Ermiyaas","chapter":"48","verse":"14","text":"“Isin akkamiin ‘Nu gootota;"},{"book":"Ermiyaas","chapter":"48","verse":"15","text":"Moʼaab ni barbadoofti; magaalaawwan ishees ni qabatamu;"},{"book":"Ermiyaas","chapter":"48","verse":"16","text":"“Kufaatiin Moʼaab gaʼeera;"},{"book":"Ermiyaas","chapter":"48","verse":"17","text":"Warri naannoo ishee jiraatan hundi,"},{"book":"Ermiyaas","chapter":"48","verse":"18","text":"“Yaa jiraattuu Diiboon,"},{"book":"Ermiyaas","chapter":"48","verse":"19","text":"Ati kan Aroʼeer keessa jiraattu,"},{"book":"Ermiyaas","chapter":"48","verse":"20","text":"Moʼaab waan diigamteef qaanofteerti."},{"book":"Ermiyaas","chapter":"48","verse":"21","text":"Murtiin dirree ol kaʼaatti:"},{"book":"Ermiyaas","chapter":"48","verse":"22","text":"Diiboonitti, Neboo fi Beet Diblaataayimitti,"},{"book":"Ermiyaas","chapter":"48","verse":"23","text":"Kiriyaataayimitti, Beet Gaamulii fi Beet Meʼoonitti,"},{"book":"Ermiyaas","chapter":"48","verse":"24","text":"Keriiyoottii fi Bozraatti,"},{"book":"Ermiyaas","chapter":"48","verse":"25","text":"Gaanfi Moʼaab irraa murameera;"},{"book":"Ermiyaas","chapter":"48","verse":"26","text":"“Waan isheen Waaqayyotti koorteef"},{"book":"Ermiyaas","chapter":"48","verse":"27","text":"Israaʼel si duratti waan qoosaa hin turree?"},{"book":"Ermiyaas","chapter":"48","verse":"28","text":"Isin warri Moʼaab keessa jiraattan,"},{"book":"Ermiyaas","chapter":"48","verse":"29","text":"“Nu waaʼee of tuulummaa Moʼaab"},{"book":"Ermiyaas","chapter":"48","verse":"30","text":"Ani addaggummaa Moʼaab beeka;  garuu faayidaa hin qabaatu” jedha Waaqayyo;"},{"book":"Ermiyaas","chapter":"48","verse":"31","text":"Kanaafuu ani Moʼaabiif nan wawwaadha;"},{"book":"Ermiyaas","chapter":"48","verse":"32","text":"Yaa muka wayinii Sibimaa,"},{"book":"Ermiyaas","chapter":"48","verse":"33","text":"Lafa biqiltuutii fi lafa qotiisaa"},{"book":"Ermiyaas","chapter":"48","verse":"34","text":"“Sababii bishaan Niimriim gogeef,"},{"book":"Ermiyaas","chapter":"48","verse":"35","text":"Ani warra iddoowwan sagadaa kanneen gaarran irraatti"},{"book":"Ermiyaas","chapter":"48","verse":"36","text":"“Kanaafuu garaan koo akkuma ulullee faarsee Moʼaabiif booʼa;"},{"book":"Ermiyaas","chapter":"48","verse":"37","text":"Mataan hundinuu haadamee"},{"book":"Ermiyaas","chapter":"48","verse":"38","text":"Waan ani akkuma okkotee namni tokko iyyuu hin feeneetti"},{"book":"Ermiyaas","chapter":"48","verse":"39","text":"“Isheen akkam akkas caccabde! Isaan akkam akkas wawwaatu!"},{"book":"Ermiyaas","chapter":"48","verse":"40","text":"Waaqayyo akkana jedha:"},{"book":"Ermiyaas","chapter":"48","verse":"41","text":"Keriiyooti ni qabamti;"},{"book":"Ermiyaas","chapter":"48","verse":"42","text":"Moʼaab sababii Waaqayyoon tuffatteef"},{"book":"Ermiyaas","chapter":"48","verse":"43","text":"Yaa saba Moʼaab sodaan,"},{"book":"Ermiyaas","chapter":"48","verse":"44","text":"“Namni sodaa sana duraa baqatu"},{"book":"Ermiyaas","chapter":"48","verse":"45","text":"“Baqattoonni waan taʼan dhabanii"},{"book":"Ermiyaas","chapter":"48","verse":"46","text":"Yaa Moʼaab siif wayyoo!"},{"book":"Ermiyaas","chapter":"48","verse":"47","text":"“Taʼu illee ani bara dhufuuf jiru keessa"}]}