{"data":[{"book":"يَبُرَ","chapter":"26","verse":"1","text":"عَلَتَلَ، عِ شَ ﭑ مَ كِيتِ سَ،"},{"book":"يَبُرَ","chapter":"26","verse":"2","text":"عَلَتَلَ، ﭑ بْحّ مَتٌ،"},{"book":"يَبُرَ","chapter":"26","verse":"3","text":"بَرِ مَ ﭑ لَشِ عِ شَ هِننّ رَ،"},{"book":"يَبُرَ","chapter":"26","verse":"4","text":"ﭑ مُ لُمَ وُلٍ قَلّيٍ يَ مَ،"},{"book":"يَبُرَ","chapter":"26","verse":"5","text":"قٍ كٌبِ رَبَيٍ مُ رَقَن ﭑ مَ،"},{"book":"يَبُرَ","chapter":"26","verse":"6","text":"عَلَتَلَ، ﭑ نَن ﭑ حّرّ كِ رَ سّنِيّن،"},{"book":"يَبُرَ","chapter":"26","verse":"7","text":"ﭑ تَنتُي بّيتِ بَمَ،"},{"book":"يَبُرَ","chapter":"26","verse":"8","text":"عَلَتَلَ، عِ شَ هْرْ مْبَنشِ رَقَن ﭑ مَ،"},{"book":"يَبُرَ","chapter":"26","verse":"9","text":"عِ نَشَ ﭑ نِي بَ عَلْ يُنُبِتْييٍ نُن قَشَتِيٍ."},{"book":"يَبُرَ","chapter":"26","verse":"10","text":"عٍ قٍ حَاشِيٍ رَبَمَ،"},{"book":"يَبُرَ","chapter":"26","verse":"11","text":"كْنْ ﭑ تَن لُمَ تِنشِنيِ كُي."},{"book":"يَبُرَ","chapter":"26","verse":"12","text":"ﭑ حّرّ كِ تِنشِن."}]}