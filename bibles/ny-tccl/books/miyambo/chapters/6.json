{"data":[{"book":"Miyambo","chapter":"6","verse":"1","text":"Mwana wanga, ngati wamuperekera mnansi wako chikole,"},{"book":"Miyambo","chapter":"6","verse":"2","text":"ngati wagwidwa ndi zimene iwe unanena,"},{"book":"Miyambo","chapter":"6","verse":"3","text":"Tsono popeza iwe mwana wanga"},{"book":"Miyambo","chapter":"6","verse":"4","text":"Usagone tulo,"},{"book":"Miyambo","chapter":"6","verse":"5","text":"Dzipulumutse monga imachitira mphoyo mʼdzanja la mlenje,"},{"book":"Miyambo","chapter":"6","verse":"6","text":"Pita kwa nyerere, mlesi iwe;"},{"book":"Miyambo","chapter":"6","verse":"7","text":"Zilibe mfumu,"},{"book":"Miyambo","chapter":"6","verse":"8","text":"komabe zimasungiratu chakudya chake nthawi ya chilimwe"},{"book":"Miyambo","chapter":"6","verse":"9","text":"Kodi uzingogonabe pamenepo mpaka liti mlesi iwe?"},{"book":"Miyambo","chapter":"6","verse":"10","text":"Ukati ndingogona pangʼono, ndingowodzera pangʼono"},{"book":"Miyambo","chapter":"6","verse":"11","text":"umphawi udzakugwira ngati mbala"},{"book":"Miyambo","chapter":"6","verse":"12","text":"Munthu wachabechabe, munthu woyipa,"},{"book":"Miyambo","chapter":"6","verse":"13","text":"amatsinzinira maso ake,"},{"book":"Miyambo","chapter":"6","verse":"14","text":"amalingalira zoyipa ndi mtima wake wachinyengo"},{"book":"Miyambo","chapter":"6","verse":"15","text":"Nʼchifukwa chake tsoka lidzamugwera modzidzimutsa;"},{"book":"Miyambo","chapter":"6","verse":"16","text":"Pali zinthu zisanu ndi chimodzi zimene Yehova amadana nazo,"},{"book":"Miyambo","chapter":"6","verse":"17","text":"maso onyada,"},{"book":"Miyambo","chapter":"6","verse":"18","text":"mtima wokonzekera kuchita zoyipa,"},{"book":"Miyambo","chapter":"6","verse":"19","text":"mboni yonama yoyankhula mabodza"},{"book":"Miyambo","chapter":"6","verse":"20","text":"Mwana wanga, usunge malamulo a abambo ako;"},{"book":"Miyambo","chapter":"6","verse":"21","text":"Zimenezi uzimatirire pa mtima pako masiku onse,"},{"book":"Miyambo","chapter":"6","verse":"22","text":"Ukamayenda, zidzakulozera njira;"},{"book":"Miyambo","chapter":"6","verse":"23","text":"Paja malamulo awa ali ngati nyale,"},{"book":"Miyambo","chapter":"6","verse":"24","text":"kukupulumutsa kwa mkazi wadama,"},{"book":"Miyambo","chapter":"6","verse":"25","text":"Mu mtima wako usakhumbire kukongola kwake,"},{"book":"Miyambo","chapter":"6","verse":"26","text":"paja mkazi wadama amakusandutsa kukhala ngati nyenyeswa za buledi"},{"book":"Miyambo","chapter":"6","verse":"27","text":"Kodi munthu angathe kutenga moto"},{"book":"Miyambo","chapter":"6","verse":"28","text":"Kodi munthu angathe kuyenda pa makala amoto"},{"book":"Miyambo","chapter":"6","verse":"29","text":"Ndizo zimachitikira munthu amene amagonana ndi mkazi wa munthu wina."},{"book":"Miyambo","chapter":"6","verse":"30","text":"Paja anthu sayinyoza mbala ikaba"},{"book":"Miyambo","chapter":"6","verse":"31","text":"Komabe ngati mbalayo igwidwa iyenera kulipira kasanu nʼkawiri,"},{"book":"Miyambo","chapter":"6","verse":"32","text":"Munthu wochita chigololo ndi wopanda nzeru."},{"book":"Miyambo","chapter":"6","verse":"33","text":"Adzalandira mabala ndi mʼnyozo,"},{"book":"Miyambo","chapter":"6","verse":"34","text":"Paja nsanje imachititsa mwini mkaziyo kukalipa,"},{"book":"Miyambo","chapter":"6","verse":"35","text":"Iye savomera dipo lililonse;"}]}