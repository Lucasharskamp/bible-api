{"data":[{"book":"Ngeche","chapter":"8","verse":"1","text":"Donge rieko luongo?"},{"book":"Ngeche","chapter":"8","verse":"2","text":"Ochungʼ e kuonde motingʼore e bath yo,"},{"book":"Ngeche","chapter":"8","verse":"3","text":"Oywak matek e bath dhorangeye"},{"book":"Ngeche","chapter":"8","verse":"4","text":"kowacho niya, “Aluongou, yaye un ji,"},{"book":"Ngeche","chapter":"8","verse":"5","text":"Un joma ngʼeyogi tin, nwangʼuru rieko;"},{"book":"Ngeche","chapter":"8","verse":"6","text":"Winjuru, nikech an gi gik mabeyo mangʼeny madwaro nyisou;"},{"book":"Ngeche","chapter":"8","verse":"7","text":"Dhoga wacho gima adiera"},{"book":"Ngeche","chapter":"8","verse":"8","text":"Weche duto mawuok e dhoga gin kare,"},{"book":"Ngeche","chapter":"8","verse":"9","text":"Ne jogo mongʼeyo pogo tiend wach,"},{"book":"Ngeche","chapter":"8","verse":"10","text":"Yier puonjna kar fedha"},{"book":"Ngeche","chapter":"8","verse":"11","text":"nikech rieko ber moloyo kite ma nengogi tek,"},{"book":"Ngeche","chapter":"8","verse":"12","text":"“An, rieko, wadak kaachiel gi ngʼado bura makare;"},{"book":"Ngeche","chapter":"8","verse":"13","text":"Luoro Jehova Nyasaye en sin gi richo;"},{"book":"Ngeche","chapter":"8","verse":"14","text":"Ngʼado rieko kod ngʼado bura malongʼo gin maga;"},{"book":"Ngeche","chapter":"8","verse":"15","text":"An ema amiyo ruodhi locho"},{"book":"Ngeche","chapter":"8","verse":"16","text":"Nikech an, amiyo yawuot ruodhi loch"},{"book":"Ngeche","chapter":"8","verse":"17","text":"Ahero jogo mohera,"},{"book":"Ngeche","chapter":"8","verse":"18","text":"Mwandu gi luor ni kuoma,"},{"book":"Ngeche","chapter":"8","verse":"19","text":"Olemba ber moloyo dhahabu maber,"},{"book":"Ngeche","chapter":"8","verse":"20","text":"Awuotho e yo mobidhore makare,"},{"book":"Ngeche","chapter":"8","verse":"21","text":"kachiwo mwandu ni jogo mohera"},{"book":"Ngeche","chapter":"8","verse":"22","text":"“Jehova Nyasaye nokwongo chweya"},{"book":"Ngeche","chapter":"8","verse":"23","text":"Noyiera aa chakruok manyaka chiengʼ,"},{"book":"Ngeche","chapter":"8","verse":"24","text":"Kapok nembe kobetie,"},{"book":"Ngeche","chapter":"8","verse":"25","text":"Nonywola ka gode madongo ne pok obedo kuonde ma gintie,"},{"book":"Ngeche","chapter":"8","verse":"26","text":"kapok ne ochweyo piny kaachiel gi pewene"},{"book":"Ngeche","chapter":"8","verse":"27","text":"Ne antie kane oketo polo kama entie,"},{"book":"Ngeche","chapter":"8","verse":"28","text":"kane oketo boche polo malo,"},{"book":"Ngeche","chapter":"8","verse":"29","text":"kane ochiwo kuonde giko mag nam matin"},{"book":"Ngeche","chapter":"8","verse":"30","text":"Ne an jatich molony mobedo bute."},{"book":"Ngeche","chapter":"8","verse":"31","text":"ka amor e pinye mangima"},{"book":"Ngeche","chapter":"8","verse":"32","text":"“Omiyo koro yawuota, winjauru;"},{"book":"Ngeche","chapter":"8","verse":"33","text":"Winjuru puonjna mondo ubed mariek,"},{"book":"Ngeche","chapter":"8","verse":"34","text":"En jahawi ngʼat mawinja,"},{"book":"Ngeche","chapter":"8","verse":"35","text":"Nimar ngʼat monwangʼa oyudo ngima"},{"book":"Ngeche","chapter":"8","verse":"36","text":"To ngʼatno ma odaga hinyore kende owuon;"}]}