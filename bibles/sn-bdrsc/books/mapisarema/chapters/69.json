{"data":[{"book":"Mapisarema","chapter":"69","verse":"1","text":"Ndiponesei, imi Mwari,"},{"book":"Mapisarema","chapter":"69","verse":"2","text":"Ndinonyura munhope yakadzika,"},{"book":"Mapisarema","chapter":"69","verse":"3","text":"Ndaneta nokuridza mhere yokuti ndibatsirwe;"},{"book":"Mapisarema","chapter":"69","verse":"4","text":"Vanondivenga ndisina mhosva vakawanda"},{"book":"Mapisarema","chapter":"69","verse":"5","text":"Munoziva upenzi hwangu, imi Mwari;"},{"book":"Mapisarema","chapter":"69","verse":"6","text":"Vaya vane tariro mamuri"},{"book":"Mapisarema","chapter":"69","verse":"7","text":"Nokuti ndinotsunga kusekwa hangu nokuda kwenyu,"},{"book":"Mapisarema","chapter":"69","verse":"8","text":"Ndiri mutorwa kuhama dzangu,"},{"book":"Mapisarema","chapter":"69","verse":"9","text":"nokuti kushingairira imba yenyu kwandipedza,"},{"book":"Mapisarema","chapter":"69","verse":"10","text":"Pandinochema uye ndichitsanya,"},{"book":"Mapisarema","chapter":"69","verse":"11","text":"pandinofuka nguo dzamasaga,"},{"book":"Mapisarema","chapter":"69","verse":"12","text":"Vaya vanogara pasuo vanondiseka,"},{"book":"Mapisarema","chapter":"69","verse":"13","text":"Asi ndinonyengetera kwamuri, imi Mwari,"},{"book":"Mapisarema","chapter":"69","verse":"14","text":"Ndinunurei mumatope,"},{"book":"Mapisarema","chapter":"69","verse":"15","text":"Musatendera mvura yamafashamu kuti indifukidze,"},{"book":"Mapisarema","chapter":"69","verse":"16","text":"Ndipindurei, imi Jehovha, zvichibva pakunaka kworudo rwenyu;"},{"book":"Mapisarema","chapter":"69","verse":"17","text":"Regai kuvanzira muranda wenyu chiso chenyu;"},{"book":"Mapisarema","chapter":"69","verse":"18","text":"Swederai pedyo mundinunure;"},{"book":"Mapisarema","chapter":"69","verse":"19","text":"Munoziva kusekwa kwangu, kunyadziswa nokusakudzwa kwangu;"},{"book":"Mapisarema","chapter":"69","verse":"20","text":"Kusekwa kwaputsa mwoyo wangu,"},{"book":"Mapisarema","chapter":"69","verse":"21","text":"Vakaisa nduru mune zvokudya zvangu,"},{"book":"Mapisarema","chapter":"69","verse":"22","text":"Tafura yagadzirwa pamberi pavo ngaive musungo;"},{"book":"Mapisarema","chapter":"69","verse":"23","text":"Meso avo ngaapofumadzwe kuti varege kuona,"},{"book":"Mapisarema","chapter":"69","verse":"24","text":"Dururirai hasha dzenyu pamusoro pavo;"},{"book":"Mapisarema","chapter":"69","verse":"25","text":"Nzvimbo yavo ngaisiyiwe;"},{"book":"Mapisarema","chapter":"69","verse":"26","text":"Nokuti vanotambudza vaya vamakarova,"},{"book":"Mapisarema","chapter":"69","verse":"27","text":"Vapei mhosva pamusoro pemhosva;"},{"book":"Mapisarema","chapter":"69","verse":"28","text":"Ngavadzimwe mubhuku roupenyu,"},{"book":"Mapisarema","chapter":"69","verse":"29","text":"Ndiri pakurwadziwa nenhamo;"},{"book":"Mapisarema","chapter":"69","verse":"30","text":"Ndicharumbidza zita raMwari norwiyo,"},{"book":"Mapisarema","chapter":"69","verse":"31","text":"Izvi zvichafadza Jehovha kukunda nzombe,"},{"book":"Mapisarema","chapter":"69","verse":"32","text":"Varombo vachazviona uye vachafara,"},{"book":"Mapisarema","chapter":"69","verse":"33","text":"Jehovha anonzwa vanoshayiwa"},{"book":"Mapisarema","chapter":"69","verse":"34","text":"Denga nenyika ngazvimurumbidze,"},{"book":"Mapisarema","chapter":"69","verse":"35","text":"nokuti Mwari achaponesa Zioni"},{"book":"Mapisarema","chapter":"69","verse":"36","text":"Vana vavaranda vake vacharipiwa senhaka,"}]}