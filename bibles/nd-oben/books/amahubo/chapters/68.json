{"data":[{"book":"AmaHubo","chapter":"68","verse":"1","text":"UNkulunkulu kaphakame, izitha zakhe azihlakazwe;"},{"book":"AmaHubo","chapter":"68","verse":"2","text":"Njengentuthu iphetshulwa ngumoya lawe baphephulele kude;"},{"book":"AmaHubo","chapter":"68","verse":"3","text":"Kodwa sekungathi abalungileyo bangajabula"},{"book":"AmaHubo","chapter":"68","verse":"4","text":"Hlabelelani kuNkulunkulu,"},{"book":"AmaHubo","chapter":"68","verse":"5","text":"Uyise wezintandane, umlamuleli wabafelokazi,"},{"book":"AmaHubo","chapter":"68","verse":"6","text":"UNkulunkulu uyabalungisela indawo ezimulini labo abalesizungu,"},{"book":"AmaHubo","chapter":"68","verse":"7","text":"Kwathi lapho ukhokhela abantu bakho, Oh Nkulunkulu,"},{"book":"AmaHubo","chapter":"68","verse":"8","text":"umhlaba wanyikinyeka, umkhathi walithulula izulu,"},{"book":"AmaHubo","chapter":"68","verse":"9","text":"Wanisa izulu elinengi, Oh Nkulunkulu;"},{"book":"AmaHubo","chapter":"68","verse":"10","text":"Abantu bakho bahlala kulo bazinza,"},{"book":"AmaHubo","chapter":"68","verse":"11","text":"INkosi yalikhupha ilizwi,"},{"book":"AmaHubo","chapter":"68","verse":"12","text":"“Makhosi lamabutho phangisani libaleke;"},{"book":"AmaHubo","chapter":"68","verse":"13","text":"Khonapho lisalele phakathi kwemililo yezihonqo,"},{"book":"AmaHubo","chapter":"68","verse":"14","text":"Lapho uSomandla wawahlakaza amakhosi elizwe,"},{"book":"AmaHubo","chapter":"68","verse":"15","text":"Izintaba zeBhashani yizintaba ezilesithunzi;"},{"book":"AmaHubo","chapter":"68","verse":"16","text":"Kungani linyonkoloza ngomona,"},{"book":"AmaHubo","chapter":"68","verse":"17","text":"Izinqola zikaNkulunkulu zingamatshumi ezinkulungwane;"},{"book":"AmaHubo","chapter":"68","verse":"18","text":"Wathi ekukhuphukeni kwakho uqansa,"},{"book":"AmaHubo","chapter":"68","verse":"19","text":"Udumo kalube kuThixo, kuNkulunkulu uMsindisi wethu,"},{"book":"AmaHubo","chapter":"68","verse":"20","text":"UNkulunkulu wethu nguNkulunkulu osindisayo;"},{"book":"AmaHubo","chapter":"68","verse":"21","text":"Ngeqiniso uNkulunkulu uzawachoboza amakhanda ezitha zakhe,"},{"book":"AmaHubo","chapter":"68","verse":"22","text":"INkosi ithi, “Ngizabaletha besuka eBhashani;"},{"book":"AmaHubo","chapter":"68","verse":"23","text":"ukuze ligxamuze inyawo zenu egazini lezitha zenu,"},{"book":"AmaHubo","chapter":"68","verse":"24","text":"Udwendwe lwakho Nkulunkulu seluqhamuka, yebo"},{"book":"AmaHubo","chapter":"68","verse":"25","text":"Phambili ngabahlabeleli, kulandele abamachacho;"},{"book":"AmaHubo","chapter":"68","verse":"26","text":"Dumisani uNkulunkulu ebandleni elikhulu;"},{"book":"AmaHubo","chapter":"68","verse":"27","text":"Masinyane isizwana sakoBhenjamini sibakhokhele,"},{"book":"AmaHubo","chapter":"68","verse":"28","text":"Veza amandla akho, Oh Nkulunkulu,"},{"book":"AmaHubo","chapter":"68","verse":"29","text":"Ngenxa yethempeli lakho eliseJerusalema"},{"book":"AmaHubo","chapter":"68","verse":"30","text":"Khuza isilo esiphakathi kwemihlanga"},{"book":"AmaHubo","chapter":"68","verse":"31","text":"Amanxusa azavela eGibhithe;"},{"book":"AmaHubo","chapter":"68","verse":"32","text":"Hlabelelani kuNkulunkulu, awu lina mibuso yomhlaba,"},{"book":"AmaHubo","chapter":"68","verse":"33","text":"kuye ohlala phezu kwesibhakabhaka sekadeni,"},{"book":"AmaHubo","chapter":"68","verse":"34","text":"Fakazani ngamandla kaNkulunkulu,"},{"book":"AmaHubo","chapter":"68","verse":"35","text":"Uyamangalisa, Oh Nkulunkulu, usendlini yakho engcwele;"}]}