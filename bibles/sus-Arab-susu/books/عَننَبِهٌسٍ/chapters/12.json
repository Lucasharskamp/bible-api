{"data":[{"book":"عَننَبِ هٌسٍ","chapter":"12","verse":"1","text":"«عٍقِرَ مِ وَ مَ ﭑ مَدَشُقٍ."},{"book":"عَننَبِ هٌسٍ","chapter":"12","verse":"2","text":"عٍقِرَ مِ قٍ قُقَقُ قٍنمَ،"},{"book":"عَننَبِ هٌسٍ","chapter":"12","verse":"3","text":"عَلَ يُدَيَكَيٍ مَكِيتِمَ نّ،"},{"book":"عَننَبِ هٌسٍ","chapter":"12","verse":"4","text":"عٍ نَ عٍ نفَ قُرِ كُي، يَشُبَ نَشَ عَ تَارَ تِنفِلِنيِ سُشُ."},{"book":"عَننَبِ هٌسٍ","chapter":"12","verse":"5","text":"عٍ نُن مَلٍكّ نَشَ فٍرٍ سٌ، عَ شُتُ سْتْ عَ مَ."},{"book":"عَننَبِ هٌسٍ","chapter":"12","verse":"6","text":"عَلَتَلَ نَن مَنفّ رَ."},{"book":"عَننَبِ هٌسٍ","chapter":"12","verse":"7","text":"عِ تَن شَ قَ عِ مَرِفِ عَلَ مَ،"},{"book":"عَننَبِ هٌسٍ","chapter":"12","verse":"8","text":"يُلّ سِكٍيلِ وُلٍ رَوَلِمَ، عَ مِشِ تْورْ."},{"book":"عَننَبِ هٌسٍ","chapter":"12","verse":"9","text":"عٍقِرَ مِ بَرَ عَ قَلَ،"},{"book":"عَننَبِ هٌسٍ","chapter":"12","verse":"10","text":"«ﭑ تَن نَن عِ مَرِفِ عَلَتَلَ رَ،"},{"book":"عَننَبِ هٌسٍ","chapter":"12","verse":"11","text":"ﭑ نُ وْيّنمَ نَمِحْنمّيٍ بّ،"},{"book":"عَننَبِ هٌسٍ","chapter":"12","verse":"12","text":"فَلَدِكَيٍ لُمَ نّ قٍ حَاشِ رَبَ رَ."},{"book":"عَننَبِ هٌسٍ","chapter":"12","verse":"13","text":"يَشُبَ نَشَ عَ فِ عَرَ مِ بْشِ مَ."},{"book":"عَننَبِ هٌسٍ","chapter":"12","verse":"14","text":"عَلَتَلَ نَشَ عِسِرَيِلَ رَ مِنِ مِسِرَ بْشِ مَ"},{"book":"عَننَبِ هٌسٍ","chapter":"12","verse":"15","text":"عٍقِرَ مِ بَرَ عَلَتَلَ رَشْنْ عَ حَاشِ رَ."}]}