{"data":[{"book":"Mapisarema","chapter":"118","verse":"1","text":"Vongai Jehovha, nokuti akanaka;"},{"book":"Mapisarema","chapter":"118","verse":"2","text":"Israeri ngaati,"},{"book":"Mapisarema","chapter":"118","verse":"3","text":"Imba yaAroni ngaiti,"},{"book":"Mapisarema","chapter":"118","verse":"4","text":"Avo vanotya Jehovha ngavati,"},{"book":"Mapisarema","chapter":"118","verse":"5","text":"Mukurwadziwa kwangu ndakadana kuna Jehovha,"},{"book":"Mapisarema","chapter":"118","verse":"6","text":"Jehovha aneni; handingatyi."},{"book":"Mapisarema","chapter":"118","verse":"7","text":"Jehovha aneni; ndiye mubatsiri wangu."},{"book":"Mapisarema","chapter":"118","verse":"8","text":"Zviri nani kutizira kuna Jehovha"},{"book":"Mapisarema","chapter":"118","verse":"9","text":"Zviri nani kutizira kuna Jehovha"},{"book":"Mapisarema","chapter":"118","verse":"10","text":"Ndudzi dzose dzakandikomba,"},{"book":"Mapisarema","chapter":"118","verse":"11","text":"Vakandikomba pamativi ose,"},{"book":"Mapisarema","chapter":"118","verse":"12","text":"Vakandimomotera senyuchi,"},{"book":"Mapisarema","chapter":"118","verse":"13","text":"Ndakasundidzirwa shure uye ndikada kuwa,"},{"book":"Mapisarema","chapter":"118","verse":"14","text":"Jehovha ndiye simba rangu norwiyo rwangu;"},{"book":"Mapisarema","chapter":"118","verse":"15","text":"Kupembera kwomufaro nokukunda"},{"book":"Mapisarema","chapter":"118","verse":"16","text":"Ruoko rworudyi rwaJehovha rwakasimudzirwa kumusoro;"},{"book":"Mapisarema","chapter":"118","verse":"17","text":"Handingafi asi ndichararama,"},{"book":"Mapisarema","chapter":"118","verse":"18","text":"Jehovha akandiranga kwazvo,"},{"book":"Mapisarema","chapter":"118","verse":"19","text":"Ndizarurireiwo masuo okururama;"},{"book":"Mapisarema","chapter":"118","verse":"20","text":"Iri ndiro suo raJehovha"},{"book":"Mapisarema","chapter":"118","verse":"21","text":"Ndichakuvongai, nokuti makandipindura;"},{"book":"Mapisarema","chapter":"118","verse":"22","text":"Dombo rakarambwa navavaki"},{"book":"Mapisarema","chapter":"118","verse":"23","text":"Jehovha akaita izvi,"},{"book":"Mapisarema","chapter":"118","verse":"24","text":"Iri ndiro zuva rakaitwa naJehovha;"},{"book":"Mapisarema","chapter":"118","verse":"25","text":"Haiwa Jehovha, tiponesei;"},{"book":"Mapisarema","chapter":"118","verse":"26","text":"Akaropafadzwa uyo anouya muzita raJehovha."},{"book":"Mapisarema","chapter":"118","verse":"27","text":"Jehovha ndiye Mwari,"},{"book":"Mapisarema","chapter":"118","verse":"28","text":"Imi muri Mwari wangu, uye ndichakuvongai;"},{"book":"Mapisarema","chapter":"118","verse":"29","text":"Vongai Jehovha, nokuti akanaka;"}]}