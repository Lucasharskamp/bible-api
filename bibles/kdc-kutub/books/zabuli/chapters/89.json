{"data":[{"book":"Zabuli","chapter":"89","verse":"1","text":"Go Mndewa Imulungu,"},{"book":"Zabuli","chapter":"89","verse":"2","text":"Nolonga kamba unogelo wako hawishaga okala milele,"},{"book":"Zabuli","chapter":"89","verse":"3","text":"Kulonga, “Nitenda lagano na ija inimsagule,"},{"book":"Zabuli","chapter":"89","verse":"4","text":"‘Nowimiza ulelo wako milele,"},{"book":"Zabuli","chapter":"89","verse":"5","text":"Gweye Mndewa Imulungu, ulanga otogola unzonza wako,"},{"book":"Zabuli","chapter":"89","verse":"6","text":"Kwavija nani uchanha kuulanga yoilinga na Mndewa Imulungu?"},{"book":"Zabuli","chapter":"89","verse":"7","text":"Kumting'hano wa iwanhu wa Imulungu, Imulungu kodumbigwa ng'hani,"},{"book":"Zabuli","chapter":"89","verse":"8","text":"Gweye Mndewa Imulungu uli na udahi wose,"},{"book":"Zabuli","chapter":"89","verse":"9","text":"Gweye kotawala bahali imemile ng'humbi,"},{"book":"Zabuli","chapter":"89","verse":"10","text":"Gweye kudihonda dizoka Lahabu kamba imoja wa iwanhu wachinjigwe,"},{"book":"Zabuli","chapter":"89","verse":"11","text":"Ulanga wako gweye na isi vivija yako,"},{"book":"Zabuli","chapter":"89","verse":"12","text":"Kulumba kasikazini na kusini,"},{"book":"Zabuli","chapter":"89","verse":"13","text":"Mkono wako umema udahi,"},{"book":"Zabuli","chapter":"89","verse":"14","text":"Haki na nhaguso acho chandusilo cha kigoda chako cha undewa,"},{"book":"Zabuli","chapter":"89","verse":"15","text":"Wamweda iwanhu oifunza kukutogola kwa deng'ho,"},{"book":"Zabuli","chapter":"89","verse":"16","text":"Odeng'helela twaga jako imisi yose,"},{"book":"Zabuli","chapter":"89","verse":"17","text":"Kwavija gweye kwa ukulu na nguvu yao,"},{"book":"Zabuli","chapter":"89","verse":"18","text":"Go Mndewa Imulungu, mkalizi wetu kalawa kumwako,"},{"book":"Zabuli","chapter":"89","verse":"19","text":"Kulonga mkipindi fulani mzinzozi,"},{"book":"Zabuli","chapter":"89","verse":"20","text":"Nimpata Daudi msang'hanaji wangu,"},{"book":"Zabuli","chapter":"89","verse":"21","text":"Ganza jangu domgogeleza,"},{"book":"Zabuli","chapter":"89","verse":"22","text":"Haduhu imunhu yomwihila yonda yamuhume,"},{"book":"Zabuli","chapter":"89","verse":"23","text":"Nowahonda iwanhu omwihila kuulongozi wake,"},{"book":"Zabuli","chapter":"89","verse":"24","text":"Kutamanilwa kwangu na unogelo wangu hawishaga okala hamoja na heyo,"},{"book":"Zabuli","chapter":"89","verse":"25","text":"Nougangamiza utawala wake na nguvu,"},{"book":"Zabuli","chapter":"89","verse":"26","text":"Na heyo konikema, ‘Gweye kwa tatangu,"},{"book":"Zabuli","chapter":"89","verse":"27","text":"Vivija nomsagula yawe chaudele wangu,"},{"book":"Zabuli","chapter":"89","verse":"28","text":"Nowika unogelo wangu hawishaga kumwake milele,"},{"book":"Zabuli","chapter":"89","verse":"29","text":"Nowika ulelo wake milele,"},{"book":"Zabuli","chapter":"89","verse":"30","text":"Kamba wanage ogaleka malagizo gangu,"},{"book":"Zabuli","chapter":"89","verse":"31","text":"kamba odilema lagizo jangu,"},{"book":"Zabuli","chapter":"89","verse":"32","text":"noikoboza nzambi yao kwa fimbo,"},{"book":"Zabuli","chapter":"89","verse":"33","text":"Lakini siusegesa unogelo wangu hawishaga kumwake,"},{"book":"Zabuli","chapter":"89","verse":"34","text":"Niye sibena lagano jangu,"},{"book":"Zabuli","chapter":"89","verse":"35","text":"Mwanza umoja na kwa siku zose,"},{"book":"Zabuli","chapter":"89","verse":"36","text":"kamba ulelo wake osigala milele,"},{"book":"Zabuli","chapter":"89","verse":"37","text":"Chosigala milele kamba mbalamwezi,"},{"book":"Zabuli","chapter":"89","verse":"38","text":"Lakini sambi gweye kumulema,"},{"book":"Zabuli","chapter":"89","verse":"39","text":"Kudilema lagano jako na msang'hanaji wako,"},{"book":"Zabuli","chapter":"89","verse":"40","text":"Kubomola ng'huta zake zose,"},{"book":"Zabuli","chapter":"89","verse":"41","text":"Wanhu obita behi wamboka vinhu vake,"},{"book":"Zabuli","chapter":"89","verse":"42","text":"Kuwagwaa mhumo iwanhu omwihila,"},{"book":"Zabuli","chapter":"89","verse":"43","text":"Kuhindusa ukali wa sime jake,"},{"book":"Zabuli","chapter":"89","verse":"44","text":"Kumambukiza kuitapa kwake,"},{"book":"Zabuli","chapter":"89","verse":"45","text":"Kuzihungula siku za ubwanga wake,"},{"book":"Zabuli","chapter":"89","verse":"46","text":"Mbaka lini gweye Mndewa Imulungu koifisa milele?"},{"book":"Zabuli","chapter":"89","verse":"47","text":"Kumbuka ugima wangu vuubita himahima."},{"book":"Zabuli","chapter":"89","verse":"48","text":"Nani yokala mgima na hafa bule?"},{"book":"Zabuli","chapter":"89","verse":"49","text":"Mndewa Imulungu wa kwahi unogelo wako hawishaga wa aho haichanduso,"},{"book":"Zabuli","chapter":"89","verse":"50","text":"Mndewa Imulungu kumbuka msang'hanaji wako viyolongigwa,"},{"book":"Zabuli","chapter":"89","verse":"51","text":"Wanhu wanideta na kunyonela,"},{"book":"Zabuli","chapter":"89","verse":"52","text":"Mndewa Imulungu yatogolwe, milele."}]}