{"data":[{"book":"AmaHubo","chapter":"109","verse":"1","text":"Oh Nkulunkulu, wena engikudumisayo,"},{"book":"AmaHubo","chapter":"109","verse":"2","text":"ngoba abantu ababi abalenkohliso"},{"book":"AmaHubo","chapter":"109","verse":"3","text":"Bangihonqolozela ngamazwi enzondo;"},{"book":"AmaHubo","chapter":"109","verse":"4","text":"Ukubathanda kwami bakuphindisela ngokungisola,"},{"book":"AmaHubo","chapter":"109","verse":"5","text":"Bangiphindisela okuhle ngokubi,"},{"book":"AmaHubo","chapter":"109","verse":"6","text":"Bathi, â€œKhethani umuntu omubi ozamelana laye;"},{"book":"AmaHubo","chapter":"109","verse":"7","text":"Nxa esethonisiswa kafunyanwe elecala,"},{"book":"AmaHubo","chapter":"109","verse":"8","text":"Sengathi insuku zakhe zingabazilutshwana;"},{"book":"AmaHubo","chapter":"109","verse":"9","text":"Sengathi abantwabakhe bangaba zintandane"},{"book":"AmaHubo","chapter":"109","verse":"10","text":"Sengathi abantwabakhe bangaba zinzulane zeziceli;"},{"book":"AmaHubo","chapter":"109","verse":"11","text":"Sengathi lowo aboleke kuye angamemuka konke alakho;"},{"book":"AmaHubo","chapter":"109","verse":"12","text":"Akungabikhona omenzela umusa"},{"book":"AmaHubo","chapter":"109","verse":"13","text":"Sengathi izizukulwane zakhe zingaphela,"},{"book":"AmaHubo","chapter":"109","verse":"14","text":"Sengathi ububi bukayise bungakhunjulwa phambi kukaThixo,"},{"book":"AmaHubo","chapter":"109","verse":"15","text":"Sengathi izono zabo zingahlala zisobala phambi kukaThixo,"},{"book":"AmaHubo","chapter":"109","verse":"16","text":"Ngoba kazange akhumbule ukwenza umusa"},{"book":"AmaHubo","chapter":"109","verse":"17","text":"Wayekuthanda ukwethesa isithuko,"},{"book":"AmaHubo","chapter":"109","verse":"18","text":"Wayevunula ukuthuka njengesigqoko;"},{"book":"AmaHubo","chapter":"109","verse":"19","text":"Sengathi kungaba njengengubo athandelwe ngayo"},{"book":"AmaHubo","chapter":"109","verse":"20","text":"Sengathi lokhu kungaba yisijeziso sikaThixo kulabo abangethesa amacala,"},{"book":"AmaHubo","chapter":"109","verse":"21","text":"Kodwa wena, Oh Thixo woBukhosi,"},{"book":"AmaHubo","chapter":"109","verse":"22","text":"Ngoba ngingumyanga ngiyaswela,"},{"book":"AmaHubo","chapter":"109","verse":"23","text":"Ngiyafiphala njengethunzi lakusihlwa;"},{"book":"AmaHubo","chapter":"109","verse":"24","text":"Amadolo ami asebuthakathaka ngokuzila ukudla;"},{"book":"AmaHubo","chapter":"109","verse":"25","text":"Sengiyinto yokuhlekisa kulabo abangibeka amacala;"},{"book":"AmaHubo","chapter":"109","verse":"26","text":"Akungisize, Oh Thixo Nkulunkulu wami;"},{"book":"AmaHubo","chapter":"109","verse":"27","text":"Kumele bakwazi ukuthi lokhu ngokwesandla sakho;"},{"book":"AmaHubo","chapter":"109","verse":"28","text":"Bangangiqalekisa, kodwa wena uzabusisa;"},{"book":"AmaHubo","chapter":"109","verse":"29","text":"Abangivukelayo kabembeswe ihlazo"},{"book":"AmaHubo","chapter":"109","verse":"30","text":"Ngomlomo wami ngizambabaza kakhulu uThixo;"},{"book":"AmaHubo","chapter":"109","verse":"31","text":"Ngoba umi ngakwesokunene salowo oswelayo,"}]}