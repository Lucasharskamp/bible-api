{"data":[{"book":"AmaHubo","chapter":"69","verse":"1","text":"Ngisindisa, Oh Nkulunkulu,"},{"book":"AmaHubo","chapter":"69","verse":"2","text":"Ngiyagalula ezinzikini ezitotayo,"},{"book":"AmaHubo","chapter":"69","verse":"3","text":"Sengidiniwe yikumemeza ngicela uncedo;"},{"book":"AmaHubo","chapter":"69","verse":"4","text":"Labo abangizondayo kungelasizatho"},{"book":"AmaHubo","chapter":"69","verse":"5","text":"Uyabazi ubuthutha bami, wena Nkulunkulu;"},{"book":"AmaHubo","chapter":"69","verse":"6","text":"Akuthi labo abathemba kuwe"},{"book":"AmaHubo","chapter":"69","verse":"7","text":"Ngiyakubhensela ukugconwa ngenxa yakho,"},{"book":"AmaHubo","chapter":"69","verse":"8","text":"Ngingowemzini kubafowethu,"},{"book":"AmaHubo","chapter":"69","verse":"9","text":"ngoba ukutshisekela indlu yakho kufuthelene kimi,"},{"book":"AmaHubo","chapter":"69","verse":"10","text":"Ngithi lapho ngilila ngizila ukudla,"},{"book":"AmaHubo","chapter":"69","verse":"11","text":"kuthi lapho ngivunula amasaka"},{"book":"AmaHubo","chapter":"69","verse":"12","text":"Labo abalinda emasangweni bayangihleka,"},{"book":"AmaHubo","chapter":"69","verse":"13","text":"Kodwa ngikhuleka kuwe, Oh Thixo,"},{"book":"AmaHubo","chapter":"69","verse":"14","text":"Ngihlenga exhaphozini,"},{"book":"AmaHubo","chapter":"69","verse":"15","text":"Ungayekeli izikhukhula zingigalulise"},{"book":"AmaHubo","chapter":"69","verse":"16","text":"Ngiphendula, Oh Thixo, ngenxa yokulunga kothando lwakho;"},{"book":"AmaHubo","chapter":"69","verse":"17","text":"Ungayifihleli ubuso bakho inceku yakho;"},{"book":"AmaHubo","chapter":"69","verse":"18","text":"Sondela ungikhulule;"},{"book":"AmaHubo","chapter":"69","verse":"19","text":"Uyakwazi ukuthi ngigconwa njani,"},{"book":"AmaHubo","chapter":"69","verse":"20","text":"Ukugconwa sekuyephule inhliziyo yami"},{"book":"AmaHubo","chapter":"69","verse":"21","text":"Bathela inyongo ekudleni kwami"},{"book":"AmaHubo","chapter":"69","verse":"22","text":"Sengathi itafula eliphambi kwabo lingaba ngumjibila;"},{"book":"AmaHubo","chapter":"69","verse":"23","text":"Sengathi amehlo abo angafiphala ukuze bangaboni,"},{"book":"AmaHubo","chapter":"69","verse":"24","text":"Thululela ulaka lwakho phezu kwabo;"},{"book":"AmaHubo","chapter":"69","verse":"25","text":"Sengathi indawo yabo ingaba ngamanxiwa;"},{"book":"AmaHubo","chapter":"69","verse":"26","text":"Ngoba bahlupha labo obalimazayo"},{"book":"AmaHubo","chapter":"69","verse":"27","text":"Bethese amacala phezu kwamacala;"},{"book":"AmaHubo","chapter":"69","verse":"28","text":"Sengathi bangesulwa encwadini yokuphila"},{"book":"AmaHubo","chapter":"69","verse":"29","text":"Ngisebuhlungwini, ngikhathazekile;"},{"book":"AmaHubo","chapter":"69","verse":"30","text":"Ngizalidumisa ibizo likaNkulunkulu ngengoma,"},{"book":"AmaHubo","chapter":"69","verse":"31","text":"Lokhu kuzamthabisa uThixo okwedlula inkabi,"},{"book":"AmaHubo","chapter":"69","verse":"32","text":"Abayanga bazabona bajabule"},{"book":"AmaHubo","chapter":"69","verse":"33","text":"UThixo uyabezwa abaswelayo,"},{"book":"AmaHubo","chapter":"69","verse":"34","text":"Akuthi izulu lomhlaba kumdumise,"},{"book":"AmaHubo","chapter":"69","verse":"35","text":"ngoba uNkulunkulu uzayisindisa iZiyoni"},{"book":"AmaHubo","chapter":"69","verse":"36","text":"abantwana bezinceku zayo lizakuba yilifa labo,"}]}