{"data":[{"book":"Zabuli","chapter":"119","verse":"1","text":"Wamweda iwanhu okala bila kitala,"},{"book":"Zabuli","chapter":"119","verse":"2","text":"Wamweda iwanhu ozigoga amuli zake,"},{"book":"Zabuli","chapter":"119","verse":"3","text":"Awo hawatenda gehile,"},{"book":"Zabuli","chapter":"119","verse":"4","text":"Gweye kutugwaa usi,"},{"book":"Zabuli","chapter":"119","verse":"5","text":"Nolonda nzila zangu zikale goya,"},{"book":"Zabuli","chapter":"119","verse":"6","text":"Abaho niye sigwa kinyala bule,"},{"book":"Zabuli","chapter":"119","verse":"7","text":"Nokushukulu kwa moyo unogile,"},{"book":"Zabuli","chapter":"119","verse":"8","text":"Nogagoga malagizo gako,"},{"book":"Zabuli","chapter":"119","verse":"9","text":"Koigendeleza vilihi mbwanga nzila yake ing'ale?"},{"book":"Zabuli","chapter":"119","verse":"10","text":"Kwa moyo wangu wose nikuzahila,"},{"book":"Zabuli","chapter":"119","verse":"11","text":"Mmoyo wangu niika mbuli yako,"},{"book":"Zabuli","chapter":"119","verse":"12","text":"Go Mndewa Imulungu, utogolwe,"},{"book":"Zabuli","chapter":"119","verse":"13","text":"Kwa mulomo wangu nolonga,"},{"book":"Zabuli","chapter":"119","verse":"14","text":"Nodeng'helela kukweleleza amuli zako,"},{"book":"Zabuli","chapter":"119","verse":"15","text":"Noifunza malagizo gako,"},{"book":"Zabuli","chapter":"119","verse":"16","text":"Nogadeng'helela malagizo gako,"},{"book":"Zabuli","chapter":"119","verse":"17","text":"Unitendele ganogile niye msang'hanaji wako vivino vinili mgima,"},{"book":"Zabuli","chapter":"119","verse":"18","text":"Ugafumbule meso gangu,"},{"book":"Zabuli","chapter":"119","verse":"19","text":"Niye na mgeni hano mwiisi,"},{"book":"Zabuli","chapter":"119","verse":"20","text":"Loho yangu yoluma siku zose,"},{"book":"Zabuli","chapter":"119","verse":"21","text":"Gweye kowakomhokela awo oitapaga,"},{"book":"Zabuli","chapter":"119","verse":"22","text":"Nisegesele bezi na maligo,"},{"book":"Zabuli","chapter":"119","verse":"23","text":"Hata kamba iwakulu okala kiseto mbuli yangu,"},{"book":"Zabuli","chapter":"119","verse":"24","text":"Amuli zako azo deng'ho jangu,"},{"book":"Zabuli","chapter":"119","verse":"25","text":"Loho yangu igogwa na timbwisi,"},{"book":"Zabuli","chapter":"119","verse":"26","text":"Nikusimulila nzila zangu na gweye kunidika,"},{"book":"Zabuli","chapter":"119","verse":"27","text":"Nifunze avija vilondigwa kugoga nzila zako,"},{"book":"Zabuli","chapter":"119","verse":"28","text":"Nidonha kwa usungu,"},{"book":"Zabuli","chapter":"119","verse":"29","text":"Unisegesele nzila za uvizi,"},{"book":"Zabuli","chapter":"119","verse":"30","text":"Nisagula nzila ya kutamanilwa,"},{"book":"Zabuli","chapter":"119","verse":"31","text":"Nizifagatila amuli zako gweye Mndewa Imulungu,"},{"book":"Zabuli","chapter":"119","verse":"32","text":"Nokweleleza malagizo gako,"},{"book":"Zabuli","chapter":"119","verse":"33","text":"Go Mndewa Imulungu, unifunze kugagoga malagizo gako,"},{"book":"Zabuli","chapter":"119","verse":"34","text":"Ngwelele nzewele ili nigagoge malagizo gako,"},{"book":"Zabuli","chapter":"119","verse":"35","text":"Nilongoze mnzila ya malagizo gako,"},{"book":"Zabuli","chapter":"119","verse":"36","text":"Ubidule moyo wangu ukweleleze amuli zako,"},{"book":"Zabuli","chapter":"119","verse":"37","text":"Uhindule meso gangu sikunilole avija havifaya,"},{"book":"Zabuli","chapter":"119","verse":"38","text":"Ugavikize gaja gaulongile kwa msang'hanaji wako,"},{"book":"Zabuli","chapter":"119","verse":"39","text":"Unisegesele maligo gonidumba,"},{"book":"Zabuli","chapter":"119","verse":"40","text":"Nosulukila kugagoga malagizo gako,"},{"book":"Zabuli","chapter":"119","verse":"41","text":"Go Mndewa Imulungu, unogelo wako hawishaga unizile,"},{"book":"Zabuli","chapter":"119","verse":"42","text":"na niye nomwidika ayo yoniliga,"},{"book":"Zabuli","chapter":"119","verse":"43","text":"Unitende nilonge ukweli wako siku zose,"},{"book":"Zabuli","chapter":"119","verse":"44","text":"Nogagoga malagizo gako siku zose,"},{"book":"Zabuli","chapter":"119","verse":"45","text":"Na niye notembela kwa nyafasi,"},{"book":"Zabuli","chapter":"119","verse":"46","text":"Nozilonga amuli zako kuulongozi wa iwandewa,"},{"book":"Zabuli","chapter":"119","verse":"47","text":"Na niye nodeng'helela mmalagizo gako,"},{"book":"Zabuli","chapter":"119","verse":"48","text":"Nosulukila malagizo gako,"},{"book":"Zabuli","chapter":"119","verse":"49","text":"Kumbuka mbuli yako kwa msang'hanaji wako,"},{"book":"Zabuli","chapter":"119","verse":"50","text":"Deng'ho jangu mmagazo gangu ajo dino,"},{"book":"Zabuli","chapter":"119","verse":"51","text":"Iwanhu oitapa onibezeli siku zose,"},{"book":"Zabuli","chapter":"119","verse":"52","text":"Go Mndewa Imulungu, nogakumbuka malagizo gako ga umwaka,"},{"book":"Zabuli","chapter":"119","verse":"53","text":"Nogevuzika ng'hani mbuli ya iwanhu wehile,"},{"book":"Zabuli","chapter":"119","verse":"54","text":"Malagizo gako awo lwila wangu,"},{"book":"Zabuli","chapter":"119","verse":"55","text":"Go Mndewa Imulungu, ikilo nodikumbuka twaga jako,"},{"book":"Zabuli","chapter":"119","verse":"56","text":"Ganogile gano geza kumwangu,"},{"book":"Zabuli","chapter":"119","verse":"57","text":"Mndewa Imulungu ayo fungu jangu,"},{"book":"Zabuli","chapter":"119","verse":"58","text":"Nikuzahila gweye kwa moyo wangu wose,"},{"book":"Zabuli","chapter":"119","verse":"59","text":"Nizigelegeza nzila zangu,"},{"book":"Zabuli","chapter":"119","verse":"60","text":"Nilopola na sikawile,"},{"book":"Zabuli","chapter":"119","verse":"61","text":"Hata kamba iwanhu wehile onifunga kwa nzabi,"},{"book":"Zabuli","chapter":"119","verse":"62","text":"Kilo kikulu nolamuka na nokugwaa nhogolwa,"},{"book":"Zabuli","chapter":"119","verse":"63","text":"Niye na mbwiga wa wose okudumba gweye,"},{"book":"Zabuli","chapter":"119","verse":"64","text":"Go Mndewa Imulungu, isi yose imema unogelo wako hawishaga,"},{"book":"Zabuli","chapter":"119","verse":"65","text":"Go Mndewa Imulungu, kunitendela ganogile niye msang'hanaji wako,"},{"book":"Zabuli","chapter":"119","verse":"66","text":"Nifunze nzewele na magesa ganogile,"},{"book":"Zabuli","chapter":"119","verse":"67","text":"Ning'hali sinadununzwa niye ng'hala nyagile,"},{"book":"Zabuli","chapter":"119","verse":"68","text":"Gweye kunoga na kotenda ganogile,"},{"book":"Zabuli","chapter":"119","verse":"69","text":"Iwanhu oitapa olonga uvizi kumwangu,"},{"book":"Zabuli","chapter":"119","verse":"70","text":"Mizoyo yao haigesa ga wanhu,"},{"book":"Zabuli","chapter":"119","verse":"71","text":"Vinoga kumwangu kamba nigazika,"},{"book":"Zabuli","chapter":"119","verse":"72","text":"Malagizo gako ganoga kumwangu,"},{"book":"Zabuli","chapter":"119","verse":"73","text":"Makono gako ganilumba na ganizenga,"},{"book":"Zabuli","chapter":"119","verse":"74","text":"Wanhu okudumba gweye wanilole na kudeng'helela,"},{"book":"Zabuli","chapter":"119","verse":"75","text":"Go Mndewa Imulungu, notanga kamba gweye kotagusa kwa haki,"},{"book":"Zabuli","chapter":"119","verse":"76","text":"Nopula unogelo wako hawishaga unihoze umoyo,"},{"book":"Zabuli","chapter":"119","verse":"77","text":"Bazi jako dinizile ili mhate ugima,"},{"book":"Zabuli","chapter":"119","verse":"78","text":"Wanhu oitapa wone kinyala kwavija wanideta kwa uvizi,"},{"book":"Zabuli","chapter":"119","verse":"79","text":"Iwanhu okudumba gweye wanibwelele,"},{"book":"Zabuli","chapter":"119","verse":"80","text":"Moyo wangu utelele kwa kugoga malagizo gako,"},{"book":"Zabuli","chapter":"119","verse":"81","text":"Loho yangu yosulukila ukombola wako,"},{"book":"Zabuli","chapter":"119","verse":"82","text":"Meso gangu hagalola goya kwa kugabeta gaja gaulongile,"},{"book":"Zabuli","chapter":"119","verse":"83","text":"Hamoja niwa kamba mfuko wa ng'hwembe unyalile mdiyosi,"},{"book":"Zabuli","chapter":"119","verse":"84","text":"Msang'hanaji wako kokala miyaka mingahi?"},{"book":"Zabuli","chapter":"119","verse":"85","text":"Wanhu oitapa wanihimbila simo,"},{"book":"Zabuli","chapter":"119","verse":"86","text":"Malagizo gako gose ga kweli,"},{"book":"Zabuli","chapter":"119","verse":"87","text":"Wakala behi kunibananga mwiisi,"},{"book":"Zabuli","chapter":"119","verse":"88","text":"Ukalize ugima wangu kwa unogelo wako hawishaga,"},{"book":"Zabuli","chapter":"119","verse":"89","text":"Go Mndewa Imulungu, mbuli yako yosigala milele,"},{"book":"Zabuli","chapter":"119","verse":"90","text":"Kutamanilwa kwako kosigala ulelo mbaka ulelo,"},{"book":"Zabuli","chapter":"119","verse":"91","text":"Kwa lagizo jako vinhu vose vosigala mbaka lelo,"},{"book":"Zabuli","chapter":"119","verse":"92","text":"Maza malagizo gako hagakuwile deng'ho jangu,"},{"book":"Zabuli","chapter":"119","verse":"93","text":"Milele siisemwa malagizo gako,"},{"book":"Zabuli","chapter":"119","verse":"94","text":"Niye wako gweye, unikombole,"},{"book":"Zabuli","chapter":"119","verse":"95","text":"Wanhu wehile onisepa ili wanikome,"},{"book":"Zabuli","chapter":"119","verse":"96","text":"Nitanga kamba kila kinhu chohuhwa,"},{"book":"Zabuli","chapter":"119","verse":"97","text":"Malagizo gako nogalonda ng'hani,"},{"book":"Zabuli","chapter":"119","verse":"98","text":"Malagizo gako gokala na niye milele,"},{"book":"Zabuli","chapter":"119","verse":"99","text":"Notanga ng'hani kubanza wose wanifunzile,"},{"book":"Zabuli","chapter":"119","verse":"100","text":"Niye na mbala kubanza iwavele,"},{"book":"Zabuli","chapter":"119","verse":"101","text":"Niigomesa mwenyego sikunitende gehile,"},{"book":"Zabuli","chapter":"119","verse":"102","text":"Sigalekile malagizo gako,"},{"book":"Zabuli","chapter":"119","verse":"103","text":"Mbuli zako zamulile ng'hani kumwangu,"},{"book":"Zabuli","chapter":"119","verse":"104","text":"Kubitila malagizo gako nopata nzewele,"},{"book":"Zabuli","chapter":"119","verse":"105","text":"Mbuli yako ayo taa ya magulu gangu,"},{"book":"Zabuli","chapter":"119","verse":"106","text":"Niiduwila na nogavikiza gaja ganilongile,"},{"book":"Zabuli","chapter":"119","verse":"107","text":"Go Mndewa Imulungu, nogaya ng'hani,"},{"book":"Zabuli","chapter":"119","verse":"108","text":"Go Mndewa Imulungu, utogole kupula kwangu kwa kushukulu,"},{"book":"Zabuli","chapter":"119","verse":"109","text":"Ugima wangu wamdizumozumo,"},{"book":"Zabuli","chapter":"119","verse":"110","text":"Wanhu wehile wanitegela mitego,"},{"book":"Zabuli","chapter":"119","verse":"111","text":"Amuli zako awo uhazi wangu milele,"},{"book":"Zabuli","chapter":"119","verse":"112","text":"Nisagula kulawa mmoyo wangu,"},{"book":"Zabuli","chapter":"119","verse":"113","text":"Nowehila iwanhu watapitapi,"},{"book":"Zabuli","chapter":"119","verse":"114","text":"Gweye kwa ngao yangu, kumwako nokimbilila,"},{"book":"Zabuli","chapter":"119","verse":"115","text":"Segeleni kumwangu mweye iwanhu wehile,"},{"book":"Zabuli","chapter":"119","verse":"116","text":"Unigangamize kamba vuulongile na niye nokuwa mgima,"},{"book":"Zabuli","chapter":"119","verse":"117","text":"Unigoge ng'hale goya,"},{"book":"Zabuli","chapter":"119","verse":"118","text":"Gweye kowalema wose ogaleka malagizo gako,"},{"book":"Zabuli","chapter":"119","verse":"119","text":"Wanhu wose wehile kuwasegesa kamba mikupi,"},{"book":"Zabuli","chapter":"119","verse":"120","text":"Nogwaya kwa kukudumba gweye,"},{"book":"Zabuli","chapter":"119","verse":"121","text":"Nitenda ganogile na ga haki,"},{"book":"Zabuli","chapter":"119","verse":"122","text":"Unizamini niye msang'hanaji wako,"},{"book":"Zabuli","chapter":"119","verse":"123","text":"Meso gangu gavulala kwa kubeta ukombola wako,"},{"book":"Zabuli","chapter":"119","verse":"124","text":"Unitendele niye msang'hanaji wako kamba unogelo wako hawishaga viwili,"},{"book":"Zabuli","chapter":"119","verse":"125","text":"Niye na msang'hanaji wako, unigwelele nzewele,"},{"book":"Zabuli","chapter":"119","verse":"126","text":"Go Mndewa Imulungu, sambi acho kipindi cha kutenda kinhu,"},{"book":"Zabuli","chapter":"119","verse":"127","text":"Niye noganogela malagizo gako kubanza zahabu,"},{"book":"Zabuli","chapter":"119","verse":"128","text":"Avo, nogoga malagizo gako gose,"},{"book":"Zabuli","chapter":"119","verse":"129","text":"Amuli zako za unzonza,"},{"book":"Zabuli","chapter":"119","verse":"130","text":"Kusimulila malagizo gako kogala mulenge,"},{"book":"Zabuli","chapter":"119","verse":"131","text":"Nofungula mulomo na nosulukila kamba umbwa mwene ng'hilu,"},{"book":"Zabuli","chapter":"119","verse":"132","text":"Unibwelele na unyonele bazi,"},{"book":"Zabuli","chapter":"119","verse":"133","text":"Uzigoge hatua zangu kamba vuulongile,"},{"book":"Zabuli","chapter":"119","verse":"134","text":"Unikombole na uzulumati wa iwanhu,"},{"book":"Zabuli","chapter":"119","verse":"135","text":"Unimwemwesele kihanga chako niye msang'hanaji wako,"},{"book":"Zabuli","chapter":"119","verse":"136","text":"Meso gangu kolava mahozi kamba lwanda,"},{"book":"Zabuli","chapter":"119","verse":"137","text":"Go Mndewa Imulungu, gweye kwa mwene haki,"},{"book":"Zabuli","chapter":"119","verse":"138","text":"Amuli zako zuulavile za haki,"},{"book":"Zabuli","chapter":"119","verse":"139","text":"Unogelo wangu kumwako onitenda nyone usungu,"},{"book":"Zabuli","chapter":"119","verse":"140","text":"Gaja gaulongile gang'ala,"},{"book":"Zabuli","chapter":"119","verse":"141","text":"Niye na mdodo na onibezeli,"},{"book":"Zabuli","chapter":"119","verse":"142","text":"Haki yako inoga milele,"},{"book":"Zabuli","chapter":"119","verse":"143","text":"Magazo na giogio vingoga,"},{"book":"Zabuli","chapter":"119","verse":"144","text":"Milele amuli zako za haki,"},{"book":"Zabuli","chapter":"119","verse":"145","text":"Nokulilila kwa moyo wangu wose,"},{"book":"Zabuli","chapter":"119","verse":"146","text":"Nokulilila unikombole,"},{"book":"Zabuli","chapter":"119","verse":"147","text":"Usiku ung'hali haunakucha nolamuka na nokupula unitaze,"},{"book":"Zabuli","chapter":"119","verse":"148","text":"Nokala meso ikilo chose,"},{"book":"Zabuli","chapter":"119","verse":"149","text":"Go Mndewa Imulungu, kwa unogelo wako hawishaga unitegeleze,"},{"book":"Zabuli","chapter":"119","verse":"150","text":"Awo wehile onikwesela behi,"},{"book":"Zabuli","chapter":"119","verse":"151","text":"Lakini gweye Mndewa Imulungu kwa behi na niye,"},{"book":"Zabuli","chapter":"119","verse":"152","text":"Tangu umwaka niifunza amuli zako,"},{"book":"Zabuli","chapter":"119","verse":"153","text":"Ulole magazo gangu na unikombole,"},{"book":"Zabuli","chapter":"119","verse":"154","text":"Unigombele na unikombole,"},{"book":"Zabuli","chapter":"119","verse":"155","text":"Wanhu wehile hawakomboligwa ng'o,"},{"book":"Zabuli","chapter":"119","verse":"156","text":"Go Mndewa Imulungu, gweye kumema bazi,"},{"book":"Zabuli","chapter":"119","verse":"157","text":"Iwanhu onidununza na onihila wengi,"},{"book":"Zabuli","chapter":"119","verse":"158","text":"Nowalola iwanhu hawatamanilwa na nowehila,"},{"book":"Zabuli","chapter":"119","verse":"159","text":"Lola go Mndewa Imulungu, vinigalonda malagizo gako,"},{"book":"Zabuli","chapter":"119","verse":"160","text":"Gose kwa gose kwa mbuli yako awo ukweli,"},{"book":"Zabuli","chapter":"119","verse":"161","text":"Iwakulu wanidununza bila kitala chochose,"},{"book":"Zabuli","chapter":"119","verse":"162","text":"Noideng'helela mbuli yako,"},{"book":"Zabuli","chapter":"119","verse":"163","text":"Nowihila ng'hani uvizi,"},{"book":"Zabuli","chapter":"119","verse":"164","text":"Nokutogola miyanza saba kwa siku dimoja,"},{"book":"Zabuli","chapter":"119","verse":"165","text":"Awo oganogela malagizo gako wamema tindiwalo,"},{"book":"Zabuli","chapter":"119","verse":"166","text":"Go Mndewa Imulungu, nokubeta unikombole,"},{"book":"Zabuli","chapter":"119","verse":"167","text":"Moyo wangu uzigoga amuli zako,"},{"book":"Zabuli","chapter":"119","verse":"168","text":"Nigagoga malagizo gako na amuli zako,"},{"book":"Zabuli","chapter":"119","verse":"169","text":"Kililo changu kivike kumwako gweye Mndewa Imulungu!"},{"book":"Zabuli","chapter":"119","verse":"170","text":"Kupula kwangu kuvike kumwako,"},{"book":"Zabuli","chapter":"119","verse":"171","text":"Nolonga nhogolwa zako siku zose,"},{"book":"Zabuli","chapter":"119","verse":"172","text":"Lulimi lwangu lokwimba kwa mbuli yako,"},{"book":"Zabuli","chapter":"119","verse":"173","text":"Siku zose ulonde kunitaza,"},{"book":"Zabuli","chapter":"119","verse":"174","text":"Go Mndewa Imulungu, nosulukila ng'hani unikombole,"},{"book":"Zabuli","chapter":"119","verse":"175","text":"Leka niwe mgima na nikutogole gweye,"},{"book":"Zabuli","chapter":"119","verse":"176","text":"Niwa tanga nzulule kamba ng'hondolo yagile,"}]}