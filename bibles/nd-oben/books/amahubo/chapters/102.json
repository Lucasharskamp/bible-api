{"data":[{"book":"AmaHubo","chapter":"102","verse":"1","text":"Zwana umkhuleko wami, Oh Thixo;"},{"book":"AmaHubo","chapter":"102","verse":"2","text":"Ungangifihleli ubuso bakho"},{"book":"AmaHubo","chapter":"102","verse":"3","text":"Ngoba insuku zami ziyanyamalala njengentuthu;"},{"book":"AmaHubo","chapter":"102","verse":"4","text":"Inhliziyo yami yonakele, ibunile njengotshani;"},{"book":"AmaHubo","chapter":"102","verse":"5","text":"Ngenxa yokububula kwami okukhulu sengiphelile,"},{"book":"AmaHubo","chapter":"102","verse":"6","text":"Sengifana lesikhova senkangala,"},{"book":"AmaHubo","chapter":"102","verse":"7","text":"Ngilala ngikhangele;"},{"book":"AmaHubo","chapter":"102","verse":"8","text":"Ilanga lonke izitha zami ziyangichukuluza;"},{"book":"AmaHubo","chapter":"102","verse":"9","text":"Ngoba ngidla umlotha njengokudla kwami,"},{"book":"AmaHubo","chapter":"102","verse":"10","text":"ngenxa yokuthukuthela kwakho okukhulu,"},{"book":"AmaHubo","chapter":"102","verse":"11","text":"Insuku zami zifana lesithunzi sakusihlwa;"},{"book":"AmaHubo","chapter":"102","verse":"12","text":"Kodwa wena, Oh Thixo, uhlezi ebukhosini lanininini;"},{"book":"AmaHubo","chapter":"102","verse":"13","text":"Uzaphakama uzwele iZiyoni isihawu,"},{"book":"AmaHubo","chapter":"102","verse":"14","text":"Ngoba amatshe ayo aligugu ezincekwini zakho;"},{"book":"AmaHubo","chapter":"102","verse":"15","text":"Izizwe zizalesaba ibizo likaThixo,"},{"book":"AmaHubo","chapter":"102","verse":"16","text":"Ngoba uThixo uzayakha kutsha iZiyoni"},{"book":"AmaHubo","chapter":"102","verse":"17","text":"Uzaphendula imikhuleko yabahluphekayo;"},{"book":"AmaHubo","chapter":"102","verse":"18","text":"Lokhu kakulotshwe phansi kube ngokwesizukulwane sakusasa,"},{"book":"AmaHubo","chapter":"102","verse":"19","text":"â€œUThixo wakhangela phansi ephezulu endlini yakhe engcwele,"},{"book":"AmaHubo","chapter":"102","verse":"20","text":"ukuze ezwe ukububula kwezibotshwa"},{"book":"AmaHubo","chapter":"102","verse":"21","text":"Ngalokho-ke ibizo likaThixo lizamenyezelwa eZiyoni"},{"book":"AmaHubo","chapter":"102","verse":"22","text":"lapha izizwe lemibuso"},{"book":"AmaHubo","chapter":"102","verse":"23","text":"Wawanciphisa amandla ami ngisaphila;"},{"book":"AmaHubo","chapter":"102","verse":"24","text":"Yikho ngasengisithi,"},{"book":"AmaHubo","chapter":"102","verse":"25","text":"Ekuqaleni wabeka izisekelo zomhlaba,"},{"book":"AmaHubo","chapter":"102","verse":"26","text":"Konke kuzabhubha, kodwa wena uzasala;"},{"book":"AmaHubo","chapter":"102","verse":"27","text":"Kodwa wena uhlala unjalo,"},{"book":"AmaHubo","chapter":"102","verse":"28","text":"Abantwana bezinceku zakho bazahlala besemehlweni akho;"}]}