{"data":[{"book":"Zekariya","chapter":"10","verse":"1","text":"Pemphani mvula kwa Yehova pa nthawi yamphukira;"},{"book":"Zekariya","chapter":"10","verse":"2","text":"Mafano amayankhula zachinyengo,"},{"book":"Zekariya","chapter":"10","verse":"3","text":"“Ine ndawakwiyira kwambiri abusa,"},{"book":"Zekariya","chapter":"10","verse":"4","text":"Mu Yuda mudzachokera mwala wapangodya,"},{"book":"Zekariya","chapter":"10","verse":"5","text":"Onse pamodzi adzakhala ngati ngwazi zankhondo"},{"book":"Zekariya","chapter":"10","verse":"6","text":"“Ndidzalimbitsa nyumba ya Yuda"},{"book":"Zekariya","chapter":"10","verse":"7","text":"Aefereimu adzakhala ngati ankhondo amphamvu"},{"book":"Zekariya","chapter":"10","verse":"8","text":"Ndidzaliza mluzu"},{"book":"Zekariya","chapter":"10","verse":"9","text":"Ndinawamwaza pakati pa mitundu ina ya anthu,"},{"book":"Zekariya","chapter":"10","verse":"10","text":"Ndidzawabweretsa kuchokera ku Igupto"},{"book":"Zekariya","chapter":"10","verse":"11","text":"Adzawoloka nyanja ya masautso;"},{"book":"Zekariya","chapter":"10","verse":"12","text":"Ndidzalimbitsa iwo mwa Yehova"}]}