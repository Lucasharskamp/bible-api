{"data":[{"book":"زەبوورەکان","chapter":"73","verse":"1","text":"بێگومان خودا چاکە بۆ ئیسرائیل،"},{"book":"زەبوورەکان","chapter":"73","verse":"2","text":"بەڵام من خەریک بوو پێم بخزێت و"},{"book":"زەبوورەکان","chapter":"73","verse":"3","text":"چونکە بەغیلیم بە لووتبەرزەکان برد،"},{"book":"زەبوورەکان","chapter":"73","verse":"4","text":"ترسیان لە مردن نییە،"},{"book":"زەبوورەکان","chapter":"73","verse":"5","text":"لە ڕەنجکێشانی مرۆڤ بەدەرن،"},{"book":"زەبوورەکان","chapter":"73","verse":"6","text":"بۆیە لووتبەرزییان وەک ملوانکە لە مل کردووە و"},{"book":"زەبوورەکان","chapter":"73","verse":"7","text":"خراپە لە دڵی بێ بەزەییانەوە دێت،"},{"book":"زەبوورەکان","chapter":"73","verse":"8","text":"گاڵتەجاڕی دەکەن و بە خراپە دەدوێن،"},{"book":"زەبوورەکان","chapter":"73","verse":"9","text":"ڕووی دەمیان لە ئاسمان کردووە،"},{"book":"زەبوورەکان","chapter":"73","verse":"10","text":"بۆیە خەڵکی خۆیان دێنە لایان و"},{"book":"زەبوورەکان","chapter":"73","verse":"11","text":"دەڵێن: «چۆن خودا دەزانێت؟"},{"book":"زەبوورەکان","chapter":"73","verse":"12","text":"ئەوەتا ئەمانە بەدکارن،"},{"book":"زەبوورەکان","chapter":"73","verse":"13","text":"بێگومان بە بێهوودە دڵی خۆمم بە پاکی ڕاگرتووە،"},{"book":"زەبوورەکان","chapter":"73","verse":"14","text":"بە درێژایی ڕۆژ لێم قەومابوو،"},{"book":"زەبوورەکان","chapter":"73","verse":"15","text":"ئەگەر بە ڕاشکاوی قسەم دەکرد،"},{"book":"زەبوورەکان","chapter":"73","verse":"16","text":"کاتێک ویستم لەمە تێبگەم،"},{"book":"زەبوورەکان","chapter":"73","verse":"17","text":"هەتا چوومە ناو پیرۆزگای خودا،"},{"book":"زەبوورەکان","chapter":"73","verse":"18","text":"بە ڕاستی لە جێگایەکی خلیسک داتناون،"},{"book":"زەبوورەکان","chapter":"73","verse":"19","text":"چۆن بە چاوتروکانێک وێران بوون،"},{"book":"زەبوورەکان","chapter":"73","verse":"20","text":"ئەی پەروەردگار، وەک خەون کاتی بەئاگا هاتنەوە،"},{"book":"زەبوورەکان","chapter":"73","verse":"21","text":"کاتێک دڵم تەنگ بوو،"},{"book":"زەبوورەکان","chapter":"73","verse":"22","text":"گێل و نەزان بووم،"},{"book":"زەبوورەکان","chapter":"73","verse":"23","text":"لەگەڵ ئەوەشدا، من هەمیشە لەلای تۆم،"},{"book":"زەبوورەکان","chapter":"73","verse":"24","text":"بە ئامۆژگاریت ڕێنماییم دەکەیت،"},{"book":"زەبوورەکان","chapter":"73","verse":"25","text":"بێجگە لە تۆ لە ئاسمان کێم هەیە؟"},{"book":"زەبوورەکان","chapter":"73","verse":"26","text":"جەسته و دڵم کۆتاییان پێدێت،"},{"book":"زەبوورەکان","chapter":"73","verse":"27","text":"ئەوەتا ئەوانەی لە تۆ دوورن وێڵ دەبن،"},{"book":"زەبوورەکان","chapter":"73","verse":"28","text":"بەڵام نزیکبوونەوە لە خودا بۆ من باشە،"}]}