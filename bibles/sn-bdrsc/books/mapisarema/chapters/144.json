{"data":[{"book":"Mapisarema","chapter":"144","verse":"1","text":"Jehovha Dombo rangu ngaarumbidzwe,"},{"book":"Mapisarema","chapter":"144","verse":"2","text":"Ndiye Mwari anondida uye nhare yangu,"},{"book":"Mapisarema","chapter":"144","verse":"3","text":"Haiwa Jehovha, munhu chiiko zvamune hanya naye?"},{"book":"Mapisarema","chapter":"144","verse":"4","text":"Munhu akafanana nomweya wokufema;"},{"book":"Mapisarema","chapter":"144","verse":"5","text":"Zarurai matenga enyu, imi Jehovha, uye muburuke;"},{"book":"Mapisarema","chapter":"144","verse":"6","text":"Tumirai mheni mugoparadzira vavengi;"},{"book":"Mapisarema","chapter":"144","verse":"7","text":"Tambanudzai ruoko rwenyu kubva kumusoro;"},{"book":"Mapisarema","chapter":"144","verse":"8","text":"vane miromo izere nenhema,"},{"book":"Mapisarema","chapter":"144","verse":"9","text":"Haiwa Mwari, ndichakuimbirai rwiyo rutsva;"},{"book":"Mapisarema","chapter":"144","verse":"10","text":"iye anopa kukunda kumadzimambo,"},{"book":"Mapisarema","chapter":"144","verse":"11","text":"Ndirwirei uye mundinunure"},{"book":"Mapisarema","chapter":"144","verse":"12","text":"Ipapo vanakomana vedu pauduku"},{"book":"Mapisarema","chapter":"144","verse":"13","text":"Matura edu achazadzwa"},{"book":"Mapisarema","chapter":"144","verse":"14","text":"nzombe dzedu dzichatakura mitoro inorema."},{"book":"Mapisarema","chapter":"144","verse":"15","text":"Vakaropafadzwa vanhu avo vachaitirwa saizvozvi;"}]}