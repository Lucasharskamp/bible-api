{"data":[{"book":"Ywagruok","chapter":"4","verse":"1","text":"Mano kaka dhahabu osewito berne,"},{"book":"Ywagruok","chapter":"4","verse":"2","text":"Mano kaka yawuot Sayun mabeyo,"},{"book":"Ywagruok","chapter":"4","verse":"3","text":"Kata mana ondiegi chiwo thundegi"},{"book":"Ywagruok","chapter":"4","verse":"4","text":"Nikech riyo oloyo nyathi madhoth"},{"book":"Ywagruok","chapter":"4","verse":"5","text":"Joma yande chamo chiemo mamit,"},{"book":"Ywagruok","chapter":"4","verse":"6","text":"Kum mar joga"},{"book":"Ywagruok","chapter":"4","verse":"7","text":"Jotendgi ne ler marieny moloyo pe"},{"book":"Ywagruok","chapter":"4","verse":"8","text":"To koro gin rotenge moloyo oyare"},{"book":"Ywagruok","chapter":"4","verse":"9","text":"Joma notho e lweny ber moloyo"},{"book":"Ywagruok","chapter":"4","verse":"10","text":"Mon madhoth"},{"book":"Ywagruok","chapter":"4","verse":"11","text":"Jehova Nyasaye oseolo mirimbe mangʼeny"},{"book":"Ywagruok","chapter":"4","verse":"12","text":"Ruodhi mag piny ne ok oyie,"},{"book":"Ywagruok","chapter":"4","verse":"13","text":"To mano notimore"},{"book":"Ywagruok","chapter":"4","verse":"14","text":"Koro gidigni e wangʼ yore"},{"book":"Ywagruok","chapter":"4","verse":"15","text":"Ji ywak kagoyonegi koko niya,"},{"book":"Ywagruok","chapter":"4","verse":"16","text":"Jehova Nyasaye owuon osekeyogi e pinje;"},{"book":"Ywagruok","chapter":"4","verse":"17","text":"Kata kamano, wengewa nojony,"},{"book":"Ywagruok","chapter":"4","verse":"18","text":"Ji ne manyowa kamoro amora mawaluwo,"},{"book":"Ywagruok","chapter":"4","verse":"19","text":"Jogo mane lawowa ne ringo mapiyo"},{"book":"Ywagruok","chapter":"4","verse":"20","text":"Ngʼatno mowir mar Jehova Nyasaye,"},{"book":"Ywagruok","chapter":"4","verse":"21","text":"Moruru kendo beduru moil, yaye Nyar Edom,"},{"book":"Ywagruok","chapter":"4","verse":"22","text":"Yaye jo-Sayun, kum mane okumugo biro rumo;"}]}