{"data":[{"book":"عَننَبِ سَكَرِ","chapter":"9","verse":"1","text":"عَلَتَلَ بَرَ مَسٍنيِ شْرْشْي تِ"},{"book":"عَننَبِ سَكَرِ","chapter":"9","verse":"2","text":"نَ مَسٍنيِ بَرَ شَمَتَ تَا لِ،"},{"book":"عَننَبِ سَكَرِ","chapter":"9","verse":"3","text":"تِرٍ بَرَ تَا مَكَنتَشِ تِ،"},{"book":"عَننَبِ سَكَرِ","chapter":"9","verse":"4","text":"كْنْ مَرِفِ نَ بِرِن بَمَ نّ عَ يِ،"},{"book":"عَننَبِ سَكَرِ","chapter":"9","verse":"5","text":"عَسِكَلْن نَ نَ تٌ،"},{"book":"عَننَبِ سَكَرِ","chapter":"9","verse":"6","text":"سِ مَسُنبُشِ قَمَ نّ سَبَتِدٍ عَسِدٌدِ."},{"book":"عَننَبِ سَكَرِ","chapter":"9","verse":"7","text":"ﭑ قَشّ تِقٍ حْنمَ نّ مّننِ.»"},{"book":"عَننَبِ سَكَرِ","chapter":"9","verse":"8","text":"«ﭑ نَ ﭑ مَ بَنشِ كَنتَ مَ نّ،"},{"book":"عَننَبِ سَكَرِ","chapter":"9","verse":"9","text":"سِيٌنكَيٍ، وٌ شَ حّلّشِن."},{"book":"عَننَبِ سَكَرِ","chapter":"9","verse":"10","text":"«ﭑ سْورِ رَفِسٍ بَمَ نّ عٍقِرَ مِ بْشِ مَ،"},{"book":"عَننَبِ سَكَرِ","chapter":"9","verse":"11","text":"«ﭑ بَرَ وٌ شَ مِشِ سُشُشِيٍ شُن سَرَ"},{"book":"عَننَبِ سَكَرِ","chapter":"9","verse":"12","text":"وٌ شَ فبِلٍن تَا مَكَنتَشِ كُي،"},{"book":"عَننَبِ سَكَرِ","chapter":"9","verse":"13","text":"يُدَيَ قِندِ مَ نّ ﭑ مَ شَلِ رَ."},{"book":"عَننَبِ سَكَرِ","chapter":"9","verse":"14","text":"عَلَتَلَ عَ يّتّ مَكّنّنمَ نّ عٍ بّ،"},{"book":"عَننَبِ سَكَرِ","chapter":"9","verse":"15","text":"مَنفّ عَلَتَلَ عٍ مَكَنتَمَ نّ."},{"book":"عَننَبِ سَكَرِ","chapter":"9","verse":"16","text":"عٍ مَرِفِ عَلَتَلَ عٍ رَكِسِمَ نّ نَ لْشْي،"},{"book":"عَننَبِ سَكَرِ","chapter":"9","verse":"17","text":"عٍ تٌقَنمَ نّ، عٍ رَيَبُ."}]}