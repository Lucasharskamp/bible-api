{"data":[{"book":"Jongʼad Bura","chapter":"5","verse":"1","text":"Chiengʼno Debora kod Barak wuod Abinoam nowero wendni:"},{"book":"Jongʼad Bura","chapter":"5","verse":"2","text":"“Pakuru Jehova Nyasaye"},{"book":"Jongʼad Bura","chapter":"5","verse":"3","text":"“Winjuru, un ruodhi! Chikuru itu, un jotelo!"},{"book":"Jongʼad Bura","chapter":"5","verse":"4","text":"“Yaye Jehova Nyasaye, piny noyiengni,"},{"book":"Jongʼad Bura","chapter":"5","verse":"5","text":"Got Sinai noyiengni"},{"book":"Jongʼad Bura","chapter":"5","verse":"6","text":"“E ndalo mag Shamgar wuod Anath,"},{"book":"Jongʼad Bura","chapter":"5","verse":"7","text":"Teko mar jolweny e miech Israel norumo,"},{"book":"Jongʼad Bura","chapter":"5","verse":"8","text":"Kane giyiero nyiseche manyien,"},{"book":"Jongʼad Bura","chapter":"5","verse":"9","text":"Opak Jehova Nyasaye"},{"book":"Jongʼad Bura","chapter":"5","verse":"10","text":"“Un joma oidho punde marochere,"},{"book":"Jongʼad Bura","chapter":"5","verse":"11","text":"dwond joma wer koa kuonde mamon umboe pi;"},{"book":"Jongʼad Bura","chapter":"5","verse":"12","text":"Debora, chiew, chiew!"},{"book":"Jongʼad Bura","chapter":"5","verse":"13","text":"“Eka jogo mane owe"},{"book":"Jongʼad Bura","chapter":"5","verse":"14","text":"Jomoko kuomgi nowuok e gode mag Efraim,"},{"book":"Jongʼad Bura","chapter":"5","verse":"15","text":"Jotend Isakar ne nigi Debora;"},{"book":"Jongʼad Bura","chapter":"5","verse":"16","text":"En angʼo momiyo ne usiko"},{"book":"Jongʼad Bura","chapter":"5","verse":"17","text":"Gilead nodongʼ loka Jordan."},{"book":"Jongʼad Bura","chapter":"5","verse":"18","text":"Jo-Zebulun nochiwo ngimagi ne tho;"},{"book":"Jongʼad Bura","chapter":"5","verse":"19","text":"“Ruodhi mag Kanaan nobiro,"},{"book":"Jongʼad Bura","chapter":"5","verse":"20","text":"Koa e polo, sulwe nokedo,"},{"book":"Jongʼad Bura","chapter":"5","verse":"21","text":"Aora Kishon noywoyogi,"},{"book":"Jongʼad Bura","chapter":"5","verse":"22","text":"Bangʼ mano mor mar tiende farese"},{"book":"Jongʼad Bura","chapter":"5","verse":"23","text":"Malaika mar Jehova Nyasaye nowacho ni,"},{"book":"Jongʼad Bura","chapter":"5","verse":"24","text":"“Jael ma chi Heber ja-Keni en dhako"},{"book":"Jongʼad Bura","chapter":"5","verse":"25","text":"Nokwayo pi modho, to nomiye chak;"},{"book":"Jongʼad Bura","chapter":"5","verse":"26","text":"Norieyo lwete mondo okaw sigingi mar hema,"},{"book":"Jongʼad Bura","chapter":"5","verse":"27","text":"Nopodho e tiende,"},{"book":"Jongʼad Bura","chapter":"5","verse":"28","text":"“Min Sisera ne ngʼicho gie dirisa;"},{"book":"Jongʼad Bura","chapter":"5","verse":"29","text":"Achiel kuom jotichne ma nyiri mariek nodwoke;"},{"book":"Jongʼad Bura","chapter":"5","verse":"30","text":"‘Donge gipogo gik moyaki ka ngʼato"},{"book":"Jongʼad Bura","chapter":"5","verse":"31","text":"“Omiyo mad wasikgi duto rum, yaye Jehova Nyasaye!"}]}