{"data":[{"book":"Izaga","chapter":"8","verse":"1","text":"Ukuhlakanipha kakumemezi yini?"},{"book":"Izaga","chapter":"8","verse":"2","text":"Emiqolweni yezindlela,"},{"book":"Izaga","chapter":"8","verse":"3","text":"emasangweni angena phakathi kwedolobho,"},{"book":"Izaga","chapter":"8","verse":"4","text":"â€œNgimemeza lina, madoda;"},{"book":"Izaga","chapter":"8","verse":"5","text":"Lina eliyizithutha, zuzani ukuqonda;"},{"book":"Izaga","chapter":"8","verse":"6","text":"Lalelani ngoba ngilitshela izinto eziqakathekileyo;"},{"book":"Izaga","chapter":"8","verse":"7","text":"Umlomo wami ukhuluma okuliqiniso,"},{"book":"Izaga","chapter":"8","verse":"8","text":"Wonke amazwi omlomo wami aqondile;"},{"book":"Izaga","chapter":"8","verse":"9","text":"Kulowo ozwisisayo wonke alungile;"},{"book":"Izaga","chapter":"8","verse":"10","text":"Khetha iseluleko sami kulesiliva,"},{"book":"Izaga","chapter":"8","verse":"11","text":"ngoba ukuhlakanipha kuligugu okudlula amarubhi,"},{"book":"Izaga","chapter":"8","verse":"12","text":"Mina, kuhlakanipha, ngihlala ndawonye lokuqondisisa;"},{"book":"Izaga","chapter":"8","verse":"13","text":"Ukwesaba uThixo yikuzonda ububi;"},{"book":"Izaga","chapter":"8","verse":"14","text":"Ukweluleka lokwahlulela okuhle kungokwami;"},{"book":"Izaga","chapter":"8","verse":"15","text":"Amakhosi abusa ngenxa yami"},{"book":"Izaga","chapter":"8","verse":"16","text":"amakhosana abusa ngami,"},{"book":"Izaga","chapter":"8","verse":"17","text":"Ngiyabathanda labo abangithandayo,"},{"book":"Izaga","chapter":"8","verse":"18","text":"Kimi kukhona inotho lodumo,"},{"book":"Izaga","chapter":"8","verse":"19","text":"Izithelo zami zingcono kulegolide elicengiweyo;"},{"book":"Izaga","chapter":"8","verse":"20","text":"Ngihamba ngendlela yokulunga,"},{"book":"Izaga","chapter":"8","verse":"21","text":"ngiyabela inotho kulabo abangithandayo"},{"book":"Izaga","chapter":"8","verse":"22","text":"UThixo wangenza ngaba khona"},{"book":"Izaga","chapter":"8","verse":"23","text":"ngaba khona ekudabukeni kwezinto,"},{"book":"Izaga","chapter":"8","verse":"24","text":"Kungakabi lezilwandle, ngasengizelwe,"},{"book":"Izaga","chapter":"8","verse":"25","text":"izintaba zingakamiswa ensikeni zazo,"},{"book":"Izaga","chapter":"8","verse":"26","text":"uThixo engakawenzi umhlaba lamaganga,"},{"book":"Izaga","chapter":"8","verse":"27","text":"Ngangikhona uThixo ehlela kuhle izulu,"},{"book":"Izaga","chapter":"8","verse":"28","text":"esenza amayezi emkhathini"},{"book":"Izaga","chapter":"8","verse":"29","text":"ebeka imingcele yolwandle"},{"book":"Izaga","chapter":"8","verse":"30","text":"Ngalesosikhathi ngangingumbumbi eceleni kwakhe."},{"book":"Izaga","chapter":"8","verse":"31","text":"ngithokoza ngomhlaba wakhe wonke"},{"book":"Izaga","chapter":"8","verse":"32","text":"Ngakho-ke, madodana ami, ngilalelani;"},{"book":"Izaga","chapter":"8","verse":"33","text":"Lalelani imfundiso yami ukuze lihlakaniphe;"},{"book":"Izaga","chapter":"8","verse":"34","text":"Ubusisiwe lowomuntu ongilalelayo,"},{"book":"Izaga","chapter":"8","verse":"35","text":"Ngoba lowo ongifumanayo uzuza ukuphila"},{"book":"Izaga","chapter":"8","verse":"36","text":"Kodwa lowo owehlulekayo ukungifumana uyazilimaza;"}]}