{"data":[{"book":"Minkunga","chapter":"106","verse":"1","text":"Luzitisa Yave,"},{"book":"Minkunga","chapter":"106","verse":"2","text":"Nani wulenda yamikisa mavanga ma lulendo ma Yave ?"},{"book":"Minkunga","chapter":"106","verse":"3","text":"Lusakumunu kuidi bobo basimbididi busonga ;"},{"book":"Minkunga","chapter":"106","verse":"4","text":"Wuthebukila moyo, a Yave, mu thangu wumvangila batu baku mamboti"},{"book":"Minkunga","chapter":"106","verse":"5","text":"muingi ndibaka bu muena khini mu mamboti ma batu baku bobo wusobula ;"},{"book":"Minkunga","chapter":"106","verse":"6","text":"Tuvola masumu banga buvodila madise meto ;"},{"book":"Minkunga","chapter":"106","verse":"7","text":"Thangu madise meto baba ku Ezipite,"},{"book":"Minkunga","chapter":"106","verse":"8","text":"Vayi wuba vukisa mu diambu di nkembo wu dizina diandi"},{"book":"Minkunga","chapter":"106","verse":"9","text":"Wutemina mbu wu mbuaki, buna wawu wuyuma ;"},{"book":"Minkunga","chapter":"106","verse":"10","text":"Wuba vukisa mu koko ku mutu wowo wu balenda ;"},{"book":"Minkunga","chapter":"106","verse":"11","text":"Minlangu midiamisa bambeni ziawu ;"},{"book":"Minkunga","chapter":"106","verse":"12","text":"Buna bawilukila zitsila ziandi"},{"book":"Minkunga","chapter":"106","verse":"13","text":"Vayi mu nsualu bavika zimbakana momo kavanga,"},{"book":"Minkunga","chapter":"106","verse":"14","text":"Mu dikanga, bakiyekula mu zinzinunu ziawu ;"},{"book":"Minkunga","chapter":"106","verse":"15","text":"Buna wuba vana biobi balomba ;"},{"book":"Minkunga","chapter":"106","verse":"16","text":"Bavangila Moyize kiphala ayi Aloni ku ka,"},{"book":"Minkunga","chapter":"106","verse":"17","text":"Ntoto wuzibuka ayi wumina Datani ;"},{"book":"Minkunga","chapter":"106","verse":"18","text":"Mbazu yilema va khatitsika batu baba kubakuka,"},{"book":"Minkunga","chapter":"106","verse":"19","text":"Ku Holebi, bavanga muana ngombi ;"},{"book":"Minkunga","chapter":"106","verse":"20","text":"Baviakisa nkembo awu mu mfikula yi ngombi yeti dia biti"},{"book":"Minkunga","chapter":"106","verse":"21","text":"Bazimbakana Nzambi, mutu wubavukisa,"},{"book":"Minkunga","chapter":"106","verse":"22","text":"bikumu mu tsi Hami"},{"book":"Minkunga","chapter":"106","verse":"23","text":"Buna wutuba ti wukana ku babunga ;"},{"book":"Minkunga","chapter":"106","verse":"24","text":"Buna balenza tsi yi makhini,"},{"book":"Minkunga","chapter":"106","verse":"25","text":"Baniunguta mu zinzo ziawu zi ngoto ;"},{"book":"Minkunga","chapter":"106","verse":"26","text":"Buna wuvumuna koko kuandi mu leva ndefi mu diambu diawu"},{"book":"Minkunga","chapter":"106","verse":"27","text":"ti wela buisa nkunꞌawu va khatitsika makanda ;"},{"book":"Minkunga","chapter":"106","verse":"28","text":"Bakiyekula bawu veka kuidi Bali-Pewoli"},{"book":"Minkunga","chapter":"106","verse":"29","text":"Basoka Yave muingi kafuema mu diambu di mavanga mawu mambimbi,"},{"book":"Minkunga","chapter":"106","verse":"30","text":"Buna Pineyasi wutelama ayi wuvuata nsamu ;"},{"book":"Minkunga","chapter":"106","verse":"31","text":"Buna bantangila diawu banga busonga"},{"book":"Minkunga","chapter":"106","verse":"32","text":"Ku minlangu mi Meliba, bafuemisa Yave ;"},{"book":"Minkunga","chapter":"106","verse":"33","text":"Bila batinguna Pheve yi Nzambi"},{"book":"Minkunga","chapter":"106","verse":"34","text":"Basi bunga ko batu"},{"book":"Minkunga","chapter":"106","verse":"35","text":"Vayi bafubakana ayi makanda mankaka"},{"book":"Minkunga","chapter":"106","verse":"36","text":"Babuongimina bitumba biawu,"},{"book":"Minkunga","chapter":"106","verse":"37","text":"Batambika bana bawu bababakala"},{"book":"Minkunga","chapter":"106","verse":"38","text":"Batengula menga ma batu bakambulu zaba diambu ;"},{"book":"Minkunga","chapter":"106","verse":"39","text":"Bakisumuna bawu veka mu momo bavanga,"},{"book":"Minkunga","chapter":"106","verse":"40","text":"Muaki Yave wufuemina batu bandi"},{"book":"Minkunga","chapter":"106","verse":"41","text":"Wuba yekula kuidi makanda"},{"book":"Minkunga","chapter":"106","verse":"42","text":"Bambeni bawu ziba yamisa"},{"book":"Minkunga","chapter":"106","verse":"43","text":"Zikhumbu ziwombo kaba kudila"},{"book":"Minkunga","chapter":"106","verse":"44","text":"Vayi wubela ziphasi ziawu mayindu"},{"book":"Minkunga","chapter":"106","verse":"45","text":"Mu bila kiawu, wutebukila nguizani andi moyo ;"},{"book":"Minkunga","chapter":"106","verse":"46","text":"Niandi wuvanga ti bamueno kiadi"},{"book":"Minkunga","chapter":"106","verse":"47","text":"Wutuvukisa, a Yave, Nzambi eto ;"},{"book":"Minkunga","chapter":"106","verse":"48","text":"Yave kazitusu, Nzambi yi Iseli !"}]}