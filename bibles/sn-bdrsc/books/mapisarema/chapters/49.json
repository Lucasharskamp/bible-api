{"data":[{"book":"Mapisarema","chapter":"49","verse":"1","text":"Inzwai izvi, imi vanhu mose;"},{"book":"Mapisarema","chapter":"49","verse":"2","text":"vapasi navapamusoro,"},{"book":"Mapisarema","chapter":"49","verse":"3","text":"Muromo wangu uchataura nouchenjeri;"},{"book":"Mapisarema","chapter":"49","verse":"4","text":"Ndicharerekera nzeve yangu kuchirahwe;"},{"book":"Mapisarema","chapter":"49","verse":"5","text":"Ndinotyireiko kana mazuva akaipa achiuya,"},{"book":"Mapisarema","chapter":"49","verse":"6","text":"vaya vanovimba nepfuma yavo"},{"book":"Mapisarema","chapter":"49","verse":"7","text":"Hakuna munhu angadzikinura upenyu hwomumwe"},{"book":"Mapisarema","chapter":"49","verse":"8","text":"rudzikinuro rwoupenyu runokosha,"},{"book":"Mapisarema","chapter":"49","verse":"9","text":"kuti ararame nokusingaperi"},{"book":"Mapisarema","chapter":"49","verse":"10","text":"Nokuti vose vanogona kuona kuti vanhu vakachenjera vanofa;"},{"book":"Mapisarema","chapter":"49","verse":"11","text":"Marinda avo achagara ari dzimba dzavo nokusingaperi,"},{"book":"Mapisarema","chapter":"49","verse":"12","text":"Asi munhu, kunyange ane pfuma yake, haagari;"},{"book":"Mapisarema","chapter":"49","verse":"13","text":"Ndiwo magumo aivavo vanovimba nezvavanoita,"},{"book":"Mapisarema","chapter":"49","verse":"14","text":"Samakwai vakatarirwa kuenda kuguva,"},{"book":"Mapisarema","chapter":"49","verse":"15","text":"Asi Mwari achadzikinura upenyu hwangu kubva muguva,"},{"book":"Mapisarema","chapter":"49","verse":"16","text":"Usanyanya kutya kana munhu apfuma,"},{"book":"Mapisarema","chapter":"49","verse":"17","text":"nokuti haana chaachatora paanofa;"},{"book":"Mapisarema","chapter":"49","verse":"18","text":"Kunyange paairarama aizviti akaropafadzwa,"},{"book":"Mapisarema","chapter":"49","verse":"19","text":"achabatana norudzi rwamadzibaba ake,"},{"book":"Mapisarema","chapter":"49","verse":"20","text":"Munhu ane pfuma zhinji asinganzwisisi"}]}