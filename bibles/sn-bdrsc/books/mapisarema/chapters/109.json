{"data":[{"book":"Mapisarema","chapter":"109","verse":"1","text":"Haiwa Mwari, imi wandinorumbidza,"},{"book":"Mapisarema","chapter":"109","verse":"2","text":"nokuti vanhu vakaipa vanonyengera vakandishamira miromo yavo;"},{"book":"Mapisarema","chapter":"109","verse":"3","text":"Vakandikomba namashoko okuvenga;"},{"book":"Mapisarema","chapter":"109","verse":"4","text":"Pachinzvimbo choushamwari hwangu navo, vanondipa mhosva,"},{"book":"Mapisarema","chapter":"109","verse":"5","text":"Vanonditsivira zvakaipa pane zvakanaka,"},{"book":"Mapisarema","chapter":"109","verse":"6","text":"Tumai munhu akaipa kuti apikisane naye;"},{"book":"Mapisarema","chapter":"109","verse":"7","text":"Paanotongwa, ngaawanikwe ane mhosva,"},{"book":"Mapisarema","chapter":"109","verse":"8","text":"Mazuva ake ngaave mashoma;"},{"book":"Mapisarema","chapter":"109","verse":"9","text":"Vana vake ngavave nherera,"},{"book":"Mapisarema","chapter":"109","verse":"10","text":"Vana vake ngavave vapemhi vanodzungaira;"},{"book":"Mapisarema","chapter":"109","verse":"11","text":"Waakakwereta ngaatore zvose zvaanazvo;"},{"book":"Mapisarema","chapter":"109","verse":"12","text":"Ngakurege kuva neanomunzwira ngoni"},{"book":"Mapisarema","chapter":"109","verse":"13","text":"Zvizvarwa zvake ngazvirove,"},{"book":"Mapisarema","chapter":"109","verse":"14","text":"Mhosva yamadzibaba ake ngairangarirwe pamberi paJehovha;"},{"book":"Mapisarema","chapter":"109","verse":"15","text":"Zvivi zvavo ngazvirambe zviri pamberi paJehovha,"},{"book":"Mapisarema","chapter":"109","verse":"16","text":"Nokuti haana kumbofunga kuita zvakanaka,"},{"book":"Mapisarema","chapter":"109","verse":"17","text":"Aifarira kutaura chituko, ngachichiuya pamusoro pake;"},{"book":"Mapisarema","chapter":"109","verse":"18","text":"Aifuka kutuka senguo yake;"},{"book":"Mapisarema","chapter":"109","verse":"19","text":"Ngazvive sejasi rakamonerwa paari,"},{"book":"Mapisarema","chapter":"109","verse":"20","text":"Uyu ngauve muripo waJehovha kuvapomeri vangu,"},{"book":"Mapisarema","chapter":"109","verse":"21","text":"Asi imi, Ishe Jehovha,"},{"book":"Mapisarema","chapter":"109","verse":"22","text":"Nokuti ndiri murombo uye ndinoshayiwa,"},{"book":"Mapisarema","chapter":"109","verse":"23","text":"Ndinopera somumvuri wamadekwana;"},{"book":"Mapisarema","chapter":"109","verse":"24","text":"Mabvi angu apera simba nokutsanya;"},{"book":"Mapisarema","chapter":"109","verse":"25","text":"Ndiri chinhu chinosekwa navapomeri vangu;"},{"book":"Mapisarema","chapter":"109","verse":"26","text":"Ndibatsirei, imi Jehovha Mwari wangu;"},{"book":"Mapisarema","chapter":"109","verse":"27","text":"Ngavazive kuti ndirwo ruoko rwenyu,"},{"book":"Mapisarema","chapter":"109","verse":"28","text":"Ivo vangatuka havo, asi imi mucharopafadza;"},{"book":"Mapisarema","chapter":"109","verse":"29","text":"Vapomeri vangu vachafukidzwa nenyadzi,"},{"book":"Mapisarema","chapter":"109","verse":"30","text":"Ndicharumbidza Jehovha nomuromo wangu zvikuru;"},{"book":"Mapisarema","chapter":"109","verse":"31","text":"Nokuti anomira kuruoko rworudyi rwouyo anoshayiwa,"}]}