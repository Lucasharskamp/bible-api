{"data":[{"book":"Masalimo","chapter":"48","verse":"1","text":"Wamkulu ndi Yehova, ndi woyenera kwambiri matamando"},{"book":"Masalimo","chapter":"48","verse":"2","text":"Lokongola mu utali mwake,"},{"book":"Masalimo","chapter":"48","verse":"3","text":"Mulungu ali mu malinga ake;"},{"book":"Masalimo","chapter":"48","verse":"4","text":"Pamene mafumu anasonkhana pamodzi,"},{"book":"Masalimo","chapter":"48","verse":"5","text":"iwo anaona mzindawo ndipo anadabwa kwambiri;"},{"book":"Masalimo","chapter":"48","verse":"6","text":"Pomwepo anagwidwa nako kunjenjemera,"},{"book":"Masalimo","chapter":"48","verse":"7","text":"Inu munawawononga monga sitima zapamadzi za ku Tarisisi"},{"book":"Masalimo","chapter":"48","verse":"8","text":"Monga momwe tinamvera,"},{"book":"Masalimo","chapter":"48","verse":"9","text":"Mʼkati mwa Nyumba yanu Mulungu,"},{"book":"Masalimo","chapter":"48","verse":"10","text":"Monga dzina lanu, Inu Mulungu,"},{"book":"Masalimo","chapter":"48","verse":"11","text":"Phiri la Ziyoni likukondwera,"},{"book":"Masalimo","chapter":"48","verse":"12","text":"Yendayendani mu Ziyoni, uzungulireni mzindawo,"},{"book":"Masalimo","chapter":"48","verse":"13","text":"Yangʼanitsitsani bwino mipanda yake,"},{"book":"Masalimo","chapter":"48","verse":"14","text":"Pakuti Mulungu uyu ndi Mulungu wathu ku nthawi zosatha;"}]}