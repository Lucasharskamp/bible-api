{"data":[{"book":"Masalimo","chapter":"22","verse":"1","text":"Mulungu wanga, Mulungu wanga, nʼchifukwa chiyani mwandisiya?"},{"book":"Masalimo","chapter":"22","verse":"2","text":"Inu Mulungu wanga, ine ndimalira masana, koma simuyankha,"},{"book":"Masalimo","chapter":"22","verse":"3","text":"Inu ndinu Woyera, wokhala pa mpando waufumu;"},{"book":"Masalimo","chapter":"22","verse":"4","text":"Pa inu makolo athu anadalira;"},{"book":"Masalimo","chapter":"22","verse":"5","text":"Analirira kwa inu ndipo munawapulumutsa."},{"book":"Masalimo","chapter":"22","verse":"6","text":"Koma ine ndine nyongolotsi osati munthu,"},{"book":"Masalimo","chapter":"22","verse":"7","text":"Onse amene amandiona amandiseka;"},{"book":"Masalimo","chapter":"22","verse":"8","text":"“Iyeyu amadalira Yehova,"},{"book":"Masalimo","chapter":"22","verse":"9","text":"Komabe Inu ndinu amene munandibadwitsa, munanditulutsa mʼmimba mwa amayi anga."},{"book":"Masalimo","chapter":"22","verse":"10","text":"Chibadwire ine ndinaperekedwa kwa Inu;"},{"book":"Masalimo","chapter":"22","verse":"11","text":"Musakhale kutali ndi ine,"},{"book":"Masalimo","chapter":"22","verse":"12","text":"Ngʼombe zazimuna zandizungulira;"},{"book":"Masalimo","chapter":"22","verse":"13","text":"Mikango yobangula pokadzula nyama,"},{"book":"Masalimo","chapter":"22","verse":"14","text":"Ine ndatayika pansi ngati madzi"},{"book":"Masalimo","chapter":"22","verse":"15","text":"Mphamvu zanga zauma ngati phale,"},{"book":"Masalimo","chapter":"22","verse":"16","text":"Agalu andizungulira;"},{"book":"Masalimo","chapter":"22","verse":"17","text":"Ine nditha kuwerenga mafupa anga onse;"},{"book":"Masalimo","chapter":"22","verse":"18","text":"Iwo agawana zovala zanga pakati pawo"},{"book":"Masalimo","chapter":"22","verse":"19","text":"Koma Inu Yehova, musakhale kutali;"},{"book":"Masalimo","chapter":"22","verse":"20","text":"Pulumutsani moyo wanga ku lupanga,"},{"book":"Masalimo","chapter":"22","verse":"21","text":"Ndilanditseni mʼkamwa mwa mikango;"},{"book":"Masalimo","chapter":"22","verse":"22","text":"Ine ndidzalengeza dzina lanu kwa abale anga;"},{"book":"Masalimo","chapter":"22","verse":"23","text":"Inu amene mumaopa Yehova mutamandeni!"},{"book":"Masalimo","chapter":"22","verse":"24","text":"Pakuti Iye sanapeputse kapena kunyoza"},{"book":"Masalimo","chapter":"22","verse":"25","text":"Ndidzakutamandani pa msonkhano waukulu chifukwa cha zimene mwandichitira."},{"book":"Masalimo","chapter":"22","verse":"26","text":"Osauka adzadya ndipo adzakhuta;"},{"book":"Masalimo","chapter":"22","verse":"27","text":"Malekezero onse a dziko lapansi"},{"book":"Masalimo","chapter":"22","verse":"28","text":"pakuti ulamuliro ndi wake wa Yehova"},{"book":"Masalimo","chapter":"22","verse":"29","text":"Anthu olemera onse a dziko lapansi adzachita phwando ndi kulambira;"},{"book":"Masalimo","chapter":"22","verse":"30","text":"Zidzukulu zamʼtsogolo zidzamutumikira Iye;"},{"book":"Masalimo","chapter":"22","verse":"31","text":"Iwo adzalengeza za chilungamo chake"}]}