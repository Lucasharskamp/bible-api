{"data":[{"book":"Zaburi","chapter":"104","verse":"1","text":"Ee nafsi yangu, umhimidi Bwana."},{"book":"Zaburi","chapter":"104","verse":"2","text":"Amejifunika katika nuru kama vile kwa vazi,"},{"book":"Zaburi","chapter":"104","verse":"3","text":"na kuziweka nguzo za orofa yake juu ya maji."},{"book":"Zaburi","chapter":"104","verse":"4","text":"Huzifanya pepo kuwa wajumbe104:4 Au: malaika. wake,"},{"book":"Zaburi","chapter":"104","verse":"5","text":"Ameiweka dunia kwenye misingi yake,"},{"book":"Zaburi","chapter":"104","verse":"6","text":"Uliifunika kwa kilindi kama kwa vazi,"},{"book":"Zaburi","chapter":"104","verse":"7","text":"Lakini kwa kukemea kwako maji yalikimbia,"},{"book":"Zaburi","chapter":"104","verse":"8","text":"yakapanda milima, yakateremka mabondeni,"},{"book":"Zaburi","chapter":"104","verse":"9","text":"Uliyawekea mpaka ambao hayawezi kuuvuka,"},{"book":"Zaburi","chapter":"104","verse":"10","text":"Huzifanya chemchemi zimwage maji mabondeni,"},{"book":"Zaburi","chapter":"104","verse":"11","text":"Huwapa maji wanyama wote wa kondeni,"},{"book":"Zaburi","chapter":"104","verse":"12","text":"Ndege wa angani hufanya viota kando ya maji,"},{"book":"Zaburi","chapter":"104","verse":"13","text":"Huinyeshea milima kutoka orofa zake,"},{"book":"Zaburi","chapter":"104","verse":"14","text":"Huyafanya majani ya mifugo yaote,"},{"book":"Zaburi","chapter":"104","verse":"15","text":"divai ya kuufurahisha moyo wa mwanadamu,"},{"book":"Zaburi","chapter":"104","verse":"16","text":"Miti ya Bwana inanyeshewa vizuri,"},{"book":"Zaburi","chapter":"104","verse":"17","text":"Humo ndege hufanya viota vyao,"},{"book":"Zaburi","chapter":"104","verse":"18","text":"Milima mirefu ni makao ya mbuzi-mwitu,"},{"book":"Zaburi","chapter":"104","verse":"19","text":"Mwezi hugawanya majira,"},{"book":"Zaburi","chapter":"104","verse":"20","text":"Unaleta giza, kunakuwa usiku,"},{"book":"Zaburi","chapter":"104","verse":"21","text":"Simba hunguruma kwa mawindo yao,"},{"book":"Zaburi","chapter":"104","verse":"22","text":"Jua huchomoza, nao huondoka,"},{"book":"Zaburi","chapter":"104","verse":"23","text":"Kisha mwanadamu huenda kazini mwake,"},{"book":"Zaburi","chapter":"104","verse":"24","text":"Ee Bwana, jinsi matendo yako yalivyo mengi!"},{"book":"Zaburi","chapter":"104","verse":"25","text":"Pale kuna bahari, kubwa na yenye nafasi tele,"},{"book":"Zaburi","chapter":"104","verse":"26","text":"Huko meli huenda na kurudi,"},{"book":"Zaburi","chapter":"104","verse":"27","text":"Hawa wote wanakutazamia wewe,"},{"book":"Zaburi","chapter":"104","verse":"28","text":"Wakati unapowapa,"},{"book":"Zaburi","chapter":"104","verse":"29","text":"Unapoficha uso wako,"},{"book":"Zaburi","chapter":"104","verse":"30","text":"Unapopeleka Roho wako,"},{"book":"Zaburi","chapter":"104","verse":"31","text":"Utukufu wa Bwana na udumu milele,"},{"book":"Zaburi","chapter":"104","verse":"32","text":"yeye aitazamaye dunia, nayo ikatetemeka,"},{"book":"Zaburi","chapter":"104","verse":"33","text":"Nitamwimbia Bwana maisha yangu yote;"},{"book":"Zaburi","chapter":"104","verse":"34","text":"Kutafakari kwangu na kumpendeze yeye,"},{"book":"Zaburi","chapter":"104","verse":"35","text":"Lakini wenye dhambi na watoweke katika dunia"}]}