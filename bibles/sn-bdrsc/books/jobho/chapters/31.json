{"data":[{"book":"Jobho","chapter":"31","verse":"1","text":"“Ndakaita sungano nameso angu"},{"book":"Jobho","chapter":"31","verse":"2","text":"Nokuti mugove womunhu unobva kumusoro kuna Mwari ndoweiko,"},{"book":"Jobho","chapter":"31","verse":"3","text":"Ko, hakuzi kuparadzwa kwavakaipa,"},{"book":"Jobho","chapter":"31","verse":"4","text":"Ko, iye haaoni nzira dzangu"},{"book":"Jobho","chapter":"31","verse":"5","text":"“Kana ndakafamba nenzira yenhema"},{"book":"Jobho","chapter":"31","verse":"6","text":"Mwari ngaandiyere pachiyero chakatendeka"},{"book":"Jobho","chapter":"31","verse":"7","text":"kana tsoka dzangu dzakatsauka kubva pagwara,"},{"book":"Jobho","chapter":"31","verse":"8","text":"ipapo vamwe ngavadye zvandakadyara,"},{"book":"Jobho","chapter":"31","verse":"9","text":"“Kana mwoyo wangu wakanyengerwa nomukadzi,"},{"book":"Jobho","chapter":"31","verse":"10","text":"ipapo mukadzi wangu ngaakuye zviyo zvomumwe murume,"},{"book":"Jobho","chapter":"31","verse":"11","text":"Nokuti chingadai chiri chinhu chinonyadzisa,"},{"book":"Jobho","chapter":"31","verse":"12","text":"Ndiwo moto unopfuta kusvikira pakuparadzwa;"},{"book":"Jobho","chapter":"31","verse":"13","text":"“Kana ndichinge ndaramba kururamisira varanda vangu,"},{"book":"Jobho","chapter":"31","verse":"14","text":"ndichaita sei pandichasangana naMwari?"},{"book":"Jobho","chapter":"31","verse":"15","text":"Ko, iye akandiisa muchizvaro haazi iye akavaita here?"},{"book":"Jobho","chapter":"31","verse":"16","text":"“Kana ndakaramba zvido zvevarombo"},{"book":"Jobho","chapter":"31","verse":"17","text":"kana ndakazvidyira chingwa changu,"},{"book":"Jobho","chapter":"31","verse":"18","text":"asi kubva paujaya hwangu ndakamurera sezvinoitwa nababa,"},{"book":"Jobho","chapter":"31","verse":"19","text":"kana ndakaona mumwe achiparara nokuda kwokushaya zvokupfeka,"},{"book":"Jobho","chapter":"31","verse":"20","text":"uye mwoyo wake ukasandiropafadza"},{"book":"Jobho","chapter":"31","verse":"21","text":"kana ndakasimudzira nherera ruoko rwangu"},{"book":"Jobho","chapter":"31","verse":"22","text":"ipapo ruoko rwangu ngaruwe papfudzi rangu,"},{"book":"Jobho","chapter":"31","verse":"23","text":"Nokuti ndaitya kuparadza kunobva kuna Mwari,"},{"book":"Jobho","chapter":"31","verse":"24","text":"“Kana ndakaisa chivimbo changu pagoridhe,"},{"book":"Jobho","chapter":"31","verse":"25","text":"kana ndakafadzwa nepfuma yangu huru,"},{"book":"Jobho","chapter":"31","verse":"26","text":"kana ndakava nehanya nezuva pakubwinya kwaro,"},{"book":"Jobho","chapter":"31","verse":"27","text":"zvokuti mwoyo wangu wakanyengereka pakavanda,"},{"book":"Jobho","chapter":"31","verse":"28","text":"zvino izvozviwo zvingangova zvivi zvinofanira kutongwa,"},{"book":"Jobho","chapter":"31","verse":"29","text":"“Kana ndakafadzwa nokurasikirwa kwomuvengi wangu,"},{"book":"Jobho","chapter":"31","verse":"30","text":"handina kutendera muromo wangu kutadza,"},{"book":"Jobho","chapter":"31","verse":"31","text":"kana vanhu veimba yangu vasina kumboti,"},{"book":"Jobho","chapter":"31","verse":"32","text":"Hapana mutorwa akavata usiku hwose munzira dzomumusha,"},{"book":"Jobho","chapter":"31","verse":"33","text":"Kana ndakavanza chivi changu saAdhamu,"},{"book":"Jobho","chapter":"31","verse":"34","text":"nokuda kwokuti ndaitya kwazvo vanhu vazhinji,"},{"book":"Jobho","chapter":"31","verse":"35","text":"“Haiwa, dai ndaiva nomumwe angandinzwa!"},{"book":"Jobho","chapter":"31","verse":"36","text":"Zvirokwazvo ndairutakura pamapfudzi angu,"},{"book":"Jobho","chapter":"31","verse":"37","text":"Ndaimupa kuzvidavirira kwangu kwenhambwe imwe neimwe;"},{"book":"Jobho","chapter":"31","verse":"38","text":"“Kana munda wangu ukadanidzira kwandiri uchindipa mhosva,"},{"book":"Jobho","chapter":"31","verse":"39","text":"kana ndakadya zvibereko zvawo ndisingaripi,"},{"book":"Jobho","chapter":"31","verse":"40","text":"ipapo rukato ngarumere pachinzvimbo chegorosi,"}]}