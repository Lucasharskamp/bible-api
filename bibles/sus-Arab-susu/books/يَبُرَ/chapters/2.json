{"data":[{"book":"يَبُرَ","chapter":"2","verse":"1","text":"مُنقٍ رَ سِيٍ بْحّ تٍشِ قُقَقُ؟"},{"book":"يَبُرَ","chapter":"2","verse":"2","text":"دُنِحَ مَنفّيٍ بَرَ كٍلِ،"},{"book":"يَبُرَ","chapter":"2","verse":"3","text":"نَ دُنِحَ مَنفّيٍ نَشّ،"},{"book":"يَبُرَ","chapter":"2","verse":"4","text":"مَرِفِ نَشَن نَ كٌورٍ مَ،"},{"book":"يَبُرَ","chapter":"2","verse":"5","text":"عَ شْنْمَ عٍ مَ،"},{"book":"يَبُرَ","chapter":"2","verse":"6","text":"عَلَ شَ مَسٍنيِ نَن يَ،"},{"book":"يَبُرَ","chapter":"2","verse":"7","text":"نَ مَنفّ سُفَندِشِ شَ مَسٍنيِ نَن يَ،"},{"book":"يَبُرَ","chapter":"2","verse":"8","text":"عِ شَ ﭑ مَشَندِ ﭑ شَ سِ بِرِن قِ عِ مَ،"},{"book":"يَبُرَ","chapter":"2","verse":"9","text":"عِ عٍ يَمَرِ مَ سَوُرِ رَ،"},{"book":"يَبُرَ","chapter":"2","verse":"10","text":"يَكْسِ وٌ تَن مَنفّيٍ، وٌ شَشِلِ سْتْ."},{"book":"يَبُرَ","chapter":"2","verse":"11","text":"وٌ عَلَتَلَ بَتُ فَاشُي رَ،"},{"book":"يَبُرَ","chapter":"2","verse":"12","text":"وٌ وٌ شُن سِن عَ شَ مِشِ سُفَندِشِ بّ."}]}