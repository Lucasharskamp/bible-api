{"data":[{"book":"AmaHubo","chapter":"116","verse":"1","text":"Ngiyamthanda uThixo ngoba walizwa ilizwi lami;"},{"book":"AmaHubo","chapter":"116","verse":"2","text":"Ngoba wangipha indlela yakhe,"},{"book":"AmaHubo","chapter":"116","verse":"3","text":"Izibopho zokufa zangithandela,"},{"book":"AmaHubo","chapter":"116","verse":"4","text":"Ngasengibiza ibizo likaThixo :"},{"book":"AmaHubo","chapter":"116","verse":"5","text":"UThixo ulomusa njalo ulungile;"},{"book":"AmaHubo","chapter":"116","verse":"6","text":"UThixo uvikela abalenhliziyo ethobekileyo;"},{"book":"AmaHubo","chapter":"116","verse":"7","text":"Phumula njalo, we mphefumulo wami,"},{"book":"AmaHubo","chapter":"116","verse":"8","text":"Ngoba wena Thixo,"},{"book":"AmaHubo","chapter":"116","verse":"9","text":"ukuze ngihambe phambi kukaThixo"},{"book":"AmaHubo","chapter":"116","verse":"10","text":"Ngathembela kuThixo; ngakho ngasengisithi,"},{"book":"AmaHubo","chapter":"116","verse":"11","text":"Ngiphakathi kokulahla ithemba ngathi,"},{"book":"AmaHubo","chapter":"116","verse":"12","text":"Ngingambonga ngani uThixo"},{"book":"AmaHubo","chapter":"116","verse":"13","text":"Ngizayiphakamisa inkezo yensindiso"},{"book":"AmaHubo","chapter":"116","verse":"14","text":"Ngizazigcwalisa izifungo zami kuThixo"},{"book":"AmaHubo","chapter":"116","verse":"15","text":"Kuligugu emehlweni kaThixo"},{"book":"AmaHubo","chapter":"116","verse":"16","text":"Oh Thixo, ngempela ngiyinceku Yakho,"},{"book":"AmaHubo","chapter":"116","verse":"17","text":"Ngizakwenzela umhlatshelo womnikelo wokubonga"},{"book":"AmaHubo","chapter":"116","verse":"18","text":"Ngizazigcwalisa izifungo zami kuThixo"},{"book":"AmaHubo","chapter":"116","verse":"19","text":"emagumeni endlu kaThixo"}]}