{"data":[{"book":"Izaga","chapter":"6","verse":"1","text":"Ndodana yami, nxa wenzele umakhelwane wakho isibambiso,"},{"book":"Izaga","chapter":"6","verse":"2","text":"aluba usuzihilele ngalokho okutshiloyo,"},{"book":"Izaga","chapter":"6","verse":"3","text":"lapho-ke yenza lokhu, ndodana yami, ukuzikhulula,"},{"book":"Izaga","chapter":"6","verse":"4","text":"Ungabuvumeli ubuthongo emehlweni akho,"},{"book":"Izaga","chapter":"6","verse":"5","text":"Zikhulule njengempala ezandleni zomzingeli,"},{"book":"Izaga","chapter":"6","verse":"6","text":"Khangela ubunyonyo, wena vila;"},{"book":"Izaga","chapter":"6","verse":"7","text":"Kabulamphathi,"},{"book":"Izaga","chapter":"6","verse":"8","text":"kodwa buyaqoqa umphako wabo ehlobo"},{"book":"Izaga","chapter":"6","verse":"9","text":"Uzalala khonapho kuze kube nini, vila ndini?"},{"book":"Izaga","chapter":"6","verse":"10","text":"Ukulala okuncane, ukuwozela okuncane,"},{"book":"Izaga","chapter":"6","verse":"11","text":"ubuyanga buzafika kuwe njengesela"},{"book":"Izaga","chapter":"6","verse":"12","text":"Isigangi lesixhwali"},{"book":"Izaga","chapter":"6","verse":"13","text":"esiqhweba ngelihlo"},{"book":"Izaga","chapter":"6","verse":"14","text":"esiceba ububi ngenkohliso enhliziyweni yaso"},{"book":"Izaga","chapter":"6","verse":"15","text":"Ngakho lowo uzawelwa yingozi ngokuphazima;"},{"book":"Izaga","chapter":"6","verse":"16","text":"Ziyisithupha izinto uThixo azizondayo,"},{"book":"Izaga","chapter":"6","verse":"17","text":"amehlo atshengisa ukudelela,"},{"book":"Izaga","chapter":"6","verse":"18","text":"inhliziyo egaya izibozi,"},{"book":"Izaga","chapter":"6","verse":"19","text":"umfakazi wamanga, okhuluma inkohliso"},{"book":"Izaga","chapter":"6","verse":"20","text":"Ndodana yami, gcina imilayo kayihlo"},{"book":"Izaga","chapter":"6","verse":"21","text":"Kubophele enhliziyweni yakho nini lanini;"},{"book":"Izaga","chapter":"6","verse":"22","text":"Nxa uhamba, lezizinto zizakukhokhela;"},{"book":"Izaga","chapter":"6","verse":"23","text":"Ngoba imilayo le iyisibane,"},{"book":"Izaga","chapter":"6","verse":"24","text":"zikuvikela kowesifazane ongaziphathanga,"},{"book":"Izaga","chapter":"6","verse":"25","text":"Ungasuki umfise ngenhliziyo yakho ngenxa yobuhle bakhe"},{"book":"Izaga","chapter":"6","verse":"26","text":"Ngoba isifebe siyakululazisa ufane lesinkwa,"},{"book":"Izaga","chapter":"6","verse":"27","text":"Kambe umuntu angokhela umlilo emathangazini akhe na"},{"book":"Izaga","chapter":"6","verse":"28","text":"Kambe umuntu angahamba phezu kwamalahle"},{"book":"Izaga","chapter":"6","verse":"29","text":"Kunjalo kulowo olala lomfazi wenye indoda;"},{"book":"Izaga","chapter":"6","verse":"30","text":"Abantu kabalithuki isela"},{"book":"Izaga","chapter":"6","verse":"31","text":"Kodwa lingabanjwa lihlawuliswa kasikhombisa,"},{"book":"Izaga","chapter":"6","verse":"32","text":"Kodwa indoda efebayo kayilangqondo;"},{"book":"Izaga","chapter":"6","verse":"33","text":"Izidutshulo lehlazo kuyisabelo sakhe,"},{"book":"Izaga","chapter":"6","verse":"34","text":"Ngoba ubukhwele bendoda yalowomfazi buyiqubula ulaka,"},{"book":"Izaga","chapter":"6","verse":"35","text":"Kayizukuvuma inhlawulo;"}]}