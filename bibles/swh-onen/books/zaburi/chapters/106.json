{"data":[{"book":"Zaburi","chapter":"106","verse":"1","text":"Msifuni Bwana."},{"book":"Zaburi","chapter":"106","verse":"2","text":"Ni nani awezaye kusimulia matendo makuu ya Bwana"},{"book":"Zaburi","chapter":"106","verse":"3","text":"Heri wale wanaodumisha haki,"},{"book":"Zaburi","chapter":"106","verse":"4","text":"Ee Bwana, unikumbuke unapowatendea mema watu wako,"},{"book":"Zaburi","chapter":"106","verse":"5","text":"ili niweze kufurahia mafanikio ya wateule wako,"},{"book":"Zaburi","chapter":"106","verse":"6","text":"Tumetenda dhambi, kama vile baba zetu walivyotenda,"},{"book":"Zaburi","chapter":"106","verse":"7","text":"Wakati baba zetu walipokuwa Misri,"},{"book":"Zaburi","chapter":"106","verse":"8","text":"Hata hivyo aliwaokoa kwa ajili ya jina lake,"},{"book":"Zaburi","chapter":"106","verse":"9","text":"Alikemea Bahari ya Shamu, nayo ikakauka,"},{"book":"Zaburi","chapter":"106","verse":"10","text":"Aliwaokoa mikononi mwa adui;"},{"book":"Zaburi","chapter":"106","verse":"11","text":"Maji yaliwafunika adui zao,"},{"book":"Zaburi","chapter":"106","verse":"12","text":"Ndipo walipoamini ahadi zake,"},{"book":"Zaburi","chapter":"106","verse":"13","text":"Lakini mara walisahau aliyowatendea,"},{"book":"Zaburi","chapter":"106","verse":"14","text":"Jangwani walitawaliwa na tamaa zao,"},{"book":"Zaburi","chapter":"106","verse":"15","text":"Kwa hiyo aliwapa kile walichoomba,"},{"book":"Zaburi","chapter":"106","verse":"16","text":"Kambini walimwonea wivu Mose,"},{"book":"Zaburi","chapter":"106","verse":"17","text":"Ardhi ilifunguka ikawameza Dathani,"},{"book":"Zaburi","chapter":"106","verse":"18","text":"Moto uliwaka katikati ya wafuasi wao,"},{"book":"Zaburi","chapter":"106","verse":"19","text":"Huko Horebu walitengeneza ndama,"},{"book":"Zaburi","chapter":"106","verse":"20","text":"Waliubadilisha Utukufu wao"},{"book":"Zaburi","chapter":"106","verse":"21","text":"Walimsahau Mungu aliyewaokoa,"},{"book":"Zaburi","chapter":"106","verse":"22","text":"miujiza katika nchi ya Hamu"},{"book":"Zaburi","chapter":"106","verse":"23","text":"Kwa hiyo alisema kwamba angewaangamiza:"},{"book":"Zaburi","chapter":"106","verse":"24","text":"Kisha waliidharau ile nchi nzuri,"},{"book":"Zaburi","chapter":"106","verse":"25","text":"Walinung ºunika ndani ya mahema yao,"},{"book":"Zaburi","chapter":"106","verse":"26","text":"Kwa hiyo akaapa kwa mkono ulioinuliwa"},{"book":"Zaburi","chapter":"106","verse":"27","text":"kuwatawanya wazao wao waanguke miongoni mwa mataifa,"},{"book":"Zaburi","chapter":"106","verse":"28","text":"Walijifunga nira na Baali wa Peori,"},{"book":"Zaburi","chapter":"106","verse":"29","text":"Waliichochea hasira ya Bwana,"},{"book":"Zaburi","chapter":"106","verse":"30","text":"Lakini Finehasi alisimama na kuingilia kati,"},{"book":"Zaburi","chapter":"106","verse":"31","text":"Hili likahesabiwa kwake haki,"},{"book":"Zaburi","chapter":"106","verse":"32","text":"Kwenye maji ya Meriba, walimkasirisha Bwana,"},{"book":"Zaburi","chapter":"106","verse":"33","text":"kwa maana walimwasi Roho wa Mungu,"},{"book":"Zaburi","chapter":"106","verse":"34","text":"Hawakuyaangamiza yale mataifa"},{"book":"Zaburi","chapter":"106","verse":"35","text":"bali walijichanganya na mataifa"},{"book":"Zaburi","chapter":"106","verse":"36","text":"Waliabudu sanamu zao,"},{"book":"Zaburi","chapter":"106","verse":"37","text":"Wakawatoa wana wao"},{"book":"Zaburi","chapter":"106","verse":"38","text":"Walimwaga damu isiyo na hatia,"},{"book":"Zaburi","chapter":"106","verse":"39","text":"Wakajinajisi wenyewe kwa yale waliyotenda;"},{"book":"Zaburi","chapter":"106","verse":"40","text":"Kwa hiyo Bwana akawakasirikia watu wake"},{"book":"Zaburi","chapter":"106","verse":"41","text":"Akawakabidhi kwa mataifa"},{"book":"Zaburi","chapter":"106","verse":"42","text":"Adui zao wakawaonea"},{"book":"Zaburi","chapter":"106","verse":"43","text":"Mara nyingi aliwaokoa"},{"book":"Zaburi","chapter":"106","verse":"44","text":"Lakini akaangalia mateso yao"},{"book":"Zaburi","chapter":"106","verse":"45","text":"kwa ajili yao akakumbuka agano lake,"},{"book":"Zaburi","chapter":"106","verse":"46","text":"Akawafanya wahurumiwe"},{"book":"Zaburi","chapter":"106","verse":"47","text":"Ee Bwana Mungu wetu, tuokoe."},{"book":"Zaburi","chapter":"106","verse":"48","text":"Atukuzwe Bwana, Mungu wa Israeli,"}]}