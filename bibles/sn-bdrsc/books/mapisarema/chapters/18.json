{"data":[{"book":"Mapisarema","chapter":"18","verse":"1","text":"Ndinokudai, imi Jehovha, simba rangu."},{"book":"Mapisarema","chapter":"18","verse":"2","text":"Jehovha ndiye dombo rangu, nhare yangu, murwiri wangu;"},{"book":"Mapisarema","chapter":"18","verse":"3","text":"Ndinodana kuna Jehovha, iye akafanira kurumbidzwa,"},{"book":"Mapisarema","chapter":"18","verse":"4","text":"Mabote orufu akandipinganidza;"},{"book":"Mapisarema","chapter":"18","verse":"5","text":"Mabote eguva akandimonera;"},{"book":"Mapisarema","chapter":"18","verse":"6","text":"Pakutambudzika kwangu ndakadana kuna Jehovha;"},{"book":"Mapisarema","chapter":"18","verse":"7","text":"Nyika yakadedera ikadengenyeka,"},{"book":"Mapisarema","chapter":"18","verse":"8","text":"Utsi hwakakwira huchibva mumhino dzake;"},{"book":"Mapisarema","chapter":"18","verse":"9","text":"Akatsemura matenga akauya pasi;"},{"book":"Mapisarema","chapter":"18","verse":"10","text":"Akatasva makerubhi akabhururuka;"},{"book":"Mapisarema","chapter":"18","verse":"11","text":"Akaita rima chifukidzo chake, denga rake rakamupoteredza,"},{"book":"Mapisarema","chapter":"18","verse":"12","text":"Nokupenya kwaibva pamberi pake, makore akabuda akafamba,"},{"book":"Mapisarema","chapter":"18","verse":"13","text":"Jehovha akatinhira kudenga;"},{"book":"Mapisarema","chapter":"18","verse":"14","text":"Akapfura miseve yake akaparadzira vavengi vake,"},{"book":"Mapisarema","chapter":"18","verse":"15","text":"Mipata yegungwa yakaiswa pachena,"},{"book":"Mapisarema","chapter":"18","verse":"16","text":"Akadzika pasi achibva kumusoro akanditora;"},{"book":"Mapisarema","chapter":"18","verse":"17","text":"Akandinunura kubva kuvavengi vangu vane simba,"},{"book":"Mapisarema","chapter":"18","verse":"18","text":"Vakandinanga pazuva renjodzi yangu,"},{"book":"Mapisarema","chapter":"18","verse":"19","text":"Akandibudisa akandiisa panzvimbo yakafarikana;"},{"book":"Mapisarema","chapter":"18","verse":"20","text":"Jehovha akandiitira zvakafanira kururama kwangu;"},{"book":"Mapisarema","chapter":"18","verse":"21","text":"Nokuti ndakachengeta nzira dzaJehovha;"},{"book":"Mapisarema","chapter":"18","verse":"22","text":"Mirayiro yake yose iri pamberi pangu;"},{"book":"Mapisarema","chapter":"18","verse":"23","text":"Ndakanga ndisina chandingapomerwa pamberi pake,"},{"book":"Mapisarema","chapter":"18","verse":"24","text":"Jehovha akandipa mubayiro zvakaringana nokururama kwangu,"},{"book":"Mapisarema","chapter":"18","verse":"25","text":"Kuna vakatendeka munoratidza kutendeka kwenyu,"},{"book":"Mapisarema","chapter":"18","verse":"26","text":"kuna vakachena munoratidza kuchena kwenyu,"},{"book":"Mapisarema","chapter":"18","verse":"27","text":"Munoponesa vanozvininipisa,"},{"book":"Mapisarema","chapter":"18","verse":"28","text":"Imi, iyemi Jehovha, munoita kuti mwenje wangu urambe uchipfuta;"},{"book":"Mapisarema","chapter":"18","verse":"29","text":"Norubatsiro rwenyu ndichaenda kundorwa hondo;"},{"book":"Mapisarema","chapter":"18","verse":"30","text":"Kana ari Mwari, nzira yake yakakwana;"},{"book":"Mapisarema","chapter":"18","verse":"31","text":"Nokuti ndianiko Mwari kunze kwaJehovha?"},{"book":"Mapisarema","chapter":"18","verse":"32","text":"NdiMwari anondishongedza nesimba"},{"book":"Mapisarema","chapter":"18","verse":"33","text":"Anoita kuti tsoka dzangu dzive setsoka dzenondo;"},{"book":"Mapisarema","chapter":"18","verse":"34","text":"Anodzidzisa maoko angu kurwa;"},{"book":"Mapisarema","chapter":"18","verse":"35","text":"Munondipa nhoo yenyu yokukunda,"},{"book":"Mapisarema","chapter":"18","verse":"36","text":"Munondiitira nzira yakapamhama pasi pangu,"},{"book":"Mapisarema","chapter":"18","verse":"37","text":"Ndakatevera vavengi vangu ndikavabata;"},{"book":"Mapisarema","chapter":"18","verse":"38","text":"Ndakavapwanya kuti vakoniwe kusimuka;"},{"book":"Mapisarema","chapter":"18","verse":"39","text":"Makandishongedza nesimba rokurwa;"},{"book":"Mapisarema","chapter":"18","verse":"40","text":"Makaita kuti vavengi vangu vatize,"},{"book":"Mapisarema","chapter":"18","verse":"41","text":"Vakachemera kubatsirwa, asi hakuna akavaponesa,"},{"book":"Mapisarema","chapter":"18","verse":"42","text":"Ndakavapura vakatsetseka seguruva ratakurwa nemhepo;"},{"book":"Mapisarema","chapter":"18","verse":"43","text":"Makandirwira pakurwisa kwavanhu;"},{"book":"Mapisarema","chapter":"18","verse":"44","text":"Pavanongondinzwa, vanonditeerera;"},{"book":"Mapisarema","chapter":"18","verse":"45","text":"Vose vanoora mwoyo;"},{"book":"Mapisarema","chapter":"18","verse":"46","text":"Jehovha mupenyu! Dombo rangu ngarirumbidzwe!"},{"book":"Mapisarema","chapter":"18","verse":"47","text":"Ndiye Mwari anonditsivira,"},{"book":"Mapisarema","chapter":"18","verse":"48","text":"anondiponesa pavavengi vangu."},{"book":"Mapisarema","chapter":"18","verse":"49","text":"Naizvozvo ndichakurumbidzai pakati pendudzi, imi Jehovha;"},{"book":"Mapisarema","chapter":"18","verse":"50","text":"Anopa mambo wake kukunda kukuru;"}]}