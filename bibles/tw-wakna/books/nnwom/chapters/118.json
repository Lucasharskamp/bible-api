{"data":[{"book":"Nnwom","chapter":"118","verse":"1","text":"Monna Awurade ase efisɛ oye,"},{"book":"Nnwom","chapter":"118","verse":"2","text":"Momma Israel nka se,"},{"book":"Nnwom","chapter":"118","verse":"3","text":"Momma Aaronfi nka se,"},{"book":"Nnwom","chapter":"118","verse":"4","text":"Momma wɔn a wosuro Awurade nka se,"},{"book":"Nnwom","chapter":"118","verse":"5","text":"Mʼahohia mu, misu frɛɛ Awurade;"},{"book":"Nnwom","chapter":"118","verse":"6","text":"Awurade ne me boafo; enti merensuro."},{"book":"Nnwom","chapter":"118","verse":"7","text":"Awurade ne me wɔ hɔ; ɔyɛ me boafo."},{"book":"Nnwom","chapter":"118","verse":"8","text":"Eye sɛ yebehintaw wɔ Awurade mu,"},{"book":"Nnwom","chapter":"118","verse":"9","text":"Eye sɛ yebehintaw Awurade mu,"},{"book":"Nnwom","chapter":"118","verse":"10","text":"Amanaman no nyinaa twaa me ho hyiae,"},{"book":"Nnwom","chapter":"118","verse":"11","text":"Wotwaa me ho hyiaa wɔ baabiara,"},{"book":"Nnwom","chapter":"118","verse":"12","text":"Wɔkyere guu me so sɛ nnowa,"},{"book":"Nnwom","chapter":"118","verse":"13","text":"Wosum me kɔɔ mʼakyi a anka mereyɛ ahwe ase,"},{"book":"Nnwom","chapter":"118","verse":"14","text":"Awurade yɛ mʼahoɔden ne me bammɔ;"},{"book":"Nnwom","chapter":"118","verse":"15","text":"Osebɔ ne nkonimdi"},{"book":"Nnwom","chapter":"118","verse":"16","text":"Wɔama Awurade nsa nifa so;"},{"book":"Nnwom","chapter":"118","verse":"17","text":"Merenwu, mmom mɛtena ase"},{"book":"Nnwom","chapter":"118","verse":"18","text":"Awurade atwe mʼaso dennen,"},{"book":"Nnwom","chapter":"118","verse":"19","text":"Bue trenee apon ma me;"},{"book":"Nnwom","chapter":"118","verse":"20","text":"Awurade ponkɛse ni;"},{"book":"Nnwom","chapter":"118","verse":"21","text":"Mɛda wo ase, efisɛ wugyee me so;"},{"book":"Nnwom","chapter":"118","verse":"22","text":"Ɔbo a adansifo no poe no,"},{"book":"Nnwom","chapter":"118","verse":"23","text":"Awurade na wayɛ eyi,"},{"book":"Nnwom","chapter":"118","verse":"24","text":"Nnɛ yɛ da a Awurade ayɛ."},{"book":"Nnwom","chapter":"118","verse":"25","text":"Awurade, gye yɛn nkwa;"},{"book":"Nnwom","chapter":"118","verse":"26","text":"Nhyira ne nea ɔnam Awurade din mu reba."},{"book":"Nnwom","chapter":"118","verse":"27","text":"Awurade yɛ Onyankopɔn,"},{"book":"Nnwom","chapter":"118","verse":"28","text":"Woyɛ me Nyankopɔn na meyi wo ayɛ;"},{"book":"Nnwom","chapter":"118","verse":"29","text":"Monna Awurade ase, efisɛ oye;"}]}