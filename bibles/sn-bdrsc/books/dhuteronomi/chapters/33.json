{"data":[{"book":"Dhuteronomi","chapter":"33","verse":"1","text":"Uku ndiko kuropafadza kwaMozisi munhu waMwari kwaakataura kuvaIsraeri asati afa."},{"book":"Dhuteronomi","chapter":"33","verse":"2","text":"Akati:"},{"book":"Dhuteronomi","chapter":"33","verse":"3","text":"Zvirokwazvo ndimi makada vanhu;"},{"book":"Dhuteronomi","chapter":"33","verse":"4","text":"murayiro watakapiwa naMozisi,"},{"book":"Dhuteronomi","chapter":"33","verse":"5","text":"Akanga ari mambo weJeshuruni"},{"book":"Dhuteronomi","chapter":"33","verse":"6","text":"“Rubheni ngaararame arege kufa,"},{"book":"Dhuteronomi","chapter":"33","verse":"7","text":"Uye izvi ndizvo zvaakataura pamusoro paJudha:"},{"book":"Dhuteronomi","chapter":"33","verse":"8","text":"Pamusoro paRevhi akati,"},{"book":"Dhuteronomi","chapter":"33","verse":"9","text":"Akati pamusoro pababa namai vake,"},{"book":"Dhuteronomi","chapter":"33","verse":"10","text":"Anodzidzisa mitemo yenyu kuna Jakobho"},{"book":"Dhuteronomi","chapter":"33","verse":"11","text":"Ropafadzai kubata kwake kwose, imi Jehovha,"},{"book":"Dhuteronomi","chapter":"33","verse":"12","text":"Pamusoro paBhenjamini akati:"},{"book":"Dhuteronomi","chapter":"33","verse":"13","text":"Pamusoro paJosefa akati:"},{"book":"Dhuteronomi","chapter":"33","verse":"14","text":"nezvakanakisisa zvinouyiswa nezuva"},{"book":"Dhuteronomi","chapter":"33","verse":"15","text":"nezvipo zvakanakisisa zvokumakomo ekare"},{"book":"Dhuteronomi","chapter":"33","verse":"16","text":"nezvipo zvakanakisisa zvenyika nokuzara kwayo"},{"book":"Dhuteronomi","chapter":"33","verse":"17","text":"Muumambo hwake anoita semhongora yenzombe;"},{"book":"Dhuteronomi","chapter":"33","verse":"18","text":"Pamusoro paZebhuruni akati:"},{"book":"Dhuteronomi","chapter":"33","verse":"19","text":"Vachadanira vanhu kumakomo"},{"book":"Dhuteronomi","chapter":"33","verse":"20","text":"Pamusoro paGadhi akati:"},{"book":"Dhuteronomi","chapter":"33","verse":"21","text":"Akazvisarudzira nyika yakanakisisa;"},{"book":"Dhuteronomi","chapter":"33","verse":"22","text":"Pamusoro paDhani akati:"},{"book":"Dhuteronomi","chapter":"33","verse":"23","text":"Pamusoro paNafutari akati:"},{"book":"Dhuteronomi","chapter":"33","verse":"24","text":"Pamusoro paAsheri akati:"},{"book":"Dhuteronomi","chapter":"33","verse":"25","text":"Zvipfigiso zvemasuo ako zvichava zvesimbi nendarira,"},{"book":"Dhuteronomi","chapter":"33","verse":"26","text":"“Hapana akafanana naMwari waJeshuruni,"},{"book":"Dhuteronomi","chapter":"33","verse":"27","text":"Mwari anogara nokusingaperi ndiye utiziro hwako,"},{"book":"Dhuteronomi","chapter":"33","verse":"28","text":"Naizvozvo Israeri achagara murugare oga;"},{"book":"Dhuteronomi","chapter":"33","verse":"29","text":"Wakaropafadzwa, iwe Israeri!"}]}