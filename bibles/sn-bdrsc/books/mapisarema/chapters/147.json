{"data":[{"book":"Mapisarema","chapter":"147","verse":"1","text":"Rumbidzai Jehovha."},{"book":"Mapisarema","chapter":"147","verse":"2","text":"Jehovha anovaka Jerusarema;"},{"book":"Mapisarema","chapter":"147","verse":"3","text":"Anoporesa vane mwoyo yakaputsika,"},{"book":"Mapisarema","chapter":"147","verse":"4","text":"Anotara kuwanda kwenyeredzi,"},{"book":"Mapisarema","chapter":"147","verse":"5","text":"Ishe wedu mukuru uye ane simba guru;"},{"book":"Mapisarema","chapter":"147","verse":"6","text":"Jehovha anotsigira vanozvininipisa,"},{"book":"Mapisarema","chapter":"147","verse":"7","text":"Imbirai Jehovha nokuvonga;"},{"book":"Mapisarema","chapter":"147","verse":"8","text":"Anofukidza denga namakore;"},{"book":"Mapisarema","chapter":"147","verse":"9","text":"Anopa mombe zvokudya"},{"book":"Mapisarema","chapter":"147","verse":"10","text":"Mufaro wake hausi musimba rebhiza,"},{"book":"Mapisarema","chapter":"147","verse":"11","text":"Jehovha anofadzwa naavo vanomutya,"},{"book":"Mapisarema","chapter":"147","verse":"12","text":"Kudza Jehovha, iwe Jerusarema;"},{"book":"Mapisarema","chapter":"147","verse":"13","text":"nokuti anosimbisa mazariro amasuo ako,"},{"book":"Mapisarema","chapter":"147","verse":"14","text":"Anopa rugare pamiganhu yako,"},{"book":"Mapisarema","chapter":"147","verse":"15","text":"Anotuma murayiro wake kunyika;"},{"book":"Mapisarema","chapter":"147","verse":"16","text":"Anowarira chando samakushe amakwai,"},{"book":"Mapisarema","chapter":"147","verse":"17","text":"Anokanda chimvuramabwe pasi sezvimedu."},{"book":"Mapisarema","chapter":"147","verse":"18","text":"Anotuma shoko rake rigozvinyungudisa;"},{"book":"Mapisarema","chapter":"147","verse":"19","text":"Akazarurira Jakobho shoko rake,"},{"book":"Mapisarema","chapter":"147","verse":"20","text":"Haana kumboita izvi kuno rumwe rudzi;"}]}