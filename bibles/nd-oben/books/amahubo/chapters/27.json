{"data":[{"book":"AmaHubo","chapter":"27","verse":"1","text":"UThixo uyikukhanya kwami lensindiso yami"},{"book":"AmaHubo","chapter":"27","verse":"2","text":"Nxa ababi bengihlasela"},{"book":"AmaHubo","chapter":"27","verse":"3","text":"Loba impi ingivimbezela,"},{"book":"AmaHubo","chapter":"27","verse":"4","text":"Yinye into engiyicelayo kuThixo,"},{"book":"AmaHubo","chapter":"27","verse":"5","text":"Phela ngosuku lokuhlupheka"},{"book":"AmaHubo","chapter":"27","verse":"6","text":"Lapho-ke ikhanda lami lizaphakanyiswa"},{"book":"AmaHubo","chapter":"27","verse":"7","text":"Zwana ilizwi lami lapho ngimemeza Oh Thixo;"},{"book":"AmaHubo","chapter":"27","verse":"8","text":"Ithi inhliziyo yami Ngawe, “Dinga ubuso bakhe!”"},{"book":"AmaHubo","chapter":"27","verse":"9","text":"Ungangifihleli ubuso Bakho,"},{"book":"AmaHubo","chapter":"27","verse":"10","text":"Loba obaba lomama bengangidela,"},{"book":"AmaHubo","chapter":"27","verse":"11","text":"Ngifundisa indlela Yakho, Oh Thixo;"},{"book":"AmaHubo","chapter":"27","verse":"12","text":"Unganginikeli entandweni yabangizondayo,"},{"book":"AmaHubo","chapter":"27","verse":"13","text":"Ngilokhu ngilesibindi ngalokhu:"},{"book":"AmaHubo","chapter":"27","verse":"14","text":"Lindela uThixo"}]}