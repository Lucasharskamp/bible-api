{"data":[{"book":"AmaHubo","chapter":"106","verse":"1","text":"Dumisani uThixo."},{"book":"AmaHubo","chapter":"106","verse":"2","text":"Ngubani ongamemezela imisebenzi emikhulu kaThixo"},{"book":"AmaHubo","chapter":"106","verse":"3","text":"Babusisiwe labo abahlala besenza ngendlela enhle,"},{"book":"AmaHubo","chapter":"106","verse":"4","text":"Ungikhumbule, Thixo, nxa usuveza umusa ebantwini bakho,"},{"book":"AmaHubo","chapter":"106","verse":"5","text":"ukuze lami ngikholise ukuphumelela kwalabo obakhethileyo,"},{"book":"AmaHubo","chapter":"106","verse":"6","text":"Sonile, njengalokho abakwenzayo okhokho bethu;"},{"book":"AmaHubo","chapter":"106","verse":"7","text":"Lapho okhokho bethu beseGibhithe,"},{"book":"AmaHubo","chapter":"106","verse":"8","text":"Kodwa wabasindisa ngenxa yebizo lakhe,"},{"book":"AmaHubo","chapter":"106","verse":"9","text":"Walukhuza uLwandle oluBomvu, loma qha;"},{"book":"AmaHubo","chapter":"106","verse":"10","text":"Wabasindisa esandleni sesitha;"},{"book":"AmaHubo","chapter":"106","verse":"11","text":"Amanzi asibekela izitha zabo,"},{"book":"AmaHubo","chapter":"106","verse":"12","text":"Lapho-ke base bezikholwa izithembiso zakhe"},{"book":"AmaHubo","chapter":"106","verse":"13","text":"Kodwa baphanga bakukhohlwa lokho ayekwenzile"},{"book":"AmaHubo","chapter":"106","verse":"14","text":"Enkangala bakhulelwa yizinkanuko zabo;"},{"book":"AmaHubo","chapter":"106","verse":"15","text":"Ngakho wabapha lokho abakucelayo,"},{"book":"AmaHubo","chapter":"106","verse":"16","text":"Ezihonqweni basuka babalomhawu loMosi njalo lango-Aroni,"},{"book":"AmaHubo","chapter":"106","verse":"17","text":"Umhlaba wavuleka waginya uDathani;"},{"book":"AmaHubo","chapter":"106","verse":"18","text":"Umlilo walavuza phakathi kwabalandeli babo;"},{"book":"AmaHubo","chapter":"106","verse":"19","text":"EHorebhi benza ithole,"},{"book":"AmaHubo","chapter":"106","verse":"20","text":"Badela ukuKhazimula kwabo"},{"book":"AmaHubo","chapter":"106","verse":"21","text":"Bakhohlwa uNkulunkulu owabasindisayo,"},{"book":"AmaHubo","chapter":"106","verse":"22","text":"imimangaliso elizweni likaHamu"},{"book":"AmaHubo","chapter":"106","verse":"23","text":"Ngakho wathi uzababhubhisa"},{"book":"AmaHubo","chapter":"106","verse":"24","text":"Kwathi njalo baleyisa ilizwe elihle;"},{"book":"AmaHubo","chapter":"106","verse":"25","text":"Bakhonona besemathenteni abo"},{"book":"AmaHubo","chapter":"106","verse":"26","text":"Ngakho wafunga kubo ephakamise isandla sakhe"},{"book":"AmaHubo","chapter":"106","verse":"27","text":"enze izizukulwane zabo ziqedwe ngabezizwe"},{"book":"AmaHubo","chapter":"106","verse":"28","text":"Bazibopha ingathi kusejogweni kuBhali wasePheyori"},{"book":"AmaHubo","chapter":"106","verse":"29","text":"bamthukuthelisa uThixo ngezenzo zabo ezimbi;"},{"book":"AmaHubo","chapter":"106","verse":"30","text":"Kodwa uFinehasi wema wancenga"},{"book":"AmaHubo","chapter":"106","verse":"31","text":"Lokhu kwenza kuthiwe wayelungile"},{"book":"AmaHubo","chapter":"106","verse":"32","text":"Bamzondisa uThixo ngasemthonjeni weMeribha,"},{"book":"AmaHubo","chapter":"106","verse":"33","text":"ngoba bahlamukela uMoya kaNkulunkulu,"},{"book":"AmaHubo","chapter":"106","verse":"34","text":"Kabababhubhisanga abantu bezizwe"},{"book":"AmaHubo","chapter":"106","verse":"35","text":"kodwa baphithikana labezizwe"},{"book":"AmaHubo","chapter":"106","verse":"36","text":"Bakhonza izithombe zazo,"},{"book":"AmaHubo","chapter":"106","verse":"37","text":"Benza imihlatshelo ngamadodana abo"},{"book":"AmaHubo","chapter":"106","verse":"38","text":"Bachitha igazi elalingelacala,"},{"book":"AmaHubo","chapter":"106","verse":"39","text":"Bazingcolisa ngalokho abakwenzayo;"},{"book":"AmaHubo","chapter":"106","verse":"40","text":"Ngakho uThixo wabathukuthelela abantu bakhe"},{"book":"AmaHubo","chapter":"106","verse":"41","text":"Wabanikela kwabezizwe,"},{"book":"AmaHubo","chapter":"106","verse":"42","text":"Izitha zabo zabancindezela"},{"book":"AmaHubo","chapter":"106","verse":"43","text":"Izikhathi ezinengi wabakhulula,"},{"book":"AmaHubo","chapter":"106","verse":"44","text":"Kodwa wananzelela ukuhlupheka kwabo"},{"book":"AmaHubo","chapter":"106","verse":"45","text":"ngenxa yokubahawukela wakhumbula isivumelwano sakhe,"},{"book":"AmaHubo","chapter":"106","verse":"46","text":"Wenza bonke labo ababebathumbile"},{"book":"AmaHubo","chapter":"106","verse":"47","text":"Sisindise, Oh Thixo Nkulunkulu wethu,"},{"book":"AmaHubo","chapter":"106","verse":"48","text":"Kabongwe uThixo, uNkulunkulu ka-Israyeli,"}]}