{"data":[{"book":"Mapisarema","chapter":"44","verse":"1","text":"Haiwa Mwari, takanzwa nenzeve dzedu;"},{"book":"Mapisarema","chapter":"44","verse":"2","text":"Noruoko rwenyu makadzinga ndudzi"},{"book":"Mapisarema","chapter":"44","verse":"3","text":"Havana kuzviwanira nyika nomunondo wavo,"},{"book":"Mapisarema","chapter":"44","verse":"4","text":"Ndimi Mambo wangu naMwari wangu,"},{"book":"Mapisarema","chapter":"44","verse":"5","text":"Kubudikidza nemi, tinosunda vavengi vedu shure;"},{"book":"Mapisarema","chapter":"44","verse":"6","text":"Handivimbi nouta hwangu,"},{"book":"Mapisarema","chapter":"44","verse":"7","text":"asi imi munotipa kukunda pamusoro pavavengi vedu,"},{"book":"Mapisarema","chapter":"44","verse":"8","text":"Tinozvirumbidza muna Mwari zuva rose,"},{"book":"Mapisarema","chapter":"44","verse":"9","text":"Asi zvino matiramba uye matininipisa;"},{"book":"Mapisarema","chapter":"44","verse":"10","text":"Makaita kuti tidududze pamberi pavavengi vedu,"},{"book":"Mapisarema","chapter":"44","verse":"11","text":"Makatiramwira kumudyi samakwai"},{"book":"Mapisarema","chapter":"44","verse":"12","text":"Makatengesa vanhu venyu pasina,"},{"book":"Mapisarema","chapter":"44","verse":"13","text":"Makatiita chiseko kuvavakidzani vedu,"},{"book":"Mapisarema","chapter":"44","verse":"14","text":"Makatiita tsumo pakati pendudzi;"},{"book":"Mapisarema","chapter":"44","verse":"15","text":"Kunyadziswa kwangu kuri pamberi pangu zuva rose,"},{"book":"Mapisarema","chapter":"44","verse":"16","text":"pakuseka kwaavo vanondizvidza nokundirwisa,"},{"book":"Mapisarema","chapter":"44","verse":"17","text":"Zvose izvi zvakaitika kwatiri"},{"book":"Mapisarema","chapter":"44","verse":"18","text":"Mwoyo yedu yakanga isati yafuratira;"},{"book":"Mapisarema","chapter":"44","verse":"19","text":"Asi makatipwanya mukatiita nzvimbo yamakava"},{"book":"Mapisarema","chapter":"44","verse":"20","text":"Dai takanga takanganwa zita raMwari wedu,"},{"book":"Mapisarema","chapter":"44","verse":"21","text":"Mwari haaizviziva here,"},{"book":"Mapisarema","chapter":"44","verse":"22","text":"Asi nokuda kwenyu takatarisana norufu zuva rose;"},{"book":"Mapisarema","chapter":"44","verse":"23","text":"Mukai, imi Ishe! Munovatireiko?"},{"book":"Mapisarema","chapter":"44","verse":"24","text":"Ko, munovanzirei chiso chenyu"},{"book":"Mapisarema","chapter":"44","verse":"25","text":"Takawisirwa muguruva;"},{"book":"Mapisarema","chapter":"44","verse":"26","text":"Simukai mutibatsire;"}]}