{"data":[{"book":"Naumi","chapter":"3","verse":"1","text":"Mawa na engumba oyo mosala na yango"},{"book":"Naumi","chapter":"3","verse":"2","text":"Tango nyonso, kaka makelele ya bafimbu,"},{"book":"Naumi","chapter":"3","verse":"3","text":"Basoda oyo batambolaka likolo ya bampunda"},{"book":"Naumi","chapter":"3","verse":"4","text":"Makambo oyo nyonso esalemi"},{"book":"Naumi","chapter":"3","verse":"5","text":"« Natombokeli yo, »"},{"book":"Naumi","chapter":"3","verse":"6","text":"Nakobwakela yo bosoto,"},{"book":"Naumi","chapter":"3","verse":"7","text":"Bato nyonso oyo bakomona yo"},{"book":"Naumi","chapter":"3","verse":"8","text":"Boni, okanisi ete ozali kitoko"},{"book":"Naumi","chapter":"3","verse":"9","text":"Makasi na yango ezalaki kowuta"},{"book":"Naumi","chapter":"3","verse":"10","text":"Nzokande, yango mpe ekweyaki,"},{"book":"Naumi","chapter":"3","verse":"11","text":"Yo mpe okokoma lokola moto"},{"book":"Naumi","chapter":"3","verse":"12","text":"Bandako na yo nyonso, oyo batonga makasi"},{"book":"Naumi","chapter":"3","verse":"13","text":"Tala malamu mampinga na yo,"},{"book":"Naumi","chapter":"3","verse":"14","text":"Bino bato ya engumba oyo,"},{"book":"Naumi","chapter":"3","verse":"15","text":"Kuna nde moto ekoya kozikisa yo,"},{"book":"Naumi","chapter":"3","verse":"16","text":"Okomisi motango ya bato na yo ya mombongo"},{"book":"Naumi","chapter":"3","verse":"17","text":"Bakengeli na yo bazali lokola mankoko,"},{"book":"Naumi","chapter":"3","verse":"18","text":"Oh mokonzi ya Asiri,"},{"book":"Naumi","chapter":"3","verse":"19","text":"Ezali ata na kisi moko te"}]}