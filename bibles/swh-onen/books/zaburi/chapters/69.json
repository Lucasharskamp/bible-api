{"data":[{"book":"Zaburi","chapter":"69","verse":"1","text":"Ee Mungu, niokoe,"},{"book":"Zaburi","chapter":"69","verse":"2","text":"Ninazama kwenye vilindi vya matope,"},{"book":"Zaburi","chapter":"69","verse":"3","text":"Nimechoka kwa kuomba msaada,"},{"book":"Zaburi","chapter":"69","verse":"4","text":"Wale wanaonichukia bila sababu"},{"book":"Zaburi","chapter":"69","verse":"5","text":"Ee Mungu, wewe unajua upumbavu wangu,"},{"book":"Zaburi","chapter":"69","verse":"6","text":"Ee Bwana, ewe Bwana Mwenye Nguvu Zote,"},{"book":"Zaburi","chapter":"69","verse":"7","text":"Kwa maana nimestahimili dharau kwa ajili yako,"},{"book":"Zaburi","chapter":"69","verse":"8","text":"Nimekuwa mgeni kwa ndugu zangu,"},{"book":"Zaburi","chapter":"69","verse":"9","text":"Kwa kuwa wivu wa nyumba yako unanila,"},{"book":"Zaburi","chapter":"69","verse":"10","text":"Ninapolia na kufunga,"},{"book":"Zaburi","chapter":"69","verse":"11","text":"Ninapovaa nguo ya gunia,"},{"book":"Zaburi","chapter":"69","verse":"12","text":"Wale wanaoketi mlangoni wananisimanga,"},{"book":"Zaburi","chapter":"69","verse":"13","text":"Lakini Ee Bwana, ninakuomba,"},{"book":"Zaburi","chapter":"69","verse":"14","text":"Uniokoe katika matope,"},{"book":"Zaburi","chapter":"69","verse":"15","text":"Usiache mafuriko yanigharikishe"},{"book":"Zaburi","chapter":"69","verse":"16","text":"Ee Bwana, unijibu, kwa wema wa upendo wako;"},{"book":"Zaburi","chapter":"69","verse":"17","text":"Usimfiche mtumishi wako uso wako,"},{"book":"Zaburi","chapter":"69","verse":"18","text":"Njoo karibu uniokoe,"},{"book":"Zaburi","chapter":"69","verse":"19","text":"Unajua jinsi ninavyodharauliwa,"},{"book":"Zaburi","chapter":"69","verse":"20","text":"Dharau zimenivunja moyo"},{"book":"Zaburi","chapter":"69","verse":"21","text":"Waliweka nyongo katika chakula changu"},{"book":"Zaburi","chapter":"69","verse":"22","text":"Meza iliyoandaliwa mbele yao na iwe mtego,"},{"book":"Zaburi","chapter":"69","verse":"23","text":"Macho yao yatiwe giza ili wasiweze kuona,"},{"book":"Zaburi","chapter":"69","verse":"24","text":"Uwamwagie ghadhabu yako,"},{"book":"Zaburi","chapter":"69","verse":"25","text":"Mahali pao na pawe ukiwa,"},{"book":"Zaburi","chapter":"69","verse":"26","text":"Kwa kuwa wanawatesa wale uliowajeruhi,"},{"book":"Zaburi","chapter":"69","verse":"27","text":"Walipize uovu juu ya uovu,"},{"book":"Zaburi","chapter":"69","verse":"28","text":"Wafutwe kutoka kitabu cha uzima"},{"book":"Zaburi","chapter":"69","verse":"29","text":"Mimi niko katika maumivu na dhiki;"},{"book":"Zaburi","chapter":"69","verse":"30","text":"Nitalisifu jina la Mungu kwa wimbo,"},{"book":"Zaburi","chapter":"69","verse":"31","text":"Hili litampendeza Bwana kuliko ng ºombe dume,"},{"book":"Zaburi","chapter":"69","verse":"32","text":"Maskini wataona na kufurahi:"},{"book":"Zaburi","chapter":"69","verse":"33","text":"Bwana huwasikia wahitaji"},{"book":"Zaburi","chapter":"69","verse":"34","text":"Mbingu na dunia zimsifu,"},{"book":"Zaburi","chapter":"69","verse":"35","text":"kwa maana Mungu ataiokoa Sayuni"},{"book":"Zaburi","chapter":"69","verse":"36","text":"watoto wa watumishi wake watairithi"}]}