{"data":[{"book":"Mmebusɛm","chapter":"10","verse":"1","text":"Salomo mmebusɛm:"},{"book":"Mmebusɛm","chapter":"10","verse":"2","text":"Ɔkwan bɔne so ahonya nni bo,"},{"book":"Mmebusɛm","chapter":"10","verse":"3","text":"Awurade remma ɔkɔm nne ɔtreneeni"},{"book":"Mmebusɛm","chapter":"10","verse":"4","text":"Nsa a ɛnyɛ adwuma ma onipa di hia,"},{"book":"Mmebusɛm","chapter":"10","verse":"5","text":"Nea ɔboaboa nnɔbae ano wɔ ahuhurubere no yɛ ɔba nyansafo"},{"book":"Mmebusɛm","chapter":"10","verse":"6","text":"Atreneefo hyɛ nhyira kyɛw"},{"book":"Mmebusɛm","chapter":"10","verse":"7","text":"Ɔtreneeni nkae yɛ nhyira,"},{"book":"Mmebusɛm","chapter":"10","verse":"8","text":"Koma mu nyansafo tie ɔhyɛ nsɛm,"},{"book":"Mmebusɛm","chapter":"10","verse":"9","text":"Ɔnokwafo nantew dwoodwoo,"},{"book":"Mmebusɛm","chapter":"10","verse":"10","text":"Nea ɔde nitan bu nʼani no de ɔhaw ba,"},{"book":"Mmebusɛm","chapter":"10","verse":"11","text":"Ɔtreneeni anom yɛ nkwa asuti,"},{"book":"Mmebusɛm","chapter":"10","verse":"12","text":"Ɔtan kanyan mpaapaemu,"},{"book":"Mmebusɛm","chapter":"10","verse":"13","text":"Wohu nyansa wɔ nhumufo anom,"},{"book":"Mmebusɛm","chapter":"10","verse":"14","text":"Anyansafo kora nimdeɛ,"},{"book":"Mmebusɛm","chapter":"10","verse":"15","text":"Adefo ahonya yɛ wɔn kuropɔn a wɔabɔ ho ban,"},{"book":"Mmebusɛm","chapter":"10","verse":"16","text":"Atreneefo akatua de nkwa brɛ wɔn,"},{"book":"Mmebusɛm","chapter":"10","verse":"17","text":"Nea otie ntetew pa no kyerɛ nkwa kwan,"},{"book":"Mmebusɛm","chapter":"10","verse":"18","text":"Nea ɔkata nitan so no yɛ ɔtorofo,"},{"book":"Mmebusɛm","chapter":"10","verse":"19","text":"Mfomso mpa ɔkasa bebree mu,"},{"book":"Mmebusɛm","chapter":"10","verse":"20","text":"Ɔtreneeni tɛkrɛma yɛ dwetɛ ankasa"},{"book":"Mmebusɛm","chapter":"10","verse":"21","text":"Atreneefo ano ma bebree aduan,"},{"book":"Mmebusɛm","chapter":"10","verse":"22","text":"Awurade nhyira de ahonya ba,"},{"book":"Mmebusɛm","chapter":"10","verse":"23","text":"Bɔneyɛ yɛ anigyede ma ɔkwasea,"},{"book":"Mmebusɛm","chapter":"10","verse":"24","text":"Nea amumɔyɛfo suro no bɛba wɔn so;"},{"book":"Mmebusɛm","chapter":"10","verse":"25","text":"Sɛ ahum no betwa mu a, amumɔyɛfo yera,"},{"book":"Mmebusɛm","chapter":"10","verse":"26","text":"Sɛnea nsa keka ɛse, na wusiw kɔ aniwa no,"},{"book":"Mmebusɛm","chapter":"10","verse":"27","text":"Awurade suro ma nkwa tenten,"},{"book":"Mmebusɛm","chapter":"10","verse":"28","text":"Atreneefo anidaso de ahotɔ ba,"},{"book":"Mmebusɛm","chapter":"10","verse":"29","text":"Awurade kwan yɛ guankɔbea ma ɔtreneeni,"},{"book":"Mmebusɛm","chapter":"10","verse":"30","text":"Wɔrentɔre atreneefo ase da,"},{"book":"Mmebusɛm","chapter":"10","verse":"31","text":"Nyansa fi ɔtreneeni anom ba"},{"book":"Mmebusɛm","chapter":"10","verse":"32","text":"Ɔtreneeni ano nim ade a ɛfata"}]}