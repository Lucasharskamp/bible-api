{"data":[{"book":"زەبوورەکان","chapter":"119","verse":"1","text":"خۆزگە بەوانە دەخوازرێ کە ڕێگایان تەواوە،"},{"book":"زەبوورەکان","chapter":"119","verse":"2","text":"خۆزگە بەوانە دەخوازرێ کە یاساکەی پەیڕەو دەکەن،"},{"book":"زەبوورەکان","chapter":"119","verse":"3","text":"هەروەها خراپە ناکەن و"},{"book":"زەبوورەکان","chapter":"119","verse":"4","text":"تۆ ڕێنماییەکانت داوە،"},{"book":"زەبوورەکان","chapter":"119","verse":"5","text":"خۆزگە ڕێگاکانم بچەسپابوونایە"},{"book":"زەبوورەکان","chapter":"119","verse":"6","text":"ئەو کاتە شەرمەزار نەدەبووم،"},{"book":"زەبوورەکان","chapter":"119","verse":"7","text":"بە دڵی پاکەوە ستایشت دەکەم،"},{"book":"زەبوورەکان","chapter":"119","verse":"8","text":"فەرزەکانت بەجێدەهێنم،"},{"book":"زەبوورەکان","chapter":"119","verse":"9","text":"گەنج بە چی هەڵسوکەوتی خۆی بێگەرد دەکات؟"},{"book":"زەبوورەکان","chapter":"119","verse":"10","text":"بە هەموو دڵەوە ڕوو لە تۆ دەکەم،"},{"book":"زەبوورەکان","chapter":"119","verse":"11","text":"وشەکانی تۆم لەناو دڵم پاراستووە،"},{"book":"زەبوورەکان","chapter":"119","verse":"12","text":"ئەی یەزدان، ستایش بۆ تۆ!"},{"book":"زەبوورەکان","chapter":"119","verse":"13","text":"بە لێوەکانی خۆم،"},{"book":"زەبوورەکان","chapter":"119","verse":"14","text":"بە پەیڕەوکردنی یاساکانت شاد دەبم،"},{"book":"زەبوورەکان","chapter":"119","verse":"15","text":"لە ڕێنماییەکانت ورد دەبمەوە،"},{"book":"زەبوورەکان","chapter":"119","verse":"16","text":"بە فەرزەکانت دڵخۆش دەبم،"},{"book":"زەبوورەکان","chapter":"119","verse":"17","text":"چاکە لەگەڵ خزمەتکاری خۆت بکە،"},{"book":"زەبوورەکان","chapter":"119","verse":"18","text":"چاوم بکەرەوە تاکو ئەوانە ببینم،"},{"book":"زەبوورەکان","chapter":"119","verse":"19","text":"من نامۆم لە زەوی،"},{"book":"زەبوورەکان","chapter":"119","verse":"20","text":"گیانم توایەوە لە تامەزرۆیی"},{"book":"زەبوورەکان","chapter":"119","verse":"21","text":"سەرزەنشتی لووت بەرزە نەفرەت لێکراوەکانت کرد،"},{"book":"زەبوورەکان","chapter":"119","verse":"22","text":"ڕیسوایی و سووکایەتیم لەسەر لابە،"},{"book":"زەبوورەکان","chapter":"119","verse":"23","text":"هەرچەندە میرەکان دادەنیشن و لە دژم پیلان دەگێڕن،"},{"book":"زەبوورەکان","chapter":"119","verse":"24","text":"یاساکانت شادمانی منن،"},{"book":"زەبوورەکان","chapter":"119","verse":"25","text":"گیانم بە خۆڵەوە نووساوە،"},{"book":"زەبوورەکان","chapter":"119","verse":"26","text":"باسی ڕێگاکانی خۆمم کرد، تۆ بەدەنگمەوە هاتیت،"},{"book":"زەبوورەکان","chapter":"119","verse":"27","text":"ڕێگای ڕێنماییەکانی خۆتم تێبگەیەنە،"},{"book":"زەبوورەکان","chapter":"119","verse":"28","text":"گیانم لە خەفەتان توایەوە،"},{"book":"زەبوورەکان","chapter":"119","verse":"29","text":"ڕێگای درۆم لێ دووربخەوە،"},{"book":"زەبوورەکان","chapter":"119","verse":"30","text":"ڕێگای ڕاستیم هەڵبژاردووە،"},{"book":"زەبوورەکان","chapter":"119","verse":"31","text":"دەست بە یاساکانی تۆوە دەگرم،"},{"book":"زەبوورەکان","chapter":"119","verse":"32","text":"بە ڕاکردن لەسەر ڕێگای ڕاسپاردەکانت دەڕۆم،"},{"book":"زەبوورەکان","chapter":"119","verse":"33","text":"ئەی یەزدان، فێری ڕێگای فەرزەکانی خۆتم بکە،"},{"book":"زەبوورەکان","chapter":"119","verse":"34","text":"تێمبگەیەنە و فێرکردنت پەیڕەو دەکەم،"},{"book":"زەبوورەکان","chapter":"119","verse":"35","text":"بمخە سەر ڕێگای ڕاسپاردەکانت،"},{"book":"زەبوورەکان","chapter":"119","verse":"36","text":"دڵم بخەرە سەر یاساکانت،"},{"book":"زەبوورەکان","chapter":"119","verse":"37","text":"چاوم وەرگێڕە لە تەماشاکردنی شتی پووچ،"},{"book":"زەبوورەکان","chapter":"119","verse":"38","text":"بەڵێنی خۆت بۆ خزتمەتکارەکەت بهێنە دی،"},{"book":"زەبوورەکان","chapter":"119","verse":"39","text":"ئەو سووکایەتییەم لەسەر لابە کە لێی دەترسم،"},{"book":"زەبوورەکان","chapter":"119","verse":"40","text":"ئەوەتا پەرۆشی ڕێنماییەکانی تۆم،"},{"book":"زەبوورەکان","chapter":"119","verse":"41","text":"ئەی یەزدان، با خۆشەویستییە نەگۆڕەکەتم بۆ بێت،"},{"book":"زەبوورەکان","chapter":"119","verse":"42","text":"جا وەڵامی ئەوە دەدەمەوە کە گاڵتەم پێ دەکات،"},{"book":"زەبوورەکان","chapter":"119","verse":"43","text":"وشەی ڕاست هەرگیز لە دەمم دوور مەخەوە،"},{"book":"زەبوورەکان","chapter":"119","verse":"44","text":"بە بەردەوامی گوێڕایەڵی فێرکردنی تۆ دەبم،"},{"book":"زەبوورەکان","chapter":"119","verse":"45","text":"بە ئازادی دەڕۆم،"},{"book":"زەبوورەکان","chapter":"119","verse":"46","text":"لەبەردەم پاشاکانیش باسی یاساکانی تۆ دەکەم،"},{"book":"زەبوورەکان","chapter":"119","verse":"47","text":"خۆشی لە ڕاسپاردەکانت دەبینم،"},{"book":"زەبوورەکان","chapter":"119","verse":"48","text":"دەست بۆ ڕاسپاردەکانت بەرز دەکەمەوە، ئەوەی خۆشم دەوێت،"},{"book":"زەبوورەکان","chapter":"119","verse":"49","text":"پەیمانی خۆت لەبیر بێت بۆ خزمەتکارەکەت،"},{"book":"زەبوورەکان","chapter":"119","verse":"50","text":"لە کاتی تەنگانەمدا دڵنەوایی من ئەمەیە:"},{"book":"زەبوورەکان","chapter":"119","verse":"51","text":"لووتبەرزەکان زۆر گاڵتەم پێ دەکەن،"},{"book":"زەبوورەکان","chapter":"119","verse":"52","text":"ئەی یەزدان، حوکمە لەمێژینەکانی تۆم هاتەوە یاد،"},{"book":"زەبوورەکان","chapter":"119","verse":"53","text":"لەبەر بەدکاران تووڕەییم جۆشا،"},{"book":"زەبوورەکان","chapter":"119","verse":"54","text":"فەرزەکانت بوونە بابەت گۆرانییەکانم،"},{"book":"زەبوورەکان","chapter":"119","verse":"55","text":"ئەی یەزدان، بە شەو یادی ناوی تۆ دەکەمەوە و"},{"book":"زەبوورەکان","chapter":"119","verse":"56","text":"ئەم نەریتە بۆ من بوو:"},{"book":"زەبوورەکان","chapter":"119","verse":"57","text":"ئەی یەزدان، تۆ بەشی منی،"},{"book":"زەبوورەکان","chapter":"119","verse":"58","text":"بە هەموو دڵەوە داوای ڕەزامەندی تۆ دەکەم،"},{"book":"زەبوورەکان","chapter":"119","verse":"59","text":"لە ڕێگای خۆم ڕامام،"},{"book":"زەبوورەکان","chapter":"119","verse":"60","text":"خێرا بووم و دوا نەکەوتم"},{"book":"زەبوورەکان","chapter":"119","verse":"61","text":"گوریسی بەدکاران منیان بەستووەتەوە،"},{"book":"زەبوورەکان","chapter":"119","verse":"62","text":"لە نیوەشەودا هەڵدەستم بۆ ئەوەی ستایشت بکەم"},{"book":"زەبوورەکان","chapter":"119","verse":"63","text":"من هاوڕێی هەموو ئەوانەم لە تۆ دەترسن،"},{"book":"زەبوورەکان","chapter":"119","verse":"64","text":"ئەی یەزدان، خۆشەویستییە نەگۆڕەکەت زەوی پڕکردووە،"},{"book":"زەبوورەکان","chapter":"119","verse":"65","text":"ئەی یەزدان، چاکەت لەگەڵ خزمەتکارەکەت کرد،"},{"book":"زەبوورەکان","chapter":"119","verse":"66","text":"هەستی باش و زانینم فێر بکە،"},{"book":"زەبوورەکان","chapter":"119","verse":"67","text":"بەر لەوەی زەلیل بم گومڕا بووم،"},{"book":"زەبوورەکان","chapter":"119","verse":"68","text":"تۆ چاکیت و چاکەکاری،"},{"book":"زەبوورەکان","chapter":"119","verse":"69","text":"لووتبەرزان درۆم بۆ هەڵدەبەستن،"},{"book":"زەبوورەکان","chapter":"119","verse":"70","text":"دڵیان وەک پیو بێ هەستە،"},{"book":"زەبوورەکان","chapter":"119","verse":"71","text":"چاک بوو بۆ من کە زەلیل بم،"},{"book":"زەبوورەکان","chapter":"119","verse":"72","text":"فێرکردنی دەمی تۆ"},{"book":"زەبوورەکان","chapter":"119","verse":"73","text":"دەستی تۆ دروستی کردم و دایڕشتم،"},{"book":"زەبوورەکان","chapter":"119","verse":"74","text":"ئەوانەی لێت دەترسن شادمان دەبن کاتێک دەمبینن،"},{"book":"زەبوورەکان","chapter":"119","verse":"75","text":"ئەی یەزدان، زانیم کە حوکمەکانت ڕاستودروستە،"},{"book":"زەبوورەکان","chapter":"119","verse":"76","text":"با خۆشەویستییە نەگۆڕەکەت بۆ دڵنەواییم بێت،"},{"book":"زەبوورەکان","chapter":"119","verse":"77","text":"با بەزەییت پێمدا بێتەوە بۆ ئەوەی بژیم،"},{"book":"زەبوورەکان","chapter":"119","verse":"78","text":"با لووتبەرزان شەرمەزار بن، چونکە بوختانیان بۆ هەڵبەستم،"},{"book":"زەبوورەکان","chapter":"119","verse":"79","text":"با ئەوانەی لێت دەترسن بگەڕێنەوە لای من،"},{"book":"زەبوورەکان","chapter":"119","verse":"80","text":"با دڵم تەواو بێت لە فەرزەکانت،"},{"book":"زەبوورەکان","chapter":"119","verse":"81","text":"گیانم پەرۆشی ڕزگاریی تۆیە،"},{"book":"زەبوورەکان","chapter":"119","verse":"82","text":"چاوم کز بوو لە چاوەڕوانی بەڵێنەکەت،"},{"book":"زەبوورەکان","chapter":"119","verse":"83","text":"هەرچەندە وەک مەشکەیەکم لێهاتووە کەوا دووکەڵ دایپۆشیوە،"},{"book":"زەبوورەکان","chapter":"119","verse":"84","text":"خزمەتکارەکەت دەبێ چەند ڕۆژ چاوەڕێ بکات؟"},{"book":"زەبوورەکان","chapter":"119","verse":"85","text":"لووتبەرزان چاڵیان بۆم هەڵکەندووە،"},{"book":"زەبو