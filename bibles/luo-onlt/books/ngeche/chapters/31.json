{"data":[{"book":"Ngeche","chapter":"31","verse":"1","text":"Weche mag Ruoth Lemuel, ma gin weche matut ma min mare nopuonje."},{"book":"Ngeche","chapter":"31","verse":"2","text":"Yaye wuoda, yaye wuoda mane anywolo!"},{"book":"Ngeche","chapter":"31","verse":"3","text":"Kik iket tekri kuom mon,"},{"book":"Ngeche","chapter":"31","verse":"4","text":"Ok en gima owinjore ne ruodhi, yaye Lemuel,"},{"book":"Ngeche","chapter":"31","verse":"5","text":"nono ka gimetho to wigi diwil gi gima chik ochiko,"},{"book":"Ngeche","chapter":"31","verse":"6","text":"Chiw kongʼo ne jogo ma chunygi ool,"},{"book":"Ngeche","chapter":"31","verse":"7","text":"Wegi mondo gimethi mondo wigi owil gi chan-gi"},{"book":"Ngeche","chapter":"31","verse":"8","text":"Wuo ka ichwako joma ok nyal wacho wachgi gin giwegi,"},{"book":"Ngeche","chapter":"31","verse":"9","text":"Wuo kendo ngʼad bura kare,"},{"book":"Ngeche","chapter":"31","verse":"10","text":"Dhako ma kite longʼo en ngʼa manyalo nwangʼo?"},{"book":"Ngeche","chapter":"31","verse":"11","text":"Chwore ogenogo chutho"},{"book":"Ngeche","chapter":"31","verse":"12","text":"Okelone mana ber,"},{"book":"Ngeche","chapter":"31","verse":"13","text":"Oyiero yie rombe kod tworo"},{"book":"Ngeche","chapter":"31","verse":"14","text":"Ochalo gi yiedhi mag ohala,"},{"book":"Ngeche","chapter":"31","verse":"15","text":"Ochiewo aa e nindo ka pod piny mudho,"},{"book":"Ngeche","chapter":"31","verse":"16","text":"Omanyo puodho maber mi ongʼiew,"},{"book":"Ngeche","chapter":"31","verse":"17","text":"Otiyo tijene gi teko duto;"},{"book":"Ngeche","chapter":"31","verse":"18","text":"Oneno ni gik moloso miye ohala"},{"book":"Ngeche","chapter":"31","verse":"19","text":"Ei lwete otingʼo yie rombe"},{"book":"Ngeche","chapter":"31","verse":"20","text":"Oyawo bade ne joma jochan"},{"book":"Ngeche","chapter":"31","verse":"21","text":"Ka piny ngʼich, oonge luoro ne joode,"},{"book":"Ngeche","chapter":"31","verse":"22","text":"Oloso raum ne kitandane,"},{"book":"Ngeche","chapter":"31","verse":"23","text":"Chwore imiyo luor e dhoranga dala maduongʼ,"},{"book":"Ngeche","chapter":"31","verse":"24","text":"Oloso lewni marep-rep mi ousgi,"},{"book":"Ngeche","chapter":"31","verse":"25","text":"Orwakore gi teko kod dimbruok;"},{"book":"Ngeche","chapter":"31","verse":"26","text":"Owuoyo gi rieko"},{"book":"Ngeche","chapter":"31","verse":"27","text":"Orito weche mag ode"},{"book":"Ngeche","chapter":"31","verse":"28","text":"Nyithinde chungo kendo luonge ni en ngʼat mogwedhi,"},{"book":"Ngeche","chapter":"31","verse":"29","text":"“Mon mangʼeny timo gik mabeyo,"},{"book":"Ngeche","chapter":"31","verse":"30","text":"Ber mar wangʼ wuondo ji, to chia bende rumo;"},{"book":"Ngeche","chapter":"31","verse":"31","text":"Miye pak moromo kode mondo onwangʼ"}]}