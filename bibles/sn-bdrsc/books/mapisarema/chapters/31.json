{"data":[{"book":"Mapisarema","chapter":"31","verse":"1","text":"Ndinovanda mamuri, imi Jehovha;"},{"book":"Mapisarema","chapter":"31","verse":"2","text":"Rerekerai nzeve yenyu kwandiri,"},{"book":"Mapisarema","chapter":"31","verse":"3","text":"Sezvo murimi dombo rangu nenhare yangu,"},{"book":"Mapisarema","chapter":"31","verse":"4","text":"Ndisunungurei pamusungo wandakadzikirwa,"},{"book":"Mapisarema","chapter":"31","verse":"5","text":"Ndinoisa mweya wangu mumaoko enyu;"},{"book":"Mapisarema","chapter":"31","verse":"6","text":"Ndinovenga vanonamatira kuzvifananidzo zvisina maturo;"},{"book":"Mapisarema","chapter":"31","verse":"7","text":"Ndichafara nokufarisisa murudo rwenyu,"},{"book":"Mapisarema","chapter":"31","verse":"8","text":"Hamuna kundiisa kumuvengi,"},{"book":"Mapisarema","chapter":"31","verse":"9","text":"Haiwa Jehovha, ndinzwirei ngoni nokuti ndine nhamo;"},{"book":"Mapisarema","chapter":"31","verse":"10","text":"Upenyu hwangu hwopera nokurwadziwa,"},{"book":"Mapisarema","chapter":"31","verse":"11","text":"Nokuda kwavavengi vangu,"},{"book":"Mapisarema","chapter":"31","verse":"12","text":"Ndakanganwikwa navo sendakafa;"},{"book":"Mapisarema","chapter":"31","verse":"13","text":"nokuti ndinonzwa guhwa ravazhinji;"},{"book":"Mapisarema","chapter":"31","verse":"14","text":"Asi ndinovimba nemi, imi Jehovha;"},{"book":"Mapisarema","chapter":"31","verse":"15","text":"Nguva dzangu dziri mumaoko enyu;"},{"book":"Mapisarema","chapter":"31","verse":"16","text":"Chiso chenyu ngachipenye pamusoro pomuranda wenyu;"},{"book":"Mapisarema","chapter":"31","verse":"17","text":"Haiwa Jehovha, itai kuti ndisanyadziswa,"},{"book":"Mapisarema","chapter":"31","verse":"18","text":"Miromo yavo inoreva nhema ngainyaradzwe,"},{"book":"Mapisarema","chapter":"31","verse":"19","text":"Haiwa, kunaka kwenyu kukuru sei,"},{"book":"Mapisarema","chapter":"31","verse":"20","text":"Mukudzivirira kwokuvapo kwenyu munovavanza"},{"book":"Mapisarema","chapter":"31","verse":"21","text":"Jehovha ngaarumbidzwe,"},{"book":"Mapisarema","chapter":"31","verse":"22","text":"Mukuvhunduka kwangu, ndakati,"},{"book":"Mapisarema","chapter":"31","verse":"23","text":"Idai Jehovha, imi vatsvene mose!"},{"book":"Mapisarema","chapter":"31","verse":"24","text":"Simbai uye mutsunge mwoyo,"}]}