{"data":[{"book":"Jobho","chapter":"28","verse":"1","text":"“Mugodhi wesirivha uripo,"},{"book":"Jobho","chapter":"28","verse":"2","text":"Simbi inocherwa pasi,"},{"book":"Jobho","chapter":"28","verse":"3","text":"Munhu anoita kuti rima ripere;"},{"book":"Jobho","chapter":"28","verse":"4","text":"Anovhura mugodhi kure nokunogara vanhu,"},{"book":"Jobho","chapter":"28","verse":"5","text":"Iyo nyika, zvokudya zvichibva mairi,"},{"book":"Jobho","chapter":"28","verse":"6","text":"safire inobva mumatombo ayo,"},{"book":"Jobho","chapter":"28","verse":"7","text":"Hakuna gondo rinoziva nzira iyo yakavanzika,"},{"book":"Jobho","chapter":"28","verse":"8","text":"Mhuka dzinozvikudza hadziisi rutsokapo,"},{"book":"Jobho","chapter":"28","verse":"9","text":"Ruoko rwomunhu runorova dombo romusarasara"},{"book":"Jobho","chapter":"28","verse":"10","text":"Anochera mugero nomumatombo;"},{"book":"Jobho","chapter":"28","verse":"11","text":"Anotsvakisisa panotangira nzizi"},{"book":"Jobho","chapter":"28","verse":"12","text":"“Asi uchenjeri hungawanikwepiko?"},{"book":"Jobho","chapter":"28","verse":"13","text":"Munhu haanzwisisi kukosha kwahwo;"},{"book":"Jobho","chapter":"28","verse":"14","text":"Pakadzika panoti, ‘Hahuzi mandiri.’"},{"book":"Jobho","chapter":"28","verse":"15","text":"Hahungatengwi negoridhe yakaisvonaka,"},{"book":"Jobho","chapter":"28","verse":"16","text":"Hahungatengwi negoridhe reOfiri,"},{"book":"Jobho","chapter":"28","verse":"17","text":"Hahungaenzaniswi negoridhe kana kristari,"},{"book":"Jobho","chapter":"28","verse":"18","text":"Korari nejasipa hazvingatongorehwi;"},{"book":"Jobho","chapter":"28","verse":"19","text":"Topazi yeEtiopia haingaenzaniswi nahwo;"},{"book":"Jobho","chapter":"28","verse":"20","text":"“Zvino uchenjeri hunobvepiko?"},{"book":"Jobho","chapter":"28","verse":"21","text":"Kwakavanzirwa meso ezvipenyu zvose,"},{"book":"Jobho","chapter":"28","verse":"22","text":"Kuparadza noRufu zvinoti,"},{"book":"Jobho","chapter":"28","verse":"23","text":"Mwari oga ndiye anonzwisisa nzira inoenda kwahuri,"},{"book":"Jobho","chapter":"28","verse":"24","text":"nokuti anoona magumo enyika,"},{"book":"Jobho","chapter":"28","verse":"25","text":"Paakasimbisa simba remhepo"},{"book":"Jobho","chapter":"28","verse":"26","text":"paakaita chirevo chemvura"},{"book":"Jobho","chapter":"28","verse":"27","text":"ipapo akatarira kuuchenjeri akahunzvera;"},{"book":"Jobho","chapter":"28","verse":"28","text":"Zvino akati kumunhu,"}]}