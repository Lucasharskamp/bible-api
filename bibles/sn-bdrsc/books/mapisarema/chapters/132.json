{"data":[{"book":"Mapisarema","chapter":"132","verse":"1","text":"Haiwa Jehovha, rangarirai Dhavhidhi"},{"book":"Mapisarema","chapter":"132","verse":"2","text":"Akapika mhiko kuna Jehovha,"},{"book":"Mapisarema","chapter":"132","verse":"3","text":"“Handingapindi mumba mangu"},{"book":"Mapisarema","chapter":"132","verse":"4","text":"handingatenderi hope mumaziso angu,"},{"book":"Mapisarema","chapter":"132","verse":"5","text":"kusvikira ndawana nzvimbo yaJehovha,"},{"book":"Mapisarema","chapter":"132","verse":"6","text":"Takazvinzwa muEfurata,"},{"book":"Mapisarema","chapter":"132","verse":"7","text":"“Ngatiendei kunzvimbo yake yokugara;"},{"book":"Mapisarema","chapter":"132","verse":"8","text":"haiwa Jehovha, simukai muuye kunzvimbo yenyu yokuzorora,"},{"book":"Mapisarema","chapter":"132","verse":"9","text":"Vaprista venyu dai vafukidzwa nokururama;"},{"book":"Mapisarema","chapter":"132","verse":"10","text":"Nokuda kwaDhavhidhi muranda wenyu,"},{"book":"Mapisarema","chapter":"132","verse":"11","text":"Jehovha akapika mhiko kuna Dhavhidhi,"},{"book":"Mapisarema","chapter":"132","verse":"12","text":"kana vanakomana vako vakachengeta sungano yangu"},{"book":"Mapisarema","chapter":"132","verse":"13","text":"Nokuti Jehovha akasarudza Zioni,"},{"book":"Mapisarema","chapter":"132","verse":"14","text":"“Iyi ndiyo nzvimbo yangu yokuzorora nokusingaperi-peri;"},{"book":"Mapisarema","chapter":"132","verse":"15","text":"ndichariropafadza nezvakawanda;"},{"book":"Mapisarema","chapter":"132","verse":"16","text":"Ndichafukidza vaprista varo noruponeso,"},{"book":"Mapisarema","chapter":"132","verse":"17","text":"“Pano ndipo pandichameresa nyanga yaDhavhidhi,"},{"book":"Mapisarema","chapter":"132","verse":"18","text":"Ndichafukidza vavengi vake nenyadzi,"}]}