{"data":[{"book":"UDutheronomi","chapter":"33","verse":"1","text":"Lesi yiso isibusiso esethulwa nguMosi umuntu kaNkulunkulu kwabako-Israyeli engakafi."},{"book":"UDutheronomi","chapter":"33","verse":"2","text":"Wathi:"},{"book":"UDutheronomi","chapter":"33","verse":"3","text":"Ngempela nguwe othanda abantu,"},{"book":"UDutheronomi","chapter":"33","verse":"4","text":"umthetho esawuphiwa nguMosi,"},{"book":"UDutheronomi","chapter":"33","verse":"5","text":"Wayeyinkosi phezu kukaJeshuruni"},{"book":"UDutheronomi","chapter":"33","verse":"6","text":"Kaphile uRubheni njalo angafi,"},{"book":"UDutheronomi","chapter":"33","verse":"7","text":"Lokhu wakutsho ngoJuda:"},{"book":"UDutheronomi","chapter":"33","verse":"8","text":"NgoLevi wathi:"},{"book":"UDutheronomi","chapter":"33","verse":"9","text":"Wathi ngoyise lonina,"},{"book":"UDutheronomi","chapter":"33","verse":"10","text":"Ufundisa amanyathelo akho kuJakhobe"},{"book":"UDutheronomi","chapter":"33","verse":"11","text":"Busisa bonke ubuciko bakhe, Thixo,"},{"book":"UDutheronomi","chapter":"33","verse":"12","text":"NgoBhenjamini wathi:"},{"book":"UDutheronomi","chapter":"33","verse":"13","text":"NgoJosefa wathi:"},{"book":"UDutheronomi","chapter":"33","verse":"14","text":"langobuhle obuyingqala obulethwa lilanga"},{"book":"UDutheronomi","chapter":"33","verse":"15","text":"lezipho zekhethelo zasezintabeni zendulo"},{"book":"UDutheronomi","chapter":"33","verse":"16","text":"langezipho eziyingqala zomhlaba lokugcwala kwawo"},{"book":"UDutheronomi","chapter":"33","verse":"17","text":"Ebukhosini unjengenkunzi encane elizibulo;"},{"book":"UDutheronomi","chapter":"33","verse":"18","text":"NgoZebhuluni wathi:"},{"book":"UDutheronomi","chapter":"33","verse":"19","text":"Bazabizela abantu ezintabeni khonale"},{"book":"UDutheronomi","chapter":"33","verse":"20","text":"NgoGadi wathi:"},{"book":"UDutheronomi","chapter":"33","verse":"21","text":"Wazikhethela umhlaba omuhle okwamagama waba ngowakhe;"},{"book":"UDutheronomi","chapter":"33","verse":"22","text":"NgoDani wathi:"},{"book":"UDutheronomi","chapter":"33","verse":"23","text":"NgoNafithali wathi:"},{"book":"UDutheronomi","chapter":"33","verse":"24","text":"Ngo-Asheri wathi:"},{"book":"UDutheronomi","chapter":"33","verse":"25","text":"Amabhawudo emasangweni akho azakuba ngawensimbi lethusi"},{"book":"UDutheronomi","chapter":"33","verse":"26","text":"Kakho ofana loNkulunkulu kaJeshuruni,"},{"book":"UDutheronomi","chapter":"33","verse":"27","text":"UNkulunkulu ongusimakade uyisiphephelo sakho,"},{"book":"UDutheronomi","chapter":"33","verse":"28","text":"Ngakho u-Israyeli uzaphila yedwa evikelekile,"},{"book":"UDutheronomi","chapter":"33","verse":"29","text":"Ubusisiwe wena, Oh Israyeli!"}]}