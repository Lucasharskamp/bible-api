{"data":[{"book":"Zaburi","chapter":"22","verse":"1","text":"Nyasacha, Nyasacha, iweya nangʼo?"},{"book":"Zaburi","chapter":"22","verse":"2","text":"Yaye Nyasacha, aywagora odiechiengʼ duto to ok idwoka,"},{"book":"Zaburi","chapter":"22","verse":"3","text":"To eka ibet e kom duongʼ kaka Ngʼama Ler;"},{"book":"Zaburi","chapter":"22","verse":"4","text":"Kuomi ema wuonewa noketoe genogi;"},{"book":"Zaburi","chapter":"22","verse":"5","text":"Ne giywakni kendo ne iwarogi;"},{"book":"Zaburi","chapter":"22","verse":"6","text":"An to an mana kudni ma ok dhano,"},{"book":"Zaburi","chapter":"22","verse":"7","text":"Ji duto monena jara;"},{"book":"Zaburi","chapter":"22","verse":"8","text":"“Ogeno kuom Jehova Nyasaye;"},{"book":"Zaburi","chapter":"22","verse":"9","text":"To in ema ne igola e ich;"},{"book":"Zaburi","chapter":"22","verse":"10","text":"Ne oriwa kodi chakre chiengʼ mane onywolae;"},{"book":"Zaburi","chapter":"22","verse":"11","text":"Kik ibed mabor koda,"},{"book":"Zaburi","chapter":"22","verse":"12","text":"Rwedhi mangʼeny olwora;"},{"book":"Zaburi","chapter":"22","verse":"13","text":"Sibuoche maruto makidho le ma gisemako"},{"book":"Zaburi","chapter":"22","verse":"14","text":"Ipuka oko ka pi,"},{"book":"Zaburi","chapter":"22","verse":"15","text":"Tekona osetwo ka balatago,"},{"book":"Zaburi","chapter":"22","verse":"16","text":"Guogi oselwora;"},{"book":"Zaburi","chapter":"22","verse":"17","text":"Anyalo kwano chokena duto;"},{"book":"Zaburi","chapter":"22","verse":"18","text":"Ne gipogore lepa e kindgi giwegi"},{"book":"Zaburi","chapter":"22","verse":"19","text":"To in, yaye Jehova Nyasaye, kik ibed mabor koda."},{"book":"Zaburi","chapter":"22","verse":"20","text":"Res ngimana kuom ligangla,"},{"book":"Zaburi","chapter":"22","verse":"21","text":"Resa e dho sibuoche;"},{"book":"Zaburi","chapter":"22","verse":"22","text":"Abiro hulo nyingi ne owetena;"},{"book":"Zaburi","chapter":"22","verse":"23","text":"Un muluoro Jehova Nyasaye, pakeuru!"},{"book":"Zaburi","chapter":"22","verse":"24","text":"Nikech pok ochayo kata ojaro"},{"book":"Zaburi","chapter":"22","verse":"25","text":"Kuomi ema thoro mara mar pak e chokruok maduongʼ aye;"},{"book":"Zaburi","chapter":"22","verse":"26","text":"Joma odhier biro chiemo mi yiengʼ;"},{"book":"Zaburi","chapter":"22","verse":"27","text":"Tunge piny duto"},{"book":"Zaburi","chapter":"22","verse":"28","text":"Nikech loch en mar Jehova Nyasaye"},{"book":"Zaburi","chapter":"22","verse":"29","text":"Jo-mwandu duto manie piny biro chiemo kendo lemo;"},{"book":"Zaburi","chapter":"22","verse":"30","text":"Tienge mabiro noti ne Jehova Nyasaye,"},{"book":"Zaburi","chapter":"22","verse":"31","text":"Gibiro hulo timne makare,"}]}