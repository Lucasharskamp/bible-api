{"data":[{"book":"Mapisarema","chapter":"107","verse":"1","text":"Vongai Jehovha, nokuti akanaka;"},{"book":"Mapisarema","chapter":"107","verse":"2","text":"Vakadzikinurwa naJehovha ngavadaro,"},{"book":"Mapisarema","chapter":"107","verse":"3","text":"avo vaakaunganidza kubva panyika dzose,"},{"book":"Mapisarema","chapter":"107","verse":"4","text":"Vamwe vakadzungaira murenje nomugwenga,"},{"book":"Mapisarema","chapter":"107","verse":"5","text":"Vakava nenzara nenyota,"},{"book":"Mapisarema","chapter":"107","verse":"6","text":"Ipapo vakachema kuna Jehovha pakutambudzika kwavo,"},{"book":"Mapisarema","chapter":"107","verse":"7","text":"Akavafambisa nenzira yakarurama"},{"book":"Mapisarema","chapter":"107","verse":"8","text":"Vanhu ngavavonge Jehovha nokuda kworudo rwake rusingaperi,"},{"book":"Mapisarema","chapter":"107","verse":"9","text":"nokuti anogutsa vane nyota,"},{"book":"Mapisarema","chapter":"107","verse":"10","text":"Vamwe vakagara murima nokusurikirwa kwakadzama,"},{"book":"Mapisarema","chapter":"107","verse":"11","text":"nokuti vakanga vamukira mashoko aMwari"},{"book":"Mapisarema","chapter":"107","verse":"12","text":"Saka akaita kuti vashande zvinorwadza;"},{"book":"Mapisarema","chapter":"107","verse":"13","text":"Ipapo vakachema kuna Jehovha pakutambudzika kwavo,"},{"book":"Mapisarema","chapter":"107","verse":"14","text":"Akavabudisa murima nomukusviba kwakadzika dzika,"},{"book":"Mapisarema","chapter":"107","verse":"15","text":"Vanhu ngavavonge Jehovha nokuda kworudo rwake rusingaperi,"},{"book":"Mapisarema","chapter":"107","verse":"16","text":"nokuti anopwanya masuo endarira,"},{"book":"Mapisarema","chapter":"107","verse":"17","text":"Vamwe vakava mapenzi nokuda kwenzira dzavo dzokumukira,"},{"book":"Mapisarema","chapter":"107","verse":"18","text":"Vakasema zvokudya zvose,"},{"book":"Mapisarema","chapter":"107","verse":"19","text":"Ipapo vakachema kuna Jehovha pakutambudzika kwavo,"},{"book":"Mapisarema","chapter":"107","verse":"20","text":"Akatuma shoko rake uye akavaporesa;"},{"book":"Mapisarema","chapter":"107","verse":"21","text":"Vanhu ngavavonge Jehovha nokuda kworudo rwake rusingaperi,"},{"book":"Mapisarema","chapter":"107","verse":"22","text":"Ngavabayire zvibayiro zvokuvonga,"},{"book":"Mapisarema","chapter":"107","verse":"23","text":"Vamwe vakafamba rwendo pagungwa nezvikepe;"},{"book":"Mapisarema","chapter":"107","verse":"24","text":"Vakaona mabasa aJehovha,"},{"book":"Mapisarema","chapter":"107","verse":"25","text":"Nokuti akataura uye akamutsa dutu"},{"book":"Mapisarema","chapter":"107","verse":"26","text":"Vakaenda kumusoro kumatenga vakaendawo pasi kwakadzika;"},{"book":"Mapisarema","chapter":"107","verse":"27","text":"Vakandeya vakadzedzereka savanhu vadhakwa; vakasvika pakupererwa namazano."},{"book":"Mapisarema","chapter":"107","verse":"28","text":"Ipapo vakachema kuna Jehovha pakutambudzika kwavo,"},{"book":"Mapisarema","chapter":"107","verse":"29","text":"Akanyaradza dutu remhepo nezevezeve;"},{"book":"Mapisarema","chapter":"107","verse":"30","text":"Vakafara parakadzikama,"},{"book":"Mapisarema","chapter":"107","verse":"31","text":"Vanhu ngavavonge Jehovha nokuda kworudo rwake rusingaperi,"},{"book":"Mapisarema","chapter":"107","verse":"32","text":"Ngavamukudze paungano yavanhu,"},{"book":"Mapisarema","chapter":"107","verse":"33","text":"Akashandura nzizi dzikava gwenga,"},{"book":"Mapisarema","chapter":"107","verse":"34","text":"uye nyika yezvibereko ikava gwenga romunyu,"},{"book":"Mapisarema","chapter":"107","verse":"35","text":"Akashandura gwenga rikava madziva emvura,"},{"book":"Mapisarema","chapter":"107","verse":"36","text":"ndipo paakagarisa vane nzara,"},{"book":"Mapisarema","chapter":"107","verse":"37","text":"Vakadyara minda, uye vakasima minda yemizambiringa,"},{"book":"Mapisarema","chapter":"107","verse":"38","text":"akavaropafadza, uye akavawanza zvikuru,"},{"book":"Mapisarema","chapter":"107","verse":"39","text":"Ipapo vakava vashoma, uye vakaninipiswa vakadzvinyirirwa,"},{"bo