{"data":[{"book":"Sandane","chapter":"6","verse":"1","text":"N ma diina,"},{"book":"Sandane","chapter":"6","verse":"2","text":"xa i bata i xunna raso fena nde yi i fala xuiin xɔn"},{"book":"Sandane","chapter":"6","verse":"3","text":"nayi i xa ito nan liga"},{"book":"Sandane","chapter":"6","verse":"4","text":"I nama i matabu,"},{"book":"Sandane","chapter":"6","verse":"5","text":"fanni i munma xunba a yii,"},{"book":"Sandane","chapter":"6","verse":"6","text":"I tan muxu narixina,"},{"book":"Sandane","chapter":"6","verse":"7","text":"Manga yo mi dondonla xun na,"},{"book":"Sandane","chapter":"6","verse":"8","text":"Anu, a donseen namarama a waxatini,"},{"book":"Sandane","chapter":"6","verse":"9","text":"I tan muxu narixina,"},{"book":"Sandane","chapter":"6","verse":"10","text":"I na i sa,"},{"book":"Sandane","chapter":"6","verse":"11","text":"yiigelitɔyaan yi baga i ma"},{"book":"Sandane","chapter":"6","verse":"12","text":"Muxun naxan wule falane tima"},{"book":"Sandane","chapter":"6","verse":"13","text":"A yɛɛne magirama bodene yanfadeni,"},{"book":"Sandane","chapter":"6","verse":"14","text":"Tinxintareyaan nan a bɔɲɛni."},{"book":"Sandane","chapter":"6","verse":"15","text":"Nanara, gbalona a sɔtɔma nɛn xulɛn,"},{"book":"Sandane","chapter":"6","verse":"16","text":"Se sennin nan na"},{"book":"Sandane","chapter":"6","verse":"17","text":"Yɛɛ wasoxine,"},{"book":"Sandane","chapter":"6","verse":"18","text":"bɔɲɛn naxan kɔtɛ ɲaxini tɔnma,"},{"book":"Sandane","chapter":"6","verse":"19","text":"wule seren naxanye wulen falama,"},{"book":"Sandane","chapter":"6","verse":"20","text":"N ma diina,"},{"book":"Sandane","chapter":"6","verse":"21","text":"E sa i xaxinli habadan,"},{"book":"Sandane","chapter":"6","verse":"22","text":"E tima nɛn i yɛɛ ra i ya sigatine yi."},{"book":"Sandane","chapter":"6","verse":"23","text":"Amasɔtɔ yamarini itoe luxi nɛn"},{"book":"Sandane","chapter":"6","verse":"24","text":"E i ratangama nɛn ɲaxalan yalunxin ma"},{"book":"Sandane","chapter":"6","verse":"25","text":"I nama kunfa na ɲaxalan sifan xɔn a tofanna ma."},{"book":"Sandane","chapter":"6","verse":"26","text":"Amasɔtɔ yalunden na feen yatɛma"},{"book":"Sandane","chapter":"6","verse":"27","text":"Muxun nɔɛ tɛɛn sɛ nɛn a yibaan kui"},{"book":"Sandane","chapter":"6","verse":"28","text":"Muxun nɔɛ sigan tiyɛ tɛɛ wolonne fari"},{"book":"Sandane","chapter":"6","verse":"29","text":"Na luxi nɛn"},{"book":"Sandane","chapter":"6","verse":"30","text":"Yamaan mi muɲaden naɲaxuma"},{"book":"Sandane","chapter":"6","verse":"31","text":"Koni hali na, a na suxu,"},{"book":"Sandane","chapter":"6","verse":"32","text":"Koni xɛmɛn naxan na yalunyaan liga"},{"book":"Sandane","chapter":"6","verse":"33","text":"A bɔnbɔma nɛn, a rayarabi,"},{"book":"Sandane","chapter":"6","verse":"34","text":"Amasɔtɔ xɔxɔlɔnna xɛmɛn naxaɲɛma ayi nɛn,"},{"book":"Sandane","chapter":"6","verse":"35","text":"A mi yangin yo rasuxɛ,"}]}