{"data":[{"book":"Izaga","chapter":"23","verse":"1","text":"Nxa uhlala phansi usidla lombusi,"},{"book":"Izaga","chapter":"23","verse":"2","text":"ungazifaki ingqamu entanyeni"},{"book":"Izaga","chapter":"23","verse":"3","text":"Ungahawukeli izibiliboco zakhe"},{"book":"Izaga","chapter":"23","verse":"4","text":"Ungazihlukuluzi uzigugise ngomsebenzi uzama inotho;"},{"book":"Izaga","chapter":"23","verse":"5","text":"Ungayibona manje inotho, masinyane isinyamalele,"},{"book":"Izaga","chapter":"23","verse":"6","text":"Ungakudli ukudla komuntu oncitshanayo,"},{"book":"Izaga","chapter":"23","verse":"7","text":"ngoba yena uhlezi ebalisa ngendleko."},{"book":"Izaga","chapter":"23","verse":"8","text":"Uzakuhlanza okuncane lokho okudlileyo"},{"book":"Izaga","chapter":"23","verse":"9","text":"Ungakhulumi lesithutha"},{"book":"Izaga","chapter":"23","verse":"10","text":"Ungasusi ilitshe lakudala elomngcele"},{"book":"Izaga","chapter":"23","verse":"11","text":"ngoba uMhlengi wazo ulamandla;"},{"book":"Izaga","chapter":"23","verse":"12","text":"Nikela inhliziyo yakho ekufundisweni"},{"book":"Izaga","chapter":"23","verse":"13","text":"Ungayekeli ukumlaya umntwana;"},{"book":"Izaga","chapter":"23","verse":"14","text":"Mjezise ngoswazi"},{"book":"Izaga","chapter":"23","verse":"15","text":"Ndodana yami, aluba inhliziyo yakho ihlakaniphile,"},{"book":"Izaga","chapter":"23","verse":"16","text":"ingaphakathi yami izajabula"},{"book":"Izaga","chapter":"23","verse":"17","text":"Inhliziyo yakho mayingahawukeli izoni,"},{"book":"Izaga","chapter":"23","verse":"18","text":"Ngeqiniso lihle ikusasa lakho,"},{"book":"Izaga","chapter":"23","verse":"19","text":"Lalela, ndodana yami, uhlakaniphe,"},{"book":"Izaga","chapter":"23","verse":"20","text":"Ungahlangani leziminzi zewayini"},{"book":"Izaga","chapter":"23","verse":"21","text":"ngoba izidakwa lezihwaba ziba ngabayanga,"},{"book":"Izaga","chapter":"23","verse":"22","text":"Lalela uyihlo owakuletha emhlabeni,"},{"book":"Izaga","chapter":"23","verse":"23","text":"Thenga iqiniso ungalithengisi;"},{"book":"Izaga","chapter":"23","verse":"24","text":"Uyise womuntu olungileyo uyathokoza kakhulu;"},{"book":"Izaga","chapter":"23","verse":"25","text":"Sengathi uyihlo lonyoko bangathokoza;"},{"book":"Izaga","chapter":"23","verse":"26","text":"Ndodana yami, ngipha inhliziyo yakho,"},{"book":"Izaga","chapter":"23","verse":"27","text":"ngoba isifebe singumgodi otshonayo"},{"book":"Izaga","chapter":"23","verse":"28","text":"Uyacathamela njengesigebenga,"},{"book":"Izaga","chapter":"23","verse":"29","text":"Ngubani ohlaba umkhosi?"},{"book":"Izaga","chapter":"23","verse":"30","text":"Yilabo abalibala ngewayini,"},{"book":"Izaga","chapter":"23","verse":"31","text":"Ungahugwa liwayini ulibona libomvu,"},{"book":"Izaga","chapter":"23","verse":"32","text":"Ekucineni liluma njengenyoka"},{"book":"Izaga","chapter":"23","verse":"33","text":"Amehlo akho azabona izinto ezingaqedakaliyo"},{"book":"Izaga","chapter":"23","verse":"34","text":"Uzakuba njengomuntu ondendayo phakathi kolwandle,"},{"book":"Izaga","chapter":"23","verse":"35","text":"Uzakuthi, â€œBangitshayile kodwa kangilimalanga!"}]}