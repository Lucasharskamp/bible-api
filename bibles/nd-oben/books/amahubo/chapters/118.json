{"data":[{"book":"AmaHubo","chapter":"118","verse":"1","text":"Mbongeni uThixo ngoba ulungile;"},{"book":"AmaHubo","chapter":"118","verse":"2","text":"U-Israyeli katsho athi:"},{"book":"AmaHubo","chapter":"118","verse":"3","text":"Kayithi indlu ka-Aroni:"},{"book":"AmaHubo","chapter":"118","verse":"4","text":"Akuthi bonke abamesabayo uThixo bathi:"},{"book":"AmaHubo","chapter":"118","verse":"5","text":"Ngathi ngidabukile ngakhala kuThixo,"},{"book":"AmaHubo","chapter":"118","verse":"6","text":"UThixo ulami, angiyikwesaba."},{"book":"AmaHubo","chapter":"118","verse":"7","text":"UThixo ulami; ungumsizi wami."},{"book":"AmaHubo","chapter":"118","verse":"8","text":"Kungcono ukuphephela kuThixo"},{"book":"AmaHubo","chapter":"118","verse":"9","text":"Kungcono ukuphephela kuThixo"},{"book":"AmaHubo","chapter":"118","verse":"10","text":"Zonke izizwe zangihanqa,"},{"book":"AmaHubo","chapter":"118","verse":"11","text":"Zangihanqa inxa zonke"},{"book":"AmaHubo","chapter":"118","verse":"12","text":"Zangiminyezela njengezinyosi"},{"book":"AmaHubo","chapter":"118","verse":"13","text":"Ngasunduzelwa emuva ngaphose ngawa,"},{"book":"AmaHubo","chapter":"118","verse":"14","text":"UThixo ungamandla ami lengoma yami;"},{"book":"AmaHubo","chapter":"118","verse":"15","text":"Imisindo yentokozo lokunqoba"},{"book":"AmaHubo","chapter":"118","verse":"16","text":"Isandla sokunene sikaThixo siphakanyisiwe kakhulu;"},{"book":"AmaHubo","chapter":"118","verse":"17","text":"Angizukufa kodwa ngizaphila,"},{"book":"AmaHubo","chapter":"118","verse":"18","text":"UThixo ungitshayile kakhulu,"},{"book":"AmaHubo","chapter":"118","verse":"19","text":"Ngivulela amasango okulunga;"},{"book":"AmaHubo","chapter":"118","verse":"20","text":"Nanti isango likaThixo"},{"book":"AmaHubo","chapter":"118","verse":"21","text":"Ngizakubonga, ngoba ungiphendulile;"},{"book":"AmaHubo","chapter":"118","verse":"22","text":"Ilitshe ababelilahlile"},{"book":"AmaHubo","chapter":"118","verse":"23","text":"uThixo ukwenzile lokhu;"},{"book":"AmaHubo","chapter":"118","verse":"24","text":"Leli lilanga alenzileyo uThixo;"},{"book":"AmaHubo","chapter":"118","verse":"25","text":"Oh Thixo, sisindise;"},{"book":"AmaHubo","chapter":"118","verse":"26","text":"Ubusisiwe lowo obuya ngebizo likaThixo."},{"book":"AmaHubo","chapter":"118","verse":"27","text":"UThixo unguNkulunkulu,"},{"book":"AmaHubo","chapter":"118","verse":"28","text":"UnguNkulunkulu wami, ngizakubonga;"},{"book":"AmaHubo","chapter":"118","verse":"29","text":"Mbongeni uThixo ngoba ulungile;"}]}