{"data":[{"book":"Zaburi","chapter":"147","verse":"1","text":"Msifuni Bwana."},{"book":"Zaburi","chapter":"147","verse":"2","text":"Bwana hujenga Yerusalemu,"},{"book":"Zaburi","chapter":"147","verse":"3","text":"Anawaponya waliovunjika mioyo"},{"book":"Zaburi","chapter":"147","verse":"4","text":"Huzihesabu nyota"},{"book":"Zaburi","chapter":"147","verse":"5","text":"Bwana wetu ni mkuu na mwenye uwezo mwingi,"},{"book":"Zaburi","chapter":"147","verse":"6","text":"Bwana huwahifadhi wanyenyekevu"},{"book":"Zaburi","chapter":"147","verse":"7","text":"Mwimbieni Bwana kwa shukrani,"},{"book":"Zaburi","chapter":"147","verse":"8","text":"Yeye huzifunika anga kwa mawingu,"},{"book":"Zaburi","chapter":"147","verse":"9","text":"Huwapa chakula mifugo"},{"book":"Zaburi","chapter":"147","verse":"10","text":"Furaha yake haipo katika nguvu za farasi,"},{"book":"Zaburi","chapter":"147","verse":"11","text":"Bwana hupendezwa na wale wamchao,"},{"book":"Zaburi","chapter":"147","verse":"12","text":"Mtukuze Bwana, ee Yerusalemu,"},{"book":"Zaburi","chapter":"147","verse":"13","text":"kwa maana huimarisha makomeo ya malango yako"},{"book":"Zaburi","chapter":"147","verse":"14","text":"Huwapa amani mipakani mwenu"},{"book":"Zaburi","chapter":"147","verse":"15","text":"Hutuma amri yake duniani,"},{"book":"Zaburi","chapter":"147","verse":"16","text":"Anatandaza theluji kama sufu"},{"book":"Zaburi","chapter":"147","verse":"17","text":"Huvurumisha mvua yake ya mawe kama changarawe."},{"book":"Zaburi","chapter":"147","verse":"18","text":"Hutuma neno lake na kuviyeyusha,"},{"book":"Zaburi","chapter":"147","verse":"19","text":"Amemfunulia Yakobo neno lake,"},{"book":"Zaburi","chapter":"147","verse":"20","text":"Hajafanya hivyo kwa taifa lingine lolote,"}]}