{"data":[{"book":"2 Samueri","chapter":"22","verse":"1","text":"Dhavhidhi akaimbira Jehovha mashoko orwiyo urwu paakarwirwa naJehovha kubva muruoko rwavavengi vake vose nomuruoko rwaSauro."},{"book":"2 Samueri","chapter":"22","verse":"2","text":"Akati:"},{"book":"2 Samueri","chapter":"22","verse":"3","text":"Mwari wangu ndiye dombo rangu, ndinovanda maari,"},{"book":"2 Samueri","chapter":"22","verse":"4","text":"Ndinodana kuna Jehovha, iye anofanira kurumbidzwa,"},{"book":"2 Samueri","chapter":"22","verse":"5","text":"“Mafungu orufu akandikomba;"},{"book":"2 Samueri","chapter":"22","verse":"6","text":"Mabote eguva akandimonera;"},{"book":"2 Samueri","chapter":"22","verse":"7","text":"Mukutambudzika kwangu ndakadana kuna Jehovha;"},{"book":"2 Samueri","chapter":"22","verse":"8","text":"“Nyika yakadedera uye ikazungunuka,"},{"book":"2 Samueri","chapter":"22","verse":"9","text":"Utsi hwakakwira kubva mumhino dzake;"},{"book":"2 Samueri","chapter":"22","verse":"10","text":"Akakamura matenga akaburuka pasi;"},{"book":"2 Samueri","chapter":"22","verse":"11","text":"Akatasva makerubhi akabhururuka;"},{"book":"2 Samueri","chapter":"22","verse":"12","text":"Akaita kuti rima rimupoteredze setende,"},{"book":"2 Samueri","chapter":"22","verse":"13","text":"Kubva pakupenya kwokuvapo kwake"},{"book":"2 Samueri","chapter":"22","verse":"14","text":"Jehovha akatinhira ari kudenga;"},{"book":"2 Samueri","chapter":"22","verse":"15","text":"Akapfura miseve akaparadzira vavengi,"},{"book":"2 Samueri","chapter":"22","verse":"16","text":"Mipata yegungwa yakaiswa pachena"},{"book":"2 Samueri","chapter":"22","verse":"17","text":"“Akatambanudza ruoko ari kudenga akandibata;"},{"book":"2 Samueri","chapter":"22","verse":"18","text":"Akandinunura kubva pamuvengi wangu ane simba,"},{"book":"2 Samueri","chapter":"22","verse":"19","text":"Vakandivinga muzuva renjodzi yangu,"},{"book":"2 Samueri","chapter":"22","verse":"20","text":"Akandibudisira kunzvimbo yakafarikana;"},{"book":"2 Samueri","chapter":"22","verse":"21","text":"“Jehovha akandiitira zviri maererano nokururama kwangu;"},{"book":"2 Samueri","chapter":"22","verse":"22","text":"Nokuti ndakachengeta nzira dzaJehovha;"},{"book":"2 Samueri","chapter":"22","verse":"23","text":"Mirayiro yake yose iri pamberi pangu;"},{"book":"2 Samueri","chapter":"22","verse":"24","text":"Ndakanga ndisina chandinopomerwa pamberi pake,"},{"book":"2 Samueri","chapter":"22","verse":"25","text":"Jehovha akandipa mubayiro maererano nokururama kwangu,"},{"book":"2 Samueri","chapter":"22","verse":"26","text":"“Kune vakatendeka munoratidza kutendeka kwenyu,"},{"book":"2 Samueri","chapter":"22","verse":"27","text":"kune vakachena munozviratidza makachena,"},{"book":"2 Samueri","chapter":"22","verse":"28","text":"Munoponesa vanozvininipisa,"},{"book":"2 Samueri","chapter":"22","verse":"29","text":"Imi muri mwenje wangu, Jehovha;"},{"book":"2 Samueri","chapter":"22","verse":"30","text":"Norubatsiro rwenyu ndinogona kuenda kundorwa neboka ravarwi;"},{"book":"2 Samueri","chapter":"22","verse":"31","text":"“Kana ari Mwari, nzira yake yakarurama kwazvo;"},{"book":"2 Samueri","chapter":"22","verse":"32","text":"Nokuti ndianiko Mwari kunze kwaJehovha?"},{"book":"2 Samueri","chapter":"22","verse":"33","text":"NdiMwari anondishongedza nesimba"},{"book":"2 Samueri","chapter":"22","verse":"34","text":"Anoita tsoka dzangu kuti dzive setsoka dzenondo;"},{"book":"2 Samueri","chapter":"22","verse":"35","text":"Anodzidzisa maoko angu kurwa;"},{"book":"2 Samueri","chapter":"22","verse":"36","text":"Munondipa nhoo yenyu yokukunda,"},{"book":"2 Samueri","chapter":"22","verse":"37","text":"Munotambanudza nzira pasi pangu,"},{"book":"2 Samueri","chapter":"22","verse":"38","text":"“Ndakatevera vavengi vangu ndikavaparadza;"},{"book":"2 Samueri","chapter":"22","verse":"39","text":"Ndakavaparadza zvachose,"},{"book":"2 Samueri","chapter":"22","verse":"40","text":"Makandishongedza nesimba rokurwa;"},{"book":"2 Samueri","chapter":"22","verse":"41","text":"Makaita kuti vavengi vangu vatize pakurwa,"},{"book":"2 Samueri","chapter":"22","verse":"42","text":"Vakachemera kubatsirwa, asi hakuna munhu akavaponesa,"},{"book":"2 Samueri","chapter":"22","verse":"43","text":"Ndakavapwanya vakatsetseka seguruva renyika;"},{"book":"2 Samueri","chapter":"22","verse":"44","text":"“Makandinunura kubva pakurwisa kwavanhu vangu;"},{"book":"2 Samueri","chapter":"22","verse":"45","text":"Uye vatorwa vanouya kwandiri vachidedera,"},{"book":"2 Samueri","chapter":"22","verse":"46","text":"Vose vanoora mwoyo;"},{"book":"2 Samueri","chapter":"22","verse":"47","text":"“Jehovha mupenyu! Dombo rangu ngarirumbidzwe!"},{"book":"2 Samueri","chapter":"22","verse":"48","text":"Ndiye Mwari anonditsivira,"},{"book":"2 Samueri","chapter":"22","verse":"49","text":"anondisunungura kubva kuvavengi vangu."},{"book":"2 Samueri","chapter":"22","verse":"50","text":"Naizvozvo ndichakurumbidzai, imi Jehovha, pakati pendudzi;"},{"book":"2 Samueri","chapter":"22","verse":"51","text":"“Iye anopa mambo wake kukunda kukuru;"}]}