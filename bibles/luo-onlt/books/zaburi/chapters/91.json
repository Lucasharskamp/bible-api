{"data":[{"book":"Zaburi","chapter":"91","verse":"1","text":"Ngʼat modak ei kar pondo mar Nyasaye Man Malo Moloyo"},{"book":"Zaburi","chapter":"91","verse":"2","text":"Ngʼat ma kamano nyalo wacho kuom Jehova Nyasaye niya,"},{"book":"Zaburi","chapter":"91","verse":"3","text":"Jehova Nyasaye biro resi adier kuom gik mochikigo"},{"book":"Zaburi","chapter":"91","verse":"4","text":"Obiro umi gi bwombene,"},{"book":"Zaburi","chapter":"91","verse":"5","text":"Ok iniluor masira mar otieno,"},{"book":"Zaburi","chapter":"91","verse":"6","text":"kata tuoche maricho maliwo ji e mudho,"},{"book":"Zaburi","chapter":"91","verse":"7","text":"Ji alufu achiel nyalo podho e bathi,"},{"book":"Zaburi","chapter":"91","verse":"8","text":"Iningʼigi angʼiya gi wengeni"},{"book":"Zaburi","chapter":"91","verse":"9","text":"Ka iwacho niya, “Jehova Nyasaye e kar pondona,”"},{"book":"Zaburi","chapter":"91","verse":"10","text":"to onge gimoro amora marach ma notimreni,"},{"book":"Zaburi","chapter":"91","verse":"11","text":"Nimar obiro miyo malaika mage chik nikech in"},{"book":"Zaburi","chapter":"91","verse":"12","text":"gibiro tingʼi malo e lwetgi,"},{"book":"Zaburi","chapter":"91","verse":"13","text":"Ininyon sibuor gi thuond fu;"},{"book":"Zaburi","chapter":"91","verse":"14","text":"Jehova Nyasaye wacho niya, “Abiro rese nikech ohera;"},{"book":"Zaburi","chapter":"91","verse":"15","text":"Enoluonga, kendo anadwoke;"},{"book":"Zaburi","chapter":"91","verse":"16","text":"Anami odag amingʼa"}]}