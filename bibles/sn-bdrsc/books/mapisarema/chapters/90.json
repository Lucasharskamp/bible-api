{"data":[{"book":"Mapisarema","chapter":"90","verse":"1","text":"Jehovha, imi makava ugaro hwedu"},{"book":"Mapisarema","chapter":"90","verse":"2","text":"Makomo asati azvarwa"},{"book":"Mapisarema","chapter":"90","verse":"3","text":"Munodzosera vanhu kuguruva zvakare, muchiti,"},{"book":"Mapisarema","chapter":"90","verse":"4","text":"Nokuti pamberi penyu makore chiuru"},{"book":"Mapisarema","chapter":"90","verse":"5","text":"Munokukura vanhu pakuvata kworufu;"},{"book":"Mapisarema","chapter":"90","verse":"6","text":"kunyange richimera riri idzva mangwanani,"},{"book":"Mapisarema","chapter":"90","verse":"7","text":"Tapera nokutsamwa kwenyu"},{"book":"Mapisarema","chapter":"90","verse":"8","text":"Makaisa zvakaipa zvedu pamberi penyu,"},{"book":"Mapisarema","chapter":"90","verse":"9","text":"Mazuva edu ose anopfuura napasi pehasha dzenyu;"},{"book":"Mapisarema","chapter":"90","verse":"10","text":"Uwandu hwamazuva edu hunosvika makore makumi manomwe,"},{"book":"Mapisarema","chapter":"90","verse":"11","text":"Ndianiko anoziva simba rokutsamwa kwenyu?"},{"book":"Mapisarema","chapter":"90","verse":"12","text":"Tidzidzisei kuverenga mazuva edu zvakanaka,"},{"book":"Mapisarema","chapter":"90","verse":"13","text":"Dzokai, imi Jehovha! Zvichadaro kusvikira riniko?"},{"book":"Mapisarema","chapter":"90","verse":"14","text":"Tigutsei mangwanani norudo rwenyu rusingaperi,"},{"book":"Mapisarema","chapter":"90","verse":"15","text":"Tifadzei zvinoenzana namazuva amakatitambudza,"},{"book":"Mapisarema","chapter":"90","verse":"16","text":"Mabasa enyu ngaaratidzwe kuvaranda venyu,"},{"book":"Mapisarema","chapter":"90","verse":"17","text":"Nyasha dzaIshe Mwari wedu ngadzive pamusoro pedu;"}]}