{"data":[{"book":"Zvirevo","chapter":"23","verse":"1","text":"Kana ukagara pakudya pamwe chete nomubati,"},{"book":"Zvirevo","chapter":"23","verse":"2","text":"uise banga pahuro"},{"book":"Zvirevo","chapter":"23","verse":"3","text":"Usapanga zvokudya zvake zvinonaka"},{"book":"Zvirevo","chapter":"23","verse":"4","text":"Usazvionza nokuda kuwana pfuma;"},{"book":"Zvirevo","chapter":"23","verse":"5","text":"Kungoti tarisei papfuma, wanei yaenda,"},{"book":"Zvirevo","chapter":"23","verse":"6","text":"Usadya zvokudya zvomunhu anonyima,"},{"book":"Zvirevo","chapter":"23","verse":"7","text":"nokuti munhu uya anongofunga chete mutengo wazvo."},{"book":"Zvirevo","chapter":"23","verse":"8","text":"Ucharutsa zvishoma izvozvo zvawadya,"},{"book":"Zvirevo","chapter":"23","verse":"9","text":"Usataura nebenzi,"},{"book":"Zvirevo","chapter":"23","verse":"10","text":"Usabvisa dombo romuganhu wakare"},{"book":"Zvirevo","chapter":"23","verse":"11","text":"nokuti Mudziviriri wavo ane simba;"},{"book":"Zvirevo","chapter":"23","verse":"12","text":"Isa mwoyo wako kumurayiro"},{"book":"Zvirevo","chapter":"23","verse":"13","text":"Usarega kuranga mwana;"},{"book":"Zvirevo","chapter":"23","verse":"14","text":"Murange neshamhu"},{"book":"Zvirevo","chapter":"23","verse":"15","text":"Mwanakomana wangu, kana mwoyo wako wakachenjera,"},{"book":"Zvirevo","chapter":"23","verse":"16","text":"mukatikati mangu muchapembera"},{"book":"Zvirevo","chapter":"23","verse":"17","text":"Usarega mwoyo wako uchichiva vatadzi,"},{"book":"Zvirevo","chapter":"23","verse":"18","text":"Zvirokwazvo ramangwana rako rine tariro,"},{"book":"Zvirevo","chapter":"23","verse":"19","text":"Teerera, mwanakomana wangu, ugova nouchenjeri,"},{"book":"Zvirevo","chapter":"23","verse":"20","text":"Usabatana navaya vanonwa waini zhinji,"},{"book":"Zvirevo","chapter":"23","verse":"21","text":"nokuti zvidhakwa nevane madyo vachava varombo,"},{"book":"Zvirevo","chapter":"23","verse":"22","text":"Teerera kuna baba vako, ivo vakakubereka,"},{"book":"Zvirevo","chapter":"23","verse":"23","text":"Tenga zvokwadi urege kuzoitengesa;"},{"book":"Zvirevo","chapter":"23","verse":"24","text":"Baba vomunhu akarurama vane mufaro mukuru;"},{"book":"Zvirevo","chapter":"23","verse":"25","text":"Baba vako namai vako ngavafare;"},{"book":"Zvirevo","chapter":"23","verse":"26","text":"Mwanakomana wangu, ndipe mwoyo wako"},{"book":"Zvirevo","chapter":"23","verse":"27","text":"nokuti chifeve igomba rakadzika,"},{"book":"Zvirevo","chapter":"23","verse":"28","text":"Segororo anohwandira,"},{"book":"Zvirevo","chapter":"23","verse":"29","text":"Ndiani ane nhamo? Ndiani akasuwa?"},{"book":"Zvirevo","chapter":"23","verse":"30","text":"Vaya vanogara pawaini,"},{"book":"Zvirevo","chapter":"23","verse":"31","text":"Usatarisa waini kana yatsvuka,"},{"book":"Zvirevo","chapter":"23","verse":"32","text":"Pakupedzisira inoruma senyoka"},{"book":"Zvirevo","chapter":"23","verse":"33","text":"Meso ako achaona zvisakamboonekwa"},{"book":"Zvirevo","chapter":"23","verse":"34","text":"Uchava souya akarara pamusoro pamafungu amakungwa,"},{"book":"Zvirevo","chapter":"23","verse":"35","text":"Uchati, â€œVakandirova, asi handina kukuvara!"}]}