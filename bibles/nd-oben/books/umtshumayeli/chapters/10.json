{"data":[{"book":"UmTshumayeli","chapter":"10","verse":"1","text":"Njengoba impukane ezifele phakathi kwamakha ziwenza anuke kubi,"},{"book":"UmTshumayeli","chapter":"10","verse":"2","text":"Inhliziyo yolungileyo ikhungathekela ekulungeni,"},{"book":"UmTshumayeli","chapter":"10","verse":"3","text":"Loba ehamba nje emgwaqweni"},{"book":"UmTshumayeli","chapter":"10","verse":"4","text":"Aluba umbusi ekuthukuthelela,"},{"book":"UmTshumayeli","chapter":"10","verse":"5","text":"Bukhona ububi esengibubonile ngaphansi kwelanga,"},{"book":"UmTshumayeli","chapter":"10","verse":"6","text":"Iziwula ziphakanyiselwa ezikhundleni ezinengi eziphezulu,"},{"book":"UmTshumayeli","chapter":"10","verse":"7","text":"Sengibone izichaka zigade amabhiza,"},{"book":"UmTshumayeli","chapter":"10","verse":"8","text":"Lowo ogebha umgodi angawela kuwo;"},{"book":"UmTshumayeli","chapter":"10","verse":"9","text":"Oqhekeza amatshe angalinyazwa yiwo;"},{"book":"UmTshumayeli","chapter":"10","verse":"10","text":"Lapho ihloka libuthundu"},{"book":"UmTshumayeli","chapter":"10","verse":"11","text":"Nxa inyoka ihle ilume ingakalunjwa,"},{"book":"UmTshumayeli","chapter":"10","verse":"12","text":"Amazwi aphuma emlonyeni wesihlakaniphi amnandi,"},{"book":"UmTshumayeli","chapter":"10","verse":"13","text":"Ekuqaleni amazwi aso ayibuwula;"},{"book":"UmTshumayeli","chapter":"10","verse":"14","text":"njalo isiwula sanda ngamazwi."},{"book":"UmTshumayeli","chapter":"10","verse":"15","text":"Umtshikatshika wesiwula uyasidinisa;"},{"book":"UmTshumayeli","chapter":"10","verse":"16","text":"Maye kuwe, wena lizwe onkosi yalo yayiyisichaka,"},{"book":"UmTshumayeli","chapter":"10","verse":"17","text":"Ubusisiwe wena, wena lizwe onkosi yalo izalwa esikhosini"},{"book":"UmTshumayeli","chapter":"10","verse":"18","text":"Nxa indoda ilivila intungo ziyabhensa;"},{"book":"UmTshumayeli","chapter":"10","verse":"19","text":"Idili lenzelwa ukujabula,"},{"book":"UmTshumayeli","chapter":"10","verse":"20","text":"Ungayithuki inkosi lasemicabangweni yakho,"}]}