{"data":[{"book":"Mapisarema","chapter":"116","verse":"1","text":"Ndinoda Jehovha, nokuti akanzwa inzwi rangu;"},{"book":"Mapisarema","chapter":"116","verse":"2","text":"Nokuti akarerekera nzeve yake kwandiri,"},{"book":"Mapisarema","chapter":"116","verse":"3","text":"Misungo yorufu yakandipinganidza,"},{"book":"Mapisarema","chapter":"116","verse":"4","text":"Ipapo ndakadana kuzita raJehovha ndikati,"},{"book":"Mapisarema","chapter":"116","verse":"5","text":"Jehovha ane nyasha uye akarurama;"},{"book":"Mapisarema","chapter":"116","verse":"6","text":"Jehovha anodzivirira vakatendeka pamwoyo;"},{"book":"Mapisarema","chapter":"116","verse":"7","text":"Haiwa mweya wangu, chizorora zvakare,"},{"book":"Mapisarema","chapter":"116","verse":"8","text":"Nokuti imi, iyemi Jehovha, makarwira mweya wangu parufu,"},{"book":"Mapisarema","chapter":"116","verse":"9","text":"kuti ndifambe pamberi paJehovha"},{"book":"Mapisarema","chapter":"116","verse":"10","text":"Ndakatenda; naizvozvo ndakati,"},{"book":"Mapisarema","chapter":"116","verse":"11","text":"Uye pakuvhunduka kwangu ndakati,"},{"book":"Mapisarema","chapter":"116","verse":"12","text":"Ndingaripira seiko Jehovha"},{"book":"Mapisarema","chapter":"116","verse":"13","text":"Ndichasimudza mukombe woruponeso"},{"book":"Mapisarema","chapter":"116","verse":"14","text":"Ndichazadzisa mhiko dzangu kuna Jehovha,"},{"book":"Mapisarema","chapter":"116","verse":"15","text":"Chinhu chinokosha pamberi paJehovha"},{"book":"Mapisarema","chapter":"116","verse":"16","text":"Haiwa Jehovha, zvirokwazvo ndiri muranda wenyu;"},{"book":"Mapisarema","chapter":"116","verse":"17","text":"Ndichabayira chipo chokuvonga kwamuri,"},{"book":"Mapisarema","chapter":"116","verse":"18","text":"Ndichazadzisa mhiko dzangu kuna Jehovha,"},{"book":"Mapisarema","chapter":"116","verse":"19","text":"pavanze dzeimba yaJehovha,"}]}