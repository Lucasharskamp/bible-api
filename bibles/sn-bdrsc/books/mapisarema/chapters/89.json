{"data":[{"book":"Mapisarema","chapter":"89","verse":"1","text":"Ndichaimba nezvorudo rukuru rwaJehovha nokusingaperi;"},{"book":"Mapisarema","chapter":"89","verse":"2","text":"Ndichaparidza kuti rudo rwenyu runomira rwakasimba nokusingaperi,"},{"book":"Mapisarema","chapter":"89","verse":"3","text":"Imi makati, “Ndakaita sungano nomusanangurwa wangu,"},{"book":"Mapisarema","chapter":"89","verse":"4","text":"‘Ndichasimbisa imba yako nokusingaperi,"},{"book":"Mapisarema","chapter":"89","verse":"5","text":"Matenga anorumbidza zvishamiso zvenyu, imi Jehovha,"},{"book":"Mapisarema","chapter":"89","verse":"6","text":"Nokuti ndianiko kudenga kumusoro angaenzaniswa naJehovha?"},{"book":"Mapisarema","chapter":"89","verse":"7","text":"Mwari anotyiwa zvikuru paungano yavatsvene;"},{"book":"Mapisarema","chapter":"89","verse":"8","text":"Haiwa Jehovha Mwari Wamasimba Ose, ndianiko akaita semi?"},{"book":"Mapisarema","chapter":"89","verse":"9","text":"Munotonga pamusoro pokusunda kwegungwa;"},{"book":"Mapisarema","chapter":"89","verse":"10","text":"Makapwanya Rahabhi somumwe wavakaurayiwa;"},{"book":"Mapisarema","chapter":"89","verse":"11","text":"Matenga ndeenyu, nenyika ndeyenyuwo;"},{"book":"Mapisarema","chapter":"89","verse":"12","text":"Ndimi makasika kumusoro nezasi;"},{"book":"Mapisarema","chapter":"89","verse":"13","text":"Ruoko rwenyu rune simba;"},{"book":"Mapisarema","chapter":"89","verse":"14","text":"Kururama nokururamisira ndidzo nheyo dzechigaro chenyu choushe;"},{"book":"Mapisarema","chapter":"89","verse":"15","text":"Vakaropafadzwa avo vakadzidza kukurumbidzai,"},{"book":"Mapisarema","chapter":"89","verse":"16","text":"Vanofara muzita renyu zuva rose;"},{"book":"Mapisarema","chapter":"89","verse":"17","text":"Nokuti imi ndimi kubwinya nesimba ravo,"},{"book":"Mapisarema","chapter":"89","verse":"18","text":"Zvirokwazvo, nhoo yedu ndeyaJehovha,"},{"book":"Mapisarema","chapter":"89","verse":"19","text":"Ipapo makataura muchiratidzo,"},{"book":"Mapisarema","chapter":"89","verse":"20","text":"Ndakawana Dhavhidhi muranda wangu;"},{"book":"Mapisarema","chapter":"89","verse":"21","text":"Ruoko rwangu ruchamutsigira;"},{"book":"Mapisarema","chapter":"89","verse":"22","text":"Hakuna muvengi achamuripisa mutero;"},{"book":"Mapisarema","chapter":"89","verse":"23","text":"Ndichapwanya vavengi vake pamberi pake,"},{"book":"Mapisarema","chapter":"89","verse":"24","text":"Rudo rwangu rwakatendeka ruchava naye,"},{"book":"Mapisarema","chapter":"89","verse":"25","text":"Ndichaisa ruoko rwake pamusoro pegungwa,"},{"book":"Mapisarema","chapter":"89","verse":"26","text":"Iye achadana kwandiri, achiti, ‘Ndimi Baba vangu,"},{"book":"Mapisarema","chapter":"89","verse":"27","text":"Ndichamugadzawo kuti ave dangwe rangu,"},{"book":"Mapisarema","chapter":"89","verse":"28","text":"Ndichachengetedza rudo rwangu kwaari nokusingaperi,"},{"book":"Mapisarema","chapter":"89","verse":"29","text":"Ndichasimbisa imba yake nokusingaperi,"},{"book":"Mapisarema","chapter":"89","verse":"30","text":"“Kana vana vake vakasiya murayiro wangu,"},{"book":"Mapisarema","chapter":"89","verse":"31","text":"kana vakazvidza mitemo yangu,"},{"book":"Mapisarema","chapter":"89","verse":"32","text":"ndicharanga chivi chavo neshamhu,"},{"book":"Mapisarema","chapter":"89","verse":"33","text":"asi handizobvisi rudo rwangu kwavari,"},{"book":"Mapisarema","chapter":"89","verse":"34","text":"Handizozvidzi sungano yangu"},{"book":"Mapisarema","chapter":"89","verse":"35","text":"Kamwe chete, ndakapika noutsvene hwangu,"},{"book":"Mapisarema","chapter":"89","verse":"36","text":"kuti imba yake icharamba iripo nokusingaperi"},{"book":"Mapisarema","chapter":"89","verse":"37","text":"chichasimbiswa nokusingaperi somwedzi,"},{"book":"Mapisarema","chapter":"89","verse":"38","text":"Asi imi makaramba, makavenga,"},{"book":"Mapisarema","chapter":"89","verse":"39","text":"Makaparadza sungano yomuranda wenyu"},{"book":"Mapisarema","chapter":"89","verse":"40","text":"Makaputsa masvingo ake ose"},{"book":"Mapisarema","chapter":"89","verse":"41","text":"Vose vanopfuura napo vanomupamba;"},{"book":"Mapisarema","chapter":"89","verse":"42","text":"Makasimudzira ruoko rworudyi rwavavengi vake;"},{"book":"Mapisarema","chapter":"89","verse":"43","text":"Makadzosera shure munondo wake unopinza,"},{"book":"Mapisarema","chapter":"89","verse":"44","text":"Makagumisa kubwinya kwake,"},{"book":"Mapisarema","chapter":"89","verse":"45","text":"Makatapudza mazuva oujaya hwake;"},{"book":"Mapisarema","chapter":"89","verse":"46","text":"Haiwa Jehovha kusvikira riniko? Muchazvivanza nokusingaperi here?"},{"book":"Mapisarema","chapter":"89","verse":"47","text":"Rangarirai kuti upenyu hwangu hunopfuura sei."},{"book":"Mapisarema","chapter":"89","verse":"48","text":"Ko, munhu ndoupi angararama akasaona rufu,"},{"book":"Mapisarema","chapter":"89","verse":"49","text":"Haiwa Ishe, rudo rwenyu rukuru rwekare rwuripiko,"},{"book":"Mapisarema","chapter":"89","verse":"50","text":"Rangarirai, Ishe kuti muranda wenyu aisekwa sei,"},{"book":"Mapisarema","chapter":"89","verse":"51","text":"kutuka uko vavengi venyu vaikusekai nako, imi Jehovha,"},{"book":"Mapisarema","chapter":"89","verse":"52","text":"Jehovha ngaarumbidzwe nokusingaperi!"}]}