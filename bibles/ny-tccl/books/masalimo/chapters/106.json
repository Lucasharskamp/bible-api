{"data":[{"book":"Masalimo","chapter":"106","verse":"1","text":"Tamandani Yehova."},{"book":"Masalimo","chapter":"106","verse":"2","text":"Ndani angathe kufotokoza za ntchito zamphamvu za Yehova"},{"book":"Masalimo","chapter":"106","verse":"3","text":"Odala ndi amene amasunga chilungamo,"},{"book":"Masalimo","chapter":"106","verse":"4","text":"Mundikumbukire Yehova, pamene mukuonetsa kukoma mtima kwanu kwa anthu anu,"},{"book":"Masalimo","chapter":"106","verse":"5","text":"kuti ndidzasangalale ndi chuma cha anthu anu osankhika,"},{"book":"Masalimo","chapter":"106","verse":"6","text":"Ife tachimwa monga momwe anachitira makolo athu;"},{"book":"Masalimo","chapter":"106","verse":"7","text":"Pamene makolo athu anali mu Igupto,"},{"book":"Masalimo","chapter":"106","verse":"8","text":"Komabe Iye anawapulumutsa chifukwa cha dzina lake,"},{"book":"Masalimo","chapter":"106","verse":"9","text":"Anadzudzula Nyanja Yofiira, ndipo inawuma;"},{"book":"Masalimo","chapter":"106","verse":"10","text":"Anawapulumutsa mʼdzanja la amaliwongo;"},{"book":"Masalimo","chapter":"106","verse":"11","text":"Madzi anamiza adani awo,"},{"book":"Masalimo","chapter":"106","verse":"12","text":"Kenaka iwo anakhulupirira malonjezo ake"},{"book":"Masalimo","chapter":"106","verse":"13","text":"Koma posachedwa anayiwala zimene Iye anachita,"},{"book":"Masalimo","chapter":"106","verse":"14","text":"Mʼchipululu, anadzipereka ku zilakolako zawo;"},{"book":"Masalimo","chapter":"106","verse":"15","text":"Choncho Iye anawapatsa chimene anapempha,"},{"book":"Masalimo","chapter":"106","verse":"16","text":"Mʼmisasa, anachitira nsanje Mose ndi Aaroni,"},{"book":"Masalimo","chapter":"106","verse":"17","text":"Nthaka inatsekuka ndipo inameza Datani;"},{"book":"Masalimo","chapter":"106","verse":"18","text":"Moto unayaka pakati pa otsatira awo;"},{"book":"Masalimo","chapter":"106","verse":"19","text":"Iwo anapanga mwana wangʼombe pa Horebu"},{"book":"Masalimo","chapter":"106","verse":"20","text":"Anasinthanitsa ulemerero wawo"},{"book":"Masalimo","chapter":"106","verse":"21","text":"Anayiwala Mulungu amene anawapulumutsa,"},{"book":"Masalimo","chapter":"106","verse":"22","text":"zozizwitsa mʼdziko la Hamu"},{"book":"Masalimo","chapter":"106","verse":"23","text":"Choncho Iye anawawuza kuti adzawawononga,"},{"book":"Masalimo","chapter":"106","verse":"24","text":"Motero iwo ananyoza dziko lokoma;"},{"book":"Masalimo","chapter":"106","verse":"25","text":"Anangʼungʼudza mʼmatenti mwawo"},{"book":"Masalimo","chapter":"106","verse":"26","text":"Kotero Iye analumbira atakweza dzanja lake,"},{"book":"Masalimo","chapter":"106","verse":"27","text":"kuchititsa kuti zidzukulu zawo zifere pakati pa anthu a mitundu ina"},{"book":"Masalimo","chapter":"106","verse":"28","text":"Iwo anayamba kupembedza Baala-Peori"},{"book":"Masalimo","chapter":"106","verse":"29","text":"anaputa mkwiyo wa Yehova pa machitidwe awo oyipa,"},{"book":"Masalimo","chapter":"106","verse":"30","text":"Koma Finehasi anayimirira ndi kulowererapo,"},{"book":"Masalimo","chapter":"106","verse":"31","text":"Chimenechi ndicho chinayesedwa chilungamo chake,"},{"book":"Masalimo","chapter":"106","verse":"32","text":"Pa madzi a ku Meriba iwo anakwiyitsa Yehova"},{"book":"Masalimo","chapter":"106","verse":"33","text":"pakuti iwowo anawukira mzimu wa Mulungu,"},{"book":"Masalimo","chapter":"106","verse":"34","text":"Aisraeliwo sanawononge mitundu ya anthu"},{"book":"Masalimo","chapter":"106","verse":"35","text":"Koma anasakanizana ndi anthu a mitundu inayo"},{"book":"Masalimo","chapter":"106","verse":"36","text":"Ndipo anapembedza mafano awo,"},{"book":"Masalimo","chapter":"106","verse":"37","text":"Anapereka nsembe ana awo aamuna"},{"book":"Masalimo","chapter":"106","verse":"38","text":"Anakhetsa magazi a anthu osalakwa,"},{"book":"Masalimo","chapter":"106","verse":"39","text":"Iwo anadzidetsa okha ndi zimene anachita;"},{"book":"Masalimo","chapter":"106","verse":"40","text":"Tsono Yehova anakwiya ndi anthu ake"},{"book":"Masalimo","chapter":"106","verse":"41","text":"Iye anawapereka kwa anthu a mitundu ina,"},{"book":"Masalimo","chapter":"106","verse":"42","text":"Adani awo anawazunza"},{"book":"Masalimo","chapter":"106","verse":"43","text":"Iye ankawapulumutsa nthawi zambiri,"},{"book":"Masalimo","chapter":"106","verse":"44","text":"Koma Iye anaona kuzunzika kwawo"},{"book":"Masalimo","chapter":"106","verse":"45","text":"Chifukwa cha iwo Iye anakumbukira pangano lake"},{"book":"Masalimo","chapter":"106","verse":"46","text":"Iye anachititsa kuti amene anawagwira iwo ukapolo"},{"book":"Masalimo","chapter":"106","verse":"47","text":"Tipulumutseni Inu Yehova Mulungu wathu,"},{"book":"Masalimo","chapter":"106","verse":"48","text":"Atamandike Yehova, Mulungu wa Israeli,"}]}