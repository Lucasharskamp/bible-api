{"data":[{"book":"Rwiyo Rukuru rwaSoromoni","chapter":"5","verse":"1","text":"Ndauya zvangu mubindu rangu, hanzvadzi yangu, iwe mwenga wangu;"},{"book":"Rwiyo Rukuru rwaSoromoni","chapter":"5","verse":"2","text":"Ndakavata asi mwoyo wangu wakanga wakasvinura."},{"book":"Rwiyo Rukuru rwaSoromoni","chapter":"5","verse":"3","text":"Ndabvisa nguo yangu,"},{"book":"Rwiyo Rukuru rwaSoromoni","chapter":"5","verse":"4","text":"Mudiwa wangu akapinza ruoko rwake nepahwangwadza yomukova;"},{"book":"Rwiyo Rukuru rwaSoromoni","chapter":"5","verse":"5","text":"Ndakasimuka kuti ndizarurire mudiwa wangu,"},{"book":"Rwiyo Rukuru rwaSoromoni","chapter":"5","verse":"6","text":"Ndakazarurira mudiwa wangu,"},{"book":"Rwiyo Rukuru rwaSoromoni","chapter":"5","verse":"7","text":"Varindi vakandiona"},{"book":"Rwiyo Rukuru rwaSoromoni","chapter":"5","verse":"8","text":"Imi vanasikana veJerusarema, ndinokurayirai,"},{"book":"Rwiyo Rukuru rwaSoromoni","chapter":"5","verse":"9","text":"Mudiwa wako akanaka kukunda vamwe pakudii,"},{"book":"Rwiyo Rukuru rwaSoromoni","chapter":"5","verse":"10","text":"Mudiwa wangu akanaka uye mutsvuku,"},{"book":"Rwiyo Rukuru rwaSoromoni","chapter":"5","verse":"11","text":"Musoro wake igoridhe chairo;"},{"book":"Rwiyo Rukuru rwaSoromoni","chapter":"5","verse":"12","text":"Meso ake akafanana nenjiva"},{"book":"Rwiyo Rukuru rwaSoromoni","chapter":"5","verse":"13","text":"Matama ake akaita semihomba yezvinonhuhwira"},{"book":"Rwiyo Rukuru rwaSoromoni","chapter":"5","verse":"14","text":"Maoko ake itsvimbo dzegoridhe"},{"book":"Rwiyo Rukuru rwaSoromoni","chapter":"5","verse":"15","text":"Makumbo ake imbiru dzamabwe machena"},{"book":"Rwiyo Rukuru rwaSoromoni","chapter":"5","verse":"16","text":"Muromo wake inhapitapi pachayo;"}]}