{"data":[{"book":"Mapisarema","chapter":"105","verse":"1","text":"Vongai Jehovha, danai kuzita rake;"},{"book":"Mapisarema","chapter":"105","verse":"2","text":"Muimbirei, muimbirei nziyo dzokurumbidza;"},{"book":"Mapisarema","chapter":"105","verse":"3","text":"Ngarikudzwe zita rake dzvene;"},{"book":"Mapisarema","chapter":"105","verse":"4","text":"Tarirai kuna Jehovha nokusimba rake;"},{"book":"Mapisarema","chapter":"105","verse":"5","text":"Rangarirai zvishamiso zvaakaita,"},{"book":"Mapisarema","chapter":"105","verse":"6","text":"imi vana vaAbhurahama muranda wake,"},{"book":"Mapisarema","chapter":"105","verse":"7","text":"Ndiye Jehovha Mwari wedu;"},{"book":"Mapisarema","chapter":"105","verse":"8","text":"Anorangarira sungano yake nokusingaperi,"},{"book":"Mapisarema","chapter":"105","verse":"9","text":"sungano yaakaita naAbhurahama,"},{"book":"Mapisarema","chapter":"105","verse":"10","text":"Akaisimbisa kuna Jakobho somutemo,"},{"book":"Mapisarema","chapter":"105","verse":"11","text":"“Ndichapa nyika yeKenani kwauri"},{"book":"Mapisarema","chapter":"105","verse":"12","text":"Pavaiva vashoma pakuwanda,"},{"book":"Mapisarema","chapter":"105","verse":"13","text":"vakadzungaira vachibva kuno rumwe rudzi kusvikira kuno rumwe rudzi."},{"book":"Mapisarema","chapter":"105","verse":"14","text":"Haana kutendera munhu kuvadzvinyirira;"},{"book":"Mapisarema","chapter":"105","verse":"15","text":"“Musabata vazodziwa vangu;"},{"book":"Mapisarema","chapter":"105","verse":"16","text":"Akadana nzara panyika"},{"book":"Mapisarema","chapter":"105","verse":"17","text":"uye akatuma munhu pamberi pavo,"},{"book":"Mapisarema","chapter":"105","verse":"18","text":"Vakakuvadza tsoka dzake nezvisungo"},{"book":"Mapisarema","chapter":"105","verse":"19","text":"kusvikira zvaakareva zvazadziswa,"},{"book":"Mapisarema","chapter":"105","verse":"20","text":"Mambo akatuma shoko kuti asunungurwe,"},{"book":"Mapisarema","chapter":"105","verse":"21","text":"Akamuita tenzi weimba yake,"},{"book":"Mapisarema","chapter":"105","verse":"22","text":"kuti arayire machinda ake sezvaaida"},{"book":"Mapisarema","chapter":"105","verse":"23","text":"Ipapo Israeri akapinda muIjipiti;"},{"book":"Mapisarema","chapter":"105","verse":"24","text":"Jehovha akawanza vanhu vake chose;"},{"book":"Mapisarema","chapter":"105","verse":"25","text":"avo vaakashandura mwoyo yavo kuti ivenge vanhu vake,"},{"book":"Mapisarema","chapter":"105","verse":"26","text":"Akatuma Mozisi muranda wake,"},{"book":"Mapisarema","chapter":"105","verse":"27","text":"Vakaita zviratidzo zvinoshamisa pakati pavo,"},{"book":"Mapisarema","chapter":"105","verse":"28","text":"Akatumira rima akaita kuti nyika ive murima,"},{"book":"Mapisarema","chapter":"105","verse":"29","text":"Akashandura mvura yavo zhinji ikava ropa,"},{"book":"Mapisarema","chapter":"105","verse":"30","text":"Nyika yavo yakazara namatatya,"},{"book":"Mapisarema","chapter":"105","verse":"31","text":"Akataura mapupira enhunzi akauya,"},{"book":"Mapisarema","chapter":"105","verse":"32","text":"Akashandura mvura yavo ikava chimvuramabwe,"},{"book":"Mapisarema","chapter":"105","verse":"33","text":"akarova mizambiringa yavo nemionde yavo,"},{"book":"Mapisarema","chapter":"105","verse":"34","text":"Akataura, mhashu dzikauya,"},{"book":"Mapisarema","chapter":"105","verse":"35","text":"zvakadya miriwo minyoro yose munyika yavo,"},{"book":"Mapisarema","chapter":"105","verse":"36","text":"Ipapo akarova matangwe munyika yavo,"},{"book":"Mapisarema","chapter":"105","verse":"37","text":"Akabudisa Israeri akaremerwa nesirivha negoridhe,"},{"book":"Mapisarema","chapter":"105","verse":"38","text":"Ijipiti yakafara pavakabva,"},{"book":"Mapisarema","chapter":"105","verse":"39","text":"Akatatamura gore rikava chifukidzo,"},{"book":"Mapisarema","chapter":"105","verse":"40","text":"Vakakumbira, iye akavauyisira zvihuta, uye akavagutsa nechingwa chakabva kudenga."},{"book":"Mapisarema","chapter":"105","verse":"41","text":"Akazarura dombo, mvura ikatubuka;"},{"book":"Mapisarema","chapter":"105","verse":"42","text":"Nokuti akarangarira mhiko yake tsvene"},{"book":"Mapisarema","chapter":"105","verse":"43","text":"Akabudisa vanhu vake nomufaro,"},{"book":"Mapisarema","chapter":"105","verse":"44","text":"akavapa nyika dzendudzi,"},{"book":"Mapisarema","chapter":"105","verse":"45","text":"kuti vachengete zvaakatema"}]}