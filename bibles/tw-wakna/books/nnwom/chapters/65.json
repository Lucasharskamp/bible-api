{"data":[{"book":"Nnwom","chapter":"65","verse":"1","text":"Yɛn Nyankopɔn, ayeyi retwɛn wo wɔ Sion,"},{"book":"Nnwom","chapter":"65","verse":"2","text":"Wo a woyɛ mpaebɔ tiefo,"},{"book":"Nnwom","chapter":"65","verse":"3","text":"Bere a yɛn bɔne menee yɛn no,"},{"book":"Nnwom","chapter":"65","verse":"4","text":"Nhyira nka wɔn a wuyi wɔn"},{"book":"Nnwom","chapter":"65","verse":"5","text":"Wode trenee nnwuma a ɛyɛ nwonwa ma yɛn mmuae,"},{"book":"Nnwom","chapter":"65","verse":"6","text":"Wonam wo tumi so yɛɛ mmepɔw"},{"book":"Nnwom","chapter":"65","verse":"7","text":"Wudwudwoo po asorɔkye ano,"},{"book":"Nnwom","chapter":"65","verse":"8","text":"Wɔn a wɔte akyirikyiri no suro wʼanwonwade;"},{"book":"Nnwom","chapter":"65","verse":"9","text":"Wohwɛ asase no gugu so nsu;"},{"book":"Nnwom","chapter":"65","verse":"10","text":"Wofɔw nea woafuntum hɔ no fɔkyee"},{"book":"Nnwom","chapter":"65","verse":"11","text":"Wode nnɔbae bebree wiee afe no,"},{"book":"Nnwom","chapter":"65","verse":"12","text":"Sare so nwura dɔɔso"},{"book":"Nnwom","chapter":"65","verse":"13","text":"Nguankuw ahyɛ adidibea hɔ ma"}]}