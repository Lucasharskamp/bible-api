{"data":[{"book":"Mapisarema","chapter":"143","verse":"1","text":"Haiwa Jehovha, inzwai munyengetero wangu,"},{"book":"Mapisarema","chapter":"143","verse":"2","text":"Musauyisa muranda wenyu mukutongwa,"},{"book":"Mapisarema","chapter":"143","verse":"3","text":"Muvengi anondidzinganisa,"},{"book":"Mapisarema","chapter":"143","verse":"4","text":"Saizvozvo mweya wangu unoziya mukati mangu;"},{"book":"Mapisarema","chapter":"143","verse":"5","text":"Ndinorangarira mazuva akare kare;"},{"book":"Mapisarema","chapter":"143","verse":"6","text":"Ndinotambanudzira maoko angu kwamuri;"},{"book":"Mapisarema","chapter":"143","verse":"7","text":"Haiwa Jehovha, kurumidzai kundipindura;"},{"book":"Mapisarema","chapter":"143","verse":"8","text":"Mangwanani ngaandivigire shoko rorudo rwenyu rusingaperi,"},{"book":"Mapisarema","chapter":"143","verse":"9","text":"Ndinunurei kubva kuvavengi vangu, imi Jehovha,"},{"book":"Mapisarema","chapter":"143","verse":"10","text":"Ndidzidzisei kuita kuda kwenyu,"},{"book":"Mapisarema","chapter":"143","verse":"11","text":"Haiwa Jehovha, chengetedzai upenyu hwangu nokuda kwezita renyu;"},{"book":"Mapisarema","chapter":"143","verse":"12","text":"Nyaradzai vavengi vangu, murudo rwenyu rusingaperi;"}]}