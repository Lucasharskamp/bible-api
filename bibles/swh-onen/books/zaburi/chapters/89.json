{"data":[{"book":"Zaburi","chapter":"89","verse":"1","text":"Nitaimba juu ya upendo mkuu wa Bwana milele;"},{"book":"Zaburi","chapter":"89","verse":"2","text":"Nitatangaza kuwa upendo wako unasimama imara milele"},{"book":"Zaburi","chapter":"89","verse":"3","text":"Ulisema, “Nimefanya agano na mteule wangu,"},{"book":"Zaburi","chapter":"89","verse":"4","text":"‘Nitaimarisha uzao wako milele"},{"book":"Zaburi","chapter":"89","verse":"5","text":"Ee Bwana, mbingu zinayasifu maajabu yako,"},{"book":"Zaburi","chapter":"89","verse":"6","text":"Kwa kuwa ni nani katika mbingu"},{"book":"Zaburi","chapter":"89","verse":"7","text":"Katika kusanyiko la watakatifu, Mungu huogopwa sana,"},{"book":"Zaburi","chapter":"89","verse":"8","text":"Ee Bwana Mwenye Nguvu Zote,"},{"book":"Zaburi","chapter":"89","verse":"9","text":"Wewe unatawala bahari yenye msukosuko;"},{"book":"Zaburi","chapter":"89","verse":"10","text":"Wewe ulimponda Rahabu89:10 Rahabu hapa ina maana jina la Misri kwa fumbo, yaani mwenye majivuno (lakini sio Rahabu wa Yos 2)."},{"book":"Zaburi","chapter":"89","verse":"11","text":"Mbingu ni zako, nayo nchi pia ni yako,"},{"book":"Zaburi","chapter":"89","verse":"12","text":"Uliumba kaskazini na kusini;"},{"book":"Zaburi","chapter":"89","verse":"13","text":"Mkono wako umejaa uwezo;"},{"book":"Zaburi","chapter":"89","verse":"14","text":"Haki na hukumu ndio msingi wa kiti chako cha enzi;"},{"book":"Zaburi","chapter":"89","verse":"15","text":"Heri ni wale ambao wamejifunza kukusifu kwa shangwe,"},{"book":"Zaburi","chapter":"89","verse":"16","text":"Wanashangilia katika jina lako mchana kutwa,"},{"book":"Zaburi","chapter":"89","verse":"17","text":"Kwa kuwa wewe ni utukufu na nguvu yao,"},{"book":"Zaburi","chapter":"89","verse":"18","text":"Naam, ngao yetu ni mali ya Bwana,"},{"book":"Zaburi","chapter":"89","verse":"19","text":"Ulizungumza wakati fulani katika maono,"},{"book":"Zaburi","chapter":"89","verse":"20","text":"Nimemwona Daudi, mtumishi wangu,"},{"book":"Zaburi","chapter":"89","verse":"21","text":"Kitanga changu kitamtegemeza,"},{"book":"Zaburi","chapter":"89","verse":"22","text":"Hakuna adui atakayemtoza ushuru,"},{"book":"Zaburi","chapter":"89","verse":"23","text":"Nitawaponda adui zake mbele zake"},{"book":"Zaburi","chapter":"89","verse":"24","text":"Upendo wangu mkamilifu utakuwa pamoja naye,"},{"book":"Zaburi","chapter":"89","verse":"25","text":"Nitauweka mkono wake juu ya bahari,"},{"book":"Zaburi","chapter":"89","verse":"26","text":"Naye ataniita kwa sauti, ‘Wewe ni Baba yangu,"},{"book":"Zaburi","chapter":"89","verse":"27","text":"Nitamteua pia awe mzaliwa wangu wa kwanza,"},{"book":"Zaburi","chapter":"89","verse":"28","text":"Nitadumisha upendo wangu kwake milele,"},{"book":"Zaburi","chapter":"89","verse":"29","text":"Nitaudumisha uzao wake milele,"},{"book":"Zaburi","chapter":"89","verse":"30","text":"“Kama wanae wataacha amri yangu"},{"book":"Zaburi","chapter":"89","verse":"31","text":"kama wakihalifu maagizo yangu"},{"book":"Zaburi","chapter":"89","verse":"32","text":"nitaadhibu dhambi yao kwa fimbo,"},{"book":"Zaburi","chapter":"89","verse":"33","text":"lakini sitauondoa upendo wangu kwake,"},{"book":"Zaburi","chapter":"89","verse":"34","text":"Mimi sitavunja agano langu"},{"book":"Zaburi","chapter":"89","verse":"35","text":"Mara moja na kwa milele, nimeapa kwa utakatifu wangu,"},{"book":"Zaburi","chapter":"89","verse":"36","text":"kwamba uzao wake utaendelea milele,"},{"book":"Zaburi","chapter":"89","verse":"37","text":"kitaimarishwa milele kama mwezi,"},{"book":"Zaburi","chapter":"89","verse":"38","text":"Lakini wewe umemkataa, umemdharau,"},{"book":"Zaburi","chapter":"89","verse":"39","text":"Umelikana agano lako na mtumishi wako,"},{"book":"Zaburi","chapter":"89","verse":"40","text":"Umebomoa kuta zake zote,"},{"book":"Zaburi","chapter":"89","verse":"41","text":"Wote wapitao karibu wamemnyangʼanya mali zake;"},{"book":"Zaburi","chapter":"89","verse":"42","text":"Umeutukuza mkono wa kuume wa adui zake,"},{"book":"Zaburi","chapter":"89","verse":"43","text":"Umegeuza makali ya upanga wake,"},{"book":"Zaburi","chapter":"89","verse":"44","text":"Umeikomesha fahari yake,"},{"book":"Zaburi","chapter":"89","verse":"45","text":"Umezifupisha siku za ujana wake,"},{"book":"Zaburi","chapter":"89","verse":"46","text":"Hata lini, Ee Bwana? Utajificha milele?"},{"book":"Zaburi","chapter":"89","verse":"47","text":"Kumbuka jinsi maisha yangu yanavyopita haraka."},{"book":"Zaburi","chapter":"89","verse":"48","text":"Ni mtu gani awezaye kuishi na asione kifo,"},{"book":"Zaburi","chapter":"89","verse":"49","text":"Ee Bwana, uko wapi upendo wako mkuu wa mwanzoni,"},{"book":"Zaburi","chapter":"89","verse":"50","text":"Bwana kumbuka jinsi mtumishi wako amesimangwa,"},{"book":"Zaburi","chapter":"89","verse":"51","text":"dhihaka ambazo kwazo adui zako"},{"book":"Zaburi","chapter":"89","verse":"52","text":"Msifuni Bwana milele!"}]}