{"data":[{"book":"Mapisarema","chapter":"29","verse":"1","text":"Ipai kuna Jehovha, imi mune simba"},{"book":"Mapisarema","chapter":"29","verse":"2","text":"Ipai kuna Jehovha rukudzo rwakafanira zita rake;"},{"book":"Mapisarema","chapter":"29","verse":"3","text":"Inzwi raJehovha riri pamusoro pemvura zhinji."},{"book":"Mapisarema","chapter":"29","verse":"4","text":"Inzwi raJehovha rine simba;"},{"book":"Mapisarema","chapter":"29","verse":"5","text":"Inzwi raJehovha rinovhuna misidhari,"},{"book":"Mapisarema","chapter":"29","verse":"6","text":"Anoita kuti Rebhanoni ikwakuke semhuru,"},{"book":"Mapisarema","chapter":"29","verse":"7","text":"Inzwi raJehovha rinorova"},{"book":"Mapisarema","chapter":"29","verse":"8","text":"Inzwi raJehovha rinozungunusa gwenga;"},{"book":"Mapisarema","chapter":"29","verse":"9","text":"Inzwi raJehovha rinomonyorora miouki"},{"book":"Mapisarema","chapter":"29","verse":"10","text":"Jehovha anogara pachigaro choushe chamafashamu emvura;"},{"book":"Mapisarema","chapter":"29","verse":"11","text":"Jehovha anopa simba kuvanhu vake;"}]}