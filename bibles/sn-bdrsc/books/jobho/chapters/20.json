{"data":[{"book":"Jobho","chapter":"20","verse":"1","text":"Ipapo Zofari muNaamati akapindura akati:"},{"book":"Jobho","chapter":"20","verse":"2","text":"“Ndangariro dzangu dzokutambudzika dzinondikurumidzisa kuti ndipindure"},{"book":"Jobho","chapter":"20","verse":"3","text":"Ndiri kunzwa kutsiura kunondizvidza,"},{"book":"Jobho","chapter":"20","verse":"4","text":"“Zvirokwazvo unoziva zvazvakanga zvakaita kubva kare,"},{"book":"Jobho","chapter":"20","verse":"5","text":"kuti kufara kwavakaipa kupfupi,"},{"book":"Jobho","chapter":"20","verse":"6","text":"Kunyange kuzvikudza kwake kuchisvika kudenga,"},{"book":"Jobho","chapter":"20","verse":"7","text":"achaparara nokusingaperi, setsvina yake pachake;"},{"book":"Jobho","chapter":"20","verse":"8","text":"Anobhururuka sokurota, haachawanikwizve,"},{"book":"Jobho","chapter":"20","verse":"9","text":"Ziso rakamuona harichazomuonazve;"},{"book":"Jobho","chapter":"20","verse":"10","text":"Vana vake vanofanira kutsvaka nyasha kuvarombo;"},{"book":"Jobho","chapter":"20","verse":"11","text":"Simba roujaya rakazadza mapfupa ake"},{"book":"Jobho","chapter":"20","verse":"12","text":"“Kunyange chakaipa chichitapira mumukanwa,"},{"book":"Jobho","chapter":"20","verse":"13","text":"kunyange asingagoni kuchiregedza"},{"book":"Jobho","chapter":"20","verse":"14","text":"asi chokudya chake chichashanduka chikavava mudumbu make;"},{"book":"Jobho","chapter":"20","verse":"15","text":"Achapfira pasi upfumi hwaakamedza;"},{"book":"Jobho","chapter":"20","verse":"16","text":"Achasveta uturu hwenyoka;"},{"book":"Jobho","chapter":"20","verse":"17","text":"Haangafadzwi nehova,"},{"book":"Jobho","chapter":"20","verse":"18","text":"Zvaakatamburira anofanira kuzvidzosazve zvisina kudyiwa;"},{"book":"Jobho","chapter":"20","verse":"19","text":"Nokuti akamanikidza varombo akavasiya vava vachena;"},{"book":"Jobho","chapter":"20","verse":"20","text":"“Zvirokwazvo haangamiri kubva pahavi yake;"},{"book":"Jobho","chapter":"20","verse":"21","text":"Hapana chaakasiyirwa kuti adye;"},{"book":"Jobho","chapter":"20","verse":"22","text":"Pakati pezvakawanda zvake, kuremerwa nepfungwa kuchamukunda;"},{"book":"Jobho","chapter":"20","verse":"23","text":"Paanenge agutsa dumbu rake,"},{"book":"Jobho","chapter":"20","verse":"24","text":"Kunyange akatiza chombo chesimbi,"},{"book":"Jobho","chapter":"20","verse":"25","text":"Anoudzura kubva kumusana kwake,"},{"book":"Jobho","chapter":"20","verse":"26","text":"rima guru richavandira pfuma yake."},{"book":"Jobho","chapter":"20","verse":"27","text":"Matenga achaisa pachena mhaka yake;"},{"book":"Jobho","chapter":"20","verse":"28","text":"Mafashamu achakukura imba yake,"},{"book":"Jobho","chapter":"20","verse":"29","text":"Aya ndiwo magumo anogoverwa vakaipa naMwari,"}]}