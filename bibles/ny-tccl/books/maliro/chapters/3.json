{"data":[{"book":"Maliro","chapter":"3","verse":"1","text":"Ine ndine munthu amene ndaona masautso"},{"book":"Maliro","chapter":"3","verse":"2","text":"Wandipititsa kutali ndipo wandiyendetsa"},{"book":"Maliro","chapter":"3","verse":"3","text":"zoonadi anandikantha ndi dzanja lake"},{"book":"Maliro","chapter":"3","verse":"4","text":"Wakalambitsa khungu langa ndi mnofu wanga,"},{"book":"Maliro","chapter":"3","verse":"5","text":"Wandizinga ndi kundizungulira"},{"book":"Maliro","chapter":"3","verse":"6","text":"Wandikhazika mu mdima"},{"book":"Maliro","chapter":"3","verse":"7","text":"Wandimangira khoma kotero kuti sindingathawe,"},{"book":"Maliro","chapter":"3","verse":"8","text":"Ngakhale pamene ndifuwula kapena kupempha chithandizo,"},{"book":"Maliro","chapter":"3","verse":"9","text":"Wanditsekera njira yanga ndi miyala yosema;"},{"book":"Maliro","chapter":"3","verse":"10","text":"Wandidikirira ngati chimbalangondo,"},{"book":"Maliro","chapter":"3","verse":"11","text":"Wandikokera pambali ndi kundingʼambangʼamba,"},{"book":"Maliro","chapter":"3","verse":"12","text":"Wakoka uta wake"},{"book":"Maliro","chapter":"3","verse":"13","text":"Walasa mtima wanga"},{"book":"Maliro","chapter":"3","verse":"14","text":"Ndinakhala choseketsa cha anthu anga onse;"},{"book":"Maliro","chapter":"3","verse":"15","text":"Wandidyetsa zowawa"},{"book":"Maliro","chapter":"3","verse":"16","text":"Wathyola mano anga ndi miyala;"},{"book":"Maliro","chapter":"3","verse":"17","text":"Wandichotsera mtendere;"},{"book":"Maliro","chapter":"3","verse":"18","text":"Choncho ndikuti, “Ulemerero wanga wachoka"},{"book":"Maliro","chapter":"3","verse":"19","text":"Kukumbukira masautso anga ndi kusowa pokhala,"},{"book":"Maliro","chapter":"3","verse":"20","text":"Ine ndikuzikumbukira bwino izi,"},{"book":"Maliro","chapter":"3","verse":"21","text":"Komabe ndimakumbukira zimenezi,"},{"book":"Maliro","chapter":"3","verse":"22","text":"Ife sitinawonongekeretu chifukwa chikondi cha Yehova ndi chachikulu,"},{"book":"Maliro","chapter":"3","verse":"23","text":"Zimaoneka zatsopano mmawa uliwonse;"},{"book":"Maliro","chapter":"3","verse":"24","text":"Mu mtima mwanga ndimati, “Yehova ndiye zanga zonse;"},{"book":"Maliro","chapter":"3","verse":"25","text":"Yehova ndi wabwino kwa amene amayembekezera Iye,"},{"book":"Maliro","chapter":"3","verse":"26","text":"nʼkwabwino kudikira chipulumutso cha"},{"book":"Maliro","chapter":"3","verse":"27","text":"Nʼkwabwino kuti munthu asenze goli"},{"book":"Maliro","chapter":"3","verse":"28","text":"Akhale chete pa yekha,"},{"book":"Maliro","chapter":"3","verse":"29","text":"Abise nkhope yake mʼfumbi"},{"book":"Maliro","chapter":"3","verse":"30","text":"Apereke tsaya lake kwa iye amene angamumenye,"},{"book":"Maliro","chapter":"3","verse":"31","text":"Chifukwa Ambuye satayiratu"},{"book":"Maliro","chapter":"3","verse":"32","text":"Ngakhale amabweretsa zowawa, Iye adzawachitira chifundo,"},{"book":"Maliro","chapter":"3","verse":"33","text":"Pakuti sabweretsa masautso mwadala,"},{"book":"Maliro","chapter":"3","verse":"34","text":"Kuphwanya ndi phazi"},{"book":"Maliro","chapter":"3","verse":"35","text":"kukaniza munthu ufulu wake"},{"book":"Maliro","chapter":"3","verse":"36","text":"kumana munthu chiweruzo cholungama—"},{"book":"Maliro","chapter":"3","verse":"37","text":"Kodi ndani angayankhule zinthu nʼkuchitika"},{"book":"Maliro","chapter":"3","verse":"38","text":"Kodi zovuta ndi zabwino sizituluka"},{"book":"Maliro","chapter":"3","verse":"39","text":"Kodi nʼchifukwa chiyani munthu aliyense wamoyo amadandaula"},{"book":"Maliro","chapter":"3","verse":"40","text":"Tiyeni tisanthule ndi kuyesa njira zathu,"},{"book":"Maliro","chapter":"3","verse":"41","text":"Tiyeni tikweze mitima yathu ndi manja athu"},{"book":"Maliro","chapter":"3","verse":"42","text":"“Ife tachimwa ndi kuwukira"},{"book":"Maliro","chapter":"3","verse":"43","text":"“Mwadzikuta ndi mkwiyo ndi kutilondola ndipo"},{"book":"Maliro","chapter":"3","verse":"44","text":"Mwadzikuta mu mtambo"},{"book":"Maliro","chapter":"3","verse":"45","text":"Mwatisandutsa zinyatsi ndi zinyalala"},{"book":"Maliro","chapter":"3","verse":"46","text":"“Adani anthu atitsekulira pakamwa."},{"book":"Maliro","chapter":"3","verse":"47","text":"Ife tadzazidwa ndi mantha pakuti tagwa mʼdzenje,"},{"book":"Maliro","chapter":"3","verse":"48","text":"Misozi mʼmaso mwanga ikungoti mbwembwembwe"},{"book":"Maliro","chapter":"3","verse":"49","text":"Misozi idzatsika kosalekeza,"},{"book":"Maliro","chapter":"3","verse":"50","text":"mpaka Yehova ayangʼane pansi"},{"book":"Maliro","chapter":"3","verse":"51","text":"Mtima wanga ukupweteka poona zimenezi"},{"book":"Maliro","chapter":"3","verse":"52","text":"Akundisaka ngati mbalame,"},{"book":"Maliro","chapter":"3","verse":"53","text":"Ayesa kundipha pondiponya mʼdzenje"},{"book":"Maliro","chapter":"3","verse":"54","text":"madzi anamiza mutu wanga"},{"book":"Maliro","chapter":"3","verse":"55","text":"Ndinayitana dzina lanu Inu Yehova,"},{"book":"Maliro","chapter":"3","verse":"56","text":"Inu munamva kudandaula kwanga. “Mundimvere"},{"book":"Maliro","chapter":"3","verse":"57","text":"Munafika pafupi pamene ndinakuyitanani,"},{"book":"Maliro","chapter":"3","verse":"58","text":"Inu Ambuye munandiwombola ku mlandu wanga;"},{"book":"Maliro","chapter":"3","verse":"59","text":"Yehova, mwaona zoyipa zimene andichitira."},{"book":"Maliro","chapter":"3","verse":"60","text":"Mwaona kuzama kwa kubwezera kwawo,"},{"book":"Maliro","chapter":"3","verse":"61","text":"Inu Yehova mwamva kunyoza kwawo,"},{"book":"Maliro","chapter":"3","verse":"62","text":"manongʼonongʼo a adani anga"},{"book":"Maliro","chapter":"3","verse":"63","text":"Tawaonani! Kaya ali pansi kapena kuyimirira,"},{"book":"Maliro","chapter":"3","verse":"64","text":"Inu Yehova, muwabwezere chowayenera,"},{"book":"Maliro","chapter":"3","verse":"65","text":"Phimbani mitima yawo,"},{"book":"Maliro","chapter":"3","verse":"66","text":"Muwalondole mwaukali ndipo"}]}