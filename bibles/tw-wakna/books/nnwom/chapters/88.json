{"data":[{"book":"Nnwom","chapter":"88","verse":"1","text":"Awurade, woyɛ me nkwagye Nyankopɔn"},{"book":"Nnwom","chapter":"88","verse":"2","text":"Ma me mpaebɔ nnu wʼanim;"},{"book":"Nnwom","chapter":"88","verse":"3","text":"Ɔhaw ahyɛ me kra ma"},{"book":"Nnwom","chapter":"88","verse":"4","text":"Wɔkan me fra wɔn a wɔrekɔ ɔda mu no;"},{"book":"Nnwom","chapter":"88","verse":"5","text":"Wɔayi me asi nkyɛn wɔ awufo mu,"},{"book":"Nnwom","chapter":"88","verse":"6","text":"Woatow me akyene ɔda ase tɔnn,"},{"book":"Nnwom","chapter":"88","verse":"7","text":"Wʼabufuwhyew ayɛ duru wɔ me so;"},{"book":"Nnwom","chapter":"88","verse":"8","text":"Woafa me nnamfonom a wɔbɛn me afi me nkyɛn"},{"book":"Nnwom","chapter":"88","verse":"9","text":"awerɛhow ama mʼani so ayɛ kusuu."},{"book":"Nnwom","chapter":"88","verse":"10","text":"Woyɛ anwonwade kyerɛ awufo ana?"},{"book":"Nnwom","chapter":"88","verse":"11","text":"Wɔka wʼadɔe ho asɛm wɔ ɔda mu,"},{"book":"Nnwom","chapter":"88","verse":"12","text":"Wohu wʼanwonwade wɔ beae a hɔ aduru sum,"},{"book":"Nnwom","chapter":"88","verse":"13","text":"Nanso Awurade, misu frɛ wo sɛ boa me;"},{"book":"Nnwom","chapter":"88","verse":"14","text":"Adɛn, Awurade, na wopo me"},{"book":"Nnwom","chapter":"88","verse":"15","text":"Mahu amane fi me mmofraase na mabɛn owu;"},{"book":"Nnwom","chapter":"88","verse":"16","text":"Wʼabufuwhyew abu afa me so;"},{"book":"Nnwom","chapter":"88","verse":"17","text":"Daa nyinaa wotwa me ho hyia sɛ nsuyiri,"},{"book":"Nnwom","chapter":"88","verse":"18","text":"Woafa me yɔnkonom ne mʼadɔfo afi me nkyɛn kɔ;"}]}