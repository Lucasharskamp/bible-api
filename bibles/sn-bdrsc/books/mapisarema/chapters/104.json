{"data":[{"book":"Mapisarema","chapter":"104","verse":"1","text":"Rumbidza Jehovha, mweya wangu."},{"book":"Mapisarema","chapter":"104","verse":"2","text":"Iye anozviputira muchiedza seanozviputira nenguo;"},{"book":"Mapisarema","chapter":"104","verse":"3","text":"uye anogadzika matanda edzimba dzake dzapamusoro pamusoro pemvura yadzo zhinji."},{"book":"Mapisarema","chapter":"104","verse":"4","text":"Anoita mhepo nhume dzake,"},{"book":"Mapisarema","chapter":"104","verse":"5","text":"Akamisa nyika pamusoro penheyo dzayo;"},{"book":"Mapisarema","chapter":"104","verse":"6","text":"Makaifukidza nokwakadzika somunofukidza nenguo;"},{"book":"Mapisarema","chapter":"104","verse":"7","text":"Asi pakutsiura kwenyu mvura zhinji yakatiza,"},{"book":"Mapisarema","chapter":"104","verse":"8","text":"yakayerera napamusoro pamakomo,"},{"book":"Mapisarema","chapter":"104","verse":"9","text":"Makatara muganhu waisingadariki;"},{"book":"Mapisarema","chapter":"104","verse":"10","text":"Anoita kuti matsime adire mvura mumipata;"},{"book":"Mapisarema","chapter":"104","verse":"11","text":"Anonwisa mhuka dzose dzesango mvura;"},{"book":"Mapisarema","chapter":"104","verse":"12","text":"Shiri dzedenga dzinovaka matendere adzo pedyo nemvura;"},{"book":"Mapisarema","chapter":"104","verse":"13","text":"Anodiridza makomo ari padzimba dzake dzapamusoro;"},{"book":"Mapisarema","chapter":"104","verse":"14","text":"Anomeresa uswa hwemombe,"},{"book":"Mapisarema","chapter":"104","verse":"15","text":"iyo waini inofadza mwoyo womunhu,"},{"book":"Mapisarema","chapter":"104","verse":"16","text":"Miti yaJehovha inodiridzwa zvakanaka,"},{"book":"Mapisarema","chapter":"104","verse":"17","text":"Shiri dzinoruka matendere adzo ipapo;"},{"book":"Mapisarema","chapter":"104","verse":"18","text":"Makomo marefu ndeengururu;"},{"book":"Mapisarema","chapter":"104","verse":"19","text":"Mwedzi unotara nguva,"},{"book":"Mapisarema","chapter":"104","verse":"20","text":"Munouyisa rima, usiku hugovapo,"},{"book":"Mapisarema","chapter":"104","verse":"21","text":"Shumba dzinoomba dzichitsvaka nyama,"},{"book":"Mapisarema","chapter":"104","verse":"22","text":"Kana zuva robuda, idzo dzinoenda kure;"},{"book":"Mapisarema","chapter":"104","verse":"23","text":"Ipapo munhu anobuda oenda kubasa rake,"},{"book":"Mapisarema","chapter":"104","verse":"24","text":"Haiwa Jehovha, mabasa enyu manganiko!"},{"book":"Mapisarema","chapter":"104","verse":"25","text":"Hero gungwa, rakakura uye rakapamhama,"},{"book":"Mapisarema","chapter":"104","verse":"26","text":"Hezvo zvikepe zvinofamba-famba, nengwena,"},{"book":"Mapisarema","chapter":"104","verse":"27","text":"Zvose izvi zvinotarira kwamuri"},{"book":"Mapisarema","chapter":"104","verse":"28","text":"Pamunozvipa, zvinozviunganidza;"},{"book":"Mapisarema","chapter":"104","verse":"29","text":"Pamunovanza chiso chenyu,"},{"book":"Mapisarema","chapter":"104","verse":"30","text":"Pamunotuma Mweya wenyu,"},{"book":"Mapisarema","chapter":"104","verse":"31","text":"Kubwinya kwaJehovha ngakugare nokusingaperi;"},{"book":"Mapisarema","chapter":"104","verse":"32","text":"iye anotarira nyika, yobva yabvunda,"},{"book":"Mapisarema","chapter":"104","verse":"33","text":"Ndichaimbira Jehovha upenyu hwangu hwose;"},{"book":"Mapisarema","chapter":"104","verse":"34","text":"Kurangarira kwangu ngakumufadze,"},{"book":"Mapisarema","chapter":"104","verse":"35","text":"Asi vatadzi ngavaparadzwe panyika,"}]}