{"data":[{"book":"Yobu","chapter":"41","verse":"1","text":"“Kodi ungathe kukoka ngʼona ndi mbedza ya nsomba"},{"book":"Yobu","chapter":"41","verse":"2","text":"Kodi ungathe kumanga chingwe mʼmphuno mwake,"},{"book":"Yobu","chapter":"41","verse":"3","text":"Kodi ngʼonayo idzakupempha kuti uyichitire chifundo?"},{"book":"Yobu","chapter":"41","verse":"4","text":"Kodi idzachita nawe mgwirizano"},{"book":"Yobu","chapter":"41","verse":"5","text":"Kodi udzasewera nayo ngati mbalame,"},{"book":"Yobu","chapter":"41","verse":"6","text":"Kodi anthu adzayitsatsa malonda?"},{"book":"Yobu","chapter":"41","verse":"7","text":"Kodi chikopa chake ungathe kuchilasa ndi zisonga,"},{"book":"Yobu","chapter":"41","verse":"8","text":"Ukayiputa udziwe kuti pali nkhondo,"},{"book":"Yobu","chapter":"41","verse":"9","text":"Chiyembekezo choti nʼkuyigonjetsa ndi chabodza;"},{"book":"Yobu","chapter":"41","verse":"10","text":"Palibe wolimba mtima kuti ndi kuyiputa."},{"book":"Yobu","chapter":"41","verse":"11","text":"Kodi ndani anandipatsa kanthu kuti ndimubwezere?"},{"book":"Yobu","chapter":"41","verse":"12","text":"“Sindidzaleka kuyankhula za ziwalo zake za chirombocho,"},{"book":"Yobu","chapter":"41","verse":"13","text":"Ndani angasende chikopa chake?"},{"book":"Yobu","chapter":"41","verse":"14","text":"Ndani angatsekule kukamwa kwake,"},{"book":"Yobu","chapter":"41","verse":"15","text":"Kumsana kwake kuli mizere ya mamba"},{"book":"Yobu","chapter":"41","verse":"16","text":"Mambawo ndi olukanalukana"},{"book":"Yobu","chapter":"41","verse":"17","text":"Ndi olumikizanalumikizana;"},{"book":"Yobu","chapter":"41","verse":"18","text":"Kuyetsemula kwake kumatulutsa mbaliwali;"},{"book":"Yobu","chapter":"41","verse":"19","text":"Mʼkamwa mwake mumatuluka nsakali zamoto ndipo"},{"book":"Yobu","chapter":"41","verse":"20","text":"Mʼmphuno mwake mumatuluka utsi"},{"book":"Yobu","chapter":"41","verse":"21","text":"Mpweya wake umayatsa makala,"},{"book":"Yobu","chapter":"41","verse":"22","text":"Mphamvu zake zili mʼkhosi mwake;"},{"book":"Yobu","chapter":"41","verse":"23","text":"Minyewa ya thupi lake ndi yolumikizana ndipo"},{"book":"Yobu","chapter":"41","verse":"24","text":"Pachifuwa pake ndi powuma ngati mwala,"},{"book":"Yobu","chapter":"41","verse":"25","text":"Ngʼonayo ikangovuwuka,"},{"book":"Yobu","chapter":"41","verse":"26","text":"Ngakhale ikanthidwe ndi lupanga, lupangalo silichita kanthu,"},{"book":"Yobu","chapter":"41","verse":"27","text":"Chitsulo imachiyesa ngati phesi chabe"},{"book":"Yobu","chapter":"41","verse":"28","text":"Muvi sungathe kuyithawitsa,"},{"book":"Yobu","chapter":"41","verse":"29","text":"Zibonga zimakhala ngati ziputu;"},{"book":"Yobu","chapter":"41","verse":"30","text":"Mamba a ku mimba kwake ali ngati chopunthira chakunthwa ndipo"},{"book":"Yobu","chapter":"41","verse":"31","text":"Imagadutsa madzi ozama ngati madzi a mʼnkhali,"},{"book":"Yobu","chapter":"41","verse":"32","text":"Kumbuyo kwake imasiya nthubwitubwi zambee,"},{"book":"Yobu","chapter":"41","verse":"33","text":"Pa dziko lapansi palibe china chofanana nacho,"},{"book":"Yobu","chapter":"41","verse":"34","text":"Chimanyoza nyama zina zonse;"}]}