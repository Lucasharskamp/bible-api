{"data":[{"book":"Zaburi","chapter":"109","verse":"1","text":"Ee Mungu, ambaye ninakusifu,"},{"book":"Zaburi","chapter":"109","verse":"2","text":"kwa maana watu waovu na wadanganyifu"},{"book":"Zaburi","chapter":"109","verse":"3","text":"Wamenizunguka kwa maneno ya chuki,"},{"book":"Zaburi","chapter":"109","verse":"4","text":"Wanachonilipa badala ya urafiki wangu ni kunishtaki,"},{"book":"Zaburi","chapter":"109","verse":"5","text":"Wananilipiza mabaya kwa mema,"},{"book":"Zaburi","chapter":"109","verse":"6","text":"Agiza mtu mwovu ampinge,"},{"book":"Zaburi","chapter":"109","verse":"7","text":"Anapohukumiwa, apatikane na hatia,"},{"book":"Zaburi","chapter":"109","verse":"8","text":"Siku zake za kuishi na ziwe chache,"},{"book":"Zaburi","chapter":"109","verse":"9","text":"Watoto wake na waachwe yatima,"},{"book":"Zaburi","chapter":"109","verse":"10","text":"Watoto wake na watangetange wakiomba,"},{"book":"Zaburi","chapter":"109","verse":"11","text":"Mtu anayemdai na ateke vyote alivyo navyo,"},{"book":"Zaburi","chapter":"109","verse":"12","text":"Asiwepo mtu yeyote wa kumtendea mema"},{"book":"Zaburi","chapter":"109","verse":"13","text":"Uzao wake na ukatiliwe mbali,"},{"book":"Zaburi","chapter":"109","verse":"14","text":"Maovu ya baba zake na yakumbukwe"},{"book":"Zaburi","chapter":"109","verse":"15","text":"Dhambi zao na zibaki daima mbele za Bwana,"},{"book":"Zaburi","chapter":"109","verse":"16","text":"Kwa maana kamwe hakuweza kutenda wema,"},{"book":"Zaburi","chapter":"109","verse":"17","text":"Alipenda kulaani,"},{"book":"Zaburi","chapter":"109","verse":"18","text":"Alivaa kulaani kama vazi lake,"},{"book":"Zaburi","chapter":"109","verse":"19","text":"Na iwe kama joho alilozungushiwa,"},{"book":"Zaburi","chapter":"109","verse":"20","text":"Haya na yawe malipo ya Bwana kwa washtaki wangu,"},{"book":"Zaburi","chapter":"109","verse":"21","text":"Lakini wewe, Ee Bwana Mwenyezi,"},{"book":"Zaburi","chapter":"109","verse":"22","text":"Maana mimi ni maskini na mhitaji,"},{"book":"Zaburi","chapter":"109","verse":"23","text":"Ninafifia kama kivuli cha jioni,"},{"book":"Zaburi","chapter":"109","verse":"24","text":"Magoti yangu yamelegea kwa kufunga,"},{"book":"Zaburi","chapter":"109","verse":"25","text":"Nimekuwa kitu cha kudharauliwa kwa washtaki wangu,"},{"book":"Zaburi","chapter":"109","verse":"26","text":"Ee Bwana, Mungu wangu nisaidie,"},{"book":"Zaburi","chapter":"109","verse":"27","text":"Watu na wafahamu kuwa ni mkono wako,"},{"book":"Zaburi","chapter":"109","verse":"28","text":"Wanaweza kulaani, lakini wewe utabariki,"},{"book":"Zaburi","chapter":"109","verse":"29","text":"Washtaki wangu watavikwa fedheha,"},{"book":"Zaburi","chapter":"109","verse":"30","text":"Kwa kinywa changu nitamtukuza sana Bwana,"},{"book":"Zaburi","chapter":"109","verse":"31","text":"Kwa maana husimama mkono wa kuume wa mhitaji,"}]}