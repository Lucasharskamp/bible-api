{"data":[{"book":"Okukungubaga","chapter":"3","verse":"1","text":"Nze muntu eyakangavvulwa"},{"book":"Okukungubaga","chapter":"3","verse":"2","text":"Angobye mu maaso ge n’antambuliza"},{"book":"Okukungubaga","chapter":"3","verse":"3","text":"ddala, omukono gwe gunnwanyisizza"},{"book":"Okukungubaga","chapter":"3","verse":"4","text":"Akaddiyizza omubiri gwange n’eddiba lyange"},{"book":"Okukungubaga","chapter":"3","verse":"5","text":"Antaayizza n’anzijuza"},{"book":"Okukungubaga","chapter":"3","verse":"6","text":"Antadde mu kizikiza"},{"book":"Okukungubaga","chapter":"3","verse":"7","text":"Ankomedde n’okuyinza ne siyinza kudduka,"},{"book":"Okukungubaga","chapter":"3","verse":"8","text":"Ne bwe mukoowoola ne mukaabira nga mmusaba anyambe,"},{"book":"Okukungubaga","chapter":"3","verse":"9","text":"Anteeredde amayinja mu kkubo lyange"},{"book":"Okukungubaga","chapter":"3","verse":"10","text":"Ng’eddubu bwe liteega,"},{"book":"Okukungubaga","chapter":"3","verse":"11","text":"yansikambula n’anziggya mu kkubo lyange n’antaagulataagula"},{"book":"Okukungubaga","chapter":"3","verse":"12","text":"Yanaanuula omutego gwe,"},{"book":"Okukungubaga","chapter":"3","verse":"13","text":"Yafumita omutima gwange"},{"book":"Okukungubaga","chapter":"3","verse":"14","text":"Nafuuka ekisekererwa eri abantu bonna,"},{"book":"Okukungubaga","chapter":"3","verse":"15","text":"Anzijuzza ebikaawa"},{"book":"Okukungubaga","chapter":"3","verse":"16","text":"Ampadde oluyinjayinja okululya amannyo gange ne gamenyeka;"},{"book":"Okukungubaga","chapter":"3","verse":"17","text":"Emmeeme yange terina mirembe,"},{"book":"Okukungubaga","chapter":"3","verse":"18","text":"Era njogera nti, “Ekitiibwa kyange kigenze,"},{"book":"Okukungubaga","chapter":"3","verse":"19","text":"Nzijukira okubonaabona kwange n’okuwankawanka kwange,"},{"book":"Okukungubaga","chapter":"3","verse":"20","text":"Mbijjukira bulungi"},{"book":"Okukungubaga","chapter":"3","verse":"21","text":"Ebyo byonna mbijjukira,"},{"book":"Okukungubaga","chapter":"3","verse":"22","text":"Olw’okwagala kwa Mukama okutaggwaawo,"},{"book":"Okukungubaga","chapter":"3","verse":"23","text":"Buli lukya ekisa kyo kiba kiggya;"},{"book":"Okukungubaga","chapter":"3","verse":"24","text":"Njogera mu mutima gwange nti, “Mukama gwe mugabo gwange,"},{"book":"Okukungubaga","chapter":"3","verse":"25","text":"Mukama mulungi eri abo abamulinamu essuubi,"},{"book":"Okukungubaga","chapter":"3","verse":"26","text":"Kirungi omuntu okulindirira"},{"book":"Okukungubaga","chapter":"3","verse":"27","text":"Kirungi omuntu okwetikka ekikoligo kye"},{"book":"Okukungubaga","chapter":"3","verse":"28","text":"Atuulenga yekka mu kasirise"},{"book":"Okukungubaga","chapter":"3","verse":"29","text":"Leka akweke amaaso ge mu nfuufu, mpozi wanaabaawo essuubi."},{"book":"Okukungubaga","chapter":"3","verse":"30","text":"Leka aweeyo oluba lwe okukubibwa,"},{"book":"Okukungubaga","chapter":"3","verse":"31","text":"Kubanga Mukama taligobera bantu bweru"},{"book":"Okukungubaga","chapter":"3","verse":"32","text":"Newaakubadde ng’aleeta obulumi, aliraga ekisa"},{"book":"Okukungubaga","chapter":"3","verse":"33","text":"Tagenderera kuleeta bulumi"},{"book":"Okukungubaga","chapter":"3","verse":"34","text":"Mukama akkiriziganya"},{"book":"Okukungubaga","chapter":"3","verse":"35","text":"n’okuggyako omuntu obwetwaze bwe"},{"book":"Okukungubaga","chapter":"3","verse":"36","text":"oba n’obutaba na bwenkanya eri omuntu?"},{"book":"Okukungubaga","chapter":"3","verse":"37","text":"Ani ayinza okwogera ekintu ne kituukirira,"},{"book":"Okukungubaga","chapter":"3","verse":"38","text":"Mu kamwa k’oyo Ali Waggulu Ennyo,"},{"book":"Okukungubaga","chapter":"3","verse":"39","text":"Lwaki omuntu omulamu yeemulugunya,"},{"book":"Okukungubaga","chapter":"3","verse":"40","text":"Twekebere engeri zaffe, era tuzeetegereze,"},{"book":"Okukungubaga","chapter":"3","verse":"41","text":"Tuyimuse emitima gyaffe n’emikono gyaffe"},{"book":"Okukungubaga","chapter":"3","verse":"42","text":"“Twayonoona ne tujeema,"},{"book":"Okukungubaga","chapter":"3","verse":"43","text":"“Ojjudde obusungu n’otugobaganya,"},{"book":"Okukungubaga","chapter":"3","verse":"44","text":"Weebisseeko ekire,"},{"book":"Okukungubaga","chapter":"3","verse":"45","text":"Otufudde obusa n’ebisasiro"},{"book":"Okukungubaga","chapter":"3","verse":"46","text":"“Abalabe baffe bonna batwogerako"},{"book":"Okukungubaga","chapter":"3","verse":"47","text":"Tubonyeebonye olw’entiisa n’emitego"},{"book":"Okukungubaga","chapter":"3","verse":"48","text":"Amaaso gange gakulukuta emigga gy’amaziga"},{"book":"Okukungubaga","chapter":"3","verse":"49","text":"Era amaaso gange ganeeyongeranga okukulukuta amaziga"},{"book":"Okukungubaga","chapter":"3","verse":"50","text":"okutuusa Mukama lw’alisinzira"},{"book":"Okukungubaga","chapter":"3","verse":"51","text":"Bye ndaba bireeta ennaku ku mutima gwange,"},{"book":"Okukungubaga","chapter":"3","verse":"52","text":"Abalabe bange banjigganya olutata"},{"book":"Okukungubaga","chapter":"3","verse":"53","text":"Bagezaako okuzikiririza obulamu bwange mu bunnya,"},{"book":"Okukungubaga","chapter":"3","verse":"54","text":"amazzi gaabikka omutwe gwange,"},{"book":"Okukungubaga","chapter":"3","verse":"55","text":"“Nakoowoola erinnya lyo, Ayi Mukama,"},{"book":"Okukungubaga","chapter":"3","verse":"56","text":"wawulira okwegayirira kwange: toziba matu go"},{"book":"Okukungubaga","chapter":"3","verse":"57","text":"Bwe nakukoowoola wansemberera"},{"book":"Okukungubaga","chapter":"3","verse":"58","text":"Mukama watunula mu nsonga yange,"},{"book":"Okukungubaga","chapter":"3","verse":"59","text":"Ayi Mukama, walaba ebibi bye bankola,"},{"book":"Okukungubaga","chapter":"3","verse":"60","text":"Walaba bwe bampalana,"},{"book":"Okukungubaga","chapter":"3","verse":"61","text":"Wawulira bye banvuma, Ayi Mukama Katonda,"},{"book":"Okukungubaga","chapter":"3","verse":"62","text":"obwama n’ebirowoozo eby’abalabe bange"},{"book":"Okukungubaga","chapter":"3","verse":"63","text":"Batunuulire mu kutuula kwabwe ne mu kuyimirira kwabwe;"},{"book":"Okukungubaga","chapter":"3","verse":"64","text":"Obasasule nga bwe kibagwanira Ayi Mukama Katonda,"},{"book":"Okukungubaga","chapter":"3","verse":"65","text":"Osseeko ekibikka ku mitima gyabwe,"},{"book":"Okukungubaga","chapter":"3","verse":"66","text":"Obayigganye mu busungu bwo obazikirize"}]}