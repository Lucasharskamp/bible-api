{"data":[{"book":"Zekaria","chapter":"9","verse":"1","text":"Yehowa ƒe nya tsi tsitre ɖe Hadraknyigba ŋu,"},{"book":"Zekaria","chapter":"9","verse":"2","text":"Ava Hamat si ƒo xlãe la hã dzi,"},{"book":"Zekaria","chapter":"9","verse":"3","text":"Tiro tu mɔ sesẽ na eɖokui,"},{"book":"Zekaria","chapter":"9","verse":"4","text":"Ke Yehowa axɔ kesinɔnuwo le esi,"},{"book":"Zekaria","chapter":"9","verse":"5","text":"Askelon akpɔe, eye vɔvɔ̃ aɖoe."},{"book":"Zekaria","chapter":"9","verse":"6","text":"Amedzrowo ava xɔ Asdod,"},{"book":"Zekaria","chapter":"9","verse":"7","text":"Maɖe ʋu ɖa le woƒe nuwo me"},{"book":"Zekaria","chapter":"9","verse":"8","text":"Ke maʋli nye gbedoxɔ la ta tso adzohawo ƒe asi me."},{"book":"Zekaria","chapter":"9","verse":"9","text":"Dzi nadzɔ wò, O Zion vinyɔnu!"},{"book":"Zekaria","chapter":"9","verse":"10","text":"Axɔ tasiaɖamwo le Efraim si"},{"book":"Zekaria","chapter":"9","verse":"11","text":"Ɖe nu si mebla kpli wò,"},{"book":"Zekaria","chapter":"9","verse":"12","text":"Mitrɔ yi ɖe miaƒe mɔ sesẽwo me,"},{"book":"Zekaria","chapter":"9","verse":"13","text":"Mabɔ Yuda abe ale si mebɔa nye da ene,"},{"book":"Zekaria","chapter":"9","verse":"14","text":"Yehowa ava do ɖe wo dzi kpoyi,"},{"book":"Zekaria","chapter":"9","verse":"15","text":"eye Yehowa, Dziƒoʋakɔwo ƒe Mawu la"},{"book":"Zekaria","chapter":"9","verse":"16","text":"Yehowa, woƒe Mawu la"},{"book":"Zekaria","chapter":"9","verse":"17","text":"Aleke woahanyo eye woahadze anii!"}]}