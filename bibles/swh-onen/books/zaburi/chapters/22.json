{"data":[{"book":"Zaburi","chapter":"22","verse":"1","text":"Mungu wangu, Mungu wangu, mbona umeniacha?"},{"book":"Zaburi","chapter":"22","verse":"2","text":"Ee Mungu wangu, ninalia mchana, lakini hunijibu,"},{"book":"Zaburi","chapter":"22","verse":"3","text":"Hata hivyo umesimikwa katika kiti cha enzi kama Uliye Mtakatifu;"},{"book":"Zaburi","chapter":"22","verse":"4","text":"Kwako wewe baba zetu waliweka tumaini lao,"},{"book":"Zaburi","chapter":"22","verse":"5","text":"Walikulilia wewe na ukawaokoa,"},{"book":"Zaburi","chapter":"22","verse":"6","text":"Mimi ni mnyoo wala si mwanadamu,"},{"book":"Zaburi","chapter":"22","verse":"7","text":"Wote wanionao hunidhihaki,"},{"book":"Zaburi","chapter":"22","verse":"8","text":"Husema, â€œAnamtegemea Bwana,"},{"book":"Zaburi","chapter":"22","verse":"9","text":"Hata hivyo ulinitoa tumboni,"},{"book":"Zaburi","chapter":"22","verse":"10","text":"Nimekabidhiwa kwako tangu kuzaliwa,"},{"book":"Zaburi","chapter":"22","verse":"11","text":"Usiwe mbali nami,"},{"book":"Zaburi","chapter":"22","verse":"12","text":"Mafahali wengi wamenizunguka,"},{"book":"Zaburi","chapter":"22","verse":"13","text":"Simba wangurumao wanaorarua mawindo"},{"book":"Zaburi","chapter":"22","verse":"14","text":"Nimemiminwa kama maji,"},{"book":"Zaburi","chapter":"22","verse":"15","text":"Nguvu zangu zimekauka kama kigae,"},{"book":"Zaburi","chapter":"22","verse":"16","text":"Mbwa wamenizunguka,"},{"book":"Zaburi","chapter":"22","verse":"17","text":"Naweza kuhesabu mifupa yangu yote,"},{"book":"Zaburi","chapter":"22","verse":"18","text":"Wanagawana nguo zangu wao kwa wao,"},{"book":"Zaburi","chapter":"22","verse":"19","text":"Lakini wewe, Ee Bwana,"},{"book":"Zaburi","chapter":"22","verse":"20","text":"Okoa maisha yangu na upanga,"},{"book":"Zaburi","chapter":"22","verse":"21","text":"Niokoe kutoka kinywani mwa simba,"},{"book":"Zaburi","chapter":"22","verse":"22","text":"Nitalitangaza jina lako kwa ndugu zangu,"},{"book":"Zaburi","chapter":"22","verse":"23","text":"Ninyi ambao mnamcha Bwana, msifuni!"},{"book":"Zaburi","chapter":"22","verse":"24","text":"Kwa maana hakupuuza wala kudharau"},{"book":"Zaburi","chapter":"22","verse":"25","text":"Kwako wewe hutoka kiini cha sifa zangu katika kusanyiko kubwa,"},{"book":"Zaburi","chapter":"22","verse":"26","text":"Maskini watakula na kushiba,"},{"book":"Zaburi","chapter":"22","verse":"27","text":"Miisho yote ya dunia itakumbuka"},{"book":"Zaburi","chapter":"22","verse":"28","text":"kwa maana ufalme ni wa Bwana"},{"book":"Zaburi","chapter":"22","verse":"29","text":"Matajiri wote wa dunia watasherehekea na kuabudu."},{"book":"Zaburi","chapter":"22","verse":"30","text":"Wazao wa baadaye watamtumikia yeye;"},{"book":"Zaburi","chapter":"22","verse":"31","text":"Watatangaza haki yake kwa watu"}]}