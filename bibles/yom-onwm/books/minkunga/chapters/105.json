{"data":[{"book":"Minkunga","chapter":"105","verse":"1","text":"Luvutudila Yave matondo, lutela dizina diandi !"},{"book":"Minkunga","chapter":"105","verse":"2","text":"Lunyimbidila ; lunyimbidila minzitusu."},{"book":"Minkunga","chapter":"105","verse":"3","text":"Nkembo kuidi dizina diandi dinlongo ;"},{"book":"Minkunga","chapter":"105","verse":"4","text":"Talanu kuidi Yave ayi kuidi zingolo ziandi ;"},{"book":"Minkunga","chapter":"105","verse":"5","text":"Lutebukilanga moyo matsiminanga momo kavanga ;"},{"book":"Minkunga","chapter":"105","verse":"6","text":"beno nkuna Abalahami, kisadi kiandi,"},{"book":"Minkunga","chapter":"105","verse":"7","text":"Yave niandi Nzambi eto ;"},{"book":"Minkunga","chapter":"105","verse":"8","text":"Wuntebukilanga moyo nguizani andi mu zithangu zioso,"},{"book":"Minkunga","chapter":"105","verse":"9","text":"Nguizani yoyi yi kawizana ayi Abalahami ;"},{"book":"Minkunga","chapter":"105","verse":"10","text":"Wukindisa yawu kuidi Yakobi banga nzengolo ;"},{"book":"Minkunga","chapter":"105","verse":"11","text":"“Kuidi ngeyo ndiela vana ntoto wu Kana"},{"book":"Minkunga","chapter":"105","verse":"12","text":"Buna bakidi thalu yi fioti kaka,"},{"book":"Minkunga","chapter":"105","verse":"13","text":"vayi baba kuenda diengilanga mu makanda-makanda"},{"book":"Minkunga","chapter":"105","verse":"14","text":"Kasia vana ko minsua kuidi kadi mutu wumosi mu kuba yamisa"},{"book":"Minkunga","chapter":"105","verse":"15","text":"“Kadi simba batu bama bobo ndivinda mafuta ;"},{"book":"Minkunga","chapter":"105","verse":"16","text":"Wutumisa kanda kingolo va ntoto"},{"book":"Minkunga","chapter":"105","verse":"17","text":"Buna wufidisa mutu wumosi ku ntualꞌawu :"},{"book":"Minkunga","chapter":"105","verse":"18","text":"Bakanga malu mandi mu zisieni ;"},{"book":"Minkunga","chapter":"105","verse":"19","text":"nate mambu makatuba mavioka ;"},{"book":"Minkunga","chapter":"105","verse":"20","text":"Ntinu wufidisa ayi bannianguna ;"},{"book":"Minkunga","chapter":"105","verse":"21","text":"Wumbieka pfumu mu nzo andi"},{"book":"Minkunga","chapter":"105","verse":"22","text":"mu longa bana ba mintinu boso bu kazodidi"},{"book":"Minkunga","chapter":"105","verse":"23","text":"Buna Iseli wukota ku Ezipite ;"},{"book":"Minkunga","chapter":"105","verse":"24","text":"Yave wuvanga ti batu bandi babutuka kuwombo"},{"book":"Minkunga","chapter":"105","verse":"25","text":"bobo kabalula mintima mu lenda batu bandi,"},{"book":"Minkunga","chapter":"105","verse":"26","text":"Wufidisa Moyize, kisadi kiandi,"},{"book":"Minkunga","chapter":"105","verse":"27","text":"Bavanga bidimbu biandi va khatitsikꞌawu,"},{"book":"Minkunga","chapter":"105","verse":"28","text":"Wufidisa tombi ayi wutula tombi mu ntoto"},{"book":"Minkunga","chapter":"105","verse":"29","text":"Wukitula minlangu miawu menga ;"},{"book":"Minkunga","chapter":"105","verse":"30","text":"Mintoto miawu miwala bisioto ;"},{"book":"Minkunga","chapter":"105","verse":"31","text":"Wutuba ayi kuyiza zinzinzi ziba mitsa"},{"book":"Minkunga","chapter":"105","verse":"32","text":"Niandi wubalula mvula mu mvula yi matadi"},{"book":"Minkunga","chapter":"105","verse":"33","text":"Wumana buisa minti miawu mi vinu ayi minti miawu mi figi,"},{"book":"Minkunga","chapter":"105","verse":"34","text":"Wutuba ayi makhoko mayiza ;"},{"book":"Minkunga","chapter":"105","verse":"35","text":"Bidia kadika kunusu kinlangu mu tsi awu ;"},{"book":"Minkunga","chapter":"105","verse":"36","text":"Buna wumana vonda bana boso batheti mu tsi awu,"},{"book":"Minkunga","chapter":"105","verse":"37","text":"Wutotula Iseli va kimosi ayi mfuna wu palata ayi nolo ;"},{"book":"Minkunga","chapter":"105","verse":"38","text":"Ezipite yimona khini bubayenda"},{"book":"Minkunga","chapter":"105","verse":"39","text":"Wuyalumuna matuti mu diambu di kuba fuka"},{"book":"Minkunga","chapter":"105","verse":"40","text":"Balomba ayi wuba fidisila zinuni"},{"book":"Minkunga","chapter":"105","verse":"41","text":"Wuzibula ditadi ayi nlangu wutotuka ;"},{"book":"Minkunga","chapter":"105","verse":"42","text":"Bila wutebukila moyo tsilꞌandi yinlongo"},{"book":"Minkunga","chapter":"105","verse":"43","text":"Wutotudila batu bandi mu khini"},{"book":"Minkunga","chapter":"105","verse":"44","text":"Wuba vana mintoto mi makanda,"},{"book":"Minkunga","chapter":"105","verse":"45","text":"Muingi balunda minsiku miandi"}]}