{"data":[{"book":"Zvirevo","chapter":"8","verse":"1","text":"Ko, uchenjeri hahudanidziri here?"},{"book":"Zvirevo","chapter":"8","verse":"2","text":"Panzvimbo dzakakwirira panzira,"},{"book":"Zvirevo","chapter":"8","verse":"3","text":"parutivi rwamasuo okupinda muguta, pamikova,"},{"book":"Zvirevo","chapter":"8","verse":"4","text":"“Kwamuri, imi varume, ndinodanidzira;"},{"book":"Zvirevo","chapter":"8","verse":"5","text":"Imi vasina mano, wanai uchenjeri;"},{"book":"Zvirevo","chapter":"8","verse":"6","text":"Teererai, nokuti ndine zvinhu zvinokosha zvokutaura;"},{"book":"Zvirevo","chapter":"8","verse":"7","text":"Muromo wangu unotaura zvokwadi,"},{"book":"Zvirevo","chapter":"8","verse":"8","text":"Mashoko ose omuromo wangu akarurama;"},{"book":"Zvirevo","chapter":"8","verse":"9","text":"Kune ane njere ose zvawo akarurama,"},{"book":"Zvirevo","chapter":"8","verse":"10","text":"Sarudza kurayira kwangu panzvimbo yesirivha,"},{"book":"Zvirevo","chapter":"8","verse":"11","text":"nokuti uchenjeri hunokosha kupfuura marubhi,"},{"book":"Zvirevo","chapter":"8","verse":"12","text":"“Ini, uchenjeri ndinogara pamwe chete navakachenjera;"},{"book":"Zvirevo","chapter":"8","verse":"13","text":"Kutya Jehovha ndiko kuvenga zvakaipa;"},{"book":"Zvirevo","chapter":"8","verse":"14","text":"Zano nokutonga kwakanaka ndezvangu;"},{"book":"Zvirevo","chapter":"8","verse":"15","text":"Madzimambo anobata ushe neni"},{"book":"Zvirevo","chapter":"8","verse":"16","text":"machinda anofambisa ushe neni,"},{"book":"Zvirevo","chapter":"8","verse":"17","text":"Ndinoda avo vanondida,"},{"book":"Zvirevo","chapter":"8","verse":"18","text":"Pfuma nokukudzwa zvineni,"},{"book":"Zvirevo","chapter":"8","verse":"19","text":"Chibereko changu chakanaka kupfuura goridhe rakaisvonaka;"},{"book":"Zvirevo","chapter":"8","verse":"20","text":"Ndinofamba munzira dzokururama,"},{"book":"Zvirevo","chapter":"8","verse":"21","text":"ndichipa upfumi kuna avo vanondida,"},{"book":"Zvirevo","chapter":"8","verse":"22","text":"“Jehovha akandibudisa sebasa rake rokutanga pamabasa ake,"},{"book":"Zvirevo","chapter":"8","verse":"23","text":"ndakagadzwa kubva pakusingaperi,"},{"book":"Zvirevo","chapter":"8","verse":"24","text":"Makungwa asati avapo, ndakaberekwa,"},{"book":"Zvirevo","chapter":"8","verse":"25","text":"makomo asati aiswa munzvimbo dzawo,"},{"book":"Zvirevo","chapter":"8","verse":"26","text":"asati aita nyika neminda"},{"book":"Zvirevo","chapter":"8","verse":"27","text":"Ndakanga ndiripo paakaisa matenga munzvimbo dzawo,"},{"book":"Zvirevo","chapter":"8","verse":"28","text":"paakaita makore kudenga"},{"book":"Zvirevo","chapter":"8","verse":"29","text":"paakapa gungwa miganhu yaro"},{"book":"Zvirevo","chapter":"8","verse":"30","text":"Ipapo ndini ndaiva mhizha parutivi rwake."},{"book":"Zvirevo","chapter":"8","verse":"31","text":"ndichifadzwa nenyika yake yose"},{"book":"Zvirevo","chapter":"8","verse":"32","text":"“Saka zvino, vanakomana vangu, teererai kwandiri;"},{"book":"Zvirevo","chapter":"8","verse":"33","text":"Teererai kurayira kwangu mugova vakachenjera;"},{"book":"Zvirevo","chapter":"8","verse":"34","text":"Akaropafadzwa munhu anoteerera kwandiri,"},{"book":"Zvirevo","chapter":"8","verse":"35","text":"Nokuti ani naani anondiwana anowana upenyu"},{"book":"Zvirevo","chapter":"8","verse":"36","text":"Asi ani naani asingandiwani anozvikuvadza;"}]}