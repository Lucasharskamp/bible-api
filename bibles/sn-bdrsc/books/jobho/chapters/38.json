{"data":[{"book":"Jobho","chapter":"38","verse":"1","text":"Ipapo Jehovha akapindura Jobho ari mudutu akati:"},{"book":"Jobho","chapter":"38","verse":"2","text":"“Ndianiko uyu anodzimaidza zano rangu"},{"book":"Jobho","chapter":"38","verse":"3","text":"Chizvishingisa somurume;"},{"book":"Jobho","chapter":"38","verse":"4","text":"“Iwe wakanga uripi pandakaisa nheyo dzenyika?"},{"book":"Jobho","chapter":"38","verse":"5","text":"Ndianiko akatara miganhu yacho?"},{"book":"Jobho","chapter":"38","verse":"6","text":"Ko, nheyo dzayo dzakanga dzimire pai,"},{"book":"Jobho","chapter":"38","verse":"7","text":"panguva yaiimba nyeredzi dzamangwanani pamwe chete,"},{"book":"Jobho","chapter":"38","verse":"8","text":"“Ndianiko akapfigira gungwa mikova"},{"book":"Jobho","chapter":"38","verse":"9","text":"pandakaita makore kuti ave nguo yaro,"},{"book":"Jobho","chapter":"38","verse":"10","text":"pandakariisira miganhu yaro,"},{"book":"Jobho","chapter":"38","verse":"11","text":"pandakati, ‘Apa ndipo paunosvika uye haungapfuuri;"},{"book":"Jobho","chapter":"38","verse":"12","text":"“Wakambopa mutemo kumangwanani here,"},{"book":"Jobho","chapter":"38","verse":"13","text":"kuti abate mipendero yenyika,"},{"book":"Jobho","chapter":"38","verse":"14","text":"Nyika inoumbika sevhu pasi pechisimbiso;"},{"book":"Jobho","chapter":"38","verse":"15","text":"Vakaipa vanonyimwa chiedza chavo,"},{"book":"Jobho","chapter":"38","verse":"16","text":"“Wakamboshanyira zvitubu zvegungwa here,"},{"book":"Jobho","chapter":"38","verse":"17","text":"Ko, masuo orufu wakamboaratidzwa here?"},{"book":"Jobho","chapter":"38","verse":"18","text":"Wati wanzwisisa upamhi hwenzvimbo dzenyika here?"},{"book":"Jobho","chapter":"38","verse":"19","text":"“Ko, nzira inoenda kunogara chiedza ndeipi?"},{"book":"Jobho","chapter":"38","verse":"20","text":"Ungagona kuzviisa kunzvimbo yazvo here?"},{"book":"Jobho","chapter":"38","verse":"21","text":"Zvirokwazvo iwe unoziva, nokuti wakanga waberekwa kare!"},{"book":"Jobho","chapter":"38","verse":"22","text":"“Wakambopinda mudura rechando here,"},{"book":"Jobho","chapter":"38","verse":"23","text":"izvo zvandakachengetera nguva dzokutambudzika,"},{"book":"Jobho","chapter":"38","verse":"24","text":"Ko, nzira inoenda kunogoverwa mheni ndeipi,"},{"book":"Jobho","chapter":"38","verse":"25","text":"Ndianiko anodziura mugero wemvura zhinji,"},{"book":"Jobho","chapter":"38","verse":"26","text":"kuti zvidiridze nyika isingagarwi nomunhu,"},{"book":"Jobho","chapter":"38","verse":"27","text":"kugutsa nyika yakaparadzwa"},{"book":"Jobho","chapter":"38","verse":"28","text":"Ko, mvura ina baba here?"},{"book":"Jobho","chapter":"38","verse":"29","text":"Chando chinobva muchizvaro chaaniko?"},{"book":"Jobho","chapter":"38","verse":"30","text":"panooma mvura ikaita sedombo,"},{"book":"Jobho","chapter":"38","verse":"31","text":"“Ungagona kusunga nyeredzi dzeChimutanhatu here?"},{"book":"Jobho","chapter":"38","verse":"32","text":"Ungabudisa mapoka enyeredzi panguva yadzo here,"},{"book":"Jobho","chapter":"38","verse":"33","text":"Unoziva mitemo yokumatenga here?"},{"book":"Jobho","chapter":"38","verse":"34","text":"“Ko, iwe ungasvitsa inzwi rako kumakore here,"},{"book":"Jobho","chapter":"38","verse":"35","text":"Ko, iwe unotuma mheni panzira yayo here?"},{"book":"Jobho","chapter":"38","verse":"36","text":"Ndianiko akaisa uchenjeri mumwoyo,"},{"book":"Jobho","chapter":"38","verse":"37","text":"Ndianiko ane uchenjeri hwokuverenga makore?"},{"book":"Jobho","chapter":"38","verse":"38","text":"kana guruva raoma,"},{"book":"Jobho","chapter":"38","verse":"39","text":"“Ko, iwe ungavhimira shumbakadzi chokudya here,"},{"book":"Jobho","chapter":"38","verse":"40","text":"kana dzichitsivama mumapako adzo"},{"book":"Jobho","chapter":"38","verse":"41","text":"Ndianiko anopa gunguo zvokudya"}]}