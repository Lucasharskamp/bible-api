{"data":[{"book":"Mapisarema","chapter":"102","verse":"1","text":"Inzwai munyengetero wangu, imi Jehovha;"},{"book":"Mapisarema","chapter":"102","verse":"2","text":"Regai kundivanzira chiso chenyu"},{"book":"Mapisarema","chapter":"102","verse":"3","text":"Nokuti mazuva angu anopera soutsi;"},{"book":"Mapisarema","chapter":"102","verse":"4","text":"Mwoyo wangu warohwa uye waoma souswa;"},{"book":"Mapisarema","chapter":"102","verse":"5","text":"Nokuda kwokugomera kwangu"},{"book":"Mapisarema","chapter":"102","verse":"6","text":"Ndafanana nezizi romugwenga,"},{"book":"Mapisarema","chapter":"102","verse":"7","text":"Ndinovata ndakasvinura,"},{"book":"Mapisarema","chapter":"102","verse":"8","text":"Zuva rose vavengi vangu vanondishungurudza;"},{"book":"Mapisarema","chapter":"102","verse":"9","text":"Nokuti ndinodya madota sechokudya changu,"},{"book":"Mapisarema","chapter":"102","verse":"10","text":"nokuda kwehasha dzenyu huru,"},{"book":"Mapisarema","chapter":"102","verse":"11","text":"Mazuva angu akaita somumvuri wamadekwana;"},{"book":"Mapisarema","chapter":"102","verse":"12","text":"Asi imi, iyemi Jehovha, munogara pachigaro choushe nokusingaperi;"},{"book":"Mapisarema","chapter":"102","verse":"13","text":"Imi muchasimuka mugonzwira Zioni tsitsi,"},{"book":"Mapisarema","chapter":"102","verse":"14","text":"Nokuti matombo aro anokosha kuvaranda venyu;"},{"book":"Mapisarema","chapter":"102","verse":"15","text":"Ndudzi dzichatya zita raJehovha,"},{"book":"Mapisarema","chapter":"102","verse":"16","text":"Nokuti Jehovha achavakazve Zioni,"},{"book":"Mapisarema","chapter":"102","verse":"17","text":"Achadavira minyengetero yavanotambura;"},{"book":"Mapisarema","chapter":"102","verse":"18","text":"Izvi ngazvinyorerwe zvizvarwa zvamangwana,"},{"book":"Mapisarema","chapter":"102","verse":"19","text":"â€œJehovha akatarira pasi ari panzvimbo yake tsvene yakakwirira,"},{"book":"Mapisarema","chapter":"102","verse":"20","text":"kuti anzwe kugomera kwavasungwa"},{"book":"Mapisarema","chapter":"102","verse":"21","text":"Saka zita raJehovha richaparidzwa muZioni,"},{"book":"Mapisarema","chapter":"102","verse":"22","text":"panoungana marudzi noushe"},{"book":"Mapisarema","chapter":"102","verse":"23","text":"Panguva youpenyu hwangu akatapudza simba rangu;"},{"book":"Mapisarema","chapter":"102","verse":"24","text":"Saka ndakati,"},{"book":"Mapisarema","chapter":"102","verse":"25","text":"Pamavambo makateya nheyo dzenyika,"},{"book":"Mapisarema","chapter":"102","verse":"26","text":"Izvo zvichaparara, asi imi munogara muripo;"},{"book":"Mapisarema","chapter":"102","verse":"27","text":"Asi imi mucharamba makadaro,"},{"book":"Mapisarema","chapter":"102","verse":"28","text":"Vana vavaranda venyu vachagara pamberi penyu;"}]}