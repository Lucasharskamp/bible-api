{"data":[{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"1","text":"Rangarirai Jehovha, zvakatiwira;"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"2","text":"Nhaka yedu yakapiwa kuvatorwa,"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"3","text":"Tava nherera, hatina vabereki,"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"4","text":"Tinofanira kutenga mvura yatinonwa;"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"5","text":"Avo vanotidzinganisa vari pedyo pedyo;"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"6","text":"Takazviisa pasi peIjipiti neAsiria,"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"7","text":"Madzibaba edu akatadza uye vakafa,"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"8","text":"Varanda vanotitonga,"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"9","text":"Tinowana chingwa chedu nokuisa upenyu hwedu munjodzi,"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"10","text":"Ganda redu rava kupisa sechoto,"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"11","text":"Vakachinya vakadzi muZioni,"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"12","text":"Machinda akasungirirwa namaoko avo;"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"13","text":"Majaya anoshanda paguyo;"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"14","text":"Vakuru vakabva pasuo reguta;"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"15","text":"Mufaro mumwoyo medu waguma;"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"16","text":"Korona yawa kubva pamusoro wedu."},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"17","text":"Nokuda kwaizvozvi mwoyo yedu yapera simba,"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"18","text":"nokuti Gomo reZioni rava dongo,"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"19","text":"Imi Jehovha, munotonga nokusingaperi;"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"20","text":"Sei muchigara muchitikanganwa?"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"21","text":"Tidzoserei kwamuri, Jehovha, kuti tigodzoka;"},{"book":"Kuungudza kwaJeremia","chapter":"5","verse":"22","text":"kana musina kutiramba zvachose"}]}