{"data":[{"book":"Banzembo","chapter":"94","verse":"1","text":"Yawe, Nzambe oyo azongisaka"},{"book":"Banzembo","chapter":"94","verse":"2","text":"Mosambisi ya mokili, telema"},{"book":"Banzembo","chapter":"94","verse":"3","text":"Yawe, kino tango nini bato mabe,"},{"book":"Banzembo","chapter":"94","verse":"4","text":"Bazali komilakisa, bazali kobimisa mafinga ;"},{"book":"Banzembo","chapter":"94","verse":"5","text":"Yawe, bazali konyokola bato na Yo"},{"book":"Banzembo","chapter":"94","verse":"6","text":"bazali koboma mwasi oyo akufisa mobali,"},{"book":"Banzembo","chapter":"94","verse":"7","text":"bazali koloba :"},{"book":"Banzembo","chapter":"94","verse":"8","text":"Bino bato ya kilikili, bososola !"},{"book":"Banzembo","chapter":"94","verse":"9","text":"Boni, Ye oyo asala litoyi akoki koyoka te ?"},{"book":"Banzembo","chapter":"94","verse":"10","text":"Ye oyo apesaka bikolo etumbu"},{"book":"Banzembo","chapter":"94","verse":"11","text":"Yawe ayebi makanisi ya bato ;"},{"book":"Banzembo","chapter":"94","verse":"12","text":"Yawe, esengo na moto oyo"},{"book":"Banzembo","chapter":"94","verse":"13","text":"mpo na kopesa ye bopemi na tango ya pasi,"},{"book":"Banzembo","chapter":"94","verse":"14","text":"Pamba te Yawe asundolaka bato na Ye te,"},{"book":"Banzembo","chapter":"94","verse":"15","text":"Bakosambisa lisusu kolanda bosembo,"},{"book":"Banzembo","chapter":"94","verse":"16","text":"Nani akotelema mpo na ngai"},{"book":"Banzembo","chapter":"94","verse":"17","text":"Ah, soki Yawe asungaki ngai te,"},{"book":"Banzembo","chapter":"94","verse":"18","text":"Soki nalobi :"},{"book":"Banzembo","chapter":"94","verse":"19","text":"Soki natungisami na makanisi ebele,"},{"book":"Banzembo","chapter":"94","verse":"20","text":"Boni, bokonzi ya mabe oyo ememelaka bato pasi"},{"book":"Banzembo","chapter":"94","verse":"21","text":"Bato mabe basanganaka mpo na kobundisa"},{"book":"Banzembo","chapter":"94","verse":"22","text":"Kasi Yawe azali, mpo na ngai,"},{"book":"Banzembo","chapter":"94","verse":"23","text":"Akozongisela bango mabe na bango"}]}