{"data":[{"book":"Zaburi","chapter":"116","verse":"1","text":"Ninampenda Bwana kwa maana amesikia sauti yangu;"},{"book":"Zaburi","chapter":"116","verse":"2","text":"Kwa sababu amenitegea sikio lake,"},{"book":"Zaburi","chapter":"116","verse":"3","text":"Kamba za mauti zilinizunguka,"},{"book":"Zaburi","chapter":"116","verse":"4","text":"Ndipo nikaliitia jina la Bwana:"},{"book":"Zaburi","chapter":"116","verse":"5","text":"Bwana ni mwenye neema na haki,"},{"book":"Zaburi","chapter":"116","verse":"6","text":"Bwana huwalinda wanyenyekevu,"},{"book":"Zaburi","chapter":"116","verse":"7","text":"Ee nafsi yangu, tulia tena,"},{"book":"Zaburi","chapter":"116","verse":"8","text":"Kwa kuwa wewe, Ee Bwana,"},{"book":"Zaburi","chapter":"116","verse":"9","text":"ili niweze kutembea mbele za Bwana,"},{"book":"Zaburi","chapter":"116","verse":"10","text":"Niliamini, kwa hiyo nilisema,"},{"book":"Zaburi","chapter":"116","verse":"11","text":"Katika taabu yangu nilisema,"},{"book":"Zaburi","chapter":"116","verse":"12","text":"Nimrudishie Bwana nini"},{"book":"Zaburi","chapter":"116","verse":"13","text":"Nitakiinua kikombe cha wokovu"},{"book":"Zaburi","chapter":"116","verse":"14","text":"Nitazitimiza nadhiri zangu kwa Bwana"},{"book":"Zaburi","chapter":"116","verse":"15","text":"Kifo cha watakatifu kina thamani"},{"book":"Zaburi","chapter":"116","verse":"16","text":"Ee Bwana, hakika mimi ni mtumishi wako,"},{"book":"Zaburi","chapter":"116","verse":"17","text":"Nitakutolea dhabihu ya kukushukuru"},{"book":"Zaburi","chapter":"116","verse":"18","text":"Nitazitimiza nadhiri zangu kwa Bwana"},{"book":"Zaburi","chapter":"116","verse":"19","text":"katika nyua za nyumba ya Bwana,"}]}