{"data":[{"book":"يَبُرَ","chapter":"10","verse":"1","text":"عَلَتَلَ، عِ عِ مَكُيَشِ ﭑ نَ مُنقٍ رَ؟"},{"book":"يَبُرَ","chapter":"10","verse":"2","text":"يّتّ عِفبٌي سٍتَرٍيٍ تْورْ مَ،"},{"book":"يَبُرَ","chapter":"10","verse":"3","text":"عَ عَ يّتّ عِفبٌمَ عَ شَ وَلِ حَاشِ شَ قٍ رَ."},{"book":"يَبُرَ","chapter":"10","verse":"4","text":"مِشِ حَاشِ يّتّ عِفبٌي نَشّ،"},{"book":"يَبُرَ","chapter":"10","verse":"5","text":"عَ شَ وَلِ بِرِن سْونّيَمَ نّ."},{"book":"يَبُرَ","chapter":"10","verse":"6","text":"عَ مَحْشُنمَ عَ بْحّ مَ،"},{"book":"يَبُرَ","chapter":"10","verse":"7","text":"عَ دّ مِشِ دَنكَمَ، عَ وُلٍ قَلَ."},{"book":"يَبُرَ","chapter":"10","verse":"8","text":"عَ مِشِ مّلّنمَ تَا قَرِ مَ،"},{"book":"يَبُرَ","chapter":"10","verse":"9","text":"عَ سٍتَرٍيٍ مّلّنمَ عَلْ يّتّ."},{"book":"يَبُرَ","chapter":"10","verse":"10","text":"نَ سٍتَرٍيٍ لُمَ عَ شَ نْي بُن مَ."},{"book":"يَبُرَ","chapter":"10","verse":"11","text":"عَ عَ قَلَمَ عَ بْحّ كُي،"},{"book":"يَبُرَ","chapter":"10","verse":"12","text":"عَلَتَلَ كٍلِ! عِ نَشَ نّيمُ سٍتَرٍ مَ."},{"book":"يَبُرَ","chapter":"10","verse":"13","text":"مُنقٍ رَ مِشِ حَاشِ يٌمَ عَلَ مَ؟"},{"book":"يَبُرَ","chapter":"10","verse":"14","text":"كْنْ عَلَ، عِ تْورّ نُن نِمِسّ تٌمَ."},{"book":"يَبُرَ","chapter":"10","verse":"15","text":"مِشِ حَاشِ سّنبّ كَنَ!"},{"book":"يَبُرَ","chapter":"10","verse":"16","text":"مَنفّ نَن عَلَتَلَ رَ."},{"book":"يَبُرَ","chapter":"10","verse":"17","text":"عَلَتَلَ، عِ عِ تُلِ تِشِ مِشِ مَفٌرٌشِيٍ رَ."},{"book":"يَبُرَ","chapter":"10","verse":"18","text":"عِ تْورْ مِشِ نُن كِرِدِ كَنتَمَ،"}]}