{"data":[{"book":"يَبُرَ","chapter":"112","verse":"1","text":"وٌ مَرِفِ عَلَ مَتْشْ!"},{"book":"يَبُرَ","chapter":"112","verse":"2","text":"عَ بْنسْي سّنبّ فبٌمَ نّ بْشِ مَ."},{"book":"يَبُرَ","chapter":"112","verse":"3","text":"نَاقُلِ نُن بَننَيَ لُمَ عَ يِ رَ،"},{"book":"يَبُرَ","chapter":"112","verse":"4","text":"يَنبّ مِنِمَ دِ مِ كُي تِنشِنتْي بّ،"},{"book":"يَبُرَ","chapter":"112","verse":"5","text":"بَرَكّ لُمَ مِشِ بّ نَشَن مِشِيٍ كِمَ،"},{"book":"يَبُرَ","chapter":"112","verse":"6","text":"نَ كَنيِ مُ بِرَ مَ،"},{"book":"يَبُرَ","chapter":"112","verse":"7","text":"عَ مُ فَاشُمَ شِبَارُ حَاشِ يَ رَ،"},{"book":"يَبُرَ","chapter":"112","verse":"8","text":"عَ بْحّ رَشَرَ، فَاشُي يٌ مُ عَ مَ."},{"book":"يَبُرَ","chapter":"112","verse":"9","text":"عَ مِسِكِينّ كِمَ،"},{"book":"يَبُرَ","chapter":"112","verse":"10","text":"مِشِ حَاشِ تْورْ مَ نَ تٌقٍ رَ،"}]}