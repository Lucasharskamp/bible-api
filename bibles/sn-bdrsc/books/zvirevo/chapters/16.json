{"data":[{"book":"Zvirevo","chapter":"16","verse":"1","text":"Kuronga kwomwoyo ndekwomunhu,"},{"book":"Zvirevo","chapter":"16","verse":"2","text":"Nzira dzose dzomunhu dzinoita sedzakanaka pakuona kwake,"},{"book":"Zvirevo","chapter":"16","verse":"3","text":"Kumikidza kuna Jehovha chose chaunoita,"},{"book":"Zvirevo","chapter":"16","verse":"4","text":"Jehovha anoitira zvinhu zvose magumo azvo;"},{"book":"Zvirevo","chapter":"16","verse":"5","text":"Jehovha anovenga vose vane mwoyo inozvikudza."},{"book":"Zvirevo","chapter":"16","verse":"6","text":"Kubudikidza norudo nokutendeka, chivi chinoyananisirwa,"},{"book":"Zvirevo","chapter":"16","verse":"7","text":"Kana nzira dzomunhu dzichifadza Jehovha,"},{"book":"Zvirevo","chapter":"16","verse":"8","text":"Zviri nani kuva nezvishoma mukururama,"},{"book":"Zvirevo","chapter":"16","verse":"9","text":"Mumwoyo make munhu anoronga gwara rake,"},{"book":"Zvirevo","chapter":"16","verse":"10","text":"Miromo yamambo inotaura seinotaura chirevo chaMwari,"},{"book":"Zvirevo","chapter":"16","verse":"11","text":"Zviyero nezvienzaniso zvechokwadi zvinobva kuna Jehovha;"},{"book":"Zvirevo","chapter":"16","verse":"12","text":"Madzimambo anovenga kuita zvakaipa,"},{"book":"Zvirevo","chapter":"16","verse":"13","text":"Madzimambo anofarira miromo inotaura chokwadi;"},{"book":"Zvirevo","chapter":"16","verse":"14","text":"Kutsamwa kwamambo inhume yorufu,"},{"book":"Zvirevo","chapter":"16","verse":"15","text":"Kana chiso chamambo chichibwinya, zvinoreva upenyu,"},{"book":"Zvirevo","chapter":"16","verse":"16","text":"Zviri nani sei kuwana uchenjeri pane goridhe,"},{"book":"Zvirevo","chapter":"16","verse":"17","text":"Mugwagwa mukuru wavakarurama unonzvenga zvakaipa;"},{"book":"Zvirevo","chapter":"16","verse":"18","text":"Kuzvikudza kunotangira kuparadzwa,"},{"book":"Zvirevo","chapter":"16","verse":"19","text":"Zviri nani kuva nomweya unozvininipisa pakati pavakadzvinyirirwa"},{"book":"Zvirevo","chapter":"16","verse":"20","text":"Ani naani anoteerera kurayirwa achabudirira,"},{"book":"Zvirevo","chapter":"16","verse":"21","text":"Vakachenjera pamwoyo vanonzi ndivo vanonzvera,"},{"book":"Zvirevo","chapter":"16","verse":"22","text":"Njere itsime roupenyu kuna avo vanadzo,"},{"book":"Zvirevo","chapter":"16","verse":"23","text":"Mwoyo womunhu akachenjera unotungamirira muromo wake,"},{"book":"Zvirevo","chapter":"16","verse":"24","text":"Mashoko anofadza akafanana nezinga rouchi,"},{"book":"Zvirevo","chapter":"16","verse":"25","text":"Kune nzira inoita seyakanaka kumunhu,"},{"book":"Zvirevo","chapter":"16","verse":"26","text":"Nzara yomushandi inomushandira;"},{"book":"Zvirevo","chapter":"16","verse":"27","text":"Munhu asina maturo anoronga zvakaipa,"},{"book":"Zvirevo","chapter":"16","verse":"28","text":"Munhu akatsauka anomutsa kupesana,"},{"book":"Zvirevo","chapter":"16","verse":"29","text":"Munhu anoita nechisimba anonyengera muvakidzani wake,"},{"book":"Zvirevo","chapter":"16","verse":"30","text":"Uyo anochonya nameso ake ari kuronga zvakaipa;"},{"book":"Zvirevo","chapter":"16","verse":"31","text":"Bvudzi rakachena ikorona yakaisvonaka;"},{"book":"Zvirevo","chapter":"16","verse":"32","text":"Zviri nani kuva munhu ane mwoyo murefu pane kuva murwi,"},{"book":"Zvirevo","chapter":"16","verse":"33","text":"Mujenya unokandirwa pamakumbo,"}]}