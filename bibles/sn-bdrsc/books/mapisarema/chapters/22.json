{"data":[{"book":"Mapisarema","chapter":"22","verse":"1","text":"Mwari wangu, Mwari wangu, mandisiyireiko?"},{"book":"Mapisarema","chapter":"22","verse":"2","text":"Haiwa Mwari wangu, ndinodanidzira masikati,"},{"book":"Mapisarema","chapter":"22","verse":"3","text":"Asi imi makagadzwa saIye Mutsvene;"},{"book":"Mapisarema","chapter":"22","verse":"4","text":"Madzibaba edu akavimba nemi;"},{"book":"Mapisarema","chapter":"22","verse":"5","text":"Vakachema kwamuri, vakaponeswa;"},{"book":"Mapisarema","chapter":"22","verse":"6","text":"Asi ini ndiri honye kwete munhu,"},{"book":"Mapisarema","chapter":"22","verse":"7","text":"Vose vanondiona vanondiseka;"},{"book":"Mapisarema","chapter":"22","verse":"8","text":"â€œAnovimba naJehovha;"},{"book":"Mapisarema","chapter":"22","verse":"9","text":"Asi ndimi makandibudisa mudumbu;"},{"book":"Mapisarema","chapter":"22","verse":"10","text":"Kubva pakuzvarwa kwangu, ndakaiswa kwamuri;"},{"book":"Mapisarema","chapter":"22","verse":"11","text":"Regai kuva kure neni,"},{"book":"Mapisarema","chapter":"22","verse":"12","text":"Hando zhinji dzakandikomba;"},{"book":"Mapisarema","chapter":"22","verse":"13","text":"Shumba dzinoomba dzichibvambura nyama,"},{"book":"Mapisarema","chapter":"22","verse":"14","text":"Ndadururwa semvura,"},{"book":"Mapisarema","chapter":"22","verse":"15","text":"Simba rangu raoma sechaenga,"},{"book":"Mapisarema","chapter":"22","verse":"16","text":"Imbwa dzakandikomba;"},{"book":"Mapisarema","chapter":"22","verse":"17","text":"Ndinokwanisa kuverenga mapfupa angu ose;"},{"book":"Mapisarema","chapter":"22","verse":"18","text":"Vanogovana nguo dzangu pakati pavo"},{"book":"Mapisarema","chapter":"22","verse":"19","text":"Asi imi, Jehovha, regai kuva kure neni;"},{"book":"Mapisarema","chapter":"22","verse":"20","text":"Rwirai upenyu hwangu pamunondo,"},{"book":"Mapisarema","chapter":"22","verse":"21","text":"Ndinunurei pamuromo weshumba;"},{"book":"Mapisarema","chapter":"22","verse":"22","text":"Ndichaparidza zita renyu kuhama dzangu;"},{"book":"Mapisarema","chapter":"22","verse":"23","text":"Imi munotya Jehovha, murumbidzei!"},{"book":"Mapisarema","chapter":"22","verse":"24","text":"Nokuti haana kuzvidza kana kusema"},{"book":"Mapisarema","chapter":"22","verse":"25","text":"Kwamuri ndiko kunobva dingindira rerumbidzo yangu paungano huru;"},{"book":"Mapisarema","chapter":"22","verse":"26","text":"Varombo vachadya uye vagoguta;"},{"book":"Mapisarema","chapter":"22","verse":"27","text":"Migumo yose yenyika"},{"book":"Mapisarema","chapter":"22","verse":"28","text":"nokuti ushe ndohwaJehovha"},{"book":"Mapisarema","chapter":"22","verse":"29","text":"Vapfumi vose venyika vachadya"},{"book":"Mapisarema","chapter":"22","verse":"30","text":"Vana vake vachamushumira;"},{"book":"Mapisarema","chapter":"22","verse":"31","text":"Vachaparidza kururama kwake"}]}