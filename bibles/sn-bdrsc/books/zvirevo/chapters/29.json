{"data":[{"book":"Zvirevo","chapter":"29","verse":"1","text":"Munhu anoramba akaomesa mutsipa wake mushure mokutsiurwa"},{"book":"Zvirevo","chapter":"29","verse":"2","text":"Kana vakarurama vachiwanda, vanhu vanofara;"},{"book":"Zvirevo","chapter":"29","verse":"3","text":"Munhu anoda uchenjeri anouyisa mufaro kuna baba vake,"},{"book":"Zvirevo","chapter":"29","verse":"4","text":"Nokururamisira mambo anosimbisa nyika,"},{"book":"Zvirevo","chapter":"29","verse":"5","text":"Ani naani anonyengera muvakidzani wake"},{"book":"Zvirevo","chapter":"29","verse":"6","text":"Munhu akaipa anoteyiwa nezvivi zvake,"},{"book":"Zvirevo","chapter":"29","verse":"7","text":"Vakarurama vane hanya nokururamisirwa kwavarombo,"},{"book":"Zvirevo","chapter":"29","verse":"8","text":"Vatuki vanomutsa bope muguta,"},{"book":"Zvirevo","chapter":"29","verse":"9","text":"Kana munhu akachenjera akaenda kumatare nebenzi,"},{"book":"Zvirevo","chapter":"29","verse":"10","text":"Vanhu vanofarira kudeura ropa vanovenga munhu akarurama,"},{"book":"Zvirevo","chapter":"29","verse":"11","text":"Benzi rinobudisa hasha dzaro dzose,"},{"book":"Zvirevo","chapter":"29","verse":"12","text":"Kana mutongi akateerera nhema"},{"book":"Zvirevo","chapter":"29","verse":"13","text":"Murombo nomunhu anomanikidza vakafanana pachinhu ichi:"},{"book":"Zvirevo","chapter":"29","verse":"14","text":"Kana mambo akatonga varombo nokururamisira,"},{"book":"Zvirevo","chapter":"29","verse":"15","text":"Shamhu yokuranga inopa uchenjeri,"},{"book":"Zvirevo","chapter":"29","verse":"16","text":"Kana vakaipa vachiwanda, nezvivi zvinowandawo,"},{"book":"Zvirevo","chapter":"29","verse":"17","text":"Ranga mwanakomana wako, ipapo achakupa rugare;"},{"book":"Zvirevo","chapter":"29","verse":"18","text":"Pasina chizaruro, vanhu vanoramba kuzvidzora;"},{"book":"Zvirevo","chapter":"29","verse":"19","text":"Muranda haarayirwi namashoko zvawo chete,"},{"book":"Zvirevo","chapter":"29","verse":"20","text":"Unoona here munhu anotaura achikurumidza?"},{"book":"Zvirevo","chapter":"29","verse":"21","text":"Kana munhu akaregerera muranda wake kubva paudiki,"},{"book":"Zvirevo","chapter":"29","verse":"22","text":"Munhu akashatirwa anomutsa kupesana,"},{"book":"Zvirevo","chapter":"29","verse":"23","text":"Kuzvikudza kwomunhu kunomudzikisira pasi,"},{"book":"Zvirevo","chapter":"29","verse":"24","text":"Anoshamwaridzana nembavha anozvivenga iye pachake;"},{"book":"Zvirevo","chapter":"29","verse":"25","text":"Kutya munhu kuchava musungo,"},{"book":"Zvirevo","chapter":"29","verse":"26","text":"Vazhinji vanotsvaka nyasha kumutongi,"},{"book":"Zvirevo","chapter":"29","verse":"27","text":"Vakarurama vanovenga vasina kutendeka;"}]}