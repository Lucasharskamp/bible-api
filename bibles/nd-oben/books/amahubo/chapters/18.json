{"data":[{"book":"AmaHubo","chapter":"18","verse":"1","text":"Ngiyakuthanda, Oh Thixo, Mandla ami."},{"book":"AmaHubo","chapter":"18","verse":"2","text":"UThixo ulidwala lami, inqaba yami, lomsindisi wami;"},{"book":"AmaHubo","chapter":"18","verse":"3","text":"Ngakhuleka kuThixo ofanele ukudunyiswa"},{"book":"AmaHubo","chapter":"18","verse":"4","text":"Izibopho zokufa zangihilela;"},{"book":"AmaHubo","chapter":"18","verse":"5","text":"Izibopho zengcwaba zangithandela;"},{"book":"AmaHubo","chapter":"18","verse":"6","text":"Ekukhathazekeni kwami ngakhuleka kuThixo;"},{"book":"AmaHubo","chapter":"18","verse":"7","text":"Umhlaba wagedezela wazamazama,"},{"book":"AmaHubo","chapter":"18","verse":"8","text":"Intuthu yalanquka emakhaleni Akhe"},{"book":"AmaHubo","chapter":"18","verse":"9","text":"Waqhekeza amazulu wehlela phansi;"},{"book":"AmaHubo","chapter":"18","verse":"10","text":"Wagada amakherubhi waphapha;"},{"book":"AmaHubo","chapter":"18","verse":"11","text":"Wenza umnyama isembeso sakhe,"},{"book":"AmaHubo","chapter":"18","verse":"12","text":"Phakathi kokukhazimula kobukhona bakhe"},{"book":"AmaHubo","chapter":"18","verse":"13","text":"UThixo wakhwaza esezulwini;"},{"book":"AmaHubo","chapter":"18","verse":"14","text":"Waciba imitshoko wazihlakaza izitha,"},{"book":"AmaHubo","chapter":"18","verse":"15","text":"Izigodi zolwandle zambulwa"},{"book":"AmaHubo","chapter":"18","verse":"16","text":"Welula isandla Sakhe esezulwini"},{"book":"AmaHubo","chapter":"18","verse":"17","text":"Wangihlenga esitheni sami esilamandla,"},{"book":"AmaHubo","chapter":"18","verse":"18","text":"Zangijamela mhla ngisebunzimeni,"},{"book":"AmaHubo","chapter":"18","verse":"19","text":"Wangikhupha wangibeka endaweni ebanzi"},{"book":"AmaHubo","chapter":"18","verse":"20","text":"UThixo ungenzele okulingene ukulunga kwami;"},{"book":"AmaHubo","chapter":"18","verse":"21","text":"Ngoba ngizigcinile izindlela zikaThixo;"},{"book":"AmaHubo","chapter":"18","verse":"22","text":"Yonke imithetho yakhe ibiphambi kwami;"},{"book":"AmaHubo","chapter":"18","verse":"23","text":"Ngibe ngilokhu ngimsulwa phambi kwakhe"},{"book":"AmaHubo","chapter":"18","verse":"24","text":"UThixo unginike umvuzo ngokufanele ukulunga kwami,"},{"book":"AmaHubo","chapter":"18","verse":"25","text":"Kwabathembekileyo lawe uthembekile,"},{"book":"AmaHubo","chapter":"18","verse":"26","text":"Kwabahlanzekileyo uziveza uhlanzekile"},{"book":"AmaHubo","chapter":"18","verse":"27","text":"Uyabasindisa abathobekileyo;"},{"book":"AmaHubo","chapter":"18","verse":"28","text":"Wena, Oh Thixo, gcina isibane sami sibhebha;"},{"book":"AmaHubo","chapter":"18","verse":"29","text":"Ngosizo lwakho ngingaphuma ngimelane lebutho lempi;"},{"book":"AmaHubo","chapter":"18","verse":"30","text":"Izindlela zikaNkulunkulu uqobo lwakhe ziphelele;"},{"book":"AmaHubo","chapter":"18","verse":"31","text":"Ngoba ngubani uNkulunkulu ngaphandle kukaThixo?"},{"book":"AmaHubo","chapter":"18","verse":"32","text":"NguNkulunkulu ongihlomisa amandla"},{"book":"AmaHubo","chapter":"18","verse":"33","text":"Wenza inyawo zami zibe njengezempala;"},{"book":"AmaHubo","chapter":"18","verse":"34","text":"Ufundisa izandla zami ukulwa impi;"},{"book":"AmaHubo","chapter":"18","verse":"35","text":"Wena uyanginika isihlangu sakho sokunqoba,"},{"book":"AmaHubo","chapter":"18","verse":"36","text":"Uqhelisa indlela enginyathela kuyo,"},{"book":"AmaHubo","chapter":"18","verse":"37","text":"Ngaxotshana lezitha zami ngazifica;"},{"book":"AmaHubo","chapter":"18","verse":"38","text":"Ngazichoboza, zazingeke zisavuka;"},{"book":"AmaHubo","chapter":"18","verse":"39","text":"Wangihlomisa ngamandla okulwa impi;"},{"book":"AmaHubo","chapter":"18","verse":"40","text":"Wenza izitha zami zafulathela zabaleka,"},{"book":"AmaHubo","chapter":"18","verse":"41","text":"Bakhala becela uncedo, kodwa wayengekho owayengabalamulela"},{"book":"AmaHubo","chapter":"18","verse":"42","text":"Ngabatshaya bacholeka njengothuli luphetshulwa ngumoya;"},{"book":"AmaHubo","chapter":"18","verse":"43","text":"Ungikhulule ekuhlaselweni yilababantu;"},{"book":"AmaHubo","chapter":"18","verse":"44","text":"Bathi bengizwa nje ngendaba, bangilalele;"},{"book":"AmaHubo","chapter":"18","verse":"45","text":"Bonke badelile;"},{"book":"AmaHubo","chapter":"18","verse":"46","text":"Uthixo uyaphila!"},{"book":"AmaHubo","chapter":"18","verse":"47","text":"UnguNkulunkulu ongiphindiselelayo,"},{"book":"AmaHubo","chapter":"18","verse":"48","text":"ongisindisayo ezitheni zami."},{"book":"AmaHubo","chapter":"18","verse":"49","text":"Ngakho ngizakudumisa phakathi kwezizwe, Oh Thixo;"},{"book":"AmaHubo","chapter":"18","verse":"50","text":"Upha inkosi yakhe ukunqoba okukhulu;"}]}