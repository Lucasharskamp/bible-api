{"data":[{"book":"AmaHubo","chapter":"55","verse":"1","text":"Lalela umkhuleko wami, Oh Nkulunkulu,"},{"book":"AmaHubo","chapter":"55","verse":"2","text":"ngizwa ungiphendule."},{"book":"AmaHubo","chapter":"55","verse":"3","text":"ngelizwi lesitha sami,"},{"book":"AmaHubo","chapter":"55","verse":"4","text":"Inhliziyo yami iyatshiseka ngaphakathi kwami;"},{"book":"AmaHubo","chapter":"55","verse":"5","text":"Ukwesaba lokuthuthumela sekungihlasele;"},{"book":"AmaHubo","chapter":"55","verse":"6","text":"Ngathi, â€œHe, aluba ngangilamaphiko njengejuba!"},{"book":"AmaHubo","chapter":"55","verse":"7","text":"Ngangizabaleka ngiye kude"},{"book":"AmaHubo","chapter":"55","verse":"8","text":"ngangizaphanga ngiye enqabeni yami,"},{"book":"AmaHubo","chapter":"55","verse":"9","text":"Sanganisa ababi, Oh Thixo,"},{"book":"AmaHubo","chapter":"55","verse":"10","text":"Emini lebusuku bayacathama emidulini yalo;"},{"book":"AmaHubo","chapter":"55","verse":"11","text":"Imimoya yokubhidliza yiyo esibusa;"},{"book":"AmaHubo","chapter":"55","verse":"12","text":"Aluba ngithukwa yisitha"},{"book":"AmaHubo","chapter":"55","verse":"13","text":"Kodwa nguwe, umuntu njengami,"},{"book":"AmaHubo","chapter":"55","verse":"14","text":"engake ngaba lobudlelwano obukhulu laye"},{"book":"AmaHubo","chapter":"55","verse":"15","text":"Yekela ukufa kuzijume izitha zami;"},{"book":"AmaHubo","chapter":"55","verse":"16","text":"Kodwa ngiyambiza uNkulunkulu,"},{"book":"AmaHubo","chapter":"55","verse":"17","text":"Kusihlwa, ekuseni lemini"},{"book":"AmaHubo","chapter":"55","verse":"18","text":"Uyangihlenga ngingakalimali"},{"book":"AmaHubo","chapter":"55","verse":"19","text":"UNkulunkulu osesihlalweni sobukhosi lanininini,"},{"book":"AmaHubo","chapter":"55","verse":"20","text":"Umkhula wami uyabahlasela abangane bakhe;"},{"book":"AmaHubo","chapter":"55","verse":"21","text":"Inkulumo yakhe ibutshelezi njengolaza,"},{"book":"AmaHubo","chapter":"55","verse":"22","text":"Yethula inswelo zakho kuThixo"},{"book":"AmaHubo","chapter":"55","verse":"23","text":"Kodwa wena, Nkulunkulu"}]}