{"data":[{"book":"Zabuli","chapter":"78","verse":"1","text":"Mweye wanhu zangu, hulikeni mafundizo gangu,"},{"book":"Zabuli","chapter":"78","verse":"2","text":"Nogubula mulomo wangu kwa mzungu,"},{"book":"Zabuli","chapter":"78","verse":"3","text":"gaja gatuhulike na kugatanga,"},{"book":"Zabuli","chapter":"78","verse":"4","text":"Hatugafisa kwa wanao,"},{"book":"Zabuli","chapter":"78","verse":"5","text":"Kalava amuli kwa Yakobo,"},{"book":"Zabuli","chapter":"78","verse":"6","text":"ili iwanhu wa ulelo ukwiza wagajuwe,"},{"book":"Zabuli","chapter":"78","verse":"7","text":"Aho okwika tamanilo jao kwa Imulungu,"},{"book":"Zabuli","chapter":"78","verse":"8","text":"Na sikuwaigale na iwahenga zao,"},{"book":"Zabuli","chapter":"78","verse":"9","text":"Kamba iwanhu wa Efulaimu hamoja wagoga upinde,"},{"book":"Zabuli","chapter":"78","verse":"10","text":"Hawadigogile lagano da Imulungu,"},{"book":"Zabuli","chapter":"78","verse":"11","text":"Waisemwa gaja gayawatendele,"},{"book":"Zabuli","chapter":"78","verse":"12","text":"Katenda unzonza mgameso ga iwahenga zao,"},{"book":"Zabuli","chapter":"78","verse":"13","text":"Kaigola bahali na kawalongoza mbaka wabita,"},{"book":"Zabuli","chapter":"78","verse":"14","text":"Kawalongoza kwa wingu imisi,"},{"book":"Zabuli","chapter":"78","verse":"15","text":"Katula mibwe kuluwala,"},{"book":"Zabuli","chapter":"78","verse":"16","text":"katenda mbwibwi kulawa mdibwe,"},{"book":"Zabuli","chapter":"78","verse":"17","text":"Lakini wagendelela kumtendela nzambi,"},{"book":"Zabuli","chapter":"78","verse":"18","text":"Wamgeza Imulungu giladi,"},{"book":"Zabuli","chapter":"78","verse":"19","text":"Wamdeta Imulungu, walonga,"},{"book":"Zabuli","chapter":"78","verse":"20","text":"Ona, katowa dibwe na mazi galawa,"},{"book":"Zabuli","chapter":"78","verse":"21","text":"Mndewa Imulungu viyawahulike kagevuzika ng'hani,"},{"book":"Zabuli","chapter":"78","verse":"22","text":"kwavija hawamtamanile Imulungu,"},{"book":"Zabuli","chapter":"78","verse":"23","text":"Avo, kalagiza ulanga uli uchanha,"},{"book":"Zabuli","chapter":"78","verse":"24","text":"kawalagazila mana kamba ndiya,"},{"book":"Zabuli","chapter":"78","verse":"25","text":"Wanhu waja ndiya ya wasenga wa kuulanga,"},{"book":"Zabuli","chapter":"78","verse":"26","text":"Kagala beho da umwambu wa kudilawila zuwa kulawa kuulanga,"},{"book":"Zabuli","chapter":"78","verse":"27","text":"Kawalagazila nyama kamba timbwisi,"},{"book":"Zabuli","chapter":"78","verse":"28","text":"Kawatenda wagwe mng'hambi yao,"},{"book":"Zabuli","chapter":"78","verse":"29","text":"Wanhu waja na kusigasa,"},{"book":"Zabuli","chapter":"78","verse":"30","text":"Wang'hali hawana mambukiza kuja acho kiwasulukile,"},{"book":"Zabuli","chapter":"78","verse":"31","text":"Imulungu kawagevuzikila,"},{"book":"Zabuli","chapter":"78","verse":"32","text":"Hamoja ago gose galawila,"},{"book":"Zabuli","chapter":"78","verse":"33","text":"Avo kazitenda siku zao zilawanye kamba mhumuzi,"},{"book":"Zabuli","chapter":"78","verse":"34","text":"Viyawakomile wamzahila,"},{"book":"Zabuli","chapter":"78","verse":"35","text":"Wakumbuka kamba Imulungu kalala dibwe jao,"},{"book":"Zabuli","chapter":"78","verse":"36","text":"Lakini wamvizila kwa milomo yao,"},{"book":"Zabuli","chapter":"78","verse":"37","text":"mizoyo yao haimgogile heyo,"},{"book":"Zabuli","chapter":"78","verse":"38","text":"Hata ivo heyo kakala na bazi,"},{"book":"Zabuli","chapter":"78","verse":"39","text":"Kakumbuka kamba hewo wakala wanhu tu,"},{"book":"Zabuli","chapter":"78","verse":"40","text":"Miyanza mingi wamulema kuluwala,"},{"book":"Zabuli","chapter":"78","verse":"41","text":"Wamgeza Imulungu kabili na kabili,"},{"book":"Zabuli","chapter":"78","verse":"42","text":"Waisemwa nguvu zake,"},{"book":"Zabuli","chapter":"78","verse":"43","text":"siku ayo viyatendile vilaguso vake ako Misili,"},{"book":"Zabuli","chapter":"78","verse":"44","text":"Kagalusa vizanda vao viwa damu,"},{"book":"Zabuli","chapter":"78","verse":"45","text":"Kawagalila mikonzi iwagaze,"},{"book":"Zabuli","chapter":"78","verse":"46","text":"Kailava ndiya yao inangigwe na mbalangulu,"},{"book":"Zabuli","chapter":"78","verse":"47","text":"Kabananga mizabibu yao kwa mvula ya mabwe,"},{"book":"Zabuli","chapter":"78","verse":"48","text":"Kawatowa ng'ombe wao kwa mvula ya mabwe,"},{"book":"Zabuli","chapter":"78","verse":"49","text":"Kawakutulila ng'hasiliki yake,"},{"book":"Zabuli","chapter":"78","verse":"50","text":"Kaitendela nzila ng'hasiliki yake,"},{"book":"Zabuli","chapter":"78","verse":"51","text":"Kawakoboza iwana chaudele wose wa Misili,"},{"book":"Zabuli","chapter":"78","verse":"52","text":"Lakini kawalava iwanhu zake kamba fugo,"},{"book":"Zabuli","chapter":"78","verse":"53","text":"Kawalongoza goya na hawadumbile,"},{"book":"Zabuli","chapter":"78","verse":"54","text":"Kawagala mbaka kuna imbaka ya isi yake ing'alile,"},{"book":"Zabuli","chapter":"78","verse":"55","text":"Kawawinga iwanhu wa isi ziyagwe kuulongozi wao,"},{"book":"Zabuli","chapter":"78","verse":"56","text":"Lakini hewo wamgeza Imulungu,"},{"book":"Zabuli","chapter":"78","verse":"57","text":"Wahinduka kuyagwe na hawatamaniligwe kamba iwahenga zao,"},{"book":"Zabuli","chapter":"78","verse":"58","text":"Wamgevuza Imulungu kwa kuzenga hanhu ha kutambikila imilungu kuvigongo,"},{"book":"Zabuli","chapter":"78","verse":"59","text":"Imulungu viyawahulike kagevuzika ng'hani,"},{"book":"Zabuli","chapter":"78","verse":"60","text":"Kadileka dihema ding'alile dikalile ako Shilo,"},{"book":"Zabuli","chapter":"78","verse":"61","text":"Katogola disanduku da lagano disoligwe,"},{"book":"Zabuli","chapter":"78","verse":"62","text":"Kawaleka iwanhu zake wakomigwe kwa sime,"},{"book":"Zabuli","chapter":"78","verse":"63","text":"Moto uwasakaza iwabwanga wao,"},{"book":"Zabuli","chapter":"78","verse":"64","text":"Iwalava nhosa wao wakomigwa kwa sime,"},{"book":"Zabuli","chapter":"78","verse":"65","text":"Abaho Mndewa kalamuka kamba munhu yawasile utulo,"},{"book":"Zabuli","chapter":"78","verse":"66","text":"Kawatowa na kawahuma iwanhu omwihila,"},{"book":"Zabuli","chapter":"78","verse":"67","text":"Abaho kadilema hema da Yusufu,"},{"book":"Zabuli","chapter":"78","verse":"68","text":"lakini kadisagula kabila da Yuda,"},{"book":"Zabuli","chapter":"78","verse":"69","text":"Kazenga hanhu hake hong'ala kamba ulanga,"},{"book":"Zabuli","chapter":"78","verse":"70","text":"Kamsagula Daudi msang'hanaji wake,"},{"book":"Zabuli","chapter":"78","verse":"71","text":"kulawa kususila ng'hondolo kamgala yawe msusilaji wa iwanhu wa Yakobo,"},{"book":"Zabuli","chapter":"78","verse":"72","text":"Na Daudi kawasusila kwa moyo unogile,"}]}