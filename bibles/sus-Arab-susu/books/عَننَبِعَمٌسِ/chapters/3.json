{"data":[{"book":"عَننَبِ عَمٌسِ","chapter":"3","verse":"1","text":"وٌ وٌ تُلِ مَتِ يِ مَسٍنيِ رَ،"},{"book":"عَننَبِ عَمٌسِ","chapter":"3","verse":"2","text":"«ﭑ وٌ تَن نَن فبَنسَن سُفَندِ دُنِحَ شَبِلّيٍ بِرِن يَ مَ."},{"book":"عَننَبِ عَمٌسِ","chapter":"3","verse":"3","text":"مِشِ قِرِن نْمَ حّرّ رَلَندٍ"},{"book":"عَننَبِ عَمٌسِ","chapter":"3","verse":"4","text":"يّتّ عَ شُي رَ مِنِمَ وُلَ عِ"},{"book":"عَننَبِ عَمٌسِ","chapter":"3","verse":"5","text":"شْنِ فٌرٌ مَ يّلّ كُي"},{"book":"عَننَبِ عَمٌسِ","chapter":"3","verse":"6","text":"قٍرِ قٍمَ تَا ندٍ كُي حَمَ كُي مُ عِقُ؟"},{"book":"عَننَبِ عَمٌسِ","chapter":"3","verse":"7","text":"مَرِفِ عَلَتَلَ مُ قٍقٍ رَبَمَ،"},{"book":"عَننَبِ عَمٌسِ","chapter":"3","verse":"8","text":"يّتّ نَ عَ شُي رَ مِنِ، ندٍ مُ فَاشُمَ؟"},{"book":"عَننَبِ عَمٌسِ","chapter":"3","verse":"9","text":"وٌ شَ يِ قَلَ عَسِدٌدِ يِرٍ مَكَنتَشِيٍ كُي،"},{"book":"عَننَبِ عَمٌسِ","chapter":"3","verse":"10","text":"عَلَتَلَ شَ مَسٍنيِ نَن يَ؛"},{"book":"عَننَبِ عَمٌسِ","chapter":"3","verse":"11","text":"مَرِفِ عَلَتَلَ نَشَن مَسٍن، نَ نَن يِ كِ."},{"book":"عَننَبِ عَمٌسِ","chapter":"3","verse":"12","text":"عَلَتَلَ نَشَ عَ مَسٍن،"},{"book":"عَننَبِ عَمٌسِ","chapter":"3","verse":"13","text":"مَنفّ عَلَتَلَ شَ مَسٍنيِ نَن يَ؛"},{"book":"عَننَبِ عَمٌسِ","chapter":"3","verse":"14","text":"ﭑ نَ عِسِرَيِلَ حَشَنكَتَ"},{"book":"عَننَبِ عَمٌسِ","chapter":"3","verse":"15","text":"ﭑ مَن عٍ شَ بَنشِ تٌقَنيِيٍ رَبِرَ مَ نّ،"}]}