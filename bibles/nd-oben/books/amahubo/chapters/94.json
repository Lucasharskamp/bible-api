{"data":[{"book":"AmaHubo","chapter":"94","verse":"1","text":"Oh Thixo, uNkulunkulu ophindiselayo,"},{"book":"AmaHubo","chapter":"94","verse":"2","text":"Phakama, awu Mahluli womhlaba;"},{"book":"AmaHubo","chapter":"94","verse":"3","text":"Koze kube nini ababi, Oh Thixo,"},{"book":"AmaHubo","chapter":"94","verse":"4","text":"Bathulula amazwi okukloloda;"},{"book":"AmaHubo","chapter":"94","verse":"5","text":"Ziyabanyathezela abantu bakho, Oh Thixo;"},{"book":"AmaHubo","chapter":"94","verse":"6","text":"Zibulala umfelokazi kanye lowezizweni;"},{"book":"AmaHubo","chapter":"94","verse":"7","text":"Zithi, “UThixo kaboni;"},{"book":"AmaHubo","chapter":"94","verse":"8","text":"Limukani, lina elingelangqondo phakathi kwabantu;"},{"book":"AmaHubo","chapter":"94","verse":"9","text":"Yena lowo owayigxumekayo indlebe kezwa yini?"},{"book":"AmaHubo","chapter":"94","verse":"10","text":"Yena lowo oqondisa izizwe kajezisi yini?"},{"book":"AmaHubo","chapter":"94","verse":"11","text":"UThixo uyayazi imicabango yomuntu;"},{"book":"AmaHubo","chapter":"94","verse":"12","text":"Ubusisiwe umuntu omqondisayo, Oh Thixo,"},{"book":"AmaHubo","chapter":"94","verse":"13","text":"uyamphumuza ensukwini zokuhlupheka,"},{"book":"AmaHubo","chapter":"94","verse":"14","text":"Ngoba uThixo kazukwala abantu bakhe;"},{"book":"AmaHubo","chapter":"94","verse":"15","text":"Ukwahlulela kuzaphinda njalo kumiswe ngokulunga,"},{"book":"AmaHubo","chapter":"94","verse":"16","text":"Ngubani ozangimela ekulweni lababi na?"},{"book":"AmaHubo","chapter":"94","verse":"17","text":"Aluba uThixo wayengangelulekanga,"},{"book":"AmaHubo","chapter":"94","verse":"18","text":"Lapho ngasengisithi, “Unyawo lwami selutshelela,”"},{"book":"AmaHubo","chapter":"94","verse":"19","text":"Nxa ukunqineka kwase kungikhulela,"},{"book":"AmaHubo","chapter":"94","verse":"20","text":"Kambe isihlalo sobukhosi esixhwalileyo singema lawe yini"},{"book":"AmaHubo","chapter":"94","verse":"21","text":"Bayahlangana ukumelana labalungileyo"},{"book":"AmaHubo","chapter":"94","verse":"22","text":"Kodwa uThixo useyinqaba yami,"},{"book":"AmaHubo","chapter":"94","verse":"23","text":"Uzabaphindisela ngenxa yezono zabo"}]}