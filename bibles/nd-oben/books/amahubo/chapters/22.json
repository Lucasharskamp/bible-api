{"data":[{"book":"AmaHubo","chapter":"22","verse":"1","text":"Nkulunkulu wami, Nkulunkulu wami, kungani usungidelile na?"},{"book":"AmaHubo","chapter":"22","verse":"2","text":"Oh Nkulunkulu wami, ngiyakhala emini,"},{"book":"AmaHubo","chapter":"22","verse":"3","text":"Kanti usesihlalweni sobukhosi uNguye oNgcwele"},{"book":"AmaHubo","chapter":"22","verse":"4","text":"Obaba babeka ithemba labo Kuwe;"},{"book":"AmaHubo","chapter":"22","verse":"5","text":"Bakhala kuwe bakhululwa;"},{"book":"AmaHubo","chapter":"22","verse":"6","text":"Kodwa ngiyimpethu nje kangisumuntu,"},{"book":"AmaHubo","chapter":"22","verse":"7","text":"Bonke abangibonayo bayangihleka;"},{"book":"AmaHubo","chapter":"22","verse":"8","text":"â€œUthemba uThixo"},{"book":"AmaHubo","chapter":"22","verse":"9","text":"Kanti wena wangikhupha esiswini;"},{"book":"AmaHubo","chapter":"22","verse":"10","text":"Kusukela ekuzalweni kwami ngabekwa ezandleni Zakho;"},{"book":"AmaHubo","chapter":"22","verse":"11","text":"Ungabi khatshana kwami,"},{"book":"AmaHubo","chapter":"22","verse":"12","text":"Inkunzi ezinengi zingihanqile;"},{"book":"AmaHubo","chapter":"22","verse":"13","text":"Izilwane ezibhongayo ezibamba zidabudabule"},{"book":"AmaHubo","chapter":"22","verse":"14","text":"Ngiyathululeka njengamanzi,"},{"book":"AmaHubo","chapter":"22","verse":"15","text":"Amandla ami asetshile qhu ingathi ludengezi,"},{"book":"AmaHubo","chapter":"22","verse":"16","text":"Izinja sezingihanqile;"},{"book":"AmaHubo","chapter":"22","verse":"17","text":"Ngingawabala wonke amathambo ami;"},{"book":"AmaHubo","chapter":"22","verse":"18","text":"Behlukaniselana izembatho zami"},{"book":"AmaHubo","chapter":"22","verse":"19","text":"Kodwa wena, Thixo, ungabi kude lami."},{"book":"AmaHubo","chapter":"22","verse":"20","text":"Phephisa impilo yami enkembeni,"},{"book":"AmaHubo","chapter":"22","verse":"21","text":"Ngilamulela emlonyeni wezilwane;"},{"book":"AmaHubo","chapter":"22","verse":"22","text":"Ngizamemezela ibizo Lakho kubafowethu;"},{"book":"AmaHubo","chapter":"22","verse":"23","text":"Lina elimesabayo uThixo, mdumiseni!"},{"book":"AmaHubo","chapter":"22","verse":"24","text":"Ngoba keyisanga loba adelele"},{"book":"AmaHubo","chapter":"22","verse":"25","text":"Sivela kuwe isithathelo sokudumisa kwami emhlanganweni omkhulu;"},{"book":"AmaHubo","chapter":"22","verse":"26","text":"Abayanga bazakudla basuthe"},{"book":"AmaHubo","chapter":"22","verse":"27","text":"Yonke imikhawulo yomhlaba"},{"book":"AmaHubo","chapter":"22","verse":"28","text":"ngoba umbuso ungokaThixo"},{"book":"AmaHubo","chapter":"22","verse":"29","text":"Zonke izinothi zomhlaba zizazitika ngokudla"},{"book":"AmaHubo","chapter":"22","verse":"30","text":"Abesikhathi esizayo bazamkhonza;"},{"book":"AmaHubo","chapter":"22","verse":"31","text":"Bazatshumayela ukulunga kwakhe,"}]}