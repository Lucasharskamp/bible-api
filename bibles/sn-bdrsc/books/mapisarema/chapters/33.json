{"data":[{"book":"Mapisarema","chapter":"33","verse":"1","text":"Imbirai Jehovha, mufarisise, imi vakarurama;"},{"book":"Mapisarema","chapter":"33","verse":"2","text":"Rumbidzai Jehovha nembira;"},{"book":"Mapisarema","chapter":"33","verse":"3","text":"Muimbirei rwiyo rutsva;"},{"book":"Mapisarema","chapter":"33","verse":"4","text":"Nokuti shoko raJehovha rakarurama uye nderechokwadi;"},{"book":"Mapisarema","chapter":"33","verse":"5","text":"Jehovha anoda kururama nokururamisira;"},{"book":"Mapisarema","chapter":"33","verse":"6","text":"Kudenga denga kwakaitwa neshoko raJehovha,"},{"book":"Mapisarema","chapter":"33","verse":"7","text":"Anounganidza mvura zhinji yegungwa muzvirongo;"},{"book":"Mapisarema","chapter":"33","verse":"8","text":"Nyika yose ngaitye Jehovha;"},{"book":"Mapisarema","chapter":"33","verse":"9","text":"Nokuti iye akataura, zvikaitika;"},{"book":"Mapisarema","chapter":"33","verse":"10","text":"Jehovha anokonesa urongwa hwendudzi;"},{"book":"Mapisarema","chapter":"33","verse":"11","text":"Asi urongwa hwaJehovha hunomira hwakasimba nokusingaperi,"},{"book":"Mapisarema","chapter":"33","verse":"12","text":"Rwakaropafadzwa rudzi rwakaita Jehovha Mwari warwo,"},{"book":"Mapisarema","chapter":"33","verse":"13","text":"Jehovha anotarira pasi ari kudenga"},{"book":"Mapisarema","chapter":"33","verse":"14","text":"ari pachigaro chake anotarira"},{"book":"Mapisarema","chapter":"33","verse":"15","text":"iye anoumba mwoyo yavanhu vose,"},{"book":"Mapisarema","chapter":"33","verse":"16","text":"Hapana mambo anoponeswa nokukura kwehondo yake;"},{"book":"Mapisarema","chapter":"33","verse":"17","text":"Bhiza itariro isina maturo pakurwirwa;"},{"book":"Mapisarema","chapter":"33","verse":"18","text":"Asi meso aJehovha ari pamusoro paavo vanomutya,"},{"book":"Mapisarema","chapter":"33","verse":"19","text":"kuti avarwire parufu"},{"book":"Mapisarema","chapter":"33","verse":"20","text":"Tinomirira Jehovha netariro;"},{"book":"Mapisarema","chapter":"33","verse":"21","text":"Mwoyo yedu inofara maari,"},{"book":"Mapisarema","chapter":"33","verse":"22","text":"Rudo rwenyu rusingaperi ngarugare pamusoro pedu, imi Jehovha,"}]}