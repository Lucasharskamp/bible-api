{"data":[{"book":"Mapisarema","chapter":"68","verse":"1","text":"Mwari ngaasimuke, vavengi vake ngavaparadzirwe;"},{"book":"Mapisarema","chapter":"68","verse":"2","text":"Sokupeperetswa kunoitwa utsi nemhepo,"},{"book":"Mapisarema","chapter":"68","verse":"3","text":"Asi vakarurama ngavafare,"},{"book":"Mapisarema","chapter":"68","verse":"4","text":"Imbirai Mwari, imbirai zita rake nziyo dzokurumbidza,"},{"book":"Mapisarema","chapter":"68","verse":"5","text":"Baba venherera, mudziviriri wechirikadzi,"},{"book":"Mapisarema","chapter":"68","verse":"6","text":"Vakanga vari voga Mwari akavagarisa mumhuri,"},{"book":"Mapisarema","chapter":"68","verse":"7","text":"Pamakatungamirira vanhu venyu, imi Mwari,"},{"book":"Mapisarema","chapter":"68","verse":"8","text":"nyika yakazungunuka,"},{"book":"Mapisarema","chapter":"68","verse":"9","text":"Imi Mwari, makanayisa mvura zhinji;"},{"book":"Mapisarema","chapter":"68","verse":"10","text":"Vanhu venyu vakagaramo,"},{"book":"Mapisarema","chapter":"68","verse":"11","text":"Ishe akataura shoko,"},{"book":"Mapisarema","chapter":"68","verse":"12","text":"“Madzimambo navarwi vakakurumidza kutiza;"},{"book":"Mapisarema","chapter":"68","verse":"13","text":"Kunyange pamunenge makavata pakati pemoto yemisasa,"},{"book":"Mapisarema","chapter":"68","verse":"14","text":"Wamasimba Ose paakaparadzira madzimambo munyika,"},{"book":"Mapisarema","chapter":"68","verse":"15","text":"Makomo eBhashani makomo oushe;"},{"book":"Mapisarema","chapter":"68","verse":"16","text":"Munotaririreiko negodo, imi makomo akati twi,"},{"book":"Mapisarema","chapter":"68","verse":"17","text":"Ngoro dzaMwari dzinosvika makumi ezviuru"},{"book":"Mapisarema","chapter":"68","verse":"18","text":"Pamakakwira kumusoro,"},{"book":"Mapisarema","chapter":"68","verse":"19","text":"Ishe ngaarumbidzwe, Mwari Muponesi wedu,"},{"book":"Mapisarema","chapter":"68","verse":"20","text":"Mwari wedu ndiMwari anoponesa;"},{"book":"Mapisarema","chapter":"68","verse":"21","text":"Zvirokwazvo Mwari achapwanya misoro yavavengi vake,"},{"book":"Mapisarema","chapter":"68","verse":"22","text":"Ishe anoti, “Ndichavabvisa kubva kuBhashani;"},{"book":"Mapisarema","chapter":"68","verse":"23","text":"kuti unyike tsoka dzako muropa ravavengi vako,"},{"book":"Mapisarema","chapter":"68","verse":"24","text":"Mudungwe wenyu wakaonekwa, imi Mwari,"},{"book":"Mapisarema","chapter":"68","verse":"25","text":"Mberi kuna vaimbi, vachiteverwa navaridzi vemitengeranwa;"},{"book":"Mapisarema","chapter":"68","verse":"26","text":"Rumbidzai Mwari paungano huru;"},{"book":"Mapisarema","chapter":"68","verse":"27","text":"Tarirai, rudzi ruduku rwaBhenjamini runovatungamirira,"},{"book":"Mapisarema","chapter":"68","verse":"28","text":"Danai simba renyu, imi Mwari;"},{"book":"Mapisarema","chapter":"68","verse":"29","text":"Nokuda kwetemberi yenyu paJerusarema,"},{"book":"Mapisarema","chapter":"68","verse":"30","text":"Tukai mhuka dziri pakati petsanga,"},{"book":"Mapisarema","chapter":"68","verse":"31","text":"Nhume dzichabva kuIjipiti;"},{"book":"Mapisarema","chapter":"68","verse":"32","text":"Imbirai Mwari, imi ushe hwepasi,"},{"book":"Mapisarema","chapter":"68","verse":"33","text":"kuna iye anotasva matenga akare kumusoro,"},{"book":"Mapisarema","chapter":"68","verse":"34","text":"Paridzai simba raMwari,"},{"book":"Mapisarema","chapter":"68","verse":"35","text":"Munotyisa imi Mwari, muri panzvimbo yenyu tsvene;"}]}