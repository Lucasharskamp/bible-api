{"data":[{"book":"Mapisarema","chapter":"103","verse":"1","text":"Rumbidza Jehovha, iwe mweya wangu;"},{"book":"Mapisarema","chapter":"103","verse":"2","text":"Rumbidza Jehovha, iwe mweya wangu,"},{"book":"Mapisarema","chapter":"103","verse":"3","text":"iye anokanganwira zvivi zvako zvose,"},{"book":"Mapisarema","chapter":"103","verse":"4","text":"anodzikinura upenyu hwako kubva pagomba,"},{"book":"Mapisarema","chapter":"103","verse":"5","text":"anogutsa zvido zvako nezvinhu zvakanaka,"},{"book":"Mapisarema","chapter":"103","verse":"6","text":"Jehovha anoita zvakarurama,"},{"book":"Mapisarema","chapter":"103","verse":"7","text":"Akazivisa nzira dzake kuna Mozisi,"},{"book":"Mapisarema","chapter":"103","verse":"8","text":"Jehovha ane tsitsi nenyasha,"},{"book":"Mapisarema","chapter":"103","verse":"9","text":"Haangarambi achipomera mhosva,"},{"book":"Mapisarema","chapter":"103","verse":"10","text":"haatiitiri sezvakafanira zvivi zvedu,"},{"book":"Mapisarema","chapter":"103","verse":"11","text":"Nokuti sokukwirira kwaakaita kudenga kumusoro kwenyika,"},{"book":"Mapisarema","chapter":"103","verse":"12","text":"sokuva kure kwamabvazuva namavirira,"},{"book":"Mapisarema","chapter":"103","verse":"13","text":"Sokunzwira tsitsi kunoita baba vana vavo,"},{"book":"Mapisarema","chapter":"103","verse":"14","text":"nokuti anoziva kuti takaumbwa sei,"},{"book":"Mapisarema","chapter":"103","verse":"15","text":"Kana ari munhu zvake, mazuva ake akaita souswa,"},{"book":"Mapisarema","chapter":"103","verse":"16","text":"mhepo inorova napamusoro paro onei raenda,"},{"book":"Mapisarema","chapter":"103","verse":"17","text":"Asi kubva pakusingaperi kusvikira pakusingaperi,"},{"book":"Mapisarema","chapter":"103","verse":"18","text":"naavo vanochengeta sungano yake"},{"book":"Mapisarema","chapter":"103","verse":"19","text":"Jehovha akasimbisa chigaro chake choushe kudenga,"},{"book":"Mapisarema","chapter":"103","verse":"20","text":"Rumbidzai Jehovha, imi vatumwa vake,"},{"book":"Mapisarema","chapter":"103","verse":"21","text":"Rumbidzai Jehovha, imi hondo dzose dzokudenga,"},{"book":"Mapisarema","chapter":"103","verse":"22","text":"Rumbidzai Jehovha, imi mabasa ake ose"}]}