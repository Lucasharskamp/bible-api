{"data":[{"book":"Zaburi","chapter":"105","verse":"1","text":"Mshukuruni Bwana, liitieni jina lake,"},{"book":"Zaburi","chapter":"105","verse":"2","text":"Mwimbieni yeye, mwimbieni yeye sifa,"},{"book":"Zaburi","chapter":"105","verse":"3","text":"Lishangilieni jina lake takatifu,"},{"book":"Zaburi","chapter":"105","verse":"4","text":"Mtafuteni Bwana na nguvu zake,"},{"book":"Zaburi","chapter":"105","verse":"5","text":"Kumbuka matendo ya ajabu aliyoyafanya,"},{"book":"Zaburi","chapter":"105","verse":"6","text":"enyi wazao wa Abrahamu mtumishi wake,"},{"book":"Zaburi","chapter":"105","verse":"7","text":"Yeye ndiye Bwana Mungu wetu,"},{"book":"Zaburi","chapter":"105","verse":"8","text":"Hulikumbuka agano lake milele,"},{"book":"Zaburi","chapter":"105","verse":"9","text":"agano alilolifanya na Abrahamu,"},{"book":"Zaburi","chapter":"105","verse":"10","text":"Alilithibitisha kwa Yakobo kuwa amri,"},{"book":"Zaburi","chapter":"105","verse":"11","text":"“Nitakupa wewe nchi ya Kanaani"},{"book":"Zaburi","chapter":"105","verse":"12","text":"Walipokuwa wachache kwa idadi,"},{"book":"Zaburi","chapter":"105","verse":"13","text":"walitangatanga kutoka taifa moja hadi jingine,"},{"book":"Zaburi","chapter":"105","verse":"14","text":"Hakuruhusu mtu yeyote awaonee;"},{"book":"Zaburi","chapter":"105","verse":"15","text":"“Msiwaguse niliowatia mafuta;"},{"book":"Zaburi","chapter":"105","verse":"16","text":"Akaiita njaa juu ya nchi"},{"book":"Zaburi","chapter":"105","verse":"17","text":"naye akatuma mtu mbele yao,"},{"book":"Zaburi","chapter":"105","verse":"18","text":"Walichubua miguu yake kwa minyororo,"},{"book":"Zaburi","chapter":"105","verse":"19","text":"hadi yale aliyotangulia kusema yalipotimia,"},{"book":"Zaburi","chapter":"105","verse":"20","text":"Mfalme alituma watu wakamfungua,"},{"book":"Zaburi","chapter":"105","verse":"21","text":"Alimfanya mkuu wa nyumba yake,"},{"book":"Zaburi","chapter":"105","verse":"22","text":"kuwaelekeza wakuu wa mfalme apendavyo"},{"book":"Zaburi","chapter":"105","verse":"23","text":"Kisha Israeli akaingia Misri,"},{"book":"Zaburi","chapter":"105","verse":"24","text":"Bwana aliwafanya watu wake kuzaana sana,"},{"book":"Zaburi","chapter":"105","verse":"25","text":"ndiye aliigeuza mioyo yao iwachukie watu wake,"},{"book":"Zaburi","chapter":"105","verse":"26","text":"Akamtuma Mose mtumishi wake,"},{"book":"Zaburi","chapter":"105","verse":"27","text":"Walifanya ishara zake za ajabu miongoni mwao,"},{"book":"Zaburi","chapter":"105","verse":"28","text":"Alituma giza na nchi ikajaa giza,"},{"book":"Zaburi","chapter":"105","verse":"29","text":"Aligeuza maji yao kuwa damu,"},{"book":"Zaburi","chapter":"105","verse":"30","text":"Nchi yao ilijaa vyura tele, ambao waliingia"},{"book":"Zaburi","chapter":"105","verse":"31","text":"Alisema, yakaja makundi ya mainzi,"},{"book":"Zaburi","chapter":"105","verse":"32","text":"Alibadilisha mvua yao ikawa mvua ya mawe,"},{"book":"Zaburi","chapter":"105","verse":"33","text":"akaharibu mizabibu yao na miti ya tini,"},{"book":"Zaburi","chapter":"105","verse":"34","text":"Alisema, nzige wakaja,"},{"book":"Zaburi","chapter":"105","verse":"35","text":"wakala kila jani katika nchi yao,"},{"book":"Zaburi","chapter":"105","verse":"36","text":"Kisha akawaua wazaliwa wote wa kwanza katika nchi yao,"},{"book":"Zaburi","chapter":"105","verse":"37","text":"Akawatoa Israeli katika nchi"},{"book":"Zaburi","chapter":"105","verse":"38","text":"Misri ilifurahi walipoondoka,"},{"book":"Zaburi","chapter":"105","verse":"39","text":"Alitandaza wingu kama kifuniko,"},{"book":"Zaburi","chapter":"105","verse":"40","text":"Waliomba, naye akawaletea kware,"},{"book":"Zaburi","chapter":"105","verse":"41","text":"Alipasua mwamba, maji yakabubujika,"},{"book":"Zaburi","chapter":"105","verse":"42","text":"Kwa maana alikumbuka ahadi yake takatifu,"},{"book":"Zaburi","chapter":"105","verse":"43","text":"Aliwatoa watu wake kwa furaha,"},{"book":"Zaburi","chapter":"105","verse":"44","text":"akawapa nchi za mataifa, wakawa warithi"},{"book":"Zaburi","chapter":"105","verse":"45","text":"alifanya haya ili wayashike mausia yake"}]}