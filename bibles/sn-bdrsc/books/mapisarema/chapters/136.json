{"data":[{"book":"Mapisarema","chapter":"136","verse":"1","text":"Vongai Jehovha, nokuti akanaka:"},{"book":"Mapisarema","chapter":"136","verse":"2","text":"Vongai Mwari wavamwari:"},{"book":"Mapisarema","chapter":"136","verse":"3","text":"Vongai Ishe wamadzishe:"},{"book":"Mapisarema","chapter":"136","verse":"4","text":"iye oga anoita zvishamiso zvikuru,"},{"book":"Mapisarema","chapter":"136","verse":"5","text":"iye akaita matenga nokunzwisisa kwake,"},{"book":"Mapisarema","chapter":"136","verse":"6","text":"iye akatambanudza nyika pamusoro pemvura zhinji,"},{"book":"Mapisarema","chapter":"136","verse":"7","text":"iye akaita zviedza zvikuru,"},{"book":"Mapisarema","chapter":"136","verse":"8","text":"zuva kuti ribate ushe masikati,"},{"book":"Mapisarema","chapter":"136","verse":"9","text":"mwedzi nenyeredzi kuti zvibate ushe usiku;"},{"book":"Mapisarema","chapter":"136","verse":"10","text":"iye akarova matangwe eIjipiti,"},{"book":"Mapisarema","chapter":"136","verse":"11","text":"uye akabudisa Israeri kubva pakati pavo,"},{"book":"Mapisarema","chapter":"136","verse":"12","text":"noruoko rune simba uye noruoko rwakatambanudzwa,"},{"book":"Mapisarema","chapter":"136","verse":"13","text":"iye akaparadzanisa Gungwa Dzvuku napakati,"},{"book":"Mapisarema","chapter":"136","verse":"14","text":"uye akayambutsa Israeri napakati paro,"},{"book":"Mapisarema","chapter":"136","verse":"15","text":"asi akakukura Faro nehondo yake muGungwa Dzvuku;"},{"book":"Mapisarema","chapter":"136","verse":"16","text":"iye akatungamirira vanhu vake nomugwenga,"},{"book":"Mapisarema","chapter":"136","verse":"17","text":"akarova madzimambo makuru,"},{"book":"Mapisarema","chapter":"136","verse":"18","text":"uye akauraya madzimambo ane simba,"},{"book":"Mapisarema","chapter":"136","verse":"19","text":"Sihoni mambo wavaAmori,"},{"book":"Mapisarema","chapter":"136","verse":"20","text":"naOgi mambo weBhashani,"},{"book":"Mapisarema","chapter":"136","verse":"21","text":"akavapa nyika yavo senhaka,"},{"book":"Mapisarema","chapter":"136","verse":"22","text":"nhaka kumuranda wake Israeri."},{"book":"Mapisarema","chapter":"136","verse":"23","text":"Iye akatirangarira pakudzikisirwa kwedu,"},{"book":"Mapisarema","chapter":"136","verse":"24","text":"uye akatisunungura kubva pavavengi vedu,"},{"book":"Mapisarema","chapter":"136","verse":"25","text":"uye anopa zvokudya kuzvisikwa zvose."},{"book":"Mapisarema","chapter":"136","verse":"26","text":"Vongai Mwari wokudenga,"}]}