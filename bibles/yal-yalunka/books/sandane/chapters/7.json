{"data":[{"book":"Sandane","chapter":"7","verse":"1","text":"N ma diina, n ma falane suxu."},{"book":"Sandane","chapter":"7","verse":"2","text":"N ma yamarine suxu"},{"book":"Sandane","chapter":"7","verse":"3","text":"E xidi i yiin na,"},{"book":"Sandane","chapter":"7","verse":"4","text":"Fe kolonna suxu alo i magilɛna,"},{"book":"Sandane","chapter":"7","verse":"5","text":"E i ratangama nɛn yalunde ɲaxanla ma,"},{"book":"Sandane","chapter":"7","verse":"6","text":"Lɔxɔna nde,"},{"book":"Sandane","chapter":"7","verse":"7","text":"n foninge keden nakɔrɔsi banxulan fe kolontarene yɛ."},{"book":"Sandane","chapter":"7","verse":"8","text":"A yi danguma kiraan xɔn,"},{"book":"Sandane","chapter":"7","verse":"9","text":"Sogen godomatɔɔn nan yi a ra,"},{"book":"Sandane","chapter":"7","verse":"10","text":"Ɲaxanla yi mini a ralandeni,"},{"book":"Sandane","chapter":"7","verse":"11","text":"Xurutaren nan yi a ra"},{"book":"Sandane","chapter":"7","verse":"12","text":"Waxatina nde, a sa kirane xɔn"},{"book":"Sandane","chapter":"7","verse":"13","text":"A yi na banxulanna suxu,"},{"book":"Sandane","chapter":"7","verse":"14","text":"“N bata yi dɛ xuiin tongo"},{"book":"Sandane","chapter":"7","verse":"15","text":"Nanara, n bata mini i ralandeni."},{"book":"Sandane","chapter":"7","verse":"16","text":"N bata n ma sadeni tɔn bitingan faɲine ra,"},{"book":"Sandane","chapter":"7","verse":"17","text":"N bata mirihi latikɔnɔnna sa a ma,"},{"book":"Sandane","chapter":"7","verse":"18","text":"Fa be, en fa kafu han xɔtɔnni,"},{"book":"Sandane","chapter":"7","verse":"19","text":"N ma xɛmɛn mi banxini,"},{"book":"Sandane","chapter":"7","verse":"20","text":"A bata a gbeti saseen xali,"},{"book":"Sandane","chapter":"7","verse":"21","text":"A to fatan falan tiyɛ,"},{"book":"Sandane","chapter":"7","verse":"22","text":"Sanɲa ma kedenni,"},{"book":"Sandane","chapter":"7","verse":"23","text":"han xalimakunla yi a bɔɲɛn sɔxɔn,"},{"book":"Sandane","chapter":"7","verse":"24","text":"Nanara, n ma diine,"},{"book":"Sandane","chapter":"7","verse":"25","text":"Ɛ bɔɲɛn nama mayenden"},{"book":"Sandane","chapter":"7","verse":"26","text":"A bata muxu wuyaxi rabira."},{"book":"Sandane","chapter":"7","verse":"27","text":"A banxin findixi gaburun kira yisoxin nan na"}]}