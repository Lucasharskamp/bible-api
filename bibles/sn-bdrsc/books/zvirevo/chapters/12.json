{"data":[{"book":"Zvirevo","chapter":"12","verse":"1","text":"Ani naani anofarira kuzvirovedza anoda zivo,"},{"book":"Zvirevo","chapter":"12","verse":"2","text":"Munhu akanaka anowana nyasha kubva kuna Jehovha,"},{"book":"Zvirevo","chapter":"12","verse":"3","text":"Munhu haangasimbiswi nezvakaipa,"},{"book":"Zvirevo","chapter":"12","verse":"4","text":"Mudzimai ane unhu hwakanaka ikorona kumurume wake,"},{"book":"Zvirevo","chapter":"12","verse":"5","text":"Urongwa hwomunhu akarurama hwakarurama,"},{"book":"Zvirevo","chapter":"12","verse":"6","text":"Mashoko owakaipa anovandira ropa,"},{"book":"Zvirevo","chapter":"12","verse":"7","text":"Vanhu vakaipa vanobviswa pachigaro uye havazovapozve,"},{"book":"Zvirevo","chapter":"12","verse":"8","text":"Munhu anorumbidzwa maererano nouchenjeri hwake,"},{"book":"Zvirevo","chapter":"12","verse":"9","text":"Zviri nani kuva munhu anoninipiswa asi iwe une muranda,"},{"book":"Zvirevo","chapter":"12","verse":"10","text":"Munhu akarurama ane hanya nechipfuwo chake,"},{"book":"Zvirevo","chapter":"12","verse":"11","text":"Uyo anoshanda mumunda wake achava nezvokudya zvakawanda,"},{"book":"Zvirevo","chapter":"12","verse":"12","text":"Vakaipa vanopanga zvakapambwa zvavanhu vakaipa,"},{"book":"Zvirevo","chapter":"12","verse":"13","text":"Munhu akaipa anobatwa nokutaura kwake kwakaipa,"},{"book":"Zvirevo","chapter":"12","verse":"14","text":"Kubva pazvibereko zvemiromo yake munhu anozadzwa nezvinhu zvakanaka,"},{"book":"Zvirevo","chapter":"12","verse":"15","text":"Nzira yebenzi inoita seyakanaka kwaari,"},{"book":"Zvirevo","chapter":"12","verse":"16","text":"Benzi rinoratidza kutsamwa pakarepo,"},{"book":"Zvirevo","chapter":"12","verse":"17","text":"Chapupu chezvokwadi chinopa uchapupu hwezvokwadi,"},{"book":"Zvirevo","chapter":"12","verse":"18","text":"Mashoko anotaurwa nokusarangarira anobaya somunondo,"},{"book":"Zvirevo","chapter":"12","verse":"19","text":"Miromo yezvokwadi ichasimbiswa nokusingaperi,"},{"book":"Zvirevo","chapter":"12","verse":"20","text":"Mune kunyengera mumwoyo yaavo vanoronga zvakaipa,"},{"book":"Zvirevo","chapter":"12","verse":"21","text":"Akarurama haangatongowirwi nezvakaipa,"},{"book":"Zvirevo","chapter":"12","verse":"22","text":"Jehovha anovenga miromo inotaura nhema,"},{"book":"Zvirevo","chapter":"12","verse":"23","text":"Munhu akachenjera anovanza zivo yake,"},{"book":"Zvirevo","chapter":"12","verse":"24","text":"Maoko anoshingaira achatonga,"},{"book":"Zvirevo","chapter":"12","verse":"25","text":"Mwoyo unopunyaira unoremedza munhu,"},{"book":"Zvirevo","chapter":"12","verse":"26","text":"Munhu akarurama anochenjerera ushamwari,"},{"book":"Zvirevo","chapter":"12","verse":"27","text":"Munhu ane usimbe haagochi mhuka yaabata,"},{"book":"Zvirevo","chapter":"12","verse":"28","text":"Munzira yokururama mune upenyu;"}]}