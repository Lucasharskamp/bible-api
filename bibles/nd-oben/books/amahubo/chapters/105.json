{"data":[{"book":"AmaHubo","chapter":"105","verse":"1","text":"Bongani uThixo, memezani ibizo lakhe;"},{"book":"AmaHubo","chapter":"105","verse":"2","text":"Hlabelelani kuye, hlabelelani indumiso kuye;"},{"book":"AmaHubo","chapter":"105","verse":"3","text":"Jabulani ebizweni lakhe elingcwele;"},{"book":"AmaHubo","chapter":"105","verse":"4","text":"Dingani uThixo lamandla akhe,"},{"book":"AmaHubo","chapter":"105","verse":"5","text":"Khumbulani izenzo zakhe ezimangalisayo azenzayo,"},{"book":"AmaHubo","chapter":"105","verse":"6","text":"lina zizukulwane zika-Abhrahama inceku yakhe,"},{"book":"AmaHubo","chapter":"105","verse":"7","text":"UnguThixo uNkulunkulu wethu;"},{"book":"AmaHubo","chapter":"105","verse":"8","text":"Uyasikhumbula isivumelwano sakhe nini lanini,"},{"book":"AmaHubo","chapter":"105","verse":"9","text":"isivumelwano asenza lo-Abhrahama,"},{"book":"AmaHubo","chapter":"105","verse":"10","text":"Wakuqinisa lokhu kuJakhobe"},{"book":"AmaHubo","chapter":"105","verse":"11","text":"wathi “Ngizalinika ilizwe laseKhenani"},{"book":"AmaHubo","chapter":"105","verse":"12","text":"Kwathi besesebalutshwana,"},{"book":"AmaHubo","chapter":"105","verse":"13","text":"bezula kuzizwe ngezizwe"},{"book":"AmaHubo","chapter":"105","verse":"14","text":"Kavumelanga loba ngubani ukubancindezela;"},{"book":"AmaHubo","chapter":"105","verse":"15","text":"“Lingabathinti abagcotshiweyo bami;"},{"book":"AmaHubo","chapter":"105","verse":"16","text":"Waletha indlala elizweni"},{"book":"AmaHubo","chapter":"105","verse":"17","text":"wathumela indoda yabandulela"},{"book":"AmaHubo","chapter":"105","verse":"18","text":"Bambhonxula inyawo zakhe ngezibopho,"},{"book":"AmaHubo","chapter":"105","verse":"19","text":"kwaze kwathi lokho ayekutsho ngaphambili kwagcwaliseka,"},{"book":"AmaHubo","chapter":"105","verse":"20","text":"Inkosi yathumela ukuthi akhululwe,"},{"book":"AmaHubo","chapter":"105","verse":"21","text":"Wamenza umbusi wendlu yakhe,"},{"book":"AmaHubo","chapter":"105","verse":"22","text":"ukuqondisa amakhosana akhe njengokubona kwakhe"},{"book":"AmaHubo","chapter":"105","verse":"23","text":"Kwathi-ke u-Israyeli wayangena eGibhithe;"},{"book":"AmaHubo","chapter":"105","verse":"24","text":"UThixo wenza abantu bakhe banda kakhulu;"},{"book":"AmaHubo","chapter":"105","verse":"25","text":"kwathi inhliziyo zabo wazenza zazonda abantu bakhe,"},{"book":"AmaHubo","chapter":"105","verse":"26","text":"Wathuma uMosi inceku yakhe,"},{"book":"AmaHubo","chapter":"105","verse":"27","text":"Benza iziboniso zakhe ezimangalisayo phakathi kwabo,"},{"book":"AmaHubo","chapter":"105","verse":"28","text":"Wathumela umnyama wenza ilizwe laba mnyama"},{"book":"AmaHubo","chapter":"105","verse":"29","text":"Waguqula amanzi abo aba ligazi,"},{"book":"AmaHubo","chapter":"105","verse":"30","text":"Ilizwe labo lanyakazela ngamaxoxo,"},{"book":"AmaHubo","chapter":"105","verse":"31","text":"Wakhuluma, kwavela umtshitshi wezibawu,"},{"book":"AmaHubo","chapter":"105","verse":"32","text":"Wenza izulu labo laba yisiqhotho,"},{"book":"AmaHubo","chapter":"105","verse":"33","text":"wacakazela phansi amavini abo lemikhiwa,"},{"book":"AmaHubo","chapter":"105","verse":"34","text":"Wakhuluma zatheleka izintethe,"},{"book":"AmaHubo","chapter":"105","verse":"35","text":"zadla yonke into eluhlaza elizweni labo,"},{"book":"AmaHubo","chapter":"105","verse":"36","text":"Ngemva kwalokho wasebulala amazibulo wonke elizweni labo,"},{"book":"AmaHubo","chapter":"105","verse":"37","text":"Wamkhupha u-Israyeli, ethwele esisitheka isiliva legolide,"},{"book":"AmaHubo","chapter":"105","verse":"38","text":"IGibhithe yathokoza ngokusuka kwabo,"},{"book":"AmaHubo","chapter":"105","verse":"39","text":"Wendlala iyezi lagubuzela,"},{"book":"AmaHubo","chapter":"105","verse":"40","text":"Bacela wabalethela izagwaca,"},{"book":"AmaHubo","chapter":"105","verse":"41","text":"Wavula ilitshe amanzi antshaza;"},{"book":"AmaHubo","chapter":"105","verse":"42","text":"Ngoba wakhumbula isithembiso sakhe esingcwele"},{"book":"AmaHubo","chapter":"105","verse":"43","text":"Wabakhupha abantu bakhe ngokuthokoza,"},{"book":"AmaHubo","chapter":"105","verse":"44","text":"Wabapha amazwe ezizwe,"},{"book":"AmaHubo","chapter":"105","verse":"45","text":"ukuze bagcine iziqondiso zakhe"}]}