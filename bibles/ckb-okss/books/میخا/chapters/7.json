{"data":[{"book":"میخا","chapter":"7","verse":"1","text":"قوڕبەسەرم!"},{"book":"میخا","chapter":"7","verse":"2","text":"خواناس لە خاک لەناوچوو،"},{"book":"میخا","chapter":"7","verse":"3","text":"بۆ خراپە هەردوو دەستەکانیان چالاکن،"},{"book":"میخا","chapter":"7","verse":"4","text":"چاکترینیان وەک وشترالووکە و"},{"book":"میخا","chapter":"7","verse":"5","text":"متمانە بە برادەر مەکە و"},{"book":"میخا","chapter":"7","verse":"6","text":"چونکە کوڕ سووکایەتی بە باوکی دەکات،"},{"book":"میخا","chapter":"7","verse":"7","text":"بەڵام من چاوم لەسەر یەزدان دەبێت،"},{"book":"میخا","chapter":"7","verse":"8","text":"ئەی دوژمنەکەم، بە نسکۆی من دڵخۆش نەبیت!"},{"book":"میخا","chapter":"7","verse":"9","text":"لەبەر ئەوەی لە دژی ئەو گوناهم کرد،"},{"book":"میخا","chapter":"7","verse":"10","text":"ئینجا دوژمنەکەم تەماشا دەکات و"},{"book":"میخا","chapter":"7","verse":"11","text":"ڕۆژی بنیادنانی دیوارەکانت دێت،"},{"book":"میخا","chapter":"7","verse":"12","text":"لەو ڕۆژەدا خەڵک دێنە لات،"},{"book":"میخا","chapter":"7","verse":"13","text":"زەوی لەبەر دانیشتووانەکەی وێران دەبێت،"},{"book":"میخا","chapter":"7","verse":"14","text":"بە گۆچانەکەت شوانایەتی گەلەکەت بکە،"},{"book":"میخا","chapter":"7","verse":"15","text":"«وەک ڕۆژانی هاتنەدەرەوەت لە خاکی میسر،"},{"book":"میخا","chapter":"7","verse":"16","text":"نەتەوەکان تەماشا دەکەن و شەرمەزار دەبن،"},{"book":"میخا","chapter":"7","verse":"17","text":"وەک مار خۆڵ دەلێسنەوە،"},{"book":"میخا","chapter":"7","verse":"18","text":"کێ خودایەکی وەک تۆیە،"},{"book":"میخا","chapter":"7","verse":"19","text":"دەگەڕێیتەوە و بەزەییت پێماندا دێتەوە،"},{"book":"میخا","chapter":"7","verse":"20","text":"دڵسۆزی بۆ یاقوب پیشان دەدەیت،"}]}