{"data":[{"book":"AmaHubo","chapter":"35","verse":"1","text":"Xabana labo, Thixo abaxabana lami;"},{"book":"AmaHubo","chapter":"35","verse":"2","text":"Thatha isihlangu lewisa;"},{"book":"AmaHubo","chapter":"35","verse":"3","text":"Phakamisa umkhonto lesijula,"},{"book":"AmaHubo","chapter":"35","verse":"4","text":"Sengathi labo abazingela impilo yami"},{"book":"AmaHubo","chapter":"35","verse":"5","text":"Sengathi bangaba njengamakhoba emoyeni,"},{"book":"AmaHubo","chapter":"35","verse":"6","text":"sengathi indlela yabo ingaba mnyama itshelele,"},{"book":"AmaHubo","chapter":"35","verse":"7","text":"Ngoba bangithiya ngamambule abo ngingonanga lutho,"},{"book":"AmaHubo","chapter":"35","verse":"8","text":"sengathi bangadilizwa belibele"},{"book":"AmaHubo","chapter":"35","verse":"9","text":"Lapho-ke umoya wami uzathokoza kuThixo"},{"book":"AmaHubo","chapter":"35","verse":"10","text":"Mina qobo lwami ngizamemeza ngithi,"},{"book":"AmaHubo","chapter":"35","verse":"11","text":"Ofakazi bolunya bayangivelela;"},{"book":"AmaHubo","chapter":"35","verse":"12","text":"Bangiphindisela okuhle ngokubi"},{"book":"AmaHubo","chapter":"35","verse":"13","text":"Ikanti kwathi bona begula ngembatha amasaka"},{"book":"AmaHubo","chapter":"35","verse":"14","text":"ngaba sesililweni"},{"book":"AmaHubo","chapter":"35","verse":"15","text":"Kodwa ngathi sengikhubekile baqwebana bokha insini;"},{"book":"AmaHubo","chapter":"35","verse":"16","text":"Bakloloda ngomona njengabangakholwayo;"},{"book":"AmaHubo","chapter":"35","verse":"17","text":"Nkosi, uzakuma ubukele kuze kube nini na?"},{"book":"AmaHubo","chapter":"35","verse":"18","text":"Ngizakubonga phakathi komhlangano omkhulu;"},{"book":"AmaHubo","chapter":"35","verse":"19","text":"Ungabayekeli abayizitha zami ngingabonelanga lutho,"},{"book":"AmaHubo","chapter":"35","verse":"20","text":"Kabakhulumi okuqondileyo"},{"book":"AmaHubo","chapter":"35","verse":"21","text":"Bangikhamisela imilomo bathi, “Hiya belo!"},{"book":"AmaHubo","chapter":"35","verse":"22","text":"Oh Thixo, usukubonile lokhu; ungathuli."},{"book":"AmaHubo","chapter":"35","verse":"23","text":"Vuka, uphakame ungilamulele!"},{"book":"AmaHubo","chapter":"35","verse":"24","text":"Ngigeza ngokulunga Kwakho,"},{"book":"AmaHubo","chapter":"35","verse":"25","text":"Ungabayekeli bacabange bathi, “Ehe, yikho ebesikufuna!”"},{"book":"AmaHubo","chapter":"35","verse":"26","text":"Sengathi bonke abantela ngokuhlupheka kwami"},{"book":"AmaHubo","chapter":"35","verse":"27","text":"Sengathi labo abathokoza ngokugezwa kwami"},{"book":"AmaHubo","chapter":"35","verse":"28","text":"Ulimi lwami luzakhuluma ngokulunga Kwakho"}]}