{"data":[{"book":"Ermiyaas","chapter":"50","verse":"1","text":"Dubbiin Waaqayyo karaa Ermiyaas raajichaatiin waaʼee Baabilonii fi waaʼee biyya Baabilonotaa dubbate kanaa dha:"},{"book":"Ermiyaas","chapter":"50","verse":"2","text":"“Saboota gidduutti lallabaatii labsaa;"},{"book":"Ermiyaas","chapter":"50","verse":"3","text":"Sabni Kaabaa dhufu tokko ishee lolee"},{"book":"Ermiyaas","chapter":"50","verse":"4","text":"“Bara sana keessa, yeroo sanatti,"},{"book":"Ermiyaas","chapter":"50","verse":"5","text":"“Isaan karaa gara Xiyoon geessu ni gaafatu;"},{"book":"Ermiyaas","chapter":"50","verse":"6","text":"“Sabni koo hoolota badan taʼaniiru;"},{"book":"Ermiyaas","chapter":"50","verse":"7","text":"Kan isaan argu hundi isaan liqimse;"},{"book":"Ermiyaas","chapter":"50","verse":"8","text":"“Baabilonii baqadhaa baʼaa;"},{"book":"Ermiyaas","chapter":"50","verse":"9","text":"Ani biyya kaabaatii garee saboota gurguddaa kaasee"},{"book":"Ermiyaas","chapter":"50","verse":"10","text":"Baabilonis ni saamamti;"},{"book":"Ermiyaas","chapter":"50","verse":"11","text":"“Isin warri handhuuraa koo saamtan"},{"book":"Ermiyaas","chapter":"50","verse":"12","text":"haati keessan akka malee qaanofti;"},{"book":"Ermiyaas","chapter":"50","verse":"13","text":"Isheen sababii dheekkamsa Waaqayyootiif guutumaan guutuutti"},{"book":"Ermiyaas","chapter":"50","verse":"14","text":"“Isin warri iddaa luqqifattan hundinuu,"},{"book":"Ermiyaas","chapter":"50","verse":"15","text":"Karaa hundaan isheetti iyyaa!"},{"book":"Ermiyaas","chapter":"50","verse":"16","text":"Nama sanyii facaasuu fi kan yeroo midhaan haamamutti"},{"book":"Ermiyaas","chapter":"50","verse":"17","text":"“Israaʼel hoolaa bittinnaaʼee dha;"},{"book":"Ermiyaas","chapter":"50","verse":"18","text":"Kanaafuu Waaqayyo Waan Hunda Dandaʼu, Waaqni Israaʼel akkana jedha:"},{"book":"Ermiyaas","chapter":"50","verse":"19","text":"Israaʼelin garuu lafa tika isaatti nan deebisa;"},{"book":"Ermiyaas","chapter":"50","verse":"20","text":"Bara sana keessa, yeroo sanatti,"},{"book":"Ermiyaas","chapter":"50","verse":"21","text":"“Biyya Meraataayimii fi"},{"book":"Ermiyaas","chapter":"50","verse":"22","text":"Sagaleen waraanaa,"},{"book":"Ermiyaas","chapter":"50","verse":"23","text":"Burruusni guutummaa lafaa akkamitti cabe!"},{"book":"Ermiyaas","chapter":"50","verse":"24","text":"Yaa Baabilon, ani kiyyoo siif diriirseera;"},{"book":"Ermiyaas","chapter":"50","verse":"25","text":"Waaqayyo mankuusaa miʼa lolaa isaa banee"},{"book":"Ermiyaas","chapter":"50","verse":"26","text":"Biyya fagootii isheetti kottaa."},{"book":"Ermiyaas","chapter":"50","verse":"27","text":"Korommii ishee hunda qalaa;"},{"book":"Ermiyaas","chapter":"50","verse":"28","text":"Akka Waaqayyo Waaqni keenya haaloo baʼe,"},{"book":"Ermiyaas","chapter":"50","verse":"29","text":"“Namoota Xiyyaan lolan"},{"book":"Ermiyaas","chapter":"50","verse":"30","text":"Kanaafuu dargaggoonni ishee karaa irratti dhumu;"},{"book":"Ermiyaas","chapter":"50","verse":"31","text":"“Yaa koortuu nana, kunoo ani sitti kaʼeera”"},{"book":"Ermiyaas","chapter":"50","verse":"32","text":"Koortuun sun gufattee lafa dhoofti;"},{"book":"Ermiyaas","chapter":"50","verse":"33","text":"Waaqayyo Waan Hunda Dandaʼu akkana jedha:"},{"book":"Ermiyaas","chapter":"50","verse":"34","text":"Taʼu illee Furaan isaanii jabaa dha;"},{"book":"Ermiyaas","chapter":"50","verse":"35","text":"“Goraadeen tokko Baabilonotatti dhufe!”"},{"book":"Ermiyaas","chapter":"50","verse":"36","text":"Goraadeen raajota ishee sobdootatti, dhufeera!"},{"book":"Ermiyaas","chapter":"50","verse":"37","text":"Goraadeen fardeenii fi gaariiwwan isheetti,"},{"book":"Ermiyaas","chapter":"50","verse":"38","text":"Caamsaan bishaan isheetti ni dhufa!"},{"book":"Ermiyaas","chapter":"50","verse":"39","text":"“Kanaafuu uumamawwan gammoojjiitii fi  waraabeyyiin achi jiraatu;"},{"book":"Ermiyaas","chapter":"50","verse":"40","text":"Akkuma Waaqni Sodoomii fi Gomoraa,"},{"book":"Ermiyaas","chapter":"50","verse":"41","text":"“Kunoo! Gareen loltootaa tokko kaaba irraa dhufaa jira;"},{"book":"Ermiyaas","chapter":"50","verse":"42","text":"Isaan xiyyaa fi eeboo qabataniiru;"},{"book":"Ermiyaas","chapter":"50","verse":"43","text":"Mootiin Baabilon oduu waaʼee isaanii dhagaʼe;"},{"book":"Ermiyaas","chapter":"50","verse":"44","text":"Ani akkuma leenca bosona Yordaanosii baʼee"},{"book":"Ermiyaas","chapter":"50","verse":"45","text":"Kanaafuu waan Waaqayyo Baabilonitti karoorfate,"},{"book":"Ermiyaas","chapter":"50","verse":"46","text":"Waca qabamuu Baabiloniitiin lafti ni raafamti;"}]}