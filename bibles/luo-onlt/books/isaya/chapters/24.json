{"data":[{"book":"Isaya","chapter":"24","verse":"1","text":"Neuru Jehova Nyasaye biro ketho piny,"},{"book":"Isaya","chapter":"24","verse":"2","text":"Notime machalre ne jodolo kata ne joma moko,"},{"book":"Isaya","chapter":"24","verse":"3","text":"Piny nodongʼ gunda"},{"book":"Isaya","chapter":"24","verse":"4","text":"Piny notwo kendo noner,"},{"book":"Isaya","chapter":"24","verse":"5","text":"Piny osedwany gi jok modakie,"},{"book":"Isaya","chapter":"24","verse":"6","text":"Kuom mano kwongʼ"},{"book":"Isaya","chapter":"24","verse":"7","text":"Divai mobi manyien notuwo"},{"book":"Isaya","chapter":"24","verse":"8","text":"Mor mar nyiduonge norum,"},{"book":"Isaya","chapter":"24","verse":"9","text":"Ok ginichak gimadh divai ka giwer,"},{"book":"Isaya","chapter":"24","verse":"10","text":"Dala maduongʼ"},{"book":"Isaya","chapter":"24","verse":"11","text":"Ji ywagore e wangʼ yore"},{"book":"Isaya","chapter":"24","verse":"12","text":"Dala maduongʼ odongʼ ka oruombore piny"},{"book":"Isaya","chapter":"24","verse":"13","text":"Ma notimre e piny"},{"book":"Isaya","chapter":"24","verse":"14","text":"Gitingʼo dwondgi malo, ka gigoyo koko gimor"},{"book":"Isaya","chapter":"24","verse":"15","text":"Omiyo miuru Jehova Nyasaye duongʼ"},{"book":"Isaya","chapter":"24","verse":"16","text":"koa e tungʼ piny duto wawinjo wer mawer niya:"},{"book":"Isaya","chapter":"24","verse":"17","text":"Masira rita, osekunyna bugo"},{"book":"Isaya","chapter":"24","verse":"18","text":"Ngʼama notem ringo kowinjo mahu mar masira"},{"book":"Isaya","chapter":"24","verse":"19","text":"Piny osebarore,"},{"book":"Isaya","chapter":"24","verse":"20","text":"Piny thembo ka jakongʼo,"},{"book":"Isaya","chapter":"24","verse":"21","text":"E kindeno Jehova Nyasaye nokum"},{"book":"Isaya","chapter":"24","verse":"22","text":"Nochokgi kaachiel kendo nolornegi"},{"book":"Isaya","chapter":"24","verse":"23","text":"Dwe norieny matip-tip kendo wangʼ"}]}