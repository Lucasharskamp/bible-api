{"data":[{"book":"Jobho","chapter":"15","verse":"1","text":"Ipapo Erifazi muTemani akapindura akati:"},{"book":"Jobho","chapter":"15","verse":"2","text":"“Ko, munhu akachenjera angapindura zvisina njere here,"},{"book":"Jobho","chapter":"15","verse":"3","text":"Ko, angakakavara namashoko asina maturo,"},{"book":"Jobho","chapter":"15","verse":"4","text":"Asi unotorwisa umwari"},{"book":"Jobho","chapter":"15","verse":"5","text":"Chivi chako chinorunzira muromo wako;"},{"book":"Jobho","chapter":"15","verse":"6","text":"Muromo wako pachako ndiwo unokupa mhosva, kwete wangu,"},{"book":"Jobho","chapter":"15","verse":"7","text":"“Ko, ndiwe munhu wakatanga kuzvarwa here?"},{"book":"Jobho","chapter":"15","verse":"8","text":"Unoteererawo here panokurukurwa mazano aMwari?"},{"book":"Jobho","chapter":"15","verse":"9","text":"Chiiko chaunoziva chatisingazivi isu?"},{"book":"Jobho","chapter":"15","verse":"10","text":"Vane bvudzi rakachena navatana vakwegura vari kudivi redu,"},{"book":"Jobho","chapter":"15","verse":"11","text":"Ko, kunyaradza kwaMwari hakuna kukukwanira here,"},{"book":"Jobho","chapter":"15","verse":"12","text":"Ko, mwoyo wako wakurasireiko,"},{"book":"Jobho","chapter":"15","verse":"13","text":"zvokuti unoitira Mwari hasha"},{"book":"Jobho","chapter":"15","verse":"14","text":"“Munhu chiiko, zvokuti angava akanaka,"},{"book":"Jobho","chapter":"15","verse":"15","text":"Kana Mwari akasavimba navatsvene vake,"},{"book":"Jobho","chapter":"15","verse":"16","text":"ko, kuzoti munhu, ane uori"},{"book":"Jobho","chapter":"15","verse":"17","text":"“Nditeerere ndigokutsanangurira;"},{"book":"Jobho","chapter":"15","verse":"18","text":"zvakataurwa navanhu vakachenjera,"},{"book":"Jobho","chapter":"15","verse":"19","text":"ivo voga vakapiwa nyika"},{"book":"Jobho","chapter":"15","verse":"20","text":"Murume akaipa anotambudzika nokurwadziwa pamazuva ake ose,"},{"book":"Jobho","chapter":"15","verse":"21","text":"Maungira anotyisa anozadza nzeve dzake;"},{"book":"Jobho","chapter":"15","verse":"22","text":"Anorasikirwa netariro yokupunyuka parima;"},{"book":"Jobho","chapter":"15","verse":"23","text":"Anofamba-famba achiita sechokudya zvamagora;"},{"book":"Jobho","chapter":"15","verse":"24","text":"Kutambudzika nokurwadziwa zvinomuzadza nokutya;"},{"book":"Jobho","chapter":"15","verse":"25","text":"nokuti anokungira Mwari chibhakera chake"},{"book":"Jobho","chapter":"15","verse":"26","text":"achimhanyira kwaari asingatyi"},{"book":"Jobho","chapter":"15","verse":"27","text":"“Kunyange chiso chake chakafukidzwa namafuta"},{"book":"Jobho","chapter":"15","verse":"28","text":"iye achagara mumaguta akaparadzwa"},{"book":"Jobho","chapter":"15","verse":"29","text":"Haachazovazve mupfumi uye pfuma yake haingagari,"},{"book":"Jobho","chapter":"15","verse":"30","text":"Haangapunyuki rima;"},{"book":"Jobho","chapter":"15","verse":"31","text":"Ngaarege kuzvinyengera nokuvimba nezvisina maturo,"},{"book":"Jobho","chapter":"15","verse":"32","text":"Acharipirwa zvakazara nguva yake isati yasvika,"},{"book":"Jobho","chapter":"15","verse":"33","text":"Achafanana nomuzambiringa wabvisirwa mazambiringa asati aibva,"},{"book":"Jobho","chapter":"15","verse":"34","text":"Nokuti ungano yavasingadi Mwari ichashaya zvibereko,"},{"book":"Jobho","chapter":"15","verse":"35","text":"Vanobata mimba yokutambudzika vagozvara zvakaipa;"}]}