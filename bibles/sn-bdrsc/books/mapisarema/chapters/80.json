{"data":[{"book":"Mapisarema","chapter":"80","verse":"1","text":"Tinzwei, imi Mufudzi weIsraeri,"},{"book":"Mapisarema","chapter":"80","verse":"2","text":"pamberi paEfuremu, Bhenjamini naManase."},{"book":"Mapisarema","chapter":"80","verse":"3","text":"Tidzorei, imi Mwari,"},{"book":"Mapisarema","chapter":"80","verse":"4","text":"Haiwa Jehovha Mwari Wamasimba Ose,"},{"book":"Mapisarema","chapter":"80","verse":"5","text":"Makavapa chingwa chemisodzi kuti vadye;"},{"book":"Mapisarema","chapter":"80","verse":"6","text":"Makatiita mavambo enharo kuna vatigere navo,"},{"book":"Mapisarema","chapter":"80","verse":"7","text":"Tidzorei, imi Mwari Wamasimba Ose;"},{"book":"Mapisarema","chapter":"80","verse":"8","text":"Makabudisa muzambiringa kubva muIjipiti;"},{"book":"Mapisarema","chapter":"80","verse":"9","text":"Makaukurira nzvimbo,"},{"book":"Mapisarema","chapter":"80","verse":"10","text":"Makomo akanga akafukidzwa nomumvuri wawo,"},{"book":"Mapisarema","chapter":"80","verse":"11","text":"Wakatandavadza matavi awo kusvikira kuGungwa,"},{"book":"Mapisarema","chapter":"80","verse":"12","text":"Seiko makaputsa masvingo awo"},{"book":"Mapisarema","chapter":"80","verse":"13","text":"Nguruve dzinobva musango dzinouparadza,"},{"book":"Mapisarema","chapter":"80","verse":"14","text":"Dzokerai kwatiri, imi Mwari Wamasimba Ose!"},{"book":"Mapisarema","chapter":"80","verse":"15","text":"iwo mudzi wakasimwa noruoko rwenyu rworudyi,"},{"book":"Mapisarema","chapter":"80","verse":"16","text":"Muzambiringa wenyu watemerwa pasi, wapiswa nomoto;"},{"book":"Mapisarema","chapter":"80","verse":"17","text":"Ruoko rwenyu rworudyi ngarugare pamusoro pomunhu ari kurudyi kwenyu,"},{"book":"Mapisarema","chapter":"80","verse":"18","text":"Ipapo hatingazobvi kwamuri;"},{"book":"Mapisarema","chapter":"80","verse":"19","text":"Tidzorei, imi Mwari Wamasimba Ose;"}]}