{"data":[{"book":"1 Kronika","chapter":"1","verse":"1","text":"Adamu, Seti, Enoṣi,"},{"book":"1 Kronika","chapter":"1","verse":"2","text":"Kenani, Mahalaleli, Jaredi,"},{"book":"1 Kronika","chapter":"1","verse":"3","text":"Enoku, Metusela, Lameki,"},{"book":"1 Kronika","chapter":"1","verse":"4","text":"Àwọn ọmọ Noa: Ṣemu, Hamu àti Jafeti."},{"book":"1 Kronika","chapter":"1","verse":"5","text":"Àwọn ọmọ Jafeti ni:"},{"book":"1 Kronika","chapter":"1","verse":"6","text":"Àwọn ọmọ Gomeri ni:"},{"book":"1 Kronika","chapter":"1","verse":"7","text":"Àwọn ọmọ Jafani ni:"},{"book":"1 Kronika","chapter":"1","verse":"8","text":"Àwọn ọmọ Hamu ni:"},{"book":"1 Kronika","chapter":"1","verse":"9","text":"Àwọn ọmọ Kuṣi ni:"},{"book":"1 Kronika","chapter":"1","verse":"10","text":"Kuṣi sì bí Nimrodu"},{"book":"1 Kronika","chapter":"1","verse":"11","text":"Ejibiti sì bí"},{"book":"1 Kronika","chapter":"1","verse":"12","text":"Patrusimu, Kasluhimu (láti ọ̀dọ̀ ẹni tí àwọn ará Filistini ti wá) àti àwọn ará Kaftorimu."},{"book":"1 Kronika","chapter":"1","verse":"13","text":"Kenaani sì bí Sidoni àkọ́bí rẹ̀,"},{"book":"1 Kronika","chapter":"1","verse":"14","text":"àti àwọn ará Jebusi, àti àwọn ará Amori, àti àwọn ará Girgaṣi,"},{"book":"1 Kronika","chapter":"1","verse":"15","text":"àti àwọn ará Hifi, àti àwọn ará Arki, àti àwọn ará Sini,"},{"book":"1 Kronika","chapter":"1","verse":"16","text":"àti àwọn ará Arfadi, àti àwọn ará Semari, àti àwọn ará Hamati."},{"book":"1 Kronika","chapter":"1","verse":"17","text":"Àwọn ọmọ Ṣemu ni:"},{"book":"1 Kronika","chapter":"1","verse":"18","text":"Arfakṣadi sì bí Ṣela,"},{"book":"1 Kronika","chapter":"1","verse":"19","text":"Eberi sì bí ọmọ méjì:"},{"book":"1 Kronika","chapter":"1","verse":"20","text":"Joktani sì bí"},{"book":"1 Kronika","chapter":"1","verse":"21","text":"Hadoramu, Usali, Dikla,"},{"book":"1 Kronika","chapter":"1","verse":"22","text":"Ebali, Abimaeli, Ṣeba."},{"book":"1 Kronika","chapter":"1","verse":"23","text":"Ofiri, Hafila, àti Jobabu. Gbogbo àwọn wọ̀nyí ni ọmọ Joktani."},{"book":"1 Kronika","chapter":"1","verse":"24","text":"Ṣemu, Arfakṣadi, Ṣela,"},{"book":"1 Kronika","chapter":"1","verse":"25","text":"Eberi, Pelegi. Reu,"},{"book":"1 Kronika","chapter":"1","verse":"26","text":"Serugu, Nahori, Tẹra,"},{"book":"1 Kronika","chapter":"1","verse":"27","text":"àti Abramu (tí ń ṣe Abrahamu)."},{"book":"1 Kronika","chapter":"1","verse":"28","text":"Àwọn ọmọ Abrahamu: Isaaki àti Iṣmaeli."},{"book":"1 Kronika","chapter":"1","verse":"29","text":"Èyí ni àwọn ọmọ náà:"},{"book":"1 Kronika","chapter":"1","verse":"30","text":"Miṣima, Duma, Massa, Hadadi, Tema,"},{"book":"1 Kronika","chapter":"1","verse":"31","text":"Jeturi, Nafiṣi, àti Kedema."},{"book":"1 Kronika","chapter":"1","verse":"32","text":"Àwọn ọmọ Ketura, obìnrin Abrahamu:"},{"book":"1 Kronika","chapter":"1","verse":"33","text":"Àwọn ọmọ Midiani:"},{"book":"1 Kronika","chapter":"1","verse":"34","text":"Abrahamu sì jẹ́ baba Isaaki."},{"book":"1 Kronika","chapter":"1","verse":"35","text":"Àwọn ọmọ Esau:"},{"book":"1 Kronika","chapter":"1","verse":"36","text":"Àwọn ọmọ Elifasi:"},{"book":"1 Kronika","chapter":"1","verse":"37","text":"Àwọn ọmọ Reueli:"},{"book":"1 Kronika","chapter":"1","verse":"38","text":"Àwọn ọmọ Seiri:"},{"book":"1 Kronika","chapter":"1","verse":"39","text":"Àwọn ọmọ Lotani:"},{"book":"1 Kronika","chapter":"1","verse":"40","text":"Àwọn ọmọ Ṣobali:"},{"book":"1 Kronika","chapter":"1","verse":"41","text":"Àwọn ọmọ Ana:"},{"book":"1 Kronika","chapter":"1","verse":"42","text":"Àwọn ọmọ Eseri:"},{"book":"1 Kronika","chapter":"1","verse":"43","text":"Àwọn wọ̀nyí ni ọba tí ó jẹ ní Edomu, kí ó tó di pé ọba kankan jẹ lórí Israẹli:"},{"book":"1 Kronika","chapter":"1","verse":"44","text":"Nígbà tí Bela kú, Jobabu ọmọ Sera ti Bosra sì jẹ ọba ní ipò rẹ̀."},{"book":"1 Kronika","chapter":"1","verse":"45","text":"Nígbà tí Jobabu kú, Huṣamu láti ilẹ̀ Temani sì jẹ ọba ní ipò rẹ̀."},{"book":"1 Kronika","chapter":"1","verse":"46","text":"Nígbà tí Huṣamu kú, Hadadi ọmọ Bedadi tí ó kọlu Midiani ní ìgbẹ́ Moabu, ó sì jẹ ọba ní ipò rẹ̀. Orúkọ ìlú rẹ̀ ni Afiti."},{"book":"1 Kronika","chapter":"1","verse":"47","text":"Nígbà tí Hadadi sì kú, Samla láti Masreka, ó sì jẹ ọba ní ipò rẹ̀."},{"book":"1 Kronika","chapter":"1","verse":"48","text":"Samla sì kú, Saulu ti Rehoboti létí odò sì jẹ ọba ní ipò rẹ̀."},{"book":"1 Kronika","chapter":"1","verse":"49","text":"Nígbà tí Saulu kú, Baali-Hanani, ọmọ Akbori jẹ ọba ní ipò rẹ̀."},{"book":"1 Kronika","chapter":"1","verse":"50","text":"Nígbà tí Baali-Hanani kú, Hadadi ni ó jẹ ọba ní ipò rẹ̀. Orúkọ ìlú rẹ̀ ni Pau; orúkọ ìyàwó sì ni Mehetabeeli ọmọbìnrin Matiredi, ọmọbìnrin Mesahabu."},{"book":"1 Kronika","chapter":"1","verse":"51","text":"Hadadi sì kú pẹ̀lú."},{"book":"1 Kronika","chapter":"1","verse":"52","text":"baálẹ̀ Oholibama, baálẹ̀ Ela, baálẹ̀ Pinoni."},{"book":"1 Kronika","chapter":"1","verse":"53","text":"Baálẹ̀ Kenasi, baálẹ̀ Temani, baálẹ̀ Mibsari,"},{"book":"1 Kronika","chapter":"1","verse":"54","text":"Magdieli àti Iramu."}]}