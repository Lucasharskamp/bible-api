{"data":[{"book":"عَننَبِ عَمٌسِ","chapter":"9","verse":"1","text":"ﭑ نَشَ لَامَتُنيِ ندٍ تٌ،"},{"book":"عَننَبِ عَمٌسِ","chapter":"9","verse":"2","text":"هَلِ عٍ فٌرٌ عَلِفِيَمَ، ﭑ نٍيٍ سُشُمَ نّ."},{"book":"عَننَبِ عَمٌسِ","chapter":"9","verse":"3","text":"شَ عٍ عٍ نْشُن ﭑ مَ كَرٍ مٍلٍ فٍيَ قَرِ،"},{"book":"عَننَبِ عَمٌسِ","chapter":"9","verse":"4","text":"شَ عٍ يَشُييٍ عٍ شَنِن كٌنيِيَ كُي،"},{"book":"عَننَبِ عَمٌسِ","chapter":"9","verse":"5","text":"مَرِفِ عَلَتَلَ، نَشَن نَ مَنفّ رَ،"},{"book":"عَننَبِ عَمٌسِ","chapter":"9","verse":"6","text":"عَ بَرَ عَ شَ لِنفِرَ تِ كٌورٍ مَ."},{"book":"عَننَبِ عَمٌسِ","chapter":"9","verse":"7","text":"عَلَتَلَ شَ مَسٍنيِ نَن يَ؛"},{"book":"عَننَبِ عَمٌسِ","chapter":"9","verse":"8","text":"وٌ وٌ يَيٍ رَبِ؛"},{"book":"عَننَبِ عَمٌسِ","chapter":"9","verse":"9","text":"عَلَتَلَ شَ مَسٍنيِ نَن يَ؛"},{"book":"عَننَبِ عَمٌسِ","chapter":"9","verse":"10","text":"كْنْ يُنُبِتْييٍ نَشٍيٍ بِرِن نَ ﭑ مَ حَمَ يَ مَ،"},{"book":"عَننَبِ عَمٌسِ","chapter":"9","verse":"11","text":"«نَ لْشْي ﭑ دَوُدَ شَ مَنفّيَ كَنَشِ عِتٍمَ نّ."},{"book":"عَننَبِ عَمٌسِ","chapter":"9","verse":"12","text":"عٍ مَن عٍدٌن مَسْتْمَ نّ،"},{"book":"عَننَبِ عَمٌسِ","chapter":"9","verse":"13","text":"عَلَتَلَ شَ مَسٍنيِ نَن يَ؛"},{"book":"عَننَبِ عَمٌسِ","chapter":"9","verse":"14","text":"ﭑ نَن ﭑ مَ حَمَ رَفبِلٍنمَ نّ عَ شْنيِ كٍلِقٍ رَ كٌنيِيَ كُي."},{"book":"عَننَبِ عَمٌسِ","chapter":"9","verse":"15","text":"ﭑ مَن عٍ رَ سَبَتِمَ نّ عٍ شَ بْشِ مَ."}]}