{"data":[{"book":"Masalimo","chapter":"118","verse":"1","text":"Yamikani Yehova chifukwa ndi wabwino;"},{"book":"Masalimo","chapter":"118","verse":"2","text":"Israeli anene kuti:"},{"book":"Masalimo","chapter":"118","verse":"3","text":"Banja la Aaroni linene kuti,"},{"book":"Masalimo","chapter":"118","verse":"4","text":"Iwo amene amaopa Yehova anene kuti:"},{"book":"Masalimo","chapter":"118","verse":"5","text":"Ndili m始masautso anga ndinalirira Yehova,"},{"book":"Masalimo","chapter":"118","verse":"6","text":"Yehova ali nane; sindidzachita mantha."},{"book":"Masalimo","chapter":"118","verse":"7","text":"Yehova ali nane; Iye ndiye thandizo langa."},{"book":"Masalimo","chapter":"118","verse":"8","text":"N始kwabwino kuthawira kwa Yehova"},{"book":"Masalimo","chapter":"118","verse":"9","text":"N始kwabwino kuthawira kwa Yehova"},{"book":"Masalimo","chapter":"118","verse":"10","text":"Anthu a mitundu yonse anandizinga,"},{"book":"Masalimo","chapter":"118","verse":"11","text":"Anandizinga mbali zonse,"},{"book":"Masalimo","chapter":"118","verse":"12","text":"Anandizinga ngati njuchi,"},{"book":"Masalimo","chapter":"118","verse":"13","text":"Anandikankha uku ndi uko ndipo ndinali pafupi kugwa,"},{"book":"Masalimo","chapter":"118","verse":"14","text":"Yehova ndiye mphamvu yanga ndi nyimbo yanga;"},{"book":"Masalimo","chapter":"118","verse":"15","text":"Mfuwu wachimwemwe ndi chipambano"},{"book":"Masalimo","chapter":"118","verse":"16","text":"Dzanja lamanja la Yehova latukulidwa mmwamba"},{"book":"Masalimo","chapter":"118","verse":"17","text":"Sindidzafa koma ndidzakhala ndi moyo"},{"book":"Masalimo","chapter":"118","verse":"18","text":"Yehova wandilanga koopsa,"},{"book":"Masalimo","chapter":"118","verse":"19","text":"Tsekulireni zipata zachilungamo,"},{"book":"Masalimo","chapter":"118","verse":"20","text":"Ichi ndicho chipata cha Yehova"},{"book":"Masalimo","chapter":"118","verse":"21","text":"Ndidzakuyamikani chifukwa Inu munandiyankha;"},{"book":"Masalimo","chapter":"118","verse":"22","text":"Mwala umene amisiri omanga nyumba anawukana"},{"book":"Masalimo","chapter":"118","verse":"23","text":"Yehova ndiye wachita zimenezi"},{"book":"Masalimo","chapter":"118","verse":"24","text":"Lero ndiye tsiku limene Yehova walipanga;"},{"book":"Masalimo","chapter":"118","verse":"25","text":"Inu Yehova, tipulumutseni;"},{"book":"Masalimo","chapter":"118","verse":"26","text":"Wodala amene akubwera m始dzina la Yehova."},{"book":"Masalimo","chapter":"118","verse":"27","text":"Yehova ndi Mulungu,"},{"book":"Masalimo","chapter":"118","verse":"28","text":"Inu ndinu Mulungu wanga, ndipo ndidzakuyamikani;"},{"book":"Masalimo","chapter":"118","verse":"29","text":"Yamikani Yehova chifukwa ndi wabwino;"}]}