{"data":[{"book":"Vatongi","chapter":"5","verse":"1","text":"Nomusi uyo Dhibhora naBharaki mwanakomana waAbhinoami vakaimba rwiyo urwu runoti:"},{"book":"Vatongi","chapter":"5","verse":"2","text":"“Kana machinda aIsraeri achitungamirira,"},{"book":"Vatongi","chapter":"5","verse":"3","text":"“Inzwai izvi, imi madzimambo! Teererai imi vatongi!"},{"book":"Vatongi","chapter":"5","verse":"4","text":"“Haiwa Jehovha, pamakabuda kubva muSeiri,"},{"book":"Vatongi","chapter":"5","verse":"5","text":"Makomo akadengenyeka pamberi paJehovha, Iye weSinai,"},{"book":"Vatongi","chapter":"5","verse":"6","text":"“Mumazuva aShamugari mwanakomana waAnati,"},{"book":"Vatongi","chapter":"5","verse":"7","text":"Upenyu hwomumisha hwakapera,"},{"book":"Vatongi","chapter":"5","verse":"8","text":"Pavakasarudza vamwari vatsva,"},{"book":"Vatongi","chapter":"5","verse":"9","text":"Mwoyo wangu uri pamachinda eIsraeri,"},{"book":"Vatongi","chapter":"5","verse":"10","text":"“Imi munotasva mbongoro chena,"},{"book":"Vatongi","chapter":"5","verse":"11","text":"inzwi ravaimbi panzvimbo dzemvura."},{"book":"Vatongi","chapter":"5","verse":"12","text":"‘Muka, muka, Dhibhora! Muka,"},{"book":"Vatongi","chapter":"5","verse":"13","text":"“Ipapo varume vakanga vasara"},{"book":"Vatongi","chapter":"5","verse":"14","text":"Vamwe vakauya vachibva kwaEfuremu, midzi yavo yakanga iri muna Amareki;"},{"book":"Vatongi","chapter":"5","verse":"15","text":"Machinda aIsakari akanga ana Dhibhora;"},{"book":"Vatongi","chapter":"5","verse":"16","text":"Seiko wakagara pakati pezvoto kuti"},{"book":"Vatongi","chapter":"5","verse":"17","text":"Gireadhi akagara mhiri kwaJorodhani."},{"book":"Vatongi","chapter":"5","verse":"18","text":"Vanhu veZebhuruni vakaisa upenyu hwavo panjodzi;"},{"book":"Vatongi","chapter":"5","verse":"19","text":"“Madzimambo akauya, akarwa;"},{"book":"Vatongi","chapter":"5","verse":"20","text":"Kubva kumatenga nyeredzi dzakarwa,"},{"book":"Vatongi","chapter":"5","verse":"21","text":"Rwizi Kishoni rwakavakukura,"},{"book":"Vatongi","chapter":"5","verse":"22","text":"Ipapo mahwanda amabhiza akatinhira,"},{"book":"Vatongi","chapter":"5","verse":"23","text":"Mutumwa waJehovha akati, ‘Tuka Merozi."},{"book":"Vatongi","chapter":"5","verse":"24","text":"“Jaeri ngaave mukadzi akaropafadzwa zvikuru,"},{"book":"Vatongi","chapter":"5","verse":"25","text":"Akakumbira mvura, iye akamupa mukaka;"},{"book":"Vatongi","chapter":"5","verse":"26","text":"Ruoko rwake rwakatambanudzirwa pambambo yetende,"},{"book":"Vatongi","chapter":"5","verse":"27","text":"Patsoka dzake, akanyura,"},{"book":"Vatongi","chapter":"5","verse":"28","text":"“Napawindo mai vaSisera vakadongorera;"},{"book":"Vatongi","chapter":"5","verse":"29","text":"Vakadzi vokwake vakachenjera kukunda vose vanomupindura;"},{"book":"Vatongi","chapter":"5","verse":"30","text":"‘Ko, havasi kuwana nokugoverana zvakapambwa here vachiti,"},{"book":"Vatongi","chapter":"5","verse":"31","text":"“Saka vavengi venyu vose ngavaparare, imi Jehovha!"}]}