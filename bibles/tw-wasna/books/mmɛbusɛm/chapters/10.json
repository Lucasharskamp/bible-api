{"data":[{"book":"Mmɛbusɛm","chapter":"10","verse":"1","text":"Salomo mmɛbusɛm:"},{"book":"Mmɛbusɛm","chapter":"10","verse":"2","text":"Ɛkwan bɔne so ahonya nni boɔ,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"3","text":"Awurade remma ɛkɔm nne ɔteneneeni"},{"book":"Mmɛbusɛm","chapter":"10","verse":"4","text":"Nsa a ɛnyɛ adwuma ma onipa di hia,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"5","text":"Deɛ ɔboaboa nnɔbaeɛ ano wɔ ahuhuro berɛ no yɛ ɔba nyansafoɔ"},{"book":"Mmɛbusɛm","chapter":"10","verse":"6","text":"Teneneefoɔ hyɛ nhyira kyɛ"},{"book":"Mmɛbusɛm","chapter":"10","verse":"7","text":"Ɔteneneeni nkaeɛ yɛ nhyira,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"8","text":"Akoma mu nyansafoɔ tie ɔhyɛ nsɛm,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"9","text":"Ɔnokwafoɔ nante dwoodwoo,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"10","text":"Deɛ ɔde nitan bu nʼani no de ɔhaw ba,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"11","text":"Ɔteneneeni anom yɛ nkwa asutire,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"12","text":"Ɔtan hwanyan mpaapaemu,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"13","text":"Wohunu nyansa wɔ nhunumufoɔ anom,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"14","text":"Anyansafoɔ kora nimdeɛ,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"15","text":"Adefoɔ ahonya yɛ wɔn kuropɔn a wɔabɔ ho ban,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"16","text":"Ateneneefoɔ akatua de nkwa brɛ wɔn,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"17","text":"Deɛ ɔtie nteteeɛ pa no kyerɛ nkwa kwan,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"18","text":"Deɛ ɔkata nitan soɔ no yɛ ɔtorofoɔ,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"19","text":"Mfomsoɔ mpa ɔkasa bebree mu,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"20","text":"Ɔteneneeni tɛkrɛma yɛ dwetɛ amapa"},{"book":"Mmɛbusɛm","chapter":"10","verse":"21","text":"Teneneefoɔ ano ho mfasoɔ ma bebree aduane,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"22","text":"Awurade nhyira de ahonya ba,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"23","text":"Bɔneyɛ yɛ anigyedeɛ ma ɔkwasea,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"24","text":"Deɛ amumuyɛfoɔ suro no bɛba wɔn so;"},{"book":"Mmɛbusɛm","chapter":"10","verse":"25","text":"Sɛ ahum no bɛtwam a, amumuyɛfoɔ yera,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"26","text":"Sɛdeɛ nsã kekakeka ka ɛse, na wisie kɔ ani no,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"27","text":"Awurade suro ma nkwa tenten,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"28","text":"Teneneefoɔ anidasoɔ de ahotɔ ba,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"29","text":"Awurade ɛkwan yɛ dwanekɔbea ma ɔteneneeni,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"30","text":"Wɔrentɔre teneneefoɔ ase da,"},{"book":"Mmɛbusɛm","chapter":"10","verse":"31","text":"Nyansa firi teneneeni anom ba"},{"book":"Mmɛbusɛm","chapter":"10","verse":"32","text":"Teneneefoɔ ano nim adeɛ a ɛfata"}]}