{"data":[{"book":"Nnwom","chapter":"7","verse":"1","text":"Ao, Awurade me Onyankopɔn,"},{"book":"Nnwom","chapter":"7","verse":"2","text":"anyɛ saa a wɔbɛtete me mu sɛ gyata"},{"book":"Nnwom","chapter":"7","verse":"3","text":"Ao, Awurade me Onyankopɔn, sɛ mayɛ yei"},{"book":"Nnwom","chapter":"7","verse":"4","text":"sɛ mayɛ bɔne atia deɛ ɔne me te yie"},{"book":"Nnwom","chapter":"7","verse":"5","text":"ɛnneɛ ma mʼatamfoɔ ntaa me na wɔmmɛto me;"},{"book":"Nnwom","chapter":"7","verse":"6","text":"Sɔre wʼabufuo mu, Ao Awurade!"},{"book":"Nnwom","chapter":"7","verse":"7","text":"Ma nnipadɔm mmɛtwa wo ho nhyia."},{"book":"Nnwom","chapter":"7","verse":"8","text":"ma Awurade mmu nnipa no atɛn."},{"book":"Nnwom","chapter":"7","verse":"9","text":"Ao Onyankopɔn teneneeni,"},{"book":"Nnwom","chapter":"7","verse":"10","text":"Mʼakokyɛm ne Ɔsorosoro Onyankopɔn"},{"book":"Nnwom","chapter":"7","verse":"11","text":"Onyankopɔn yɛ ɔtemmufoɔ teneneeni,"},{"book":"Nnwom","chapter":"7","verse":"12","text":"Sɛ wansesa nʼadwene a,"},{"book":"Nnwom","chapter":"7","verse":"13","text":"Wasiesie ne kɔdiawuo akodeɛ;"},{"book":"Nnwom","chapter":"7","verse":"14","text":"Deɛ ɔnyinsɛn amumuyɛ"},{"book":"Nnwom","chapter":"7","verse":"15","text":"Deɛ ɔtu tokuro na ɔyi mu dɔteɛ no"},{"book":"Nnwom","chapter":"7","verse":"16","text":"Ɔhaw a ɔde ba no dane bɔ nʼankasa so;"},{"book":"Nnwom","chapter":"7","verse":"17","text":"Mede aseda bɛma Awurade"}]}