{"data":[{"book":"Zvirevo","chapter":"21","verse":"1","text":"Mwoyo wamambo uri muruoko rwaJehovha;"},{"book":"Zvirevo","chapter":"21","verse":"2","text":"Nzira dzose dzomunhu dzinoita sedzakarurama kwaari,"},{"book":"Zvirevo","chapter":"21","verse":"3","text":"Kuita zvakarurama nokururamisira"},{"book":"Zvirevo","chapter":"21","verse":"4","text":"Meso ana manyawi nomwoyo unozvikudza,"},{"book":"Zvirevo","chapter":"21","verse":"5","text":"Urongwa hwavanoshingaira hunouyisa zvizhinji,"},{"book":"Zvirevo","chapter":"21","verse":"6","text":"Pfuma yakaunganidzwa norurimi"},{"book":"Zvirevo","chapter":"21","verse":"7","text":"Kumanikidza kwavakaipa kuchavazvuzvurudzira kure,"},{"book":"Zvirevo","chapter":"21","verse":"8","text":"Nzira yeane mhosva yakaminama,"},{"book":"Zvirevo","chapter":"21","verse":"9","text":"Zviri nani kugara pakona yedenga remba,"},{"book":"Zvirevo","chapter":"21","verse":"10","text":"Munhu akaipa anoshuvira zvakaipa;"},{"book":"Zvirevo","chapter":"21","verse":"11","text":"Kana museki akarangwa vasina mano vanowana uchenjeri;"},{"book":"Zvirevo","chapter":"21","verse":"12","text":"Iye Akarurama anocherechedza imba yeakaipa,"},{"book":"Zvirevo","chapter":"21","verse":"13","text":"Kana munhu akadzivira nzeve dzake kumurombo,"},{"book":"Zvirevo","chapter":"21","verse":"14","text":"Chipo chinopiwa muchivande chinonyaradza hasha,"},{"book":"Zvirevo","chapter":"21","verse":"15","text":"Kana kururamisira kwaitwa, kunouyisa mufaro kuna vakarurama,"},{"book":"Zvirevo","chapter":"21","verse":"16","text":"Munhu anorasika panzira yokunzwisisa"},{"book":"Zvirevo","chapter":"21","verse":"17","text":"Uyo anofarira mafaro achava murombo;"},{"book":"Zvirevo","chapter":"21","verse":"18","text":"Akaipa achava dzikinuro yavakarurama,"},{"book":"Zvirevo","chapter":"21","verse":"19","text":"Zviri nani kugara mugwenga,"},{"book":"Zvirevo","chapter":"21","verse":"20","text":"Mumba mowakachenjera mune zvokudya zvakaisvonaka zvakawanda namafuta,"},{"book":"Zvirevo","chapter":"21","verse":"21","text":"Uyo anotevera kururama norudo anowana upenyu,"},{"book":"Zvirevo","chapter":"21","verse":"22","text":"Munhu akachenjera anorwisa guta ravane simba"},{"book":"Zvirevo","chapter":"21","verse":"23","text":"Uyo anorinda muromo wake norurimi rwake"},{"book":"Zvirevo","chapter":"21","verse":"24","text":"Munhu anozvikudza uye ana manyawi “Mudadi” ndiro zita rake;"},{"book":"Zvirevo","chapter":"21","verse":"25","text":"Kushuva kwesimbe kunova rufu rwake,"},{"book":"Zvirevo","chapter":"21","verse":"26","text":"Muswere wose wezuva anoramba achida zvimwe,"},{"book":"Zvirevo","chapter":"21","verse":"27","text":"Chibayiro chowakaipa chinonyangadza,"},{"book":"Zvirevo","chapter":"21","verse":"28","text":"Chapupu chenhema chichaparara,"},{"book":"Zvirevo","chapter":"21","verse":"29","text":"Munhu akaipa ane chiso chisinganyari,"},{"book":"Zvirevo","chapter":"21","verse":"30","text":"Hapana uchenjeri kana njere kana urongwa"},{"book":"Zvirevo","chapter":"21","verse":"31","text":"Bhiza rinogadzirirwa zuva rokurwa,"}]}