{"data":[{"book":"AmaHubo","chapter":"104","verse":"1","text":"Dumisa uThixo, Oh mphefumulo wami."},{"book":"AmaHubo","chapter":"104","verse":"2","text":"Uyazigoqela ngokukhanya kungathi njengesivunulo;"},{"book":"AmaHubo","chapter":"104","verse":"3","text":"anqumise intungo zezindlu zangaphezulu zilale phezu kwamanzi."},{"book":"AmaHubo","chapter":"104","verse":"4","text":"Wenza izikhatha zomoya zibe yizithunywa zakhe,"},{"book":"AmaHubo","chapter":"104","verse":"5","text":"Wawugxilisa umhlaba ezisekelweni zawo;"},{"book":"AmaHubo","chapter":"104","verse":"6","text":"Wawembesa ngolwandle njengesivunulo;"},{"book":"AmaHubo","chapter":"104","verse":"7","text":"Kodwa wathi ngokukhwaza amanzi abaleka,"},{"book":"AmaHubo","chapter":"104","verse":"8","text":"ageleza phezu kwezintaba,"},{"book":"AmaHubo","chapter":"104","verse":"9","text":"Wamisa umkhawulo angeke aweqe;"},{"book":"AmaHubo","chapter":"104","verse":"10","text":"Wenza imithombo impompozele amanzi ezindongeni;"},{"book":"AmaHubo","chapter":"104","verse":"11","text":"zipha amanzi kuzozonke izinyamazana zeganga;"},{"book":"AmaHubo","chapter":"104","verse":"12","text":"Izinyoni zomoya zakhela eduze kwamanzi;"},{"book":"AmaHubo","chapter":"104","verse":"13","text":"Uyazithelezela izintaba esezindlini zakhe eziphezulu;"},{"book":"AmaHubo","chapter":"104","verse":"14","text":"Umilisela inkomo utshani,"},{"book":"AmaHubo","chapter":"104","verse":"15","text":"iwayini elijabulisa inhliziyo yomuntu,"},{"book":"AmaHubo","chapter":"104","verse":"16","text":"Izihlahla zikaThixo ziyathelelwa okwaneleyo,"},{"book":"AmaHubo","chapter":"104","verse":"17","text":"Izinyoni zakhela kuyo izidleke zazo;"},{"book":"AmaHubo","chapter":"104","verse":"18","text":"Izintaba eziphakemeyo ngezamagogo,"},{"book":"AmaHubo","chapter":"104","verse":"19","text":"Inyanga yehlukanisa izibanga zomnyaka,"},{"book":"AmaHubo","chapter":"104","verse":"20","text":"Uletha umnyama, kube sebusuku,"},{"book":"AmaHubo","chapter":"104","verse":"21","text":"Izilwane ziyabhonga zifuna inyama yazo"},{"book":"AmaHubo","chapter":"104","verse":"22","text":"Ilanga liyaphuma, zibe sezicatsha;"},{"book":"AmaHubo","chapter":"104","verse":"23","text":"Umuntu abe esephuma aye emsebenzini wakhe,"},{"book":"AmaHubo","chapter":"104","verse":"24","text":"Minengi kanganani imisebenzi yakho, Oh Thixo!"},{"book":"AmaHubo","chapter":"104","verse":"25","text":"Nantuyana ulwandle, uthwaca olungakaya,"},{"book":"AmaHubo","chapter":"104","verse":"26","text":"Kulapho imikhumbi endenda khona isiyale lale,"},{"book":"AmaHubo","chapter":"104","verse":"27","text":"Zonke lezi zikhangele kuwe"},{"book":"AmaHubo","chapter":"104","verse":"28","text":"Uthi ungazipha bese zikubuthela ndawonye;"},{"book":"AmaHubo","chapter":"104","verse":"29","text":"Uthi ungafihla ubuso bakho zitshaywe luvalo;"},{"book":"AmaHubo","chapter":"104","verse":"30","text":"Uthi ungathumela uMoya"},{"book":"AmaHubo","chapter":"104","verse":"31","text":"Sengathi inkazimulo kaThixo ingema lanini;"},{"book":"AmaHubo","chapter":"104","verse":"32","text":"yena okhangela umhlaba uthuthumele,"},{"book":"AmaHubo","chapter":"104","verse":"33","text":"Ngizahlabelela kuThixo impilo yami yonke;"},{"book":"AmaHubo","chapter":"104","verse":"34","text":"Sengathi iminakano yami ingamthokozisa,"},{"book":"AmaHubo","chapter":"104","verse":"35","text":"Kodwa sengathi izoni zinganyamalala emhlabeni"}]}