{"data":[{"book":"Nnwom","chapter":"115","verse":"1","text":"Ɛnyɛ yɛn, Ao Awurade, ɛnyɛ yɛn"},{"book":"Nnwom","chapter":"115","verse":"2","text":"Adɛn enti na aman no bisa sɛ,"},{"book":"Nnwom","chapter":"115","verse":"3","text":"Yɛn Onyankopɔn wɔ ɔsoro;"},{"book":"Nnwom","chapter":"115","verse":"4","text":"Nanso wɔn ahoni yɛ dwetɛ ne sikakɔkɔɔ a"},{"book":"Nnwom","chapter":"115","verse":"5","text":"Wɔwɔ ano, nanso wɔntumi nkasa;"},{"book":"Nnwom","chapter":"115","verse":"6","text":"Wɔwɔ aso, nanso wɔnte asɛm;"},{"book":"Nnwom","chapter":"115","verse":"7","text":"Wɔwɔ nsa, nanso wɔnte nka;"},{"book":"Nnwom","chapter":"115","verse":"8","text":"Wɔn a wɔyɛɛ ahoni no bɛyɛ sɛ wɔn ara,"},{"book":"Nnwom","chapter":"115","verse":"9","text":"Ao Israelfoɔ, momfa mo ho nto Awurade so,"},{"book":"Nnwom","chapter":"115","verse":"10","text":"Ao Aaron fie, momfa mo ho nto Awurade so,"},{"book":"Nnwom","chapter":"115","verse":"11","text":"Mo a mosuro no, momfa mo ho nto Awurade so,"},{"book":"Nnwom","chapter":"115","verse":"12","text":"Awurade kae yɛn na ɔbɛhyira yɛn:"},{"book":"Nnwom","chapter":"115","verse":"13","text":"Ɔbɛhyira wɔn a wɔsuro Awurade,"},{"book":"Nnwom","chapter":"115","verse":"14","text":"Awurade mma wʼase nnɔre,"},{"book":"Nnwom","chapter":"115","verse":"15","text":"Awurade nhyira wo,"},{"book":"Nnwom","chapter":"115","verse":"16","text":"Ɔsorosoro yɛ Awurade dea,"},{"book":"Nnwom","chapter":"115","verse":"17","text":"Ɛnyɛ awufoɔ na wɔkamfo Awurade,"},{"book":"Nnwom","chapter":"115","verse":"18","text":"Yɛn na yɛma Awurade so"}]}